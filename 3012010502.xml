    <create type="Character" obj_id="青葉 初芽" data_id="311005013" x="200" y="0" anchor="Bottom" />
    <fade_in time="1" type="Default" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <msg_window_show show="True" />
    <msg speaker="半蔵門 雪" text="私の後ろに乗りたいだなんて&#xA;急にどうしたっていうの？" speaker_show="True" stop="True" voice="hy11002_02_0341" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" loop="True" />
    <msg speaker="半蔵門 雪" text="バイクのタンデムは運転より後ろの方が&#xA;危ないから、やめておいたほうが良いわ。" speaker_show="True" stop="True" voice="hy11002_02_0342" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" />
    <msg speaker="青葉 初芽" text="そう……ですよね。&#xA;おかしな事を言って、すみませんでした。" speaker_show="True" stop="True" voice="ah11005_02_0295" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="………何かあったの？　初芽。" speaker_show="True" stop="True" voice="hy11002_02_0343" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="違うんです。ただ……以前、モモちゃんから&#xA;雪ちゃんのバイクに乗ったと聞いたので…" speaker_show="True" stop="True" voice="ah11005_02_0296" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="青葉 初芽" text="私も乗ってみたいと思っただけなんです。&#xA;少し、羨ましくなってしまって。" speaker_show="True" stop="True" voice="ah11005_02_0297" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <msg speaker="半蔵門 雪" text="羨ましい？" speaker_show="True" stop="True" voice="hy11002_02_0344" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" />
    <msg speaker="青葉 初芽" text="あ…良いんです。ごめんなさい、私が勝手に&#xA;そう思っただけで………もう、行ってください。" speaker_show="True" stop="True" voice="ah11005_02_0298" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="…………………乗りなさい。" speaker_show="True" stop="True" voice="hy11002_02_0345" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="青葉 初芽" text="…………え？" speaker_show="True" stop="True" voice="ah11005_02_0299" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="半蔵門 雪" text="落ちないように、しっかり掴まっているのよ。" speaker_show="True" stop="True" voice="hy11002_02_0346" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="青葉 初芽" text="あ………はいっ♪" speaker_show="True" stop="True" voice="ah11005_02_0300" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1.0" wait="True" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="青葉 初芽" />
