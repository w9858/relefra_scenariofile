    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="ほら……ね。風がすごいでしょう。&#xA;後悔したんじゃない？" speaker_show="True" stop="True" voice="hy11002_02_0347" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="青葉 初芽" text="いいえ、ちっとも！&#xA;とても気持ちがイイです。" speaker_show="True" stop="True" voice="ah11005_02_0301" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" loop="True" />
    <msg speaker="青葉 初芽" text="雪ちゃんは、いつも１人でこの風を&#xA;感じて走っていたんですね！" speaker_show="True" stop="True" voice="ah11005_02_0302" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽のバイクが、私を走らせてくれるのよ。&#xA;誰よりも……風よりも速く。" speaker_show="True" stop="True" voice="hy11002_02_0348" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <msg speaker="青葉 初芽" text="………？　どうして止まるんですか？&#xA;まだ、海の真ん中ですが。" speaker_show="True" stop="True" voice="ah11005_02_0303" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="景色を見たいかと思って。" speaker_show="True" stop="True" voice="hy11002_02_0349" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="青葉 初芽" text="え？　ぁあ………わああ、これは……&#xA;夕陽に染まった海がキラキラ輝いています！" speaker_show="True" stop="True" voice="ah11005_02_0304" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="半蔵門 雪" text="ツキカゲに入ってからは、初芽と２人でこんなに&#xA;ゆっくり夕陽を眺めた事などなかった。" speaker_show="True" stop="True" voice="hy11002_02_0350" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="半蔵門 雪" text="たまには、こういうのも悪くはないわ。" speaker_show="True" stop="True" voice="hy11002_02_0351" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="08_MouthBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="雪ちゃんの口から、そんなロマンチックな&#xA;言葉が聞けるなんて……もう爆発しても良いです♪" speaker_show="True" stop="True" voice="ah11005_02_0305" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="半蔵門 雪" text="それはやめてちょうだい。" speaker_show="True" stop="True" voice="hy11002_02_0352" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="青葉 初芽" text="雪ちゃん……今日は我が儘を聞いてくれて&#xA;ありがとうございました。" speaker_show="True" stop="True" voice="ah11005_02_0306" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="いいえ。私の方こそ、いつも……ありがとう。" speaker_show="True" stop="True" voice="hy11002_02_0353" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="………さあ、もう戻りましょうか。&#xA;みんなが心配しているといけませんから。" speaker_show="True" stop="True" voice="ah11005_02_0307" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" loop="True" />
    <msg speaker="半蔵門 雪" text="そうね……。" speaker_show="True" stop="True" voice="hy11002_02_0354" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="青葉 初芽" text="（でも……岸に着くまでは、師匠でも弟子でもない&#xA;私の親友の雪ちゃんでいてください……。）" speaker_show="True" stop="True" voice="ah11005_02_0308" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="…………帰りもしっかり掴まっているのよ。" speaker_show="True" stop="True" voice="hy11002_02_0355" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="青葉 初芽" text="ええ……勿論ですよ。雪ちゃん。" speaker_show="True" stop="True" voice="ah11005_02_0309" />
