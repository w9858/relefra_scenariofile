    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="全員集合とはちょうど良かったぜ。お前たち、&#xA;気をぬいてるとこ悪いが急ぎの任務が入ったぞ！" speaker_show="True" stop="True" voice="ty12005_02_0039" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="わっ、早速出動ですか！" speaker_show="True" stop="True" voice="mm11001_02_0064" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="ほほう。今日はどこで何をしろと言うのじゃ？" speaker_show="True" stop="True" voice="ss12003_02_0100" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="とある工場に潜入して、&#xA;設計図を取ってくるんだ。" speaker_show="True" stop="True" voice="ty12005_02_0040" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="その工場は、人形の違法武装を&#xA;作っているとの噂でな。" speaker_show="True" stop="True" voice="ty12005_02_0041" />
