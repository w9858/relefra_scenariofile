    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="わ、私がですか……？&#xA;ちょっと緊張するけど……、頑張ります！" speaker_show="True" stop="True" voice="mm11001_02_0066" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="よし、そうと決まれば早速、&#xA;凪の部隊、進発！" speaker_show="True" stop="True" voice="ss12003_02_0102" />
