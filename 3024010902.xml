    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="300" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="0" anchor="Bottom" />
    <create type="Character" obj_id="白虎" data_id="311007008" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="た、大変！　予算決めるの忘れてた！&#xA;どうしよう、年末でピンチなのよ！" speaker_show="True" stop="True" voice="sf11004_03_0274" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="みんなで割るんだし、そんなに高価な物は&#xA;選ばないと思うけど……。" speaker_show="True" stop="True" voice="ig11006_03_0205" />
    <char_spine_animation obj_id="白虎" face_anim_type="018_Facial_smug" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="白虎" text="わからんゾ～？　初芽のことだ。&#xA;だいあもんどの10キロくらいのを選ぶかもな。" speaker_show="True" stop="True" voice="bk11008_03_0132" />
    <delete obj_id="白虎" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="手首折れ……いや、カトリーナなら平気か。" speaker_show="True" stop="True" voice="tr11007_03_0061" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="な、なに言ってるの、テレちゃん。&#xA;ゴリラじゃないんだから、カトリー……" speaker_show="True" stop="True" voice="mm11001_03_0310" />
    <delete obj_id="テレジア" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="あら、動物園の話？" speaker_show="True" stop="True" voice="kt11009_03_0126" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" position_id="left" x="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふおっ、お帰りー！&#xA;やー、いつの間に？　さすが一流スパイ。" speaker_show="True" stop="True" voice="ym11003_03_0262" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="今だけど、どうかしたの？" speaker_show="True" stop="True" voice="kt11009_03_0127" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="いえその！　ゴリ…ゴ、ごごご５時だなー！&#xA;もうすぐ５時だ夕方だーーって！" speaker_show="True" stop="True" voice="mm11001_03_0311" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="え？　まだ３時だけど……。" speaker_show="True" stop="True" voice="tr11007_03_0062" />
