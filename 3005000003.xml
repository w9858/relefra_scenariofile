    <bg show="True" resource_type="Background" file="23_rivermo_01" effect="Immediately" wait="False" />
    <transition effect="Default" type="In" color_change="False" time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="トレーニング終了後" stop="True" />
