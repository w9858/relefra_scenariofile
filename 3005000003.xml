    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="飛粋ちゃん、いよいよ今日からツキカゲの訓練に&#xA;参加してもらいますよ。" speaker_show="True" stop="True" voice="ah11005_02_0203" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="はい！　ワクワクしてきました。" speaker_show="True" stop="True" voice="mh12001_02_0067" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="カトリーナ" data_id="311009009" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="モモちゃんと入れ替えで来たから、&#xA;指導係は雪ちゃんってことでどうかしら？" speaker_show="True" stop="True" voice="kt11009_02_0078" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="そうね。ではモモが私と一緒に普段から行っている&#xA;トレーニングをやりましょう。" speaker_show="True" stop="True" voice="hy11002_02_0131" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="はい！&#xA;雪さん、まずは何から始めましょうか？" speaker_show="True" stop="True" voice="mh12001_02_0068" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="まずは軽く、ロードワークに川での水泳、&#xA;ウェイトトレーニングから始めましょうか。" speaker_show="True" stop="True" voice="hy11002_02_0132" />
    <move obj_id="半蔵門 雪" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="False" />
    <move obj_id="宗近 飛粋" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="うへぇ。全然軽くないフルコース。ヒッスー、&#xA;キツくなったらこっそりメイに言うんだよ。" speaker_show="True" stop="True" voice="ym11003_02_0098" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="宗近 飛粋" text="メイちゃん、ありがとう。けど大丈夫。&#xA;私、凪の部隊の代表として、頑張るよ！" speaker_show="True" stop="True" voice="mh12001_02_0069" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="いい心構えね。&#xA;ではついてきなさい。" speaker_show="True" stop="True" voice="hy11002_02_0133" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
