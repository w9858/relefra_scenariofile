    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="-1000" y="0" anchor="Bottom" />
    <move obj_id="宗近 飛粋" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="よし、ゴールだー！" speaker_show="True" stop="True" voice="mh12001_02_0070" />
    <move obj_id="宗近 飛粋" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="さすが凪の部隊の一員なだけあって&#xA;フィジカル面には何の問題もないようね。" speaker_show="True" stop="True" voice="hy11002_02_0134" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ありがとうございます！&#xA;次は何をしましょうか！　何なりと！" speaker_show="True" stop="True" voice="mh12001_02_0071" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="次は話術の訓練よ。" speaker_show="True" stop="True" voice="hy11002_02_0135" />
    <transition time="0.5" wait="True" />
    <bg file="09_cafe_02" />
    <transition type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="私がこれを飲み終えるまでに、誰か１人&#xA;新しい連絡先をゲットしてきなさい。" speaker_show="True" stop="True" voice="hy11002_02_0136" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="16_BendBackward" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="えぇ、ツキカゲではそんな訓練を！&#xA;って言ってる間に、もうコーヒーが半分に！" speaker_show="True" stop="True" voice="mh12001_02_0072" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="急がなきゃ！" speaker_show="True" stop="True" voice="mh12001_02_0073" />
    <msg_window_show show="False" />
    <move obj_id="宗近 飛粋" position_id="left" x="1000" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <fade_out time="1.0" type="Default" wait="True" />
    <bg show="True" resource_type="Background" file="09_cafe_01" effect="Immediately" wait="False" color="#ffffffff" effect_time="0.3" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="宗近 飛粋" />
