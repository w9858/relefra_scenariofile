    <msg speaker="半蔵門 雪" text="（何度もやっているせいか&#xA;もうすっかり慣れているわね）" speaker_show="True" stop="True" voice="hy11002_02_0603" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ここまでにしましょう。" speaker_show="True" stop="True" voice="hy11002_02_0604" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="015_Facial_poker" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あれ、今日はこれで終わりですか？" speaker_show="True" stop="True" voice="mm11001_02_0742" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="ええ。お疲れ様。" speaker_show="True" stop="True" voice="hy11002_02_0605" />
