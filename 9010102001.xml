    <msg speaker="半蔵門 雪" text="じゃ、いつものように私と軽く手合わせを。&#xA;いくわよ、モモ。" speaker_show="True" stop="True" voice="hy11002_02_0601" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="はい、師匠！　……やあっ！　……とうっ！" speaker_show="True" stop="True" voice="mm11001_02_0740" />
