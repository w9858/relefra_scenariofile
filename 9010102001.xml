    <msg speaker="半蔵門 雪" text="油断大敵よ、モモ。" speaker_show="True" stop="True" voice="hy11002_02_0607" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="師匠!?　これ、やっぱり師匠だったんですね？" speaker_show="True" stop="True" voice="mm11001_02_0746" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ええ。登校中も自覚を持ちなさい。" speaker_show="True" stop="True" voice="hy11002_02_0608" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そのために？　でも、こんな普通の道に仕掛けて、&#xA;もし他の人が引っかかったらどうするんですか？" speaker_show="True" stop="True" voice="mm11001_02_0747" />
