    <msg speaker="園長" text="さあ、着いたわよ。&#xA;中に入ってちょうだい。" speaker_show="True" stop="True" voice="99026_02_0009" />
    <create type="Character" obj_id="相模 楓" data_id="311004013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004007" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="わぁ、可愛い動物たちがいっぱい！" speaker_show="True" stop="True" voice="th12004_02_0122" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="（あれ……？　けどこの子たちって……。）" speaker_show="True" stop="True" voice="th12004_02_0123" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="（みんなワシントン条約で輸出入の制限が&#xA;されている動物だ……。）" speaker_show="True" stop="True" voice="sf11004_02_0095" lip_sync="False" />
    <msg speaker="園長" text="今日からここに居る動物たちの世話を任せたいの。&#xA;お願いできる？" speaker_show="True" stop="True" voice="99026_02_0010" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="えっと……この動物たちは？" speaker_show="True" stop="True" voice="sf11004_02_0096" />
    <msg speaker="園長" text="この子たちは、今ではもう数も少なくなってしまった&#xA;とっても貴重な動物たちでね。" speaker_show="True" stop="True" voice="99026_02_0011" />
    <msg speaker="園長" text="これから、色々な保護地区や研究施設へと&#xA;渡っていくの。" speaker_show="True" stop="True" voice="99026_02_0012" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="そんな貴重な動物たちが、どうしてこの動物園に？" speaker_show="True" stop="True" voice="th12004_02_0124" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="園長" text="一時的な飼育場所として、数ある動物園の中から&#xA;うちが選ばれたのよ。" speaker_show="True" stop="True" voice="99026_02_0013" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="園長" text="スタッフの動物たちへの愛情が認められたからこそ、&#xA;こんな大役を任されたんだと思うと、嬉しくってね。" speaker_show="True" stop="True" voice="99026_02_0014" />
    <msg speaker="園長" text="だからここにいる間だけでも、この子たちには&#xA;幸せな時間を過ごしてもらいたいの。" speaker_show="True" stop="True" voice="99026_02_0015" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="園長さん……。" speaker_show="True" stop="True" voice="sf11004_02_0097" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="きっと大丈夫ですよ！　ここにいる間の&#xA;この子たちは本当に幸せだと思いますよ！" speaker_show="True" stop="True" voice="th12004_02_0125" />
    <msg speaker="園長" text="だったら、仕事冥利に尽きるね。" speaker_show="True" stop="True" voice="99026_02_0016" />
