    <msg speaker="源 モモ" text="おお、四聖獣のポーズ！" speaker_show="True" stop="True" voice="mm11001_01_1008" />
    <char_spine_animation obj_id="白虎" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" face_anim_type="003_Facial_laugh" />
    <msg speaker="白虎" text="わーっはっはっはっは！" speaker_show="True" stop="True" voice="bk11008_01_0267" />
