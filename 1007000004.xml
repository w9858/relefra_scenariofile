    <create type="Character" obj_id="アミィ" data_id="313002003" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="おぉ、あの二人も可愛い。" speaker_show="True" stop="True" voice="sr13001_02_0040" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="空崎は可愛い子が多いのねぇ。&#xA;こりゃあ俄然張り切っちゃうわ。" speaker_show="True" stop="True" voice="sr13001_02_0041" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="まずは表の仕事を片付けていきましょう。&#xA;折角関東に来たんです。会食がありますわ。" speaker_show="True" stop="True" voice="sm13002_02_0020" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ほいほい。明日の顔合わせを楽しみにしつつ&#xA;こなしていきましょーかい。アミィ。" speaker_show="True" stop="True" voice="sr13001_02_0042" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="はっ、どこまでもサポートする所存。" speaker_show="True" stop="True" voice="am13002_02_0018" />
