    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="-1000" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="-1000" y="0" anchor="Bottom" />
    <sound type="Se" cue_name="Walk" />
    <move obj_id="源 モモ" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="3" wait="False" />
    <move obj_id="半蔵門 雪" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="3" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あれれ。師匠、あの人達って。" speaker_show="True" stop="True" voice="mm11001_02_0001" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="敷島姉妹だわ、初芽から聞いていたＺＥＴね。&#xA;空崎に前乗りしてきたのか。" speaker_show="True" stop="True" voice="hy11002_02_0001" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="明日ご挨拶って予定ですけど、どうします？" speaker_show="True" stop="True" voice="mm11001_02_0002" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="それまでは、知らない振りをしている方が&#xA;ややこしくなくていいでしょう。" speaker_show="True" stop="True" voice="hy11002_02_0002" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="分かりました師匠。自然に自然に。" speaker_show="True" stop="True" voice="mm11001_02_0003" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="（……本当に、自然に出来ているわね、&#xA;はじめに言葉に出さなければ完璧だけども。）" speaker_show="True" stop="True" voice="hy11002_02_0003" lip_sync="False" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="True" time="1.0" wait="True" />
