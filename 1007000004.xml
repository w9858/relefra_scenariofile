    <transition type="In" color_change="True" wait="True" effect="Default" time="1.0" />
    <telop text="東扇島公園" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="アミィ" data_id="313002003" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="おぉー。工業地帯の眺めっていうのは最高ね。" speaker_show="True" stop="True" voice="sr13001_02_0039" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="ええ。落ち着きますわ。漂うケミカルな匂いも。" speaker_show="True" stop="True" voice="sm13002_02_0019" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="工場で働いている人形を見ると和みまする。" speaker_show="True" stop="True" voice="am13002_02_0017" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="True" time="1.0" wait="True" />
    <bg show="True" resource_type="Background" file="08_parkni_01" effect="Immediately" />
    <delete obj_id="敷島 来夢" />
    <delete obj_id="敷島 真留瀬" />
