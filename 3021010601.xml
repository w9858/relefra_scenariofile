    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <move obj_id="石川 五恵" position_id="left" x="0" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <msg speaker="石川 五恵" text="(葉栖美さん、慣れてる……。&#xA;これが１週間の差。私も頑張らないと。)" speaker_show="True" stop="True" voice="ig11006_03_0095" lip_sync="False" />
    <msg speaker="ドレスの御婦人" text="今週は株で２億ほど損をしたから、&#xA;ここで取り戻さないといけないわねぇ。" speaker_show="True" stop="True" voice="99025_03_0001" />
    <msg speaker="宝石の御婦人" text="なぁんだ、その程度？　私はまた&#xA;１兆くらいドブに捨てたのかと思ったわぁ。" speaker_show="True" stop="True" voice="99026_03_0001" />
    <msg speaker="御婦人達" text="オホホホホホ！" speaker_show="True" stop="True" voice="3021010601_union_001" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="(つ、ついていけない……。師匠もお金持ちだけど&#xA;こんな話したことないし……どう絡めば良いの？)" speaker_show="True" stop="True" voice="ig11006_03_0096" lip_sync="False" />
    <msg speaker="ウェイトレス" text="開店と同時にいらして下さり、ありがとうございます。&#xA;ウェルカムシャンパンのサービスでございます。" speaker_show="True" stop="True" voice="99021_03_0002" />
