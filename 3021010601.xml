    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="わぁ…開店したばかりなのにお客さんが&#xA;いっぱい入って来た……。" speaker_show="True" stop="True" voice="ig11006_03_0093" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="彼らはみんな会員。暇を持て余した&#xA;超大金持ちさん達だよ。" speaker_show="True" stop="True" voice="th12004_03_0061" />
    <msg speaker="恰幅の良い紳士" text="さあ、まずは両替をしなくてはな。&#xA;お嬢さん、これを頼む。" speaker_show="True" stop="True" voice="99024_03_0001" />
    <sound type="Se" cue_name="Clash" />
    <wait time="0.2" />
    <sound type="Se" cue_name="Clash" />
    <wait time="1" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="こ、これを全部ですか……!?&#xA;１…10…100……い、１千万円。" speaker_show="True" stop="True" voice="ig11006_03_0094" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="キャッシャーはあちらなのですが&#xA;特別に代行させていただきますね。" speaker_show="True" stop="True" voice="th12004_03_0062" />
    <msg speaker="恰幅の良い紳士" text="おお、すまんな。これはチップだ。&#xA;取っておきたまえ。" speaker_show="True" stop="True" voice="99024_03_0002" />
    <sound type="Se" cue_name="Clash" />
    <wait time="0.2" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ありがとうございます♪&#xA;では、すぐに行って参ります。" speaker_show="True" stop="True" voice="th12004_03_0063" />
