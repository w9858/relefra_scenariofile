    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="ふふっ……ありがとうございます、師匠。" speaker_show="True" stop="True" voice="ig11006_01_0569" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="こちらこそ。&#xA;これからもよろしくお願いしますね、ごえちゃん。" speaker_show="True" stop="True" voice="ah11005_01_0639" />
