    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="白虎" text="２匹？" speaker_show="True" stop="True" voice="bk11008_01_0248" />
    <delete obj_id="白虎" />
    <create type="Character" anchor="Bottom" obj_id="石川 五恵" data_id="311006006" position_id="left" x="-200" />
