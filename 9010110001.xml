  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="10_terminalmo_01" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="じゃあ、結愛、凪ちゃん、またね～。&#xA;さーて、私も帰ろう……っと、信先輩!?" speaker_show="True" stop="True" voice="mm11001_02_0923" />
    <move obj_id="源 モモ" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ん？　どうしたの、モモちゃん？" speaker_show="True" stop="True" voice="ks11010_02_0324" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、いえ……いつから見てたんですか？&#xA;見かけたなら声をかけてくれれば……。" speaker_show="True" stop="True" voice="mm11001_02_0924" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="え？　ずっと一緒にいたよ？" speaker_show="True" stop="True" voice="ks11010_02_0325" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="え？　ずっと……ですか？&#xA;す、すみません、気がつかなくて……。" speaker_show="True" stop="True" voice="mm11001_02_0925" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ああ、気にしなくていいよ～。&#xA;メイちゃんにも気づかれないこと、よくあるし。" speaker_show="True" stop="True" voice="ks11010_02_0326" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そっか、メイちゃんも気づかないのか……。" speaker_show="True" stop="True" voice="mm11001_02_0926" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="って、それってすごくないですか？&#xA;メイちゃんからも気配を消すなんて！" speaker_show="True" stop="True" voice="mm11001_02_0927" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="えへへ、それほどでもないよ。&#xA;気配を消すのは通常運転だもん。" speaker_show="True" stop="True" voice="ks11010_02_0327" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="それに、メイちゃんほどの身体能力がないぶん、&#xA;隠密行動が得意ってだけだから。" speaker_show="True" stop="True" voice="ks11010_02_0328" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="でもでも、すごいですよ、信先輩！&#xA;私にもそんな能力があればなぁ……。" speaker_show="True" stop="True" voice="mm11001_02_0928" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="モモちゃんはむしろ存在感あるほうだよね～&#xA;でも、普通はそのほうがいいんじゃないの？" speaker_show="True" stop="True" voice="ks11010_02_0329" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そうなんですか？　スパイとしては気配を&#xA;消せたほうがいいけどなあ……。羨ましいですよ。" speaker_show="True" stop="True" voice="mm11001_02_0929" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="モモちゃんにはモモちゃんの良さが&#xA;あると思うけど……。" speaker_show="True" stop="True" voice="ks11010_02_0330" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="例えばどんなことですか？" speaker_show="True" stop="True" voice="mm11001_02_0930" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="んー、そうだなあ……舌や鼻が敏感とか……。" speaker_show="True" stop="True" voice="ks11010_02_0331" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ～、やっぱりそこかぁ～。&#xA;みんなからも、よく言われるんですよ。" speaker_show="True" stop="True" voice="mm11001_02_0931" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="わかりやすい特徴だからね。&#xA;立派な特技だと思うよ。" speaker_show="True" stop="True" voice="ks11010_02_0332" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="でも、もっと他のところも&#xA;褒めてもらえるようになりたいんです。" speaker_show="True" stop="True" voice="mm11001_02_0932" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="他のところ、かあ……。&#xA;そういうのは自然と身につくものだからねえ。" speaker_show="True" stop="True" voice="ks11010_02_0333" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="もちろん自ら習得するものもあると思うけど。" speaker_show="True" stop="True" voice="ks11010_02_0334" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="でも、他の人たちみたいに変装とか話術とか……。" speaker_show="True" stop="True" voice="mm11001_02_0933" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そんなに焦らなくて大丈夫。&#xA;ツキカゲの中で身につくものも多いだろうから。" speaker_show="True" stop="True" voice="ks11010_02_0335" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そうですか……。あの、その辺りのお話、&#xA;このあと、もっと聞かせてもらえませんか？" speaker_show="True" stop="True" voice="mm11001_02_0934" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うーん、でもこんな時間だから、&#xA;そろそろ帰ったほうがいいんじゃない？" speaker_show="True" stop="True" voice="ks11010_02_0336" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="そうですね……。&#xA;それじゃ、次のときに聞かせてもらえますか？" speaker_show="True" stop="True" voice="mm11001_02_0935" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="もちろん。それじゃ、また。" speaker_show="True" stop="True" voice="ks11010_02_0337" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="はい。失礼します。&#xA;……あ、そうだ、信先輩！" speaker_show="True" stop="True" voice="mm11001_02_0936" />
    <delete obj_id="高坂 信" />
    <move obj_id="源 モモ" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="……って、もういない!?&#xA;今の今まで話していたのに……。" speaker_show="True" stop="True" voice="mm11001_02_0937" />
    <move obj_id="源 モモ" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="じゃ～ん、まだいましたー！&#xA;って、少しは参考になった？" speaker_show="True" stop="True" voice="ks11010_02_0338" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="参考も何も……さすがですね、信先輩！&#xA;私も見習わなくっちゃ！" speaker_show="True" stop="True" voice="mm11001_02_0938" />
  </command>
</adv>