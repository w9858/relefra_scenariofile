    <msg speaker="白虎" text="おかしいぞ、このトナカイ型ロボ。時間が経てば&#xA;経つほど、こっちの攻撃が通らなくなってく！" speaker_show="True" stop="True" voice="bk11008_03_0124" />
    <create type="Character" obj_id="ニコラス" data_id="398010001" position_id="left" x="-400" />
