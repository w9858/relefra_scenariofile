    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="ゆら、そっちお願い！" speaker_show="True" stop="True" voice="mh12001_02_0243" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="おうよ、任せな!!" speaker_show="True" stop="True" voice="ry12002_02_0256" />
    <sound type="Se" cue_name="Slash" />
    <Comment comment="//【SE】敵を攻撃する音" />
    <wait time="0.5" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="飛粋！&#xA;敵さん、今度はそっち行ったぞ！" speaker_show="True" stop="True" voice="ry12002_02_0257" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="了解！" speaker_show="True" stop="True" voice="mh12001_02_0244" />
    <sound type="Se" cue_name="Slash" />
    <Comment comment="//【SE】敵を攻撃する音" />
    <wait time="0.5" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="……しかし、最初は敵対してた２人が&#xA;力を合わせて戦う、か……" speaker_show="True" stop="True" voice="ry12002_02_0258" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="なんか飛粋とアタシ様みたいだな！" speaker_show="True" stop="True" voice="ry12002_02_0259" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="うんうん、確かに！" speaker_show="True" stop="True" voice="mh12001_02_0245" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="あんときゃ色々あったなー。&#xA;覚えてっか？ 最初は稚内の人形製造工場で……" speaker_show="True" stop="True" voice="ry12002_02_0260" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="乱獅子 ゆら" />
    <delete obj_id="宗近 飛粋" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="……で、そんな苦境を乗り越えた末に、&#xA;飛粋とアタシ様は唯一無二のダチになったってわけだ！" speaker_show="True" stop="True" voice="ry12002_02_0261" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="何度思い返してもいい話だなー。&#xA;泣いちゃいそうだぜ。な、飛粋？" speaker_show="True" stop="True" voice="ry12002_02_0262" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="……うん……だねー……むにゃ……" speaker_show="True" stop="True" voice="mh12001_02_0246" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="乱獅子 ゆら" text="だよなだよなー！&#xA;いやーほんと良い響きだなー「ダチ」！" speaker_show="True" stop="True" voice="ry12002_02_0263" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <create_effect effect_id="mampsleep_hisui" effect_type="MampSleep" life_time="90" target_obj="宗近 飛粋" />
    <msg speaker="宗近 飛粋" text="……う……ん……ＺＺＺ……" speaker_show="True" stop="True" voice="mh12001_02_0247" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="あ、ダチといや、こないだ隊長が&#xA;言ってたんだけどさー" speaker_show="True" stop="True" voice="ry12002_02_0264" />
    <msg speaker="宗近 飛粋" text="……ＺＺＺ……" speaker_show="True" stop="True" voice="mh12001_02_0248" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <msg speaker="宗近 飛粋" text="寝落ちた飛粋をよそに、ゆらのマシンガントークは&#xD;&#xA;夜明けまで続いたという……。" speaker_show="True" stop="True" window_type="SystemMsg" />
