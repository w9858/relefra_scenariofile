    <char_spine_animation obj_id="高坂 信" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="高坂 信" text="おおー、なるほど。&#xA;映画のワンシーンみたい。" speaker_show="True" stop="True" voice="ks11010_02_0180" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="というか、メイちゃん本当に可愛いよね。&#xA;こんなの見とれちゃうよ。" speaker_show="True" stop="True" voice="ks11010_02_0181" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="もー、嬉しいけど&#xA;師匠がやらなくちゃなんだからね？" speaker_show="True" stop="True" voice="ym11003_02_0365" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="メイが可愛いのは、&#xA;よ～～く知ってるから！" speaker_show="True" stop="True" voice="ym11003_02_0366" />
