    <msg speaker="敷島 来夢" text="さて～♪　午後の～♪　私の～♪&#xA;活動は～～♪" speaker_show="True" stop="True" voice="sr13001_02_0171" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="なぜゆえに～♪　ミュージカル風の～♪&#xA;受け答えに～♪" speaker_show="True" stop="True" voice="sm13002_02_0093" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="まぁそっちの方が聞いている側も楽しいかなって。&#xA;社長としてエンターテイメントを気にするのよ。" speaker_show="True" stop="True" voice="sr13001_02_0172" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="普通に喋って頂ければ大丈夫ですわ、お姉様。" speaker_show="True" stop="True" voice="sm13002_02_0094" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="午後の仕事は、まず妹の胸を揉むことからはじまる。&#xA;シュシュシュシュシュシュシュ！！！" speaker_show="True" stop="True" voice="sr13001_02_0173" />
    <move obj_id="敷島 来夢" x="-80" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <create_effect effect_id="EXmalu" effect_type="MampExclamation" life_time="1" target_obj="敷島 真留瀬" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <char_spine_cheek obj_id="敷島 真留瀬" type="Cheek03_strong" loop="True" />
    <msg speaker="敷島 真留瀬" text="ふわぁんっ！　くぅ、ハレンチな声を&#xA;出してしまいました。ここは使えませんわね。" speaker_show="True" stop="True" voice="sm13002_02_0095" />
    <char_spine_cheek obj_id="敷島 真留瀬" loop="True" />
    <move obj_id="敷島 来夢" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="午後も基本は研究よ。今、主にやってるのは&#xA;宇宙で精密に稼働できる人形。" speaker_show="True" stop="True" voice="sr13001_02_0174" />
