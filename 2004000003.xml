    <msg window_type="SystemMsg" text="甲冑はパーツごとに来夢に射出されていき&#xA;あっという間に装着が行われていく。" stop="True" />
    <msg window_type="SystemMsg" text="機械で出来た無敵甲冑には武器だけでなく&#xA;飛行機能やステルス機能も搭載されていた。" stop="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="じゃあノブナガ、夜のパトロールにいってくるわね。" speaker_show="True" stop="True" voice="sr13001_02_0183" />
    <move obj_id="敷島 来夢" x="-1000" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
