    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="そう、昼は社長。夜は正義の私設情報機関&#xA;ＺＥＴのリーダー。" speaker_show="True" stop="True" voice="sm13002_02_0103" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="自身で発明した無敵甲冑に身を包み&#xA;人形を悪用しようとする犯罪者を倒す……。" speaker_show="True" stop="True" voice="sm13002_02_0104" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
