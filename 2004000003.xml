    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="夕方になると他のフロアにうつってミーティング。" speaker_show="True" stop="True" voice="sr13001_02_0175" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="めまぐるしく会う人が変わっていきますわ。" speaker_show="True" stop="True" voice="sm13002_02_0096" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="私は一芸に秀でている人が好きなのよ。&#xA;その長所を人形の…敷島のために活かして欲しいわ。" speaker_show="True" stop="True" voice="sr13001_02_0176" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様は子供の頃から人形が大好きで。&#xA;明晰な頭脳のほとんどが人形工学に向けられました。" speaker_show="True" stop="True" voice="sm13002_02_0097" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="結果、わずか10代にして数々の発明を&#xA;成し遂げた、敷島来夢という天才が出現したのです。" speaker_show="True" stop="True" voice="sm13002_02_0098" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="私はお姉様が好きだから、ずっと後を追って&#xA;同じ事をやってきたのですが……。" speaker_show="True" stop="True" voice="sm13002_02_0099" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="いつしか限界を感じて、秘書として&#xA;サポートする事を選んだのです。" speaker_show="True" stop="True" voice="sm13002_02_0100" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="人形知識については来夢ガールズが、お姉様の良き&#xA;相談役になっていますから、一安心ですわ。" speaker_show="True" stop="True" voice="sm13002_02_0101" />
