    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="さて、夜になっても敷島来夢は眠らない。&#xA;……なんてことはなくて！" speaker_show="True" stop="True" voice="sr13001_02_0177" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="定時すぎたらキッチリ休む。自分も社員も。&#xA;休暇や睡眠は大事だからね。これ敷島一族の教えよ。" speaker_show="True" stop="True" voice="sr13001_02_0178" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="夜は本とか読んでいるわ。&#xA;同じく愛知生まれの長岡歌穂さんの小説とか。" speaker_show="True" stop="True" voice="sr13001_02_0179" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="以上、敷島若社長の一日でしたー。&#xA;コツは楽しむことよ、何事も。楽しんだもの勝ち。" speaker_show="True" stop="True" voice="sr13001_02_0180" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="はい。お疲れ様でしたお姉様。&#xA;良いインタビューになると思いますわ。" speaker_show="True" stop="True" voice="sm13002_02_0102" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ふっふっふ。夜は本を読むだけじゃないんだな。&#xA;皆には秘密だけども。" speaker_show="True" stop="True" voice="sr13001_02_0181" />
    <msg window_type="SystemMsg" text="敷島来夢が端末のスイッチを押すと、&#xA;部屋に隠されていた無敵甲冑が出現する。" stop="True" />
    <move obj_id="敷島 来夢" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
