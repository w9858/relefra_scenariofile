    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" lip_sync="False" text="（あれは、メイ……？&#xA;そうか、今日はストリートライブの日……。）" speaker_show="True" stop="True" voice="hy11002_02_0626" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="八千代 命" data_id="311003007" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……ちょっとしっとりしちゃったから&#xA;最後はノリのいい曲にしようかな。" speaker_show="True" stop="True" voice="ym11003_02_0742" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="じゃ、今日のラストソング、&#xA;みんな盛り上がってねー！" speaker_show="True" stop="True" voice="ym11003_02_0743" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……。" speaker_show="True" stop="True" voice="hy11002_02_0627" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="半蔵門 雪" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003007" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="はー、歌った歌った。&#xA;この稼ぎならフーも怒らないかな。" speaker_show="True" stop="True" voice="ym11003_02_0744" />
    <move obj_id="八千代 命" position_id="left" x="200" x_move_type="MoveTo" time="0.3" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="メイ、お疲れ様。" speaker_show="True" stop="True" voice="hy11002_02_0628" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あ、ユッキー。途中から見てくれてたよね。&#xA;嬉しかったよ。ありがとー。" speaker_show="True" stop="True" voice="ym11003_02_0745" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="たまたま通りかかっただけよ。" speaker_show="True" stop="True" voice="hy11002_02_0629" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そうなの？　……でも、珍しいね。&#xA;ユッキーが最後まで見て声かけてくれるなんて。" speaker_show="True" stop="True" voice="ym11003_02_0746" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうかしら。&#xA;最後、ずいぶん盛り上がってたみたいね。" speaker_show="True" stop="True" voice="hy11002_02_0630" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そうなの！　いやー、いいライブだったー。&#xA;で、ユッキーの感想は？" speaker_show="True" stop="True" voice="ym11003_02_0747" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="まあまあね。&#xA;特に２曲目と３曲目が良かった。" speaker_show="True" stop="True" voice="hy11002_02_0631" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="やったー！　ユッキーに褒められたー！&#xA;……ととっ！" speaker_show="True" stop="True" voice="ym11003_02_0748" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……危ない。これおひねりでしょ。&#xA;落としたら楓に怒られるわよ。" speaker_show="True" stop="True" voice="hy11002_02_0632" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ありがとう、ユッキー。でも、フーなら&#xA;落とした音を聞きつけて飛んできそうだけどね。" speaker_show="True" stop="True" voice="ym11003_02_0749" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="……確かに。じゃ、私はこれで。" speaker_show="True" stop="True" voice="hy11002_02_0633" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="え、ちょっと待ってよ、ユッキー。&#xA;せっかくだから、このあと１杯どう？" speaker_show="True" stop="True" voice="ym11003_02_0750" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="私はトレーニングがあるから。&#xA;メイも早く帰ったほうがいいわよ。" speaker_show="True" stop="True" voice="hy11002_02_0634" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="も～、せっかく誘ってるんだからさ、&#xA;たまには師匠同士、親睦を深めようよ。" speaker_show="True" stop="True" voice="ym11003_02_0751" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="今日はいつもより稼ぎが良かったんだ。&#xA;だから、メイがご馳走するよ。" speaker_show="True" stop="True" voice="ym11003_02_0752" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="遠慮しとくわ。" speaker_show="True" stop="True" voice="hy11002_02_0635" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="このメイの誘いを断るってことは&#xA;ライブが良くなかったってこと？" speaker_show="True" stop="True" voice="ym11003_02_0753" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そういうわけじゃ……。" speaker_show="True" stop="True" voice="hy11002_02_0636" />
    <move obj_id="半蔵門 雪" position_id="left" x="-125" x_move_type="MoveTo" time="0.3" wait="False" />
    <move obj_id="八千代 命" position_id="left" x="125" x_move_type="MoveTo" time="0.3" wait="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="いいから、いいから！&#xA;ほれほれ行くよ、ユッキー！" speaker_show="True" stop="True" voice="ym11003_02_0754" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ちょ、ちょっとメイ……。" speaker_show="True" stop="True" voice="hy11002_02_0637" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さー、今日はとことん飲むぞー！" speaker_show="True" stop="True" voice="ym11003_02_0755" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" lip_sync="False" text="（やけに嬉しそうね。&#xA;……たまには親睦を深めるのもいいか。）" speaker_show="True" stop="True" voice="hy11002_02_0638" />
