<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3022010402" />
  </info>
  <command>
    <Start />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv1" />
    <sound cue_name="EnvCruiser" volume="50" />
    <bg show="True" resource_type="Background" file="62_AutumnMountainRiver_01" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="河原" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="水音を頼りに、&#xA;命と楓は川辺へと辿り着いた。" stop="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="300" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003017" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="020_Facial_thinking" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふぃ～、メイもうヘトヘト～。&#xA;ちょっとここで休んでいこうよ。" speaker_show="True" stop="True" voice="ym11003_03_0196" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え？&#xA;でも、早く行かないと……。" speaker_show="True" stop="True" voice="sf11004_03_0213" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="結構歩いたし、少しくらいいいじゃん。&#xA;景色でも眺めながら休憩しよう？" speaker_show="True" stop="True" voice="ym11003_03_0197" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="おー、やっぱりこの辺りの川って&#xA;すごく澄んでるね。魚とか泳いでそう。" speaker_show="True" stop="True" voice="ym11003_03_0198" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="確かに、水は綺麗ですけど。" speaker_show="True" stop="True" voice="sf11004_03_0214" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーも休みな。&#xA;座らないと、疲れが取れないよ？" speaker_show="True" stop="True" voice="ym11003_03_0199" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そら、靴も脱いで脱いで！" speaker_show="True" stop="True" voice="ym11003_03_0200" />
    <sound cue_name="Gosogoso" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <msg speaker="相模 楓" text="え!?　ちょ、いいですよ。&#xA;自分でしますから。" speaker_show="True" stop="True" voice="sf11004_03_0215" />
    <create_effect effect_id="mai" effect_type="MampSmile" target_obj="八千代 命" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="遠慮すんなってー。&#xA;ほらほら！" speaker_show="True" stop="True" voice="ym11003_03_0201" />
    <delete obj_id="mai" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ひゃあっ……！&#xA;ぬ、脱がされちゃった……。" speaker_show="True" stop="True" voice="sf11004_03_0216" />
    <char_spine_cheek obj_id="相模 楓" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="やっぱりね。&#xA;フー、靴擦れ起こしてるじゃん。" speaker_show="True" stop="True" voice="ym11003_03_0202" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あ……&#xA;あの、気づいてたんですか？" speaker_show="True" stop="True" voice="sf11004_03_0217" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そりゃ、メイは師匠だもん。&#xA;弟子の様子がいつもと違えば気がつくさ。" speaker_show="True" stop="True" voice="ym11003_03_0203" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さっき歩いてる時も、足引きずってたでしょ？&#xA;どっか痛いんだろうなって思ったよ。" speaker_show="True" stop="True" voice="ym11003_03_0204" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="で、でも、このくらい平気です。&#xA;大した事ありません。" speaker_show="True" stop="True" voice="sf11004_03_0218" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="いいから、川で足を冷やしときな。&#xA;ほれ。" speaker_show="True" stop="True" voice="ym11003_03_0205" />
    <sound type="Se" cue_name="Dobon" />
    <wait time="0.5" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ひゃ！　わ、分かりましたから、&#xA;水かけないでください。" speaker_show="True" stop="True" voice="sf11004_03_0219" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" force_end="False" />
    <msg speaker="八千代 命" text="後で絆創膏も貼っとくんだよ。" speaker_show="True" stop="True" voice="ym11003_03_0206" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="はい、ありがとうございます……。" speaker_show="True" stop="True" voice="sf11004_03_0220" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" loop="True" />
    <msg speaker="相模 楓" text="(予定通り紅葉狩りをするために、もっと&#xA;急がなくちゃいけないのに。)" speaker_show="True" stop="True" voice="sf11004_03_0221" lip_sync="False" />
    <char_spine_animation />
    <msg speaker="相模 楓" text="(アタシが、師匠の足を&#xA;引っ張っちゃってる……。)" speaker_show="True" stop="True" voice="sf11004_03_0222" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さて、じゃあせっかくだし、メイは川で遊ぼっと。&#xA;おぉ～、水が冷たくて気持ちいい！" speaker_show="True" stop="True" voice="ym11003_03_0207" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あ、あ、こっちの方は意外と深いぞ!?&#xA;大自然恐るべし！" speaker_show="True" stop="True" voice="ym11003_03_0208" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠、気をつけてくださいよー！" speaker_show="True" stop="True" voice="sf11004_03_0223" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="おっけー！&#xA;あはははっ、楽しーい！" speaker_show="True" stop="True" voice="ym11003_03_0209" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>