    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="さて、最後の挑戦者はごえちゃんだね。&#xA;暫定１位はユッキー！　がんばってごえちゃん！" speaker_show="True" stop="True" voice="ym11003_02_0529" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="う、うぅ、が、頑張ってみるね。" speaker_show="True" stop="True" voice="ig11006_02_0188" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="大丈夫、ごえちゃんなら出来ますよ。&#xA;それでは、よーい、スタート！" speaker_show="True" stop="True" voice="ah11005_02_0353" />
    <msg_window_show show="False" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="青葉 初芽" />
    <Comment comment="SE:走り出す音" />
    <sound type="Se" />
    <wait time="1.0" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="今日は私の誕生日祝い。師匠も応援してくれてるし、&#xA;頑張っていいとこ見せなくちゃ。" speaker_show="True" stop="True" voice="ig11006_02_0189" />
