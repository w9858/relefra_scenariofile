    <msg speaker="源 モモ" text="ゴール！　はー、途中で滑って転びそうになって&#xA;時間、思いっきりロスしちゃった。" speaker_show="True" stop="True" voice="mm11001_02_0449" />
    <move obj_id="源 モモ" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004015" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="モモち、残念ながら今のところ最下位よ。" speaker_show="True" stop="True" voice="sf11004_02_0452" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="がーん……。&#xA;みんな、すっごく速かったもんねぇ。" speaker_show="True" stop="True" voice="mm11001_02_0450" />
    <delete obj_id="源 モモ" />
