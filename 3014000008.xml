    <create type="Character" obj_id="相模 楓" data_id="311004015" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="おー、ごえちゃん、&#xA;なかなか順調に進んでるみたいだねぇ。" speaker_show="True" stop="True" voice="ym11003_02_0530" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="ぐぐっ、このままじゃ、&#xA;アタシのタイム、越えられちゃうかも。" speaker_show="True" stop="True" voice="sf11004_02_0453" />
    <delete obj_id="相模 楓" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001015" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="あれ？　ごえちゃん、迷路の中で&#xA;立ち止まっちゃったみたい。" speaker_show="True" stop="True" voice="mm11001_02_0451" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="どうしたのかしら。&#xA;まさか怪我でも？" speaker_show="True" stop="True" voice="hy11002_02_0382" />
    <delete obj_id="源 モモ" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="いや、待って。誰かと喋ってるみたい。" speaker_show="True" stop="True" voice="ym11003_02_0531" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="噴水の水でよく見えませんが、&#xA;小さい子供のようですね。" speaker_show="True" stop="True" voice="ah11005_02_0354" />
    <delete obj_id="八千代 命" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="もー、勝負の途中だってのに、ごえっち&#xA;何してるのよ？" speaker_show="True" stop="True" voice="sf11004_02_0454" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="……ってちょっと、何よあれ！&#xA;ごえっちが噴水に向かって掌底を繰り出してる！" speaker_show="True" stop="True" voice="sf11004_02_0455" />
    <delete obj_id="相模 楓" />
    <msg speaker="石川 五恵" text="はぁぁぁぁっっ！" speaker_show="True" stop="True" voice="ig11006_02_0190" />
    <create type="Character" obj_id="源 モモ" data_id="311001015" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="うわぁぁっ、噴水を割ってゴールまでの&#xA;道を作っちゃってる！" speaker_show="True" stop="True" voice="mm11001_02_0452" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="えー、そんなのアリ!?" speaker_show="True" stop="True" voice="ym11003_02_0532" />
    <msg_window_show show="False" />
    <delete obj_id="源 モモ" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="-1500" y="0" anchor="Bottom" />
    <move obj_id="石川 五恵" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.0" wait="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="019_Facial_damage" body_anim_type="12_DropShoulders" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="はぁ、はぁ……。&#xA;ようやく出てこれた。" speaker_show="True" stop="True" voice="ig11006_02_0191" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="相模 楓" data_id="311004015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="ん？　拳と反対側の手に子供が……。&#xA;ごえっち、その手を繋いでる子、誰？" speaker_show="True" stop="True" voice="sf11004_02_0456" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="出てこれたよ。&#xA;良かったね、ボク。" speaker_show="True" stop="True" voice="ig11006_02_0192" />
    <msg speaker="子供１" text="おねーちゃん、ありがとう。&#xA;あ、ママー！　ママー！" speaker_show="True" stop="True" voice="99092_02_0001" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="ふふ、良かった。" speaker_show="True" stop="True" voice="ig11006_02_0193" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="あ、そうだ！　競争の途中だったんだ！&#xA;みんな、ごめんなさい！" speaker_show="True" stop="True" voice="ig11006_02_0194" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="迷路から出られなくなって泣いてる子が&#xA;いたから、早くゴールに連れて来たくてつい……。" speaker_show="True" stop="True" voice="ig11006_02_0195" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="さすがに今のはルール違反だよね。&#xA;最初からやり直してもいいかな？" speaker_show="True" stop="True" voice="ig11006_02_0196" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002013" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="その必要はないわ。&#xA;五恵、あなたの優勝でいいと思うわ。" speaker_show="True" stop="True" voice="hy11002_02_0383" />
    <move obj_id="半蔵門 雪" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004015" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="半蔵さん、いいんですか？" speaker_show="True" stop="True" voice="sf11004_02_0457" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="半蔵門 雪" text="ええ。水の壁を破ってはいけないというルールは&#xA;設けてなかったし、人助けまでしたのだからね。" speaker_show="True" stop="True" voice="hy11002_02_0384" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="そんな、でも私、勝負のことなんて忘れて、&#xA;早くゴールに出なくちゃって思いだけで……。" speaker_show="True" stop="True" voice="ig11006_02_0197" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001015" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="まーメイたちがやろうと思っても、&#xA;あんな鮮やかな掌底、出せなかっただろうし。" speaker_show="True" stop="True" voice="ym11003_02_0533" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="そうだね。&#xA;じゃあ優勝はごえちゃんで決定だね！" speaker_show="True" stop="True" voice="mm11001_02_0453" />
    <delete obj_id="源 モモ" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="石川 五恵" data_id="311006013" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="けど……。" speaker_show="True" stop="True" voice="ig11006_02_0198" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="ごえちゃんの実力と優しさで勝ち取った勝利ですよ。&#xA;ごえちゃん、優勝おめでとうございます。" speaker_show="True" stop="True" voice="ah11005_02_0355" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="師匠……。それにみんなも、&#xA;どうもありがとうございます！" speaker_show="True" stop="True" voice="ig11006_02_0198a" />
