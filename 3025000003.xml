    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="いきなり接客……。緊張するけど、&#xA;お正月の振袖のためにも頑張ろう！" speaker_show="True" stop="True" voice="ks11010_03_0188" lip_sync="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
