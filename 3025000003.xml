    <msg speaker="お客さん１" text="このお花、素敵ね！&#xA;お値段どのくらいなのかしら。" speaker_show="True" stop="True" voice="99059_03_0001" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="えっと、そのお花は……。" speaker_show="True" stop="True" voice="ks11010_03_0190" />
    <msg speaker="お客さん２" text="お店の人に聞いてみたら？&#xA;あれ、店員さん居ないのかな。" speaker_show="True" stop="True" voice="99060_03_0001" />
