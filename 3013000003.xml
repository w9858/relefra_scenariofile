    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="な、何でもねえよ。何時か気になっただけで……&#xA;えっと、何時になったかな……" speaker_show="True" stop="True" voice="ry12002_02_0105" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="-200" y="0" anchor="Bottom" type="Character" />
