    <msg speaker="薩摩 しぶき" text="これ、ゆら！" speaker_show="True" stop="True" voice="ss12003_02_0174" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="我先にと突っ込んでっちゃったねぇ。&#xA;１人じゃ逃げられちゃうかも。アタシたちも行こう！" speaker_show="True" stop="True" voice="th12004_02_0207" />
