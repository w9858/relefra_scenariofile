<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3022010403" />
  </info>
  <command>
    <Start />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" resource_type="Background" file="62_AutumnMountainev_01" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="山中" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <sound cue_name="Walk" />
    <wait time="0.5" />
    <msg window_type="SystemMsg" text="足の応急手当てを終えて、&#xA;命と楓は再び山頂を目指す。" stop="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="300" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003017" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="う～ん。さっきの川遊びで濡れた服が&#xA;乾いてなくて気になるなぁ。" speaker_show="True" stop="True" voice="ym11003_03_0210" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ねえ、少し休んでかない？&#xA;天日干ししたいー。" speaker_show="True" stop="True" voice="ym11003_03_0211" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ダメです。&#xA;先を急ぎましょう。" speaker_show="True" stop="True" voice="sf11004_03_0224" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ただでさえ、アタシのせいで&#xA;無駄に時間を使ったんですから。" speaker_show="True" stop="True" voice="sf11004_03_0225" />
    <char_spine_animation obj_id="相模 楓" />
    <msg speaker="相模 楓" text="(それに、このままじゃ……師匠の誕生日どころか&#xA;本当に遭難しちゃうかも知れない……。)" speaker_show="True" stop="True" voice="sf11004_03_0226" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="山で迷子になった時はね、&#xA;上の方に向かうのが鉄則なの。" speaker_show="True" stop="True" voice="ym11003_03_0212" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え？" speaker_show="True" stop="True" voice="sf11004_03_0227" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さっきから、その方針は一貫してるから&#xA;安心してていいよ。" speaker_show="True" stop="True" voice="ym11003_03_0213" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="後は水源の確保と、もちろん休息も大事。&#xA;不安な時こそ余裕を持たなくちゃね。" speaker_show="True" stop="True" voice="ym11003_03_0214" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="(師匠は……ただ思いつくままに&#xA;楽しんでただけじゃなかった。)" speaker_show="True" stop="True" voice="sf11004_03_0228" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" />
    <msg speaker="相模 楓" text="(この状況における最適な行動を、&#xA;的確に判断してたんだ……。)" speaker_show="True" stop="True" voice="sf11004_03_0229" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="(アタシは焦るばかりで、完璧にエスコートしたいって&#xA;思いで頭が一杯だったのに……。)" speaker_show="True" stop="True" voice="sf11004_03_0230" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="それにさ、頂上まで行けば&#xA;フーが言ってた絶景が一緒に見られるでしょ？" speaker_show="True" stop="True" voice="ym11003_03_0215" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="渡されたしおりを読んでから、&#xA;ずっと楽しみにしてたんだからね？" speaker_show="True" stop="True" voice="ym11003_03_0216" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠……&#xA;あの、ありがとうございます。" speaker_show="True" stop="True" voice="sf11004_03_0231" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="楽しみにしてくれて、&#xA;すっごく嬉しいです。" speaker_show="True" stop="True" voice="sf11004_03_0232" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="無理せずに頂上を目指そう。&#xA;大丈夫、きっと皆と合流出来るよ。" speaker_show="True" stop="True" voice="ym11003_03_0217" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="はいっ！" speaker_show="True" stop="True" voice="sf11004_03_0233" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1.0" wait="True" />
    <delete obj_id="八千代 命" />
    <delete obj_id="相模 楓" />
    <bg show="True" resource_type="Background" file="62_AutumnMountainev_02" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <transition effect="Default" type="In" color_change="False" time="1.0" wait="True" />
    <telop text="山頂" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="300" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003017" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あ……&#xA;見てフー、あれって。" speaker_show="True" stop="True" voice="ym11003_03_0218" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="頂上だ……頂上ですよ、師匠！&#xA;急ぎましょう！" speaker_show="True" stop="True" voice="sf11004_03_0234" />
    <sound cue_name="Run" />
    <wait time="0.5" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ちょ、走って大丈夫なの？" speaker_show="True" stop="True" voice="ym11003_03_0219" />
    <create_effect effect_id="fuu" effect_type="MampSmile" target_obj="相模 楓" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠が手当てしてくれたから、&#xA;大丈夫ですっ！" speaker_show="True" stop="True" voice="sf11004_03_0235" />
    <delete obj_id="fuu" />
    <delete obj_id="相模 楓" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、フーちゃんとメイちゃん来ました！" speaker_show="True" stop="True" voice="mm11001_03_0134" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="良かった～、２人とも&#xA;無事だったんだね！" speaker_show="True" stop="True" voice="mm11001_03_0135" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="400" anchor="Bottom" />
    <move obj_id="相模 楓" x="300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="モモち！&#xA;それに、皆も！" speaker_show="True" stop="True" voice="sf11004_03_0236" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="メイと楓以外は全員いるわ。２人の行ったルートは&#xA;どうやら相当な難所だったようね。" speaker_show="True" stop="True" voice="hy11002_03_0101" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="八千代 命" data_id="311003017" position_id="left" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="まぁね。&#xA;ここに来るまで、もー大変だったんだよ！" speaker_show="True" stop="True" voice="ym11003_03_0220" />
    <char_spine_animation obj_id="八千代 命" />
    <msg speaker="八千代 命" text="熊と戦い、荒れ地を乗り越えて&#xA;ようやく頂上に辿り着いたんだから！" speaker_show="True" stop="True" voice="ym11003_03_0221" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="話を盛らないでください、師匠。" speaker_show="True" stop="True" voice="sf11004_03_0237" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふっ、道中も&#xA;楽しかったみたいですね。" speaker_show="True" stop="True" voice="ah11005_03_0138" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="うん、とっても！&#xA;いい思い出になったよ。" speaker_show="True" stop="True" voice="ym11003_03_0222" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さあ、全員揃ってるしお弁当にしよっか！&#xA;メイ、もうお腹ペコペコだよ～！" speaker_show="True" stop="True" voice="ym11003_03_0223" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="-300" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="今日のお弁当は自信作ですよ。&#xA;よ～く味わってくださいね。" speaker_show="True" stop="True" voice="sf11004_03_0238" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーが食べさせてくれたりしたら、&#xA;美味しさ100倍だろうなぁ。" speaker_show="True" stop="True" voice="ym11003_03_0224" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <msg speaker="相模 楓" text="なっ……!?&#xA;人前で、それはちょっと……。" speaker_show="True" stop="True" voice="sf11004_03_0239" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="で、でも、師匠がどうしてもって言うんなら&#xA;アタシはいいですけど……。" speaker_show="True" stop="True" voice="sf11004_03_0240" />
    <char_spine_cheek obj_id="相模 楓" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="冗談だよ～。頑張って計画立ててくれた&#xA;フーに、そこまで甘えられないって！" speaker_show="True" stop="True" voice="ym11003_03_0225" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そ、そうですか……？&#xA;というか、師匠。からかいましたね？" speaker_show="True" stop="True" voice="sf11004_03_0241" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="えへへ、ごめんごめん。&#xA;そういうのはさ、部屋で２人の時にしよっか。" speaker_show="True" stop="True" voice="ym11003_03_0226" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <msg speaker="相模 楓" text="え？　あ、はい……って、また！" speaker_show="True" stop="True" voice="sf11004_03_0242" />
    <char_spine_cheek obj_id="相模 楓" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さ、食べよう！&#xA;空腹と皆の笑顔が、きっと最高のスパイスだよね！" speaker_show="True" stop="True" voice="ym11003_03_0227" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>