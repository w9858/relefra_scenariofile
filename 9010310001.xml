    <msg speaker="八千代 命" text="へ？ そなの？" speaker_show="True" stop="True" voice="ym11003_02_0906" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うん。&#xA;このあいだ、弟子の３人とお話したんだ。" speaker_show="True" stop="True" voice="ks11010_02_0374" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="メイちゃん、ギターの腕もさらに上がったし、&#xA;情報収集にもとっても役に立ってるって。" speaker_show="True" stop="True" voice="ks11010_02_0375" />
