    <char_spine_animation obj_id="八千代 命" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……ふ～ん。" speaker_show="True" stop="True" voice="ym11003_02_0908" />
