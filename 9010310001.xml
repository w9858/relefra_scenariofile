    <msg speaker="八千代 命" text="ふっふっふっ～。&#xA;レパートリーもかなり増えたよ。" speaker_show="True" stop="True" voice="ym11003_02_0904" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="師匠が向こうに行ってる間、&#xA;ずっと駅前で弾き語りしてたからね！" speaker_show="True" stop="True" voice="ym11003_02_0905" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="うんうん、モモちゃんたちから聞いたよ～。" speaker_show="True" stop="True" voice="ks11010_02_0373" />
