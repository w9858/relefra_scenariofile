    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_cheek obj_id="八千代 命" type="Cheek01_default" loop="True" />
    <msg speaker="高坂 信" text="実は、みんなに会う前は「どんな子なのかな」って&#xA;ちょっとだけ不安だったんだ。" speaker_show="True" stop="True" voice="ks11010_02_0378" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
