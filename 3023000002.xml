<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3023000002" />
  </info>
  <command>
    <Start />
    <load_cue_sheet cue_sheet="BGM/BGM04_Base" />
    <sound cue_name="Base" type="Bgm" />
    <bg file="03_tsukikage_001" />
    <fade_in time="1" type="Default" wait="True" />
    <telop in_time="0.5" show_time="0.5" out_time="0.5" wait="True" text="ツキカゲ基地" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="『サンデイ』『ターゲット ヴァルゴ』" speaker_show="True" stop="True" voice="kt11009_03_0089" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" x="0" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" x="300" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="-300" />
    <lip_sync target1="源 モモ" target2="八千代 命" target3="テレジア" />
    <msg speaker="モモ・命・テレジア" text="は……？" speaker_show="True" stop="True" voice="3023000002_union_001" />
    <delete obj_id="源 モモ" />
    <delete obj_id="八千代 命" />
    <delete obj_id="テレジア" />
    <create type="Character" obj_id="白虎" data_id="311007008" x="130" position_id="left" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="-200" />
    <char_spine_animation obj_id="白虎" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="白虎" text="なんだあ？　ナゾナゾか？&#xA;答えは、フライパンだろ！" speaker_show="True" stop="True" voice="bk11008_03_0078" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="絶対ナゾナゾじゃないだろうし、どうして&#xA;フライパンに辿り着いたのかが謎すぎる。" speaker_show="True" stop="True" voice="sf11004_03_0252" />
    <delete obj_id="白虎" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" x="0" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-300" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="300" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="これは、ツキカゲが傍受した違法な無線に&#xA;入っていた暗号文を解読した物です。" speaker_show="True" stop="True" voice="ah11005_03_0139" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="暗号……。でも、それに何か&#xA;事件性があるんですか？" speaker_show="True" stop="True" voice="mm11001_03_0150" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="実はね、この言葉の前に入っていた&#xA;文言に大きな問題があって……。" speaker_show="True" stop="True" voice="kt11009_03_0090" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それは来週から始まるアイスショーで&#xA;誰かを暗殺せよという指令文だったんです。" speaker_show="True" stop="True" voice="ah11005_03_0140" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="源 モモ" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" position_id="left" x="130" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="-200" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="暗殺とはまた……穏やかじゃないね。&#xA;んで？　そのアイスショーっていうのは？" speaker_show="True" stop="True" voice="ym11003_03_0229" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="もしかして、さっきモモちが言ってたやつじゃ!?&#xA;有名選手達がいっぱい出るっていう。" speaker_show="True" stop="True" voice="ig11006_03_0152" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="130" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="-200" />
    <char_spine_animation obj_id="半蔵門雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうなの？　モモ。" speaker_show="True" stop="True" voice="hy11002_03_0103" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、はい！　でも『ワールド・オン・アイスバレエ』&#xA;というタイトル以外……実はよく知らないんです。" speaker_show="True" stop="True" voice="mm11001_03_0151" />
    <delete obj_id="源 モモ" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="0" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="-300" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="300" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ＣＭで、色んな国の海外選手が何人もで&#xA;軽やかに滑ってるのを見たわ。多国籍ショーね。" speaker_show="True" stop="True" voice="sf11004_03_0253" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="この暗号から、暗殺はショーの初日に行われ&#xA;そのターゲットが『ヴァルゴ』だと解る……。" speaker_show="True" stop="True" voice="kt11009_03_0091" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="それは、人の名前ですか？" speaker_show="True" stop="True" voice="ig11006_03_0153" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="相模 楓" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-200" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="130" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="違うみたい。出演者にもスタッフにもそんな人は&#xA;いないから、たぶんそれ自体も暗号か隠語なんだよ。" speaker_show="True" stop="True" voice="ks11010_03_0076" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="『ヴァルゴ』は乙女座なので、関係者の&#xA;乙女座生まれをピックアップはしてみたんですが…" speaker_show="True" stop="True" voice="ah11005_03_0141" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="多すぎる上に、当然国籍や年齢など全てがバラバラで&#xA;狙われている人を絞り込むのは不可能でした。" speaker_show="True" stop="True" voice="ah11005_03_0142" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="白虎" data_id="311007008" x="0" position_id="left" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="-300" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="300" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="白虎" text="オトメ……っていうと若い女か。ならば！　その日は&#xA;女全員をツキカゲで捕獲しておけば安全だな！" speaker_show="True" stop="True" voice="bk11008_03_0079" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そ、その案はいくらなんでも無理があるよ……。&#xA;できたとしてもショーが台無しになっちゃう。" speaker_show="True" stop="True" voice="mm11001_03_0152" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="現段階では不明な点が多いけど、この空崎で&#xA;暗殺なんて、黙って見ている訳にはいかない。" speaker_show="True" stop="True" voice="hy11002_03_0104" />
    <delete obj_id="源 モモ" />
    <delete obj_id="白虎" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" position_id="left" x="130" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だね。現場に潜入して、何かあったらすぐに動けるよう&#xA;備えておくべきだと思うよ、うん。" speaker_show="True" stop="True" voice="ym11003_03_0230" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="とはいえ、場所はスケートリンク。ショーの開催日。&#xA;周囲には大勢の観客。最悪、厳しい任務になるわよ。" speaker_show="True" stop="True" voice="kt11009_03_0092" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" x="0" position_id="left" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="-300" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="300" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ええ、そうですね。なので、潜入のメインには私が&#xA;立候補します。モモ、一緒に来てくれるわね？" speaker_show="True" stop="True" voice="hy11002_03_0105" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="源 モモ" type="Cheek02_medium" loop="True" />
    <msg speaker="源 モモ" text="えっ!?　も、もちろんです！&#xA;師匠に指名してもらえるなんて私、私……っ！" speaker_show="True" stop="True" voice="mm11001_03_0153" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ターゲットも暗殺方法も不明だから、もし薬品や火薬の&#xA;匂いを感知できたら、すぐに教えてちょうだい。" speaker_show="True" stop="True" voice="hy11002_03_0106" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_cheek obj_id="源 モモ" type="Cheek01_default" loop="True" />
    <msg speaker="源 モモ" text="あ～、警察犬代わりは……なかなか複雑だけど、でも！&#xA;私は必ず師匠の立派な犬になってみせますっ！" speaker_show="True" stop="True" voice="mm11001_03_0154" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="009_Facial_blush" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="あと、観客の事を考えると飛び道具の使用は避けたい。&#xA;よって接近戦の得意な五恵、一緒に来てくれる？" speaker_show="True" stop="True" voice="hy11002_03_0107" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="わかりました。当日までスケートの練習をして&#xA;いざという時のために備えておきます。" speaker_show="True" stop="True" voice="ig11006_03_0154" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そっか。そうだね。ショーに潜入するなら&#xA;私達も軽やかに滑れるようでなくちゃ！" speaker_show="True" stop="True" voice="mm11001_03_0155" />
    <fade_out time="1" type="Default" wait="True" />
    <sound_stop cue_name="Base" />
  </command>
</adv>