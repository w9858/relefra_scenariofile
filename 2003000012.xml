    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="……この師匠に神を見た、ねぇ……&#xA;しっかし、ほんと幸せそうに眠ってるな～。" speaker_show="True" stop="True" voice="ym11003_01_0228" />
    <char_spine_animation obj_id="藤林 長穂" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="藤林 長穂" text="目を醒ましたら、不用意に食べ物に&#xA;パクついた説教が待っているとも知らずにねー。" speaker_show="True" stop="True" voice="fn11011_01_0042" />
