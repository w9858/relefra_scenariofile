    <create type="Character" obj_id="高坂 信" data_id="311010003" anchor="Bottom" x="1500" position_id="left" />
    <char_spine_eye obj_id="高坂 信" type="Eye03_tear" loop="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="イヴァンカ" text="（……なぜ、こんなに名残惜しい気持ちなのかしら……&#xA;何かを待っているような、期待しているような……）" speaker_show="True" stop="True" voice="kt11009_01_0190" lip_sync="False" />
    <msg_window_show show="False" />
    <move obj_id="カトリーナ" position_id="left" x="-200" x_move_type="MoveTo" time="0.5" wait="True" />
    <move obj_id="高坂 信" position_id="left" x="200" x_move_type="MoveTo" time="1.5" wait="True" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="01_Stay" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="イヴァンカさん。" speaker_show="True" stop="True" voice="ks11010_01_0168" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <lip_sync target1="カトリーナ" />
    <msg speaker="イヴァンカ" text="……今度は気づいたわ、シン。&#xA;あなたが近づいてくるの。" speaker_show="True" stop="True" voice="kt11009_01_0191" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="009_Facial_blush" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="……会ったら別れたくなくなるから、来ないでいようと&#xA;思ったんですけど……ちゃんとお別れを言いたくて……" speaker_show="True" stop="True" voice="ks11010_01_0169" />
    <lip_sync target1="カトリーナ" />
    <msg speaker="イヴァンカ" text="泣くことはないでしょう？　任務は成功したんだし、&#xA;シンもこの世界に居続けるなら、きっとまた会えるわ。" speaker_show="True" stop="True" voice="kt11009_01_0192" />
    <msg speaker="高坂 信" text="はい……。" speaker_show="True" stop="True" voice="ks11010_01_0170" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <char_spine_animation obj_id="イヴァンカ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <lip_sync target1="カトリーナ" />
    <msg speaker="イヴァンカ" text="シン。他人のために行動するというあなたの信念……&#xA;私は弱者の生き方だと言ったけど、撤回する。" speaker_show="True" stop="True" voice="kt11009_01_0193" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <lip_sync target1="カトリーナ" />
    <msg speaker="イヴァンカ" text="あなたの信念も、一族のため成すべきことを成すという&#xA;私の生き方も……根本は同じね。どちらも強い。" speaker_show="True" stop="True" voice="kt11009_01_0194" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="初めてイヴァンカさんに褒められました……&#xA;ありがとうございます。" speaker_show="True" stop="True" voice="ks11010_01_0171" />
    <lip_sync target1="カトリーナ" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="イヴァンカ" text="……！" speaker_show="True" stop="True" voice="kt11009_01_0195" />
    <sound_volume type="Se" cue_name="EnvGayaOutdoor" volume="0" time="0.5" sound_stop="True" />
    <msg speaker="イヴァンカ" text="（なぜかしら、なんだか胸が急に痛む……&#xA;どこか甘い、不思議な痛み……）" speaker_show="True" stop="True" voice="kt11009_01_0196" lip_sync="False" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <load_cue_sheet cue_sheet="BGM/BGM15_Adv5" />
    <sound cue_name="Adv5" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="……私の本当の名前は、イヴァンカじゃないわ。&#xA;カトリーナ・トビーというの。" speaker_show="True" stop="True" voice="kt11009_01_0197" />
    <msg speaker="カトリーナ" text="シン、あなたの本当の名前も教えて。" speaker_show="True" stop="True" voice="kt11009_01_0198" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" loop="False" to_idle="False" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="え？　あの……初めから本当の名前です。&#xA;高坂信。それが本当の名前です。" speaker_show="True" stop="True" voice="ks11010_01_0172" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="…………ふ、あはは。そうなのね。" speaker_show="True" stop="True" voice="kt11009_01_0199" />
    <msg speaker="カトリーナ" text="（シンは初めから私に自分を晒し、委ねていたのね。&#xA;……それができる勇気を、この子は持っている）" speaker_show="True" stop="True" voice="kt11009_01_0200" lip_sync="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="（とても普通で、けれどきっと&#xA;誰よりも普通ではない人）" speaker_show="True" stop="True" voice="kt11009_01_0201" lip_sync="False" />
    <msg speaker="カトリーナ" text="（私はこれまでずっと、神を信じずに生きてきた。&#xA;存在しないものを信じるなんて無意味だと思っていた）" speaker_show="True" stop="True" voice="kt11009_01_0202" lip_sync="False" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="（でも、もしかして……神を信じない私の心を揺らし、&#xA;こんなに綺麗な笑顔ができるこの方こそ――）" speaker_show="True" stop="True" voice="kt11009_01_0203" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" loop="True" to_idle="False" />
    <msg window_type="SystemMsg" text="カトリーナはこの時、信の笑顔に&#xA;生まれて初めて神の存在を信じたのだ。" stop="True" />
