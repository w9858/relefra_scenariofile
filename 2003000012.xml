    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="初めから自分の情報を晒し、相手に委ねる。&#xA;そうできることがシン様の強さであり深謀遠慮なのよ。" speaker_show="True" stop="True" voice="kt11009_01_0206" />
