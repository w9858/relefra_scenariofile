  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="34_sakasatower_01" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="ねえねえ、ゆらちゃん！&#xA;今日の隊長、すっごかったね！" speaker_show="True" stop="True" voice="mh12001_01_0309" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="ああ、まさか大理石の床を素手で引っぺがして、&#xA;そのまま敵にぶん投げるとはな…！" speaker_show="True" stop="True" voice="ry12002_01_0286" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="あんなの誰にも真似できねーぜ。&#xA;さすがは、ちびっ子モンスターだ。" speaker_show="True" stop="True" voice="ry12002_01_0287" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="ふっ、「ちびっ子モンスター」とはなかなかのことを&#xA;言うじゃねーか。" speaker_show="True" stop="True" voice="ty12005_01_0106" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="ウチに入って日が浅いとは言え、&#xA;ゆらもずいぶんと恐れ知らずなこったぜ。" speaker_show="True" stop="True" voice="ty12005_01_0107" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="ヨモギさん、いつの間に!?" speaker_show="True" stop="True" voice="mh12001_01_0310" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="つーか、恐れ知らずって何がだよ？" speaker_show="True" stop="True" voice="ry12002_01_0288" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="なんだ？　ひょっとして知らねーのか？&#xA;この逆さ塔で語り継がれている、しぶき伝説を。" speaker_show="True" stop="True" voice="ty12005_01_0108" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="はあ？　しぶき伝説だあ？" speaker_show="True" stop="True" voice="ry12002_01_0289" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="それ、どんな話なんですか、ヨモギさん！&#xA;私、すっごく興味があります！　聞かせてください！" speaker_show="True" stop="True" voice="mh12001_01_0311" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="ま、そりゃかまわねーが。" speaker_show="True" stop="True" voice="ty12005_01_0109" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="昔な、しぶきが凪の部隊に入ってすぐの頃、&#xA;政府のお偉いさんがここに視察に来たことがあってよ。" speaker_show="True" stop="True" voice="ty12005_01_0110" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="止せばいいのにそのお偉いさん、しぶきの忠告も&#xA;聞かず、勝手に立入禁止エリアに入っちまってなあ。" speaker_show="True" stop="True" voice="ty12005_01_0111" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" loop="True" to_idle="False" />
    <create_effect effect_id="yomogi_Smile" effect_type="MampSmile" life_time="1" target_obj="津守 ヨモギ" />
    <msg speaker="津守 ヨモギ" text="くく、あの時は笑ったぜ。" speaker_show="True" stop="True" voice="ty12005_01_0112" />
    <delete obj_id="yomogi_Smile" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="なんせしぶきは、お偉いさんを守っていた屈強なＳＰを&#xA;あっという間にぶちのめして…" speaker_show="True" stop="True" voice="ty12005_01_0113" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="そのお偉いさんに数時間に及ぶ説教をかました後で、&#xA;逆さ塔の掃除を申し付けちまったんだからな。" speaker_show="True" stop="True" voice="ty12005_01_0114" />
    <move obj_id="津守 ヨモギ" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="おいおい、ＳＰがつくほどの政府のお偉いさんって…&#xA;少なくても大臣クラスのはずじゃ…？" speaker_show="True" stop="True" voice="ry12002_01_0290" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="「だからなんじゃ？　ルールを破ったんじゃから、&#xA;懲罰を受けるのは当然じゃろう？」" speaker_show="True" stop="True" voice="ty12005_01_0115" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="それがあの時、しぶきが言い放った言葉だ。" speaker_show="True" stop="True" voice="ty12005_01_0116" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" />
    <create_effect effect_id="yura_Sweat" effect_type="MampSweat" life_time="1" target_obj="乱獅子 ゆら" />
    <msg speaker="乱獅子 ゆら" text="マ、マジかよ、それ…！&#xA;よくそれで、罷免とかされなかったな…！" speaker_show="True" stop="True" voice="ry12002_01_0291" />
    <delete obj_id="yura_Sweat" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="ふっ、罷免どころかそのお偉いさん、&#xA;逆にしぶきのことをえらく気にいってなあ。" speaker_show="True" stop="True" voice="ty12005_01_0117" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="この間もわざわざここに来て、しぶきにどやされながら&#xA;逆さ塔の掃除をしてったんだぜ？" speaker_show="True" stop="True" voice="ty12005_01_0118" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="たまには誰かに怒られて、自分の驕りを戒めたい時が&#xA;ある…とか、そんなことを言ってたっけ。" speaker_show="True" stop="True" voice="ty12005_01_0119" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="は、はは…半端ねーな、うちらの隊長は。" speaker_show="True" stop="True" voice="ry12002_01_0292" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="あいつはやると言ったら必ずやる。&#xA;相手が誰であろうとな。" speaker_show="True" stop="True" voice="ty12005_01_0120" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="だからこそ、ここの囚人もあいつには一目置いてるし、&#xA;怒らせるようなことは決して口にしない。" speaker_show="True" stop="True" voice="ty12005_01_0121" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="特に「ちびっ子」なんてのは禁句中の禁句だ。&#xA;ま、気を付けるこったな。" speaker_show="True" stop="True" voice="ty12005_01_0122" />
    <delete obj_id="乱獅子 ゆら" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="じゃあ、さっきゆらちゃんが言ってたようなことを&#xA;隊長に聞かれちゃったりしたら…？" speaker_show="True" stop="True" voice="mh12001_01_0312" />
    <move obj_id="宗近 飛粋" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="うっ…さすがのアタシ様も怖くなってきたぜ…。" speaker_show="True" stop="True" voice="ry12002_01_0293" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="お、いたいた。なんじゃ２人とも、&#xA;ヨモギさんといったい何を話していたのじゃ？" speaker_show="True" stop="True" voice="ss12003_01_0296" />
    <delete obj_id="薩摩 しぶき" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="な…なんでもねーよ、なあ飛粋？" speaker_show="True" stop="True" voice="ry12002_01_0294" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="011_Facial_surprise" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="うん！　隊長って半端なくすごいって&#xA;話していたところです！" speaker_show="True" stop="True" voice="mh12001_01_0313" />
    <move obj_id="宗近 飛粋" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="しぶきが半端ないじゃと？&#xA;それは、誉め言葉と捉えていいのか？" speaker_show="True" stop="True" voice="ss12003_01_0297" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="お、おう…だ、だよな、ヨモギさん。" speaker_show="True" stop="True" voice="ry12002_01_0295" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="乱獅子 ゆら" />
    <delete obj_id="薩摩 しぶき" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="ああ、そういうことにしといてやる。" speaker_show="True" stop="True" voice="ty12005_01_0123" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ふむ、そうか。ならば、ちとしゃがむのじゃ。&#xA;頭をヨシヨシしてやろう。" speaker_show="True" stop="True" voice="ss12003_01_0298" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" />
    <move obj_id="宗近 飛粋" position_id="left" x="-250" x_move_type="MoveTo" time="0.5" wait="False" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="0" x_move_type="MoveTo" y="0" time="0.5" wait="False" />
    <move obj_id="薩摩 しぶき" position_id="left" x="250" x_move_type="MoveTo" y="0" time="0.5" wait="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="２人とも、今日の任務はご苦労だったのう。&#xA;引き続き正義のために頑張るんじゃぞ。" speaker_show="True" stop="True" voice="ss12003_01_0299" />
  </command>
