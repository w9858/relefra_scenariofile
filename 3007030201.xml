    <char_spine_animation obj_id="東郷 美森" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="変身システムをスマホに組み込むなんて&#xA;実に素晴らしい技術です……興味深い。" speaker_show="True" stop="True" voice="ah11005_01_0469" />
