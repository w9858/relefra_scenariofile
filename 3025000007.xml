    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="引いてきたよー。" speaker_show="True" stop="True" voice="ks11010_03_0222" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あんなイレギュラーなことが起きたんだから、&#xA;次は何だかすっごいヤツがでそうだよね！" speaker_show="True" stop="True" voice="ym11003_03_0340" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="確かに、信さんの引きの強さなら、&#xA;何かとんでもないのがでそう……。" speaker_show="True" stop="True" voice="sf11004_03_0350" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そうかな。じゃあ開いてみるね。えい！" speaker_show="True" stop="True" voice="ks11010_03_0223" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="吉……。&#xA;新年そうそうパッとしないのがでちゃった……。" speaker_show="True" stop="True" voice="ks11010_03_0224" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="こんな時まで師匠は師匠らしいね。" speaker_show="True" stop="True" voice="ym11003_03_0341" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="013_Facial_angry" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
