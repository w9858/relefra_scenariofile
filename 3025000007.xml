    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <transition effect="Default" type="In" color_change="False" time="1.0" wait="True" />
    <move obj_id="高坂 信" position_id="left" x="300" x_move_type="MoveTo" y_move_type="MoveValue" time="2.0" wait="False" />
