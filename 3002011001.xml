<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3002011001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="True" />
    <msg window_type="SystemMsg" text="ある寒い日のことーー" stop="True" />
    <msg_window_show show="False" />
    <bg show="True" file="04_shopstreetmo_01" />
    <fade_in time="2" type="Default" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009009" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="玉ねぎに人参に福神漬け……今日の買い出しは&#xA;大体こんな所ですかね？　カトーさん。" speaker_show="True" stop="True" voice="ig11006_01_0243" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="ええ、これで全部だわ。&#xA;それにしてもごめんなさいね、ごえちゃん。" speaker_show="True" stop="True" voice="kt11009_01_0250" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="バイトの時間でもないのにお店の買い物に&#xA;付き合わせちゃって。" speaker_show="True" stop="True" voice="kt11009_01_0251" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="大丈夫です。私も買い物、嫌いじゃないですから。&#xA;それじゃあWasabiに戻りましょうか。" speaker_show="True" stop="True" voice="ig11006_01_0244" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <sound type="Se" />
    <Comment comment="SE　風のぴゅーぴゅー吹く音" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="今日はいつにも増して寒いわね。" speaker_show="True" stop="True" voice="kt11009_01_0252" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="どうやら空崎の周辺に、今年一番の寒波が&#xA;来ているせいらしいですよ。" speaker_show="True" stop="True" voice="ig11006_01_0245" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="101_Facial_trouble" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="そうだったの……。" speaker_show="True" stop="True" voice="kt11009_01_0253" />
    <sound type="Se" />
    <Comment comment="SE　風のぴゅーぴゅー吹く音" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="101_Facial_trouble" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="はぁ……、突き刺さるような強い北風。&#xA;日本の冬の寒さは何度経験しても身に染みるわ。" speaker_show="True" stop="True" voice="kt11009_01_0254" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="元気出してください、カトーさん。" speaker_show="True" stop="True" voice="ig11006_01_0246" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="確かに外の寒さは辛いですけど、冬にしか&#xA;楽しめない素敵なことも、沢山あるんですよ？" speaker_show="True" stop="True" voice="ig11006_01_0247" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="例えば？" speaker_show="True" stop="True" voice="kt11009_01_0255" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="False" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="熱々のお鍋とか！　みんなでコタツに入って、&#xA;大きなお鍋を囲むと幸せな気持ちになります。" speaker_show="True" stop="True" voice="ig11006_01_0248" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="そうね！　お鍋は野菜も沢山取れてヘルシーだし、&#xA;私もよく食べているわ。" speaker_show="True" stop="True" voice="kt11009_01_0256" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あとは、冬はお風呂が楽しいです！" speaker_show="True" stop="True" voice="ig11006_01_0249" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="寒い日に暖かいお風呂にゆっくり浸かるのは、&#xA;至福の時間ですよ。" speaker_show="True" stop="True" voice="ig11006_01_0250" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="確かにそうね。日本のお風呂はリラックス出来て&#xA;私も大好きよ。" speaker_show="True" stop="True" voice="kt11009_01_0257" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="ありがとう、ごえちゃん。少しだけこの寒さを&#xA;乗り切る元気が湧いてきたわ。" speaker_show="True" stop="True" voice="kt11009_01_0258" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="良かったです、お役に立てて。" speaker_show="True" stop="True" voice="ig11006_01_0251" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="あ、お風呂と言えば……" speaker_show="True" stop="True" voice="kt11009_01_0259" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="カトーさん？　どうしたんですか？" speaker_show="True" stop="True" voice="ig11006_01_0252" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="ごめんなさい、ヘアオイルの残りがもう少しで&#xA;なくなる所だったのを思い出したの。" speaker_show="True" stop="True" voice="kt11009_01_0260" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="その買い物も付き合ってもらっていい？" speaker_show="True" stop="True" voice="kt11009_01_0261" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="はい、勿論です。" speaker_show="True" stop="True" voice="ig11006_01_0253" />
  </command>
</adv>