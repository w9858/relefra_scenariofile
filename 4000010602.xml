<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010602" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="False" />
    <bg show="True" file="02_wasabimo_001" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006007" position_id="left" x="-400" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="お釣りの両替もしたし、福神漬けのストックも&#xA;オッケー……、予備のスプーンは……" speaker_show="True" stop="True" voice="ig11006_01_0582" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" position_id="left" x="150" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="ごえちゃん？　おーい、ごえちゃん！&#xA;さっきから何ブツブツ言ってんの？" speaker_show="True" stop="True" voice="ym11003_01_0809" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="あっ、ごめん！&#xA;ちょっと考え事してて。" speaker_show="True" stop="True" voice="ig11006_01_0583" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="400" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="ごえちゃん、今日は朝からずーっと&#xA;ソワソワしてるね。どうしたの？" speaker_show="True" stop="True" voice="mm11001_01_0980" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="実はカトーさんが出張で、&#xA;丸二日間も店を空けることになったんだ。" speaker_show="True" stop="True" voice="ig11006_01_0584" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="それで、私が店長代理としてWasabiの営業を&#xA;しなくちゃいけなくなっちゃって……。" speaker_show="True" stop="True" voice="ig11006_01_0585" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="えー、店長代理！&#xA;ごえちゃん、すっごーい！" speaker_show="True" stop="True" voice="mm11001_01_0981" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="凄くなんかないよ。&#xA;ちゃんと出来るか、今から不安で……。" speaker_show="True" stop="True" voice="ig11006_01_0586" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="ごえちゃんならヨユーっしょ！　メイも手伝うから&#xA;ちゃちゃっと仕込みやっちゃおう！" speaker_show="True" stop="True" voice="ym11003_01_0810" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="えっ、メイちゃんが作るの？" speaker_show="True" stop="True" voice="mm11001_01_0982" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="メイだって味見と応援くらいならラクショーだよ。&#xA;いつもフーの横でやってるし。" speaker_show="True" stop="True" voice="ym11003_01_0811" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="味見と応援か！　それなら私にもできそうだけど。&#xA;でもごえちゃん、私たちで大丈夫？" speaker_show="True" stop="True" voice="mm11001_01_0983" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ありがとう。でも仕込みはカトーさんが、&#xA;事前に終わらせておいてくれたから平気だよ。" speaker_show="True" stop="True" voice="ig11006_01_0587" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="それに、明日は師匠も手伝いに来てくれるって&#xA;言うから、大丈夫だとは思うんだけど……。" speaker_show="True" stop="True" voice="ig11006_01_0588" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="なーんだ、それなら全然、心配ないじゃん！" speaker_show="True" stop="True" voice="ym11003_01_0812" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="うん、私もそこは安心してるんだけど、ね……。" speaker_show="True" stop="True" voice="ig11006_01_0589" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="他にも何か心配が？" speaker_show="True" stop="True" voice="mm11001_01_0984" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="うん、もし私が店長代理をしている時に、&#xA;Wasabiの売上が悪かったら……。" speaker_show="True" stop="True" voice="ig11006_01_0590" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="せっかく信頼して店を任せてくれたカトーさんにも&#xA;手伝ってくれる師匠にも顔向けできないなーって。" speaker_show="True" stop="True" voice="ig11006_01_0591" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="もうっ！　やる前からそんな気持ちじゃ&#xA;売れるカレーも売れなくなっちゃうジャロ！" speaker_show="True" stop="True" voice="ym11003_01_0813" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="そうなんだけどね……。心配で心配で……。" speaker_show="True" stop="True" voice="ig11006_01_0592" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="ごえちゃんは真面目だからなぁ。&#xA;あっ、そうだ！ いいこと思いついたよ！" speaker_show="True" stop="True" voice="mm11001_01_0985" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="私とメイちゃんがお店の前に立って&#xA;お客さんの呼び込みをやるっていうのはどう？" speaker_show="True" stop="True" voice="mm11001_01_0986" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="いーねー！　メイたちが本気を出せば、大入り満員&#xA;行列の出来るカレー屋になっちゃうよー！" speaker_show="True" stop="True" voice="ym11003_01_0814" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="そこまで来ちゃったら、大変そうだけど……。&#xA;２人とも、ありがとう！" speaker_show="True" stop="True" voice="ig11006_01_0593" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>