    <msg speaker="三好 夏凜" text="え？" speaker_show="True" stop="True" voice="mk96005_01_0059" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="テレジア" text="ふざけた中学生だと思っていたのに……&#xA;変身した途端、戦士の顔になったから。" speaker_show="True" stop="True" voice="tr11007_01_0209" />
