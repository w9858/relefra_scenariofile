    <char_spine_animation obj_id="三好 夏凜" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="三好 夏凜" text="仲間と一緒なら、何だって乗り越えられるって&#xA;私はもう、十分に知ってるからね。" speaker_show="True" stop="True" voice="mk96005_01_0063" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
