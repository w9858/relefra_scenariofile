    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="テレジア" text="役目とはいえ、知らない世界に送られて&#xA;少しも不安はなかったのか……？" speaker_show="True" stop="True" voice="tr11007_01_0210" />
    <char_spine_animation obj_id="三好 夏凜" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="三好 夏凜" text="不安じゃなかったと言えば嘘になるけど……&#xA;でも、別に平気だったわ。" speaker_show="True" stop="True" voice="mk96005_01_0061" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="テレジア" text="何故？" speaker_show="True" stop="True" voice="tr11007_01_0211" />
