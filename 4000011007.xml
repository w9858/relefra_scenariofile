    <create_effect effect_id="shin01" effect_type="EffectShockWave" life_time="2" target_obj="高坂 信" attach_head_bone="True" />
    <msg speaker="高坂 信" text="よーし、これで最後だ！" speaker_show="True" stop="True" voice="ks11010_03_0485" />
    <delete obj_id="shin01" />
    <sound type="Se" cue_name="RotaryDrill" />
    <wait time="2" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="おぉっ、命中！" speaker_show="True" stop="True" voice="ym11003_03_0651" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="こちら風魔。犯人は無事確保。&#xA;人形も甚内が殲滅しました。" speaker_show="True" stop="True" voice="sf11004_03_0662" />
    <msg speaker="青葉 初芽" text="了解。ミッション完了ですね。&#xA;皆さん、お疲れ様でした。" speaker_show="True" stop="True" voice="ah11005_03_0439" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="はぁ～、よかった。&#xA;一時はどうなることかと思ったよー。" speaker_show="True" stop="True" voice="ks11010_03_0486" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="師匠～！　すごいじゃん！" speaker_show="True" stop="True" voice="ym11003_03_0652" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="信さん、ありがとうございました。&#xA;アタシと師匠だけだったら本当に危なかったです。" speaker_show="True" stop="True" voice="sf11004_03_0663" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そんなにいっぱい褒められたら、&#xA;ちょっと恥ずかしいなぁ。" speaker_show="True" stop="True" voice="ks11010_03_0487" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だって本当のことじゃないか～。&#xA;師匠はメイとフーを助けてくれた命の恩人！" speaker_show="True" stop="True" voice="ym11003_03_0653" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そうですよ、それからその左腕、&#xA;めちゃくちゃカッコよかったです。" speaker_show="True" stop="True" voice="sf11004_03_0664" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そんな……。けど、この腕が役に立って&#xA;よかったよ。" speaker_show="True" stop="True" voice="ks11010_03_0488" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そいじゃ、Wasabiに戻って、&#xA;勝利の美カレー、味わおう！" speaker_show="True" stop="True" voice="ym11003_03_0654" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="美カレーって、あんまり聞いたことないですけど&#xA;『勝利の美酒』みたいなことですか？" speaker_show="True" stop="True" voice="sf11004_03_0665" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うん、多分そうだよね。&#xA;とにかく、カトさんも待ってるし帰ろっか。" speaker_show="True" stop="True" voice="ks11010_03_0489" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="True" time="1.0" wait="True" />
