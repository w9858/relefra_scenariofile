<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="PromoAdv/PromoAdv_yuruyuri_03" />
  </info>
  <command>
    <Start />
    <auto_mode active="True" />
    <function_menu_show show="False" />
    <bg file="01_school1mo_003" />
    <load_cue_sheet cue_sheet="BGM/BGM18_Adv6" />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_in time="1" wait="True" />
    <create obj_id="歳納 京子" data_id="394002002" type="Character" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" lip_sync_volume="0.01" text="『空崎』への社会科見学まで&#xA;いよいよあと２日！" voice="tk95002_p03_0001" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" loop="False" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="002_Facial_smile" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="ヤバいね、ワクワクし過ぎて&#xA;夜しか眠れないよ。" voice="tk95002_p03_0002" />
    <char_spine_animation obj_id="歳納 京子" loop="True" body_anim_type="11_Point" face_anim_type="003_Facial_laugh" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" voice="tk95002_p03_0003" text="最近昼寝とか全然出来てないし、&#xA;このままじゃ当日爆睡しちゃうかも！" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="002_Facial_smile" loop="True" body_anim_type="03_BackBothHands" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="何しろ、今回はごらく部の皆で&#xA;空崎の街を回れるんだもんね。" voice="tk95002_p03_0004" />
    <char_spine_animation obj_id="歳納 京子" body_anim_type="06_ChestLeftHand" loop="True" mixing="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="結衣と一緒の行事は多いけど、&#xA;ちなつちゃんと過ごせるのはレアだもん。" voice="tk95002_p03_0005" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="思い出、い～～～～っぱい&#xA;作らないと！" voice="tk95002_p03_0006" />
    <char_spine_animation obj_id="歳納 京子" body_anim_type="13_Question" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="皆で何しよー。あ、海を見ながら&#xA;ラムレーズンのアイス食べたりすると青春っぽいかも？" voice="tk95002_p03_0007" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="そうだ、それと&#xA;あかりを置いて行かないようにしないと。" voice="tk95002_p03_0008" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="最近、たま～にあかりの事忘れちゃうんだよねー。&#xA;影の薄さが進行してきたっていうか。" voice="tk95002_p03_0009" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="歳納 京子" stop="True" window_type="CharMsg" text="今の内に、手に書いておこう。&#xA;「あかり忘れるな」……っと。" voice="tk95002_p03_0010" />
    <msg_window_show show="False" />
    <fade_out time="1" wait="True" />
  </command>
</adv>