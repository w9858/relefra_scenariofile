  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="商店街　夕方  04_shopstreetev_01" />
    <bg show="True" file="04_shopstreetev_01" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はぁ……。" speaker_show="True" stop="True" voice="ig11006_02_0571" />
    <move obj_id="石川 五恵" position_id="left" x="200" x_move_type="MoveTo" time="0.3" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="あれ、ごえちゃん。&#xA;ゲームセンターの前でどうしたんですか？" speaker_show="True" stop="True" voice="ah11005_02_0871" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="あ、師匠。" speaker_show="True" stop="True" voice="ig11006_02_0572" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="珍しいですね、ごえちゃんが。&#xA;何かやりたいゲームでもあるんですか？" speaker_show="True" stop="True" voice="ah11005_02_0872" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="えっと、その……。&#xA;実はクレーンゲームのぬいぐるみが気になって……。" speaker_show="True" stop="True" voice="ig11006_02_0573" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="景品？　……わぁ、これは確かに気になります！&#xA;すっごくかわいいです！　一緒にやりましょうか？" speaker_show="True" stop="True" voice="ah11005_02_0873" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="い、いいんですか？" speaker_show="True" stop="True" voice="ig11006_02_0574" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="では、先にごえちゃんからどうぞ。&#xA;ぬいぐるみのこの辺を狙ってやるとよさそうですよ。" speaker_show="True" stop="True" voice="ah11005_02_0874" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい。まず最初のアームを……。&#xA;あ、もう少し奥のほうが良かったですか……？" speaker_show="True" stop="True" voice="ig11006_02_0575" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうですね。ぬいぐるみの位置から考えると。&#xA;少し手前にアームを落として様子を見ましょうか。" speaker_show="True" stop="True" voice="ah11005_02_0875" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい。……あ、ダメみたいですね。……すみません。" speaker_show="True" stop="True" voice="ig11006_02_0576" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いえ、それじゃ、私がリベンジしましょう。&#xA;まずは様子見でアームを落として……。" speaker_show="True" stop="True" voice="ah11005_02_0876" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="む……これでもダメですか。&#xA;だとすると、アームの入射角を変えて……。" speaker_show="True" stop="True" voice="ah11005_02_0877" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" lip_sync="False" text="（師匠、いつになく熱くなってる……。）" speaker_show="True" stop="True" voice="ig11006_02_0577" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ぐぬぬ、これでもダメですか。&#xA;ガラス越しの影響で目視と微妙なズレがあるのかも……" speaker_show="True" stop="True" voice="ah11005_02_0878" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="あの、師匠……そんなにムキにならなくても……。" speaker_show="True" stop="True" voice="ig11006_02_0578" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いえ、ここは私に任せてください。&#xA;必ずこのぬいぐるみ、取って見せますから！" speaker_show="True" stop="True" voice="ah11005_02_0879" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="は、はい……！" speaker_show="True" stop="True" voice="ig11006_02_0579" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" lip_sync="False" text="（どうしよう。私が誘っちゃったから、&#xA;師匠に無理させることになっちゃって……。）" speaker_show="True" stop="True" voice="ig11006_02_0580" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="……よし、これでどうですか!?" speaker_show="True" stop="True" voice="ah11005_02_0880" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="あ……っ、すごい！" speaker_show="True" stop="True" voice="ig11006_02_0581" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふう、無事に取れました！&#xA;お待たせしてすみません、ごえちゃん。……どうぞ。" speaker_show="True" stop="True" voice="ah11005_02_0881" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="ありがとうございます、師匠。&#xA;かわいいですね。大事にします。" speaker_show="True" stop="True" voice="ig11006_02_0582" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="喜んでもらえて良かったです。&#xA;でも、それだけでいいんですか？" speaker_show="True" stop="True" voice="ah11005_02_0882" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="たくさん種類があるみたいですけど。&#xA;どうせなら全種類取っちゃいましょうか。" speaker_show="True" stop="True" voice="ah11005_02_0883" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="全種類って……30種類もあるみたいですよ？" speaker_show="True" stop="True" voice="ig11006_02_0583" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ええ。でも、こういうのって&#xA;コンプリートしたくなりませんか？" speaker_show="True" stop="True" voice="ah11005_02_0884" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="い、いえ、私はこれ１個で大丈夫です。&#xA;このぬいぐるみが欲しかったので、大満足です！" speaker_show="True" stop="True" voice="ig11006_02_0584" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうですか？　また何かあったら言ってくださいね。&#xA;１個と言わず、10個でも20個でも取りますから。" speaker_show="True" stop="True" voice="ah11005_02_0885" />
  </command>
