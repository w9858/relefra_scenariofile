<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3006000001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out type="Default" time="0" />
    <bg show="True" file="03_tsukikage_007" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="ツキカゲ基地" show_time="1.0" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="009_Facial_blush" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <create_effect effect_id="goe_aura" effect_type="EffectAura" target_obj="石川 五恵" attach_head_bone="False" />
    <create_effect effect_id="goe_aura2" effect_type="EffectAura_F" target_obj="石川 五恵" attach_head_bone="False" />
    <move obj_id="石川 五恵" x="-300" />
    <msg speaker="石川 五恵" text="し、師匠！　あのっ……、そっ、そのっ……、&#xA;ちょっと、お願いがっ……。" speaker_show="True" stop="True" voice="ig11006_01_0323" />
    <delete obj_id="goe_aura" />
    <delete obj_id="goe_aura2" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="015_Facial_poker" body_anim_type="13_Question" loop="True" to_idle="False" />
    <create_effect effect_id="hatsu_aura" effect_type="EffectAura" target_obj="青葉 初芽" attach_head_bone="False" />
    <create_effect effect_id="hatsu_aura2" effect_type="EffectAura_F" target_obj="青葉 初芽" attach_head_bone="False" />
    <move obj_id="青葉 初芽" x="300" />
    <msg speaker="青葉 初芽" text="ごえちゃん？　どうしたんですか？&#xA;そんなにガチガチに緊張して。" speaker_show="True" stop="True" voice="ah11005_01_0346" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <move obj_id="石川 五恵" x="250" />
    <msg speaker="石川 五恵" text="あのっ、とても図々しいかもしれないのですが、&#xA;折り入ってその、師匠におっ、お願いごとが……。" speaker_show="True" stop="True" voice="ig11006_01_0324" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="ごえちゃんがそんなに緊張するようなお願い事……。&#xA;ということはもしかして……？" speaker_show="True" stop="True" voice="ah11005_01_0347" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <move obj_id="青葉 初芽" x="-300" />
    <msg speaker="青葉 初芽" text="コンビニのおにぎりの海苔を破かずに開けられる&#xA;機械を作って欲しいというお願いですね！" speaker_show="True" stop="True" voice="ah11005_01_0348" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="20_PositiveDouble" loop="False" to_idle="False" face_anim_type="003_Facial_laugh" />
    <create_effect effect_id="goe_aura" effect_type="EffectAura" target_obj="石川 五恵" attach_head_bone="False" />
    <create_effect effect_id="goe_aura2" effect_type="EffectAura_F" target_obj="石川 五恵" attach_head_bone="False" />
    <msg speaker="青葉 初芽" text="わかります。あの瞬間は誰もが緊張し、そして&#xA;失敗を繰り返すものですからね。" speaker_show="True" stop="True" voice="ah11005_01_0349" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="15_Negative" loop="False" to_idle="False" face_anim_type="010_Facial_shy" />
    <move obj_id="石川 五恵" x="-400" />
    <msg speaker="石川 五恵" text="ち、違います！　そうではなくて、その……。" speaker_show="True" stop="True" voice="ig11006_01_0325" />
    <delete obj_id="goe_aura" />
    <delete obj_id="goe_aura2" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="あ、わかりました！　親猫に攻撃されずに子猫を&#xA;ナデナデするための気配除去装置、ですよね？" speaker_show="True" stop="True" voice="ah11005_01_0350" />
    <move obj_id="石川 五恵" x="400" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="そっ、それはちょっと欲しいのですが……、&#xA;そうではなくて、その……。" speaker_show="True" stop="True" voice="ig11006_01_0326" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="も、もうすぐ師匠のお誕生日ですよね。&#xA;その日は、予定を入れないで欲しいんです。" speaker_show="True" stop="True" voice="ig11006_01_0327" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="もしかして、ごえちゃんが、&#xA;一緒に過ごしてくれるんですか？" speaker_show="True" stop="True" voice="ah11005_01_0351" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="石川 五恵" text="はい、みんなで師匠のお誕生日パーティを&#xA;開いて、お祝いしたいんです。" speaker_show="True" stop="True" voice="ig11006_01_0328" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="師匠、お誕生日にやりたいこととか、&#xA;食べたいもの、教えてください！" speaker_show="True" stop="True" voice="ig11006_01_0329" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="私たちに出来ることなら、何でも準備しますから。" speaker_show="True" stop="True" voice="ig11006_01_0330" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="何でも？　それは凄いですね。" speaker_show="True" stop="True" voice="ah11005_01_0352" />
    <create_effect effect_id="goe_Sweat" effect_type="MampSweat" life_time="1" target_obj="石川 五恵" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="あっ！　と言ってもあまりゴージャスなものは&#xA;用意できないと思うんですけど……。" speaker_show="True" stop="True" voice="ig11006_01_0331" />
    <delete obj_id="goe_Sweat" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="でも、師匠への愛情はどんな有名シェフよりも&#xA;いーっぱい詰め込んで作ります！" speaker_show="True" stop="True" voice="ig11006_01_0332" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="ご馳走、何が食べたいですか？" speaker_show="True" stop="True" voice="ig11006_01_0333" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="ふふ、ごえちゃんのその気持ちが、&#xA;何よりのお誕生日プレゼントです。" speaker_show="True" stop="True" voice="ah11005_01_0353" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="そんな、遠慮しないでください！&#xA;みんなもお祝いしたいって盛り上がってますし。" speaker_show="True" stop="True" voice="ig11006_01_0334" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="そうでしたか。けど私は、みんなが&#xA;祝ってくれるだけで幸せですよ。" speaker_show="True" stop="True" voice="ah11005_01_0354" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>