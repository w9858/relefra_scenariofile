    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" loop="True" to_idle="False" force_end="False" />
    <sound type="Bgm" cue_name="Quest1" />
    <msg speaker="乱獅子 ゆら" text="おっ、おぉぉっ、なんだこれ、あれか&#xA;一種の訓練ってやつか？" speaker_show="True" stop="True" voice="ry12002_01_0136" />
    <Comment comment="警報" />
    <sound type="Se" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="や、違う！　これはマジもんの警報だ！&#xA;しぶきち、敵襲だよ！" speaker_show="True" stop="True" voice="th12004_01_0114" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="凪の拠点に攻撃を仕掛けてくるとは。&#xA;コケにしてくれたものじゃな。" speaker_show="True" stop="True" voice="ss12003_01_0169" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="101_Facial_trouble" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="おいおい、ここってそんな敵に&#xA;バレバレの所なのかよ。" speaker_show="True" stop="True" voice="ry12002_01_0137" />
    <msg_window_show show="False" />
    <delete obj_id="薩摩 しぶき" />
    <delete obj_id="乱獅子 ゆら" />
