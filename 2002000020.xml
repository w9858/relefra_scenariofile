    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" mixing="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="True" />
    <create_effect effect_id="Exclamation_yura" effect_type="MampExclamation" life_time="1.0" target_obj="乱獅子 ゆら" />
    <create_effect effect_id="Exclamation_sibuki" effect_type="MampExclamation" life_time="1.0" target_obj="薩摩 しぶき" />
