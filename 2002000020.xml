    <wait time="0.2" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="基本秘密だけどよ。囚人達を集めたりして出入りが&#xA;結構あるからな。バレてても不思議じゃねぇ。" speaker_show="True" stop="True" voice="ty12005_01_0078" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="101_Facial_trouble" body_anim_type="15_Negative" loop="True" to_idle="True" force_end="False" />
    <msg speaker="津守 ヨモギ" text="ミサイルをブチ込まれたって平気な作りしてるがな。&#xA;正面から大軍で来るクレイジーな奴がいるとは。" speaker_show="True" stop="True" voice="ty12005_01_0079" />
    <msg_window_show show="False" />
    <delete obj_id="津守 ヨモギ" />
    <wait time="0.2" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="！　まさかそれって。嫌な予感がする。&#xA;クレイジーといえば……大八洲の姉どもだぜ。" speaker_show="True" stop="True" voice="ry12002_01_0138" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="False" />
    <delete obj_id="乱獅子 ゆら" />
    <bg color="#000000ff" file="_white" show="True" />
    <Comment comment="背景　暗転" />
    <fade_in time="0.5" type="Default" wait="True" />
    <screen_effect mode="Flashback" />
    <sound type="Se" cue_name="CutinTransmission" />
    <create type="Character" obj_id="乱獅子 あやめ" data_id="315002001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="噂には聞いていたが、タワーの地下にこんなもの&#xA;作ってたとはな。国もイかれてるじゃねぇか。" speaker_show="True" stop="True" voice="ra15002_01_0018" />
    <msg speaker="大八洲兵" text="あやめ様、隔壁が展開し進路を&#xA;壁が塞いでいきます！" speaker_show="True" stop="True" voice="99027_01_0002" />
    <char_spine_animation obj_id="乱獅子 あやめ" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="壁は俺が切り開いてやるぜ。伊達に国宝の&#xA;刀を盗んできてないぜ？　オラッ！！" speaker_show="True" stop="True" voice="ra15002_01_0019" />
    <char_spine_animation obj_id="乱獅子 あやめ" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ゆらぁ！　お前と凪の連中をぶっ壊しにきたぜ。&#xA;大八洲師団、腐った国に鉄槌だ！　突入しろぉ！" speaker_show="True" stop="True" voice="ra15002_01_0020" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="CutinTransmission" />
    <screen_effect mode="Flashback" show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
    <delete obj_id="乱獅子 あやめ" />
    <delete obj_id="大八洲兵２" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="0" y="0" anchor="Bottom" />
    <bg show="True" file="34_sakasatower_01" />
    <fade_in time="0.5" type="Default" wait="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="映像見るにやっぱ、大八洲師団の連中だ。&#xA;しかも凄い数で来たぜ。馬鹿を極めてやがる。" speaker_show="True" stop="True" voice="ry12002_01_0139" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="101_Facial_trouble" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="まずいなー。隔壁は突破してるし、こっち側の&#xA;警備人形が電波か何かで無効化されてるよ。" speaker_show="True" stop="True" voice="th12004_01_0115" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="さて、しぶき。お前ならこの難局どう乗り切る？" speaker_show="True" stop="True" voice="ty12005_01_0080" />
    <delete obj_id="轟 葉栖美" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="正義に逃げるという選択肢はない。&#xA;害虫が飛び込んできたのじゃ、駆除するまでよ。" speaker_show="True" stop="True" voice="ss12003_01_0170" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="働いている職員を最深部に避難させつつ、&#xA;しぶき達が行く。囚人達も使ってな。" speaker_show="True" stop="True" voice="ss12003_01_0171" />
    <delete obj_id="薩摩 しぶき" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="その囚人達は俺に任せな。&#xA;外にいる飛粋にも連絡しておく。" speaker_show="True" stop="True" voice="ty12005_01_0081" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="こっちは警備用の人形なんとか&#xA;動かせるようにしてみるよ。" speaker_show="True" stop="True" voice="th12004_01_0116" />
    <msg_window_show show="False" />
    <delete obj_id="轟 葉栖美" />
    <delete obj_id="津守 ヨモギ" />
    <wait time="0.2" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="あやめがカチこんで来たってことは狙いは&#xA;アタシ様だろ。敵をひきつけて正面で食い止める！" speaker_show="True" stop="True" voice="ry12002_01_0140" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="ゆら、お前。" speaker_show="True" stop="True" voice="ss12003_01_0172" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="早速でかい仕事が出来そうで嬉しいねまったく。&#xA;アタシ様感涙だよ。" speaker_show="True" stop="True" voice="ry12002_01_0141" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="…ならばしぶきは攻めにでよう。&#xA;ここを囲んでいる連中を撃滅してくる。" speaker_show="True" stop="True" voice="ss12003_01_0173" />
