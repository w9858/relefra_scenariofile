    <delete obj_id="八千代 命" />
    <delete obj_id="半蔵門 雪" />
    <bg show="True" resource_type="AnimSS" file="No_035" />
    <fade_in time="0.5" type="Default" wait="True" />
    <msg speaker="青葉 初芽" text="服用すると脳が刺激されて、少しの間、超人的な&#xA;動きができます。昔から伝わる秘密道具ですね。" speaker_show="True" stop="True" voice="ah11005_01_0079" />
    <msg speaker="八千代 命" text="ソレ、若い女性にしか効かないんだよ。&#xA;効き目が落ちたら、ツキカゲは卒業。" speaker_show="True" stop="True" voice="ym11003_01_0119" />
    <msg speaker="半蔵門 雪" text="卒業後は、協力者としてツキカゲをサポートするか、&#xA;ツキカゲの記憶を消し日常に戻るか、選ぶことになる。" speaker_show="True" stop="True" voice="hy11002_01_0100" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="False" />
    <bg show="True" file="03_tsukikage_005" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="False" />
    <create type="Character" obj_id="八千代 命" data_id="311003003" position_id="left" x="300" anchor="Bottom" y="0" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="0" anchor="Bottom" y="0" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-300" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="Wasabiの店長のカトリーナさんは&#xA;元ツキカゲで、私の師匠なんですよ。" speaker_show="True" stop="True" voice="ah11005_01_0080" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <move obj_id="八千代 命" position_id="left" x="0" x_move_type="MoveTo" time="0.5" wait="False" y="0" y_move_type="MoveValue" />
    <move obj_id="半蔵門 雪" position_id="left" x="-225" x_move_type="MoveTo" time="0.5" wait="False" y="0" y_move_type="MoveValue" />
    <move obj_id="青葉 初芽" position_id="left" x="-450" x_move_type="MoveTo" time="0.5" wait="True" y="0" y_move_type="MoveValue" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="450" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="師匠……。" speaker_show="True" stop="True" voice="mm11001_01_0207" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="ツキカゲは少数精鋭。現役期間中に弟子を&#xA;１人育てて、技術と魂を継承していく。" speaker_show="True" stop="True" voice="hy11002_01_0101" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ちなみにフーは、マイ弟子でーす。" speaker_show="True" stop="True" voice="ym11003_01_0120" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="私はごえちゃんの師匠です。" speaker_show="True" stop="True" voice="ah11005_01_0081" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="そ、それで！&#xA;私が半蔵門先輩の弟子ということですね！" speaker_show="True" stop="True" voice="mm11001_01_0208" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="私のことは師匠と呼びなさい。……まずは修行よ。&#xA;訓練を積み試験をパスすれば、晴れてツキカゲとなる。" speaker_show="True" stop="True" voice="hy11002_01_0102" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="早速、今夜から修行開始。&#xA;一九〇〇に公園に来ること。" speaker_show="True" stop="True" voice="hy11002_01_0103" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="はい！　師匠！" speaker_show="True" stop="True" voice="mm11001_01_0209" />
