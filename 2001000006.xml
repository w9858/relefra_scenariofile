    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="この街は、昔から交通の要衝として&#xA;栄えていたけれど、治安は悪かった。" speaker_show="True" stop="True" voice="hy11002_01_0097" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="そこで街を護る自警団が設立されたんだ。&#xA;ツキカゲのはじまりね。" speaker_show="True" stop="True" voice="ym11003_01_0117" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="以後ツキカゲは、正義の私設情報機関……&#xA;いわゆるスパイとして、犯罪組織と戦っている。" speaker_show="True" stop="True" voice="hy11002_01_0098" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="知ってた？　スパイの語源って、&#xA;香辛料、つまりスパイスの省略形なんだよ。" speaker_show="True" stop="True" voice="ym11003_01_0118" />
    <msg_window_show show="False" />
    <move obj_id="半蔵門 雪" position_id="left" x="0" x_move_type="MoveTo" time="0.5" wait="False" y="0" y_move_type="MoveValue" />
    <move obj_id="八千代 命" position_id="left" x="300" x_move_type="MoveTo" time="0.5" wait="True" y="0" y_move_type="MoveValue" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-300" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="注意・あくまで一説にすぎません。" speaker_show="True" stop="True" voice="ah11005_01_0078" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="では、次はそのスパイスについて説明するわ。&#xA;スパイスは、ツキカゲの生命線でもあるの。" speaker_show="True" stop="True" voice="hy11002_01_0099" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="False" />
