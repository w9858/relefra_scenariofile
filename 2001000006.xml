    <msg window_type="SystemMsg" text="モモがツキカゲと出会った翌日。&#xA;楓に呼び出されたモモは、屋上にやってきていた。" stop="True" />
    <msg_window_show show="False" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <create type="Character" obj_id="相模 楓" data_id="311004003" position_id="left" x="200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="01_Stay" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="相模さん、話って？" speaker_show="True" stop="True" voice="mm11001_01_0200" />
    <msg speaker="相模 楓" text="……正直アタシ、先輩みたいに純粋そうな人は&#xA;ツキカゲに向いてないと思います。" speaker_show="True" stop="True" voice="sf11004_01_0140" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="False" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="――！" speaker_show="True" stop="True" voice="mm11001_01_0201" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" face_anim_type="008_Facial_serious" />
    <msg speaker="相模 楓" text="それでもツキカゲに入ると言うなら、今日の放課後&#xA;Wasabiへ行って、こう注文して下さい。" speaker_show="True" stop="True" voice="sf11004_01_0141" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" face_anim_type="008_Facial_serious" />
    <msg speaker="相模 楓" text="「オリジナルカレー、はちみつ抜き、&#xA;ガラムマサラましまし」……じゃ、アタシはこれで。" speaker_show="True" stop="True" voice="sf11004_01_0142" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" face_anim_type="008_Facial_serious" />
    <move obj_id="相模 楓" position_id="left" x="1000" x_move_type="MoveTo" time="1.0" wait="True" y="0" y_move_type="MoveValue" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" face_anim_type="008_Facial_serious" />
    <msg speaker="源 モモ" text="……私には、向いてない……？&#xA;……ううん。やってやるっ！" speaker_show="True" stop="True" voice="mm11001_01_0202" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="なんたって半蔵門先輩が師匠になるわけだし!!&#xA;もう迷わない。この街を護るって決めたんだから!!" speaker_show="True" stop="True" voice="mm11001_01_0203" />
    <msg_window_show show="False" />
    <transition type="Out" color_change="False" wait="True" />
    <bg show="True" file="02_wasabimo_001" resource_type="Background" />
    <delete obj_id="相模 楓" />
    <delete obj_id="源 モモ" />
    <sound type="Se" cue_name="DoorOpen" />
    <transition type="In" color_change="False" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-300" anchor="Bottom" y="0" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="1000" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" loop="True" to_idle="False" force_end="True" />
    <move obj_id="源 モモ" position_id="left" x="300" x_move_type="MoveTo" time="1.0" wait="True" y="0" y_move_type="MoveValue" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="いらっしゃいませ～。" speaker_show="True" stop="True" voice="kt11009_01_0071" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あ、あのっ……オリジナルカレー、はちみつ抜き&#xA;ガラムマサラましましっ。" speaker_show="True" stop="True" voice="mm11001_01_0204" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="は～い。こちらへどうぞ～。" speaker_show="True" stop="True" voice="kt11009_01_0072" />
    <msg_window_show show="False" />
    <transition type="Out" color_change="False" wait="True" />
    <Comment comment="ツキカゲ基地" />
    <bg show="True" file="No_034" resource_type="AnimSS" />
    <delete obj_id="源 モモ" />
    <delete obj_id="カトリーナ" />
    <Comment comment="【SE】エレベーター下降音" />
    <Comment comment="【SE】エレベーター停止音" />
