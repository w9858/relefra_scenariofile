    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="待ってましたよ～、モモちゃん。&#xA;ツキカゲの秘密基地へようこそ。" speaker_show="True" stop="True" voice="ah11005_01_0077" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="それじゃあ早速、ツキカゲの基本情報について&#xA;教えるわ。しっかり聞いておくように。" speaker_show="True" stop="True" voice="hy11002_01_0096" />
