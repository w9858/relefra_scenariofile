  <command>
    <Start />
    <fade_out time="0" />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="指定は無いが恐らく、ツキカゲ基地 ラボ室 03_tsukikage_007" />
    <bg show="True" file="03_tsukikage_007" />
    <msg window_type="SystemMsg" text="ある日。命は初芽に新作のスパイ道具を&#xA;見せてもらっていた。" stop="True" />
    <msg_window_show show="False" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005004" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="おおー！&#xA;初さん、また色々作ったね～。" speaker_show="True" stop="True" voice="ym11003_02_0787" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ええ。これは自撮りライト型の傍受装置、&#xA;こっちは、シュシュのように見える拘束具です！" speaker_show="True" stop="True" voice="ah11005_02_0817" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="へ～！&#xA;いいね～！" speaker_show="True" stop="True" voice="ym11003_02_0788" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="お。こっちのは……リップ型爆弾の新色？&#xA;今までのと何か違うの？" speaker_show="True" stop="True" voice="ym11003_02_0789" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="爆破威力が大幅アップしてます。&#xA;なんと、従来品の３倍です！" speaker_show="True" stop="True" voice="ah11005_02_0818" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="すっごいじゃん！&#xA;早く使ってみたい！" speaker_show="True" stop="True" voice="ym11003_02_0790" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふ。実は、すでに昨日の任務で、&#xA;ごえちゃんが使ってくれたんですよ。" speaker_show="True" stop="True" voice="ah11005_02_0819" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="え、そうだったんだ!?" speaker_show="True" stop="True" voice="ym11003_02_0791" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="昨日の任務って、初さんとごえっちが&#xA;２人で行ったやつだよね？" speaker_show="True" stop="True" voice="ym11003_02_0792" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="ええ。ターゲットの隠れ場所の造りが頑丈だったので、&#xA;ごえちゃんがこの爆弾で突破してくれました。" speaker_show="True" stop="True" voice="ah11005_02_0820" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="しかも、私の指示じゃなく、&#xA;ごえちゃんから提案してくれたんですよ。" speaker_show="True" stop="True" voice="ah11005_02_0821" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="「あの新作を使ってみるのはどうですか？」って。" speaker_show="True" stop="True" voice="ah11005_02_0822" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="おお～。&#xA;ごえっちやるね～。" speaker_show="True" stop="True" voice="ym11003_02_0793" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="昨日だけじゃないですよ。最近のごえちゃん、&#xA;今までより積極的に意見を言ってくれるようになって…" speaker_show="True" stop="True" voice="ah11005_02_0823" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="モウリョウとの戦いを通して、&#xA;すごく成長できたみたいです。" speaker_show="True" stop="True" voice="ah11005_02_0824" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あ、フーもフーも！&#xA;なんか、一皮むけたって感じ。" speaker_show="True" stop="True" voice="ym11003_02_0794" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あの戦いの最中、メイたち師匠がいなくなった時、&#xA;フーたち、ちゃんと自力で立ち上がったもんね。" speaker_show="True" stop="True" voice="ym11003_02_0795" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ええ。……ごえちゃんたちも、&#xA;そろそろ弟子を卒業する頃合いかもですね……。" speaker_show="True" stop="True" voice="ah11005_02_0825" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……そっか。&#xA;フーにも、そのうち弟子ができるんだな～。" speaker_show="True" stop="True" voice="ym11003_02_0796" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="もちろん、喜ばしいことではあるんですけど……" speaker_show="True" stop="True" voice="ah11005_02_0826" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="……弟子離れが寂しいなんて言ったら、&#xA;師匠失格でしょうか……。" speaker_show="True" stop="True" voice="ah11005_02_0827" />
    <msg speaker="初芽・命" text="…………。" speaker_show="True" stop="True" voice="9010305001_01" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="……ふふ。&#xA;どうしても最近、この手の話題が出てきちゃいますね。" speaker_show="True" stop="True" voice="ah11005_02_0829" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だね～。&#xA;あ、ねーねー、このメガネはどんな道具なの？" speaker_show="True" stop="True" voice="ym11003_02_0798" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それは自信作です！&#xA;なんと、全人類の夢である透視眼鏡を実現……" speaker_show="True" stop="True" voice="ah11005_02_0830" />
    <msg_window_show show="False" />
    <Comment comment="【SE】ピーッピーッという警告音" />
    <sound type="Se" cue_name="Whistle" />
    <wait time="0.8" />
    <sound type="Se" cue_name="Whistle" />
    <wait time="1" />
    <flash color="#ffffffff" count="1" time="0.2" wait="False" />
    <Comment comment="【SE】ポンッとごく軽い破裂音" />
    <sound type="Se" cue_name="Cracker" />
    <wait time="1" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……壊れた？" speaker_show="True" stop="True" voice="ym11003_02_0799" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="……みたいです。" speaker_show="True" stop="True" voice="ah11005_02_0831" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="これは、改良の余地ありだね。" speaker_show="True" stop="True" voice="ym11003_02_0800" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="師匠としても、メカニックとしても、&#xA;まだまだ精進しなきゃですね。" speaker_show="True" stop="True" voice="ah11005_02_0832" />
