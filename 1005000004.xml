    <msg speaker="乱獅子 ゆら" text="ん。まぁうちの隊長が先日は熱弁ふるってたけど、&#xA;アタシ様たちとしては、むしろ絡みたいんだよね。" speaker_show="True" stop="True" voice="ry12002_01_0019" lip_sync="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="隊長の言ってる事も分かるけど、私、&#xA;もっとツキカゲの皆と話してみたいもん。" speaker_show="True" stop="True" voice="mh12001_01_0014" lip_sync="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="だから二年組だけで空崎に来てみたんだ。&#xA;隊長には秘密でね。" speaker_show="True" stop="True" voice="mh12001_01_0015" lip_sync="True" window_type="CharMsg" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="あ、じゃあこっちもモモちや楓ちゃんを&#xA;呼んで良いかな？　二人とも喜ぶと思う。" speaker_show="True" stop="True" voice="ig11006_01_0079" window_type="CharMsg" lip_sync="True" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="うんっ、大歓迎だよ。お話したい！！" speaker_show="True" stop="True" voice="mh12001_01_0016" />
