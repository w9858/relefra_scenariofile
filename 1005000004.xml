    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="02_Idle" force_end="False" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="わっ！！" speaker_show="True" stop="True" voice="ig11006_01_0076" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="02_Idle" force_end="False" loop="True" to_idle="False" />
    <msg speaker="乱獅子 ゆら" text="あはは、ほら驚いた。言った通りだろ。飛粋が&#xA;急に出ると他組織の人間からしても怖いんだ。" speaker_show="True" stop="True" voice="ry12002_01_0017" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="うーん普通だと思うんだけどなぁ。&#xA;驚かせてごめんね、ごえちゃん。" speaker_show="True" stop="True" voice="mh12001_01_0013" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" to_idle="False" />
