    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ふん。折角空崎まで来たんじゃ。&#xA;すぐ帰る方が効率が悪い。２時間だけ話してよし。" speaker_show="True" stop="True" voice="ss12003_01_0036" lip_sync="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
