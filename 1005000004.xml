    <msg speaker="石川 五恵" text="な、凪の部隊の！&#xA;どうしてここに、え、ええと。" speaker_show="True" stop="True" voice="ig11006_01_0075" lip_sync="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" force_end="False" loop="True" to_idle="False" />
