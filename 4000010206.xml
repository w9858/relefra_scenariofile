    <msg speaker="源 モモ" text="す、すみません！　これからは、洗濯にも&#xA;気をつけます！" speaker_show="True" stop="True" voice="mm11001_01_0939" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="……いいわ、私も今日は人のことを言えない。" speaker_show="True" stop="True" voice="hy11002_01_0623" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="015_Facial_poker" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="師匠？　あの、師匠はお出かけですか？" speaker_show="True" stop="True" voice="mm11001_01_0940" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="ええ。ランニングシューズの靴紐が切れたから&#xA;新しいものを買いに。" speaker_show="True" stop="True" voice="hy11002_01_0624" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="09_PutHands" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="買い物……。あ、あの！&#xA;私もご一緒させてもらえませんか!?" speaker_show="True" stop="True" voice="mm11001_01_0941" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="え？　モモと買い物へ？" speaker_show="True" stop="True" voice="hy11002_01_0625" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="はい、師匠とお買い物なんて、滅多にない&#xA;機会ですし……ダメ、でしょうか……。" speaker_show="True" stop="True" voice="mm11001_01_0942" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="ダメということはないけど……あ……。" speaker_show="True" stop="True" voice="hy11002_01_0626" />
