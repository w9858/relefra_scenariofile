<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010901" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="Wasabi　店内" />
    <bg show="True" file="02_wasabimo_001" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <Comment comment="信の服装は「私服（夏）」変更予定" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="シン様、どうぞ。&#xA;フローズンホットチョコレートです。" speaker_show="True" stop="True" voice="kt11009_01_0391" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="む…。" speaker_show="True" stop="True" voice="ks11010_01_0205" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="シン様のために腕によりをかけて作りました。&#xA;本場ニューヨーク風のフローズンホットチョコレート。" speaker_show="True" stop="True" voice="kt11009_01_0392" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="むむむ……。" speaker_show="True" stop="True" voice="ks11010_01_0206" />
    <move obj_id="カトリーナ" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="False" />
    <move obj_id="高坂 信" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="どしたの師匠、食べないの？&#xA;美味しそうなのに。" speaker_show="True" stop="True" voice="ym11003_01_0856" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="ちょっと確認したいことがあるから…。" speaker_show="True" stop="True" voice="ks11010_01_0207" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="高坂 信" />
    <delete obj_id="八千代 命" />
    <msg window_type="SystemMsg" text="信はホイップクリームをほんの少しだけ&#xA;スプーンで掬って、舐めた。" stop="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" y="0" anchor="Bottom" />
    <Comment comment="信の服装は「私服（夏）」変更予定" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="11_Point" loop="False" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="……うん。大丈夫。&#xA;薬は入ってない。" speaker_show="True" stop="True" voice="ks11010_01_0208" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="ふふ、さすがシン様、同じ過ちは犯さない…&#xA;尊敬いたします。" speaker_show="True" stop="True" voice="kt11009_01_0393" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ああ、そっかそっか！&#xA;もう２年も経ってるからメイは忘れてたよ。" speaker_show="True" stop="True" voice="ym11003_01_0857" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="カトーさんが作ったフローズンホットチョコに&#xA;睡眠薬が仕込まれてたこと…あったね。" speaker_show="True" stop="True" voice="ym11003_01_0858" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="信は覚えてるからね！&#xA;もう薬を仕込んでも無駄だよ！" speaker_show="True" stop="True" voice="ks11010_01_0209" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="現役を離れた時間が長くても、スパイとしての魂を&#xA;失わない…さすがです、シン様。" speaker_show="True" stop="True" voice="kt11009_01_0394" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="もしシン様が警戒を怠っていたら、&#xA;次に出す紅茶の中に睡眠薬を仕込むつもりでした。" speaker_show="True" stop="True" voice="kt11009_01_0395" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <msg speaker="高坂 信" text="ちょちょちょ、カトさん！&#xA;もう仕込むの自体をやめてよ～！" speaker_show="True" stop="True" voice="ks11010_01_0210" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="仕方ないんです…シン様の寝顔がかわいすぎることが&#xA;いけないんです。見たくなってしまうではないですか。" speaker_show="True" stop="True" voice="kt11009_01_0396" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="010_Facial_shy" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="も、もう…！" speaker_show="True" stop="True" voice="ks11010_01_0211" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="あはは。&#xA;こういうカトーさんを見るの久しぶりだよ。" speaker_show="True" stop="True" voice="ym11003_01_0859" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="最近じゃ、こんなはっちゃけてる姿なんて&#xA;まったく見ないからねー。" speaker_show="True" stop="True" voice="ym11003_01_0860" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="神を前にした時、人は平静ではいられないものよ。" speaker_show="True" stop="True" voice="kt11009_01_0397" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="カトさんはいつも大げさだよう。" speaker_show="True" stop="True" voice="ks11010_01_0212" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="…にしても、師匠は２年経っても&#xA;全然変わったように見えないね。" speaker_show="True" stop="True" voice="ym11003_01_0861" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="そ、そうかな？&#xA;さすがに前より大人な信になったと思うけど…。" speaker_show="True" stop="True" voice="ks11010_01_0213" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="…いや、ぜ～んぜん、変わってない！" speaker_show="True" stop="True" voice="ym11003_01_0862" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="ええ、そんなぁ…。" speaker_show="True" stop="True" voice="ks11010_01_0214" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="それだけシン様は完成されているということです。" speaker_show="True" stop="True" voice="kt11009_01_0398" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="うう、絶対昔より大人の色気とか&#xA;いろいろ出てると思うのになぁ…。" speaker_show="True" stop="True" voice="ks11010_01_0215" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="（でも、前よりもメイちゃんたちの役に立てるはず。&#xA;新しく手に入れた力で…）" speaker_show="True" stop="True" voice="ks11010_01_0216" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="まぁ、師匠が昔と変わってなくて、&#xA;メイは安心したよ。" speaker_show="True" stop="True" voice="ym11003_01_0863" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="お帰り、師匠。" speaker_show="True" stop="True" voice="ym11003_01_0864" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="お帰りなさいませ、シン様。" speaker_show="True" stop="True" voice="kt11009_01_0399" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="うん、ただいま！" speaker_show="True" stop="True" voice="ks11010_01_0217" />
  </command>
</adv>