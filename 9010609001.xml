  <info>
    <VoiceCueSheet value="AdvVoice/9010609001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="02_wasabimo_001" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006004" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" force_end="True" loop="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="あ、いらっしゃい。ごえちゃん。&#xA;待ってたわよ。" speaker_show="True" stop="True" voice="kt11009_02_0404" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="でも、ふたりきりで話したいなんて&#xA;ごえちゃんにしては珍しいわね。何かあったの？" speaker_show="True" stop="True" voice="kt11009_02_0405" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="えーっと……。" speaker_show="True" stop="True" voice="ig11006_02_0670" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ふふっ、リラックスリラックス。&#xA;ゆっくりでいいわよ。" speaker_show="True" stop="True" voice="kt11009_02_0406" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ああ、お水、持ってくるわね。&#xA;……はい、どうぞ。" speaker_show="True" stop="True" voice="kt11009_02_0407" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="すみません、いただきます。コクッ……。&#xA;ふー……ごくごく。&#xA;//シナリオ会議にて。飲み物や食べ物は先に一口含んでから。" speaker_show="True" stop="True" voice="ig11006_02_0671" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ごえちゃんがそんなに緊張するなんて、&#xA;本当に珍しいわね。もしかして悩み相談？" speaker_show="True" stop="True" voice="kt11009_02_0408" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="その……思い切って言いますね。実はこれ……。" speaker_show="True" stop="True" voice="ig11006_02_0672" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="え？　これ……って……もしかして……。" speaker_show="True" stop="True" voice="kt11009_02_0409" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="カレーです。この前、カトーさんが新作のことで&#xA;何も浮かばないって悩んでいたのを見て……。" speaker_show="True" stop="True" voice="ig11006_02_0673" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="その……何かヒントになればと思って……。" speaker_show="True" stop="True" voice="ig11006_02_0674" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="え、まさか、それでごえちゃんが&#xA;わざわざ作って来てくれたの？" speaker_show="True" stop="True" voice="kt11009_02_0410" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="はい……あの、勝手なことをしてすみま……。" speaker_show="True" stop="True" voice="ig11006_02_0675" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="わぁ、ありがとう!!&#xA;ごえちゃんの手作りカレーなんて、嬉しいわ。" speaker_show="True" stop="True" voice="kt11009_02_0411" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="早速、食べてもいいかしら？" speaker_show="True" stop="True" voice="kt11009_02_0412" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="は、はい、もちろんです！" speaker_show="True" stop="True" voice="ig11006_02_0676" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="いただきます。んー、おいしい。&#xA;さすが、ごえちゃん。料理得意だものね。" speaker_show="True" stop="True" voice="kt11009_02_0413" />
    <char_spine_cheek obj_id="石川 五恵" type="Cheek02_medium" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="そんな……作るのが好きなだけですよ。" speaker_show="True" stop="True" voice="ig11006_02_0677" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="かすかに甘いのは……。&#xA;なるほど、ドライフルーツが入っているのね。" speaker_show="True" stop="True" voice="kt11009_02_0414" />
    <char_spine_cheek obj_id="石川 五恵" type="Cheek01_default" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい。少し入れてみました。&#xA;このままじゃ無理だとは思うんですけど……。" speaker_show="True" stop="True" voice="ig11006_02_0678" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="カトリーナ" text="ううん、ありがとう、ごえちゃん。&#xA;このカレーのおかげで創作意欲が湧いてきたわ。" speaker_show="True" stop="True" voice="kt11009_02_0415" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ね、このあといくつかカレーを作るから、&#xA;良かったら味見をしてくれない？" speaker_show="True" stop="True" voice="kt11009_02_0416" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい、もちろんです。" speaker_show="True" stop="True" voice="ig11006_02_0679" />
  </command>
</adv>