    <msg speaker="八千代 命" text="あはは、さすが空崎！　カオスな街だよ。&#xA;本物のお化けや妖怪まで惹きつけるって！" speaker_show="True" stop="True" voice="ym11003_02_0680" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え、ええ……？" speaker_show="True" stop="True" voice="sf11004_02_0558" />
