    <char_spine_animation obj_id="青葉 初芽" body_anim_type="20_PositiveDouble" face_anim_type="001_Facial_blink" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="はい。ご家族の方も交えて説明して、&#xA;ハロウィンの習慣をわかってもらえました。" speaker_show="True" stop="True" voice="ah11005_02_0621" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="その後、塩や灰をかけてしまった人には、&#xA;赤星さんご自身が謝罪に行かれたとか。" speaker_show="True" stop="True" voice="ah11005_02_0622" />
    <move obj_id="青葉 初芽" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
