    <msg speaker="半蔵門 雪" text="何かあったの？" speaker_show="True" stop="True" voice="hy11002_02_0527" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="被害者の方々いわく、彼女に塩や灰をかけられた後、&#xA;健康になったり、不幸が止まったりしたそうです。" speaker_show="True" stop="True" voice="ah11005_02_0624" />
