    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="拝み屋を一般人というかどうかは微妙だけどねー。" speaker_show="True" stop="True" voice="ym11003_02_0678" />
