    <bg show="True" resource_type="Background" file="03_tsukikage_001" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="赤星さんの身元の確認が取れました。" speaker_show="True" stop="True" voice="ah11005_02_0619" />
