    <msg speaker="源 モモ" text="さて、コツは教えたし&#xA;早速実践してみようか。" speaker_show="True" stop="True" voice="mm11001_03_0056" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
