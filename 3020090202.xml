    <msg speaker="半蔵門 雪" text="私達が訓練用に使っている人形を用意したわ。&#xA;あなた用に強さは調整してあるから安心して。" speaker_show="True" stop="True" voice="hy11002_03_0055" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" force_end="False" loop="False" mixing="True" />
    <msg speaker="半蔵門 雪" text="この人形の攻撃を掻い潜って、&#xA;刀で倒すのよ。" speaker_show="True" stop="True" voice="hy11002_03_0056" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="歳納 京子" text="私が、刀で……。" speaker_show="True" stop="True" voice="tk95002_03_0090" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" force_end="False" loop="True" mixing="True" body_anim_type="07_ChestBothHands" />
    <msg speaker="源 モモ" text="京子ちゃん……&#xA;やっぱり、緊張しちゃう？" speaker_show="True" stop="True" voice="mm11001_03_0057" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="008_Facial_serious" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="歳納 京子" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="歳納 京子" text="はい、流石に。&#xA;でも……それだけじゃない感じなんです。" speaker_show="True" stop="True" voice="tk95002_03_0091" />
    <char_spine_animation obj_id="歳納 京子" body_anim_type="06_ChestLeftHand" force_end="False" loop="False" mixing="True" />
    <msg speaker="歳納 京子" text="恐いはずなのに、なんだか胸の奥から&#xA;ふつふつと熱い気持ちが湧いて来るっていうか。" speaker_show="True" stop="True" voice="tk95002_03_0092" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="それは、『滾り』だよ！" speaker_show="True" stop="True" voice="mm11001_03_0058" />
    <char_spine_animation obj_id="歳納 京子" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="歳納 京子" text="滾り……。" speaker_show="True" stop="True" voice="tk95002_03_0093" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="私もいつも同じ気持ちを感じてる。&#xA;京子ちゃんなら、きっと出来るよ！" speaker_show="True" stop="True" voice="mm11001_03_0059" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="私も信じてるわ。" speaker_show="True" stop="True" voice="hy11002_03_0057" />
