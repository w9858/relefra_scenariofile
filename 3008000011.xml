    <msg speaker="轟 葉栖美" text="よし、今日のミッション完了だね。" speaker_show="True" stop="True" voice="th12004_02_0146" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="今の警報の音や衝撃であの子たちが&#xA;びっくりしてないか確認にいきましょう。" speaker_show="True" stop="True" voice="sf11004_02_0128" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="そうだね、急いで戻ろう！" speaker_show="True" stop="True" voice="th12004_02_0147" />
