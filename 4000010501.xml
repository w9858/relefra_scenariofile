    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="まず特筆すべきはその防御力！　防弾防刃防毒機能に&#xA;加え、水深100メートルの圧力に耐えることも可能！" speaker_show="True" stop="True" voice="ah11005_01_0551" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
