    <char_spine_animation obj_id="石川 五恵" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="石川 五恵" text="（それに、すごく頭が良い。いろんなアイテムを&#xA;発明して、ツキカゲの活躍に貢献している）" speaker_show="True" stop="True" voice="ig11006_01_0519" lip_sync="False" />
