    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="それが重要なんです。" speaker_show="True" stop="True" voice="ah11005_01_0556" />
