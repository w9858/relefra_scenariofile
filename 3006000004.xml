    <msg speaker="カトリーナ" text="昨日寝ないで焼いたクッキーよね？&#xA;すぐに追いかけて、渡しておいで。" speaker_show="True" stop="True" voice="kt11009_01_0306" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" />
