    <msg speaker="相模 楓" text="（このまま、いつも通りにご飯を上げて、&#xA;寝床を整理して、動物たちに甘えてもらって……）" speaker_show="True" stop="True" voice="sf11004_02_0116" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="（何もなく過ごせればいいな……。）" speaker_show="True" stop="True" voice="sf11004_02_0117" lip_sync="False" />
