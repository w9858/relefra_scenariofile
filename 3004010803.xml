    <msg speaker="半蔵門 雪" text="待って、それには及ばないわ。&#xA;ほら……。" speaker_show="True" stop="True" voice="hy11002_01_0481" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="テレジア" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="テレジア" text="もぐもぐもぐ……。美味い！&#xA;これは美味いっ。もぐもぐ……。" speaker_show="True" stop="True" voice="tr11007_01_0181" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="ん？&#xA;どうしたんだみんな、食べないのか？" speaker_show="True" stop="True" voice="tr11007_01_0182" />
