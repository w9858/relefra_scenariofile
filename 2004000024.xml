    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="アミィ" text="はいマスター！　推進システム問題なく作動中。&#xA;一時間は飛行可能にて候。" speaker_show="True" stop="True" voice="am13002_02_0064" />
