    <msg speaker="アミィ" text="！？　マスター敵襲です。&#xA;武装人形の大軍！　リザード軍です。" speaker_show="True" stop="True" voice="am13002_02_0067" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="……こうはなって欲しくなかったけど。&#xA;まぁいい切り抜けるわよアミィ。" speaker_show="True" stop="True" voice="sr13001_02_0392" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="お供致しまする。" speaker_show="True" stop="True" voice="am13002_02_0068" />
