    <char_spine_animation obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="おおっと！　ふふふ、我ながら&#xA;素敵な武装をとりつけたわ。" speaker_show="True" stop="True" voice="sr13001_02_0390" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="続いて海中に行くわよアミィ！&#xA;ついてきなさい。" speaker_show="True" stop="True" voice="sr13001_02_0391" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
