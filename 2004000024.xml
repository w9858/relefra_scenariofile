    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="アミィの性能テストをはじめるわ。&#xA;ここは無人島。気兼ねなくやっていいから。" speaker_show="True" stop="True" voice="sr13001_02_0383" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="アミィ" text="了解でございまするマスター。&#xA;自然の破壊だけには注意して、いざ。" speaker_show="True" stop="True" voice="am13002_02_0059" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="アミィ" text="アームガトリングガン&#xA;「波切」射撃開始。" speaker_show="True" stop="True" voice="am13002_02_0060" />
