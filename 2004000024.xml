    <wait time="0.5" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ミサイルの威力はガチなものから&#xA;威嚇用まで。" speaker_show="True" stop="True" voice="sr13001_02_0386" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="アミィ" text="脚部小型ミサイル&#xA;「破軍」発射。" speaker_show="True" stop="True" voice="am13002_02_0063" />
    <sound type="Se" cue_name="Bom" />
    <wait time="0.5" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="これはホーミングできないから&#xA;とりあえず面の攻撃力を。" speaker_show="True" stop="True" voice="sr13001_02_0387" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="よーしよし。各武装それぞれ問題ないわね。&#xA;さらにアミィは力持ちだから接近戦も安心。" speaker_show="True" stop="True" voice="sr13001_02_0388" />
