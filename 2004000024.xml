    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="マル。アミィの性能テストを兼ねた&#xA;ＺＥＴの演習を無人島でやろうと思うの。" speaker_show="True" stop="True" voice="sr13001_02_0380" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="大変良いと思いますわ。いつに致しますの？" speaker_show="True" stop="True" voice="sm13002_02_0189" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="次の日曜日。" speaker_show="True" stop="True" voice="sr13001_02_0381" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="ちょっと急ではありませんか？&#xA;私、予定が入ってると前に言いましたわ。" speaker_show="True" stop="True" voice="sm13002_02_0190" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="マルがいなくても、演習だけなら私とアミィで&#xA;何とかなるわ。" speaker_show="True" stop="True" voice="sr13001_02_0382" />
    <move obj_id="敷島 来夢" x="0" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="敷島 真留瀬" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <create type="Character" obj_id="アミィ" data_id="313002003" position_id="left" x="400" y="0" anchor="Bottom" />
    <move obj_id="アミィ" x="300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="アミィ" text="何とかしてみせまする。" speaker_show="True" stop="True" voice="am13002_02_0058" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="分かりました。全くもう、いつも突然なんですから。" speaker_show="True" stop="True" voice="sm13002_02_0191" />
