    <char_spine_animation force_end="False" loop="True" obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" />
    <msg speaker="相模 楓" text="要するに射的ってのは、小さくて落ちやすそうなのを&#xA;狙えばいいんですよ。ほら。" speaker_show="True" stop="True" voice="sf11004_03_0369" />
