    <char_spine_animation force_end="False" loop="True" obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" />
    <msg speaker="八千代 命" text="ほら！　ど真ん中に的中！" speaker_show="True" stop="True" voice="ym11003_03_0370" />
    <char_spine_animation force_end="False" loop="True" obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" />
    <msg speaker="相模 楓" text="でも、ビクともしてませんね……。" speaker_show="True" stop="True" voice="sf11004_03_0375" />
    <char_spine_animation force_end="False" loop="False" obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" />
    <char_spine_animation force_end="False" loop="True" obj_id="高坂 信" face_anim_type="005_Facial_sad" />
    <msg speaker="高坂 信" text="うん。これはかなりの回数、&#xA;当てなきゃ動かないと思う。" speaker_show="True" stop="True" voice="ks11010_03_0251" />
    <char_spine_animation force_end="False" loop="True" obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="11_Point" />
    <msg speaker="八千代 命" text="動かぬなら、動くまで撃とう、ゲーム機ちゃん！&#xA;どんどんいくよー！" speaker_show="True" stop="True" voice="ym11003_03_0371" />
    <char_spine_animation force_end="False" loop="True" obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" />
