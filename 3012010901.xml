    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="高坂 信" text="そんなに言うなら良いよーだ。&#xA;今度やったら、パッて捕まえちゃうからね！" speaker_show="True" stop="True" voice="ks11010_02_0258" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" />
