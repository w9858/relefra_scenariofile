    <char_spine_animation obj_id="高坂 信" face_anim_type="020_Facial_thinking" loop="True" />
    <msg speaker="高坂 信" text="よーし……集中集中。&#xA;全神経を耳に……。聴覚を研ぎ澄ませて……。" speaker_show="True" stop="True" voice="ks11010_02_0259" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="高坂 信" text="そこだーーー！" speaker_show="True" stop="True" voice="ks11010_02_0260" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" />
    <msg speaker="八千代 命" text="えっ!?　どこ！　何っ!?" speaker_show="True" stop="True" voice="ym11003_02_0469" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <msg speaker="高坂 信" text="あ、あれ……？&#xA;メイちゃんそっちにいたの？" speaker_show="True" stop="True" voice="ks11010_02_0261" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" />
