    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" />
    <msg speaker="高坂 信" text="ファミレスの王者だね～～。&#xA;バカンスハンバーグスペシャルだ～～。" speaker_show="True" stop="True" voice="ks11010_02_0255" />
