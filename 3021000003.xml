    <sound type="Se" cue_name="EnvGaya" volume="100" />
    <wait time="1.0" />
    <sound type="Se" cue_name="tranp_pera" />
    <wait time="1.0" />
    <msg speaker="客の男Ａ" text="よし！　来た来た、フォーカードだ！" speaker_show="True" stop="True" voice="99015_03_0001" />
    <sound type="Se" cue_name="pirorirorin" />
    <wait time="1.5" />
    <sound type="Se" cue_name="Coins" />
    <wait time="1.0" />
    <msg speaker="客の女Ａ" text="やったわ！　今日はついてる♪" speaker_show="True" stop="True" voice="99016_03_0001" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="0" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="しかし、驚きました。アミューズメントビルに&#xA;隠された秘密のカジノが存在するなんて。" speaker_show="True" stop="True" voice="ah11005_03_0069" />
    <msg speaker="オーナー" text="雑居ビルを改造したんだが、ベガスにも&#xA;負けない華やかさとサービスがモットーでな。" speaker_show="True" stop="True" voice="99014_03_0006" />
    <msg speaker="オーナー" text="ドレスコードもあるし、客は富裕層ばかり。&#xA;接客にも品格を求められるから注意しろ。" speaker_show="True" stop="True" voice="99014_03_0007" />
    <msg speaker="オーナー" text="スムーズなゲーム進行と話術を駆使して&#xA;客を良い気分にさせ、金を使わせるんだぞ。" speaker_show="True" stop="True" voice="99014_03_0008" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004011" position_id="left" x="200" />
    <move obj_id="青葉 初芽" position_id="left" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="私も入って１週間だけど、ゲストの中には&#xA;３ヶ月通い詰めで遊ぶ常連さんもいるのよ。" speaker_show="True" stop="True" voice="th12004_03_0038" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="0" />
    <move obj_id="青葉 初芽" position_id="left" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="轟 葉栖美" position_id="left" x="300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="凄い！　そこまで熱中されるなんて、ここには&#xA;他店にない特別な魅力があるんですね。" speaker_show="True" stop="True" voice="ig11006_03_0054" />
    <msg speaker="オーナー" text="まあな。いくらでも海外で遊べる富豪達を&#xA;引きつけるには、新たな刺激が必要なのさ。" speaker_show="True" stop="True" voice="99014_03_0009" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="勉強になります。新たな刺激というのは&#xA;いったい、どんな事なのですか？" speaker_show="True" stop="True" voice="ah11005_03_0070" />
    <msg speaker="オーナー" text="そりゃあ……んんっ、&#xA;オーナーである俺の魅力だろうな！" speaker_show="True" stop="True" voice="99014_03_0010" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="は、はぁ。" speaker_show="True" stop="True" voice="ig11006_03_0055" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="素敵よねぇ。人々に楽しみを提供するお仕事。&#xA;オーナーさんの元で働けてラッキーだわ。" speaker_show="True" stop="True" voice="th12004_03_0039" />
    <msg speaker="オーナー" text="おお、入店１週間なのに、&#xA;よく解ってるじゃないか、ハスラー。" speaker_show="True" stop="True" voice="99014_03_0011" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ハ、ハスラー？" speaker_show="True" stop="True" voice="ah11005_03_0071" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="私のディーラーネーム。&#xA;ビリヤード台はないんだけどね。" speaker_show="True" stop="True" voice="th12004_03_0040" />
    <msg speaker="オーナー" text="ん。思いついた！　お前はブルーリーフ。&#xA;お前はストンリバーと名乗れ！" speaker_show="True" stop="True" voice="99014_03_0012" />
    <lip_sync target1="青葉 初芽" target2="石川 五恵" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <msg speaker="初芽・五恵" text="あ……ハィ。" speaker_show="True" stop="True" voice="3021000003_union_001" />
    <msg speaker="オーナー" text="それじゃあ、明日から頑張ってくれよ？&#xA;俺はこの店に全てを賭けてるんだからな。" speaker_show="True" stop="True" voice="99014_03_0013" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="え？　全てを？" speaker_show="True" stop="True" voice="ah11005_03_0073" />
    <msg speaker="オーナー" text="フッ、俺には夢があるのさ。ここで大金を&#xA;作って成し遂げたい野望がな。聞きたいか？" speaker_show="True" stop="True" voice="99014_03_0014" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい、是非！" speaker_show="True" stop="True" voice="ig11006_03_0057" />
    <msg speaker="オーナー" text="まだナイショだ。お前達が本当の意味で&#xA;俺の部下になった時に、教えてやろう。" speaker_show="True" stop="True" voice="99014_03_0015" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="本当の部下、早くなりたいですぅ～。&#xA;オーナー、カッコイイ～♪" speaker_show="True" stop="True" voice="th12004_03_0041" />
    <msg speaker="オーナー" text="そうだろう？　イケてるよな、俺♪&#xA;さあ、復唱しろ。ジャリンジャリン稼ぐぞ！" speaker_show="True" stop="True" voice="99014_03_0016" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="え……。" speaker_show="True" stop="True" voice="ig11006_03_0058" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ジャリンジャリン稼ぐぞ！" speaker_show="True" stop="True" voice="th12004_03_0042" />
    <lip_sync target1="青葉 初芽" target2="石川 五恵" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="初芽・五恵" text="ジャリンジャリン稼ぐぞー！" speaker_show="True" stop="True" voice="3021000003_union_002" />
    <msg speaker="オーナー" text="よし！　説明は以上だ。２人は帰って&#xA;明日に備えろ。遅刻するんじゃないぞ。" speaker_show="True" stop="True" voice="99014_03_0017" />
    <lip_sync target1="青葉 初芽" target2="石川 五恵" />
