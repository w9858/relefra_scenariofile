<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000020301" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0.0" type="Default" wait="False" />
    <Comment comment="黒百合学園　屋上" />
    <bg show="True" file="36_school_02" />
    <Comment comment="しぶき学校制服" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003004" position_id="left" x="0" y="0" anchor="Bottom" />
    <msg speaker="？？？" text="だーれじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0229" />
    <msg_window_show show="False" />
    <camera move_y="-100" scale_x="1.5" scale_y="1.5" />
    <fade_in time="1.0" type="Default" wait="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="しぶきじゃあ！　凪の部隊・隊長&#xA;薩摩しぶき。高校３年生。黒百合学園。" speaker_show="True" stop="True" voice="ss12003_01_0230" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="これより正義の自己紹介をするぞ。&#xA;ごほん、ごほん。" speaker_show="True" stop="True" voice="ss12003_01_0231" />
    <camera_reset time="0.5" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="まず身長！　……まぁこれは省略でよかろ。&#xA;血液型はO型。出身地は鹿児島じゃ。" speaker_show="True" stop="True" voice="ss12003_01_0232" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="コードネームは、軍荼利。&#xA;凪の中で唯一、真言解放が使えるぞ。" speaker_show="True" stop="True" voice="ss12003_01_0233" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="趣味はそうじゃな。あやとりや折り紙か。&#xA;ん、年寄りっぽい？　球技もやるぞ。" speaker_show="True" stop="True" voice="ss12003_01_0234" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="凪の部隊は警視庁に属する。隊長としては&#xA;お偉方との付き合いで、たまにゴルフをな。" speaker_show="True" stop="True" voice="ss12003_01_0235" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="その際、お偉方でも無礼な奴がいたらキリッと&#xA;注意するのじゃ。しぶきは差別せんぞ。" speaker_show="True" stop="True" voice="ss12003_01_0236" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ん？　上に注意して立場がまずくならないか？&#xA;そーいうのはないのう。それどころか……。" speaker_show="True" stop="True" voice="ss12003_01_0237" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="しぶきに怒られるために、わざとおどけてるんじゃ&#xA;ないかって奴もいるぐらいじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0238" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="あと、掃除が好きじゃな。散らかってると&#xA;片付けたくなる。部屋でも世の中でも。" speaker_show="True" stop="True" voice="ss12003_01_0239" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="特技は無双の怪力よ。これで悪を捕まえて、&#xA;ギュッ、とひねり潰すのじゃ。慈悲は無い。" speaker_show="True" stop="True" voice="ss12003_01_0240" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="しぶきは、小さい頃より力が異常に強かったという。&#xA;大人を片手で軽々と持ち上げる事もできたのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0241" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="005_Facial_sad" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="じゃが親はしぶきを普通に育てたいようで&#xA;この力を表に出すなといった。" speaker_show="True" stop="True" voice="ss12003_01_0242" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="折角これだけの力があるのじゃ。&#xA;世のために使いたいと思うじゃろ？　じゃろじゃろ？" speaker_show="True" stop="True" voice="ss12003_01_0243" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="とはいえ親の気持ちも分かる。" speaker_show="True" stop="True" voice="ss12003_01_0244" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ゆーえーに。夜な夜な変装して、耳を澄まして&#xA;悪を見つけては秘密裏に成敗していたのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0245" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="巷ではマスク・ザ・チェストと呼ばれておったの。" speaker_show="True" stop="True" voice="ss12003_01_0246" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="そしてある夜、暗躍中に津守ヨモギに出会って&#xA;凪の部隊にスカウトされたのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0247" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="以後、堂々と正義の拳をふるっておる。&#xA;逆さ塔を拠点としてな。" speaker_show="True" stop="True" voice="ss12003_01_0248" />
    <transition effect="Shutter" type="Out" time="0.5" wait="True" />
    <delete obj_id="薩摩 しぶき" />
    <msg speaker="薩摩 しぶき" text="ちょっと待つのじゃ。&#xA;んー。よし……よし、と。" speaker_show="True" stop="True" voice="ss12003_01_0249" />
    <Comment comment="しぶき・凪の部隊の服" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <transition effect="Shutter" type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="これが正義を具現化した、凪の部隊、その制服じゃ。&#xA;しぶきが着るとビシッとクールに決まっとるじゃろ。" speaker_show="True" stop="True" voice="ss12003_01_0250" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="秘密道具は一通り使うが、しぶきが一番&#xA;得意なのは、この拳。" speaker_show="True" stop="True" voice="ss12003_01_0251" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="しぶきの場合、武器を使う時などは、相手に配慮&#xA;してやってると言ってもよいわ。" speaker_show="True" stop="True" voice="ss12003_01_0252" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="交友関係はけっこう広いと自負しておるが&#xA;親友といえば、はっすーじゃな。" speaker_show="True" stop="True" voice="ss12003_01_0253" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="はっすーと出会ったのは凪の部隊が出来た後じゃが&#xA;寮でも同じ部屋で仲良くしておる。" speaker_show="True" stop="True" voice="ss12003_01_0254" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="はっすーの作る料理は尋常でなく美味い。&#xA;しぶきの胃袋は幸せものじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0255" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="後輩の飛粋とゆらは、素材は凄まじいが&#xA;まだ青いところもあるゆえ、きっちり指導するのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0256" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="とまぁ、こんな所じゃ。&#xA;誰か悪い奴を見たら連絡をくれ。" speaker_show="True" stop="True" voice="ss12003_01_0257" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="この手で消滅させにいくのでな！&#xA;ではのー。" speaker_show="True" stop="True" voice="ss12003_01_0258" />
  </command>
</adv>