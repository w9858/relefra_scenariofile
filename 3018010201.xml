    <msg speaker="半蔵門 雪" text="ええ、また明日。" speaker_show="True" stop="True" voice="hy11002_02_0539" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ところで、モモ。かなり前からこの部屋にいたから、&#xA;あなたがスマホで何を見ていたかはわかっているわ。" speaker_show="True" stop="True" voice="hy11002_02_0540" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
