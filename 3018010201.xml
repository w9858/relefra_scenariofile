    <char_spine_animation />
    <msg speaker="源 モモ" text="この衣装を選んだ私を褒めてあげたい……&#xA;グッジョブ、モモ！" speaker_show="True" stop="True" voice="mm11001_02_0642" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
