    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" x="300" y="0" anchor="Bottom" position_id="left" />
    <move obj_id="半蔵門 雪" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="リラックスする時間も必要だけど、&#xA;さすがに油断しすぎね、モモ。" speaker_show="True" stop="True" voice="hy11002_02_0532" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="009_Facial_blush" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="師匠!?　いつの間に部屋にいたんですか!?" speaker_show="True" stop="True" voice="mm11001_02_0646" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="弟子を常に見守ることも、師匠の務めよ。&#xA;ところで、何を見ていたの？" speaker_show="True" stop="True" voice="hy11002_02_0533" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ただのニュースサイトです。&#xA;スパイとして、社会情勢にも目を向けないとと思って。" speaker_show="True" stop="True" voice="mm11001_02_0647" />
    <char_spine_animation />
    <msg speaker="源 モモ" text="(スパイの必須技能、ポーカーフェイス！　日々スキルを&#xA;上げる私の嘘は、師匠でも見抜けませんよ……！)" speaker_show="True" stop="True" voice="mm11001_02_0648" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……モモ。" speaker_show="True" stop="True" voice="hy11002_02_0534" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="な、なんですか、師匠……？" speaker_show="True" stop="True" voice="mm11001_02_0649" />
    <move obj_id="源 モモ" x="-250" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="半蔵門 雪" x="-50" x_move_type="MoveTo" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……モモ……。" speaker_show="True" stop="True" voice="hy11002_02_0535" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="(あわわわ……なぜか壁際に追い詰められてる……！&#xA;これって壁ドン……!?)" speaker_show="True" stop="True" voice="mm11001_02_0650" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="……甘い。" speaker_show="True" stop="True" voice="hy11002_02_0536" />
    <move obj_id="源 モモ" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="半蔵門 雪" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あっ!?　スマホを!?" speaker_show="True" stop="True" voice="mm11001_02_0651" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……確かにニュースサイトが表示されているわね。" speaker_show="True" stop="True" voice="hy11002_02_0537" />
    <char_spine_animation />
    <msg speaker="源 モモ" text="(ふふふ……さっき壁際に追い詰められている間に、&#xA;画面の表示を切り替えておいたんだ！)" speaker_show="True" stop="True" voice="mm11001_02_0652" lip_sync="False" />
