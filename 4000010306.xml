    <msg speaker="八千代 命" text="はい、今日のおひねり。" speaker_show="True" stop="True" voice="ym11003_01_0706" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="わぁ、今日は多かったんですね！&#xA;でも、こういうのは大事にとっておいたほうが……。" speaker_show="True" stop="True" voice="sf11004_01_0676" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="えー、今日せっかく頑張ったのになあ……&#xA;ご褒美もナシってこと？" speaker_show="True" stop="True" voice="ym11003_01_0707" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="フーだって、節約ばっかりじゃなくて、&#xA;たまにはお肉、食べたいでしょ？" speaker_show="True" stop="True" voice="ym11003_01_0708" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="う……わ、わかりました。&#xA;それじゃ、何かお肉を買って……。" speaker_show="True" stop="True" voice="sf11004_01_0677" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <create_effect effect_id="mei_kirakira" effect_type="EffectKirakira" target_obj="八千代 命" />
    <msg speaker="八千代 命" text="やったー！　今日はステーキだ！" speaker_show="True" stop="True" voice="ym11003_01_0709" />
