    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="……は無理ですから、特売品のひき肉を狙いましょう。" speaker_show="True" stop="True" voice="sf11004_01_0678" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="ひき肉ぅ？" speaker_show="True" stop="True" voice="ym11003_01_0710" />
