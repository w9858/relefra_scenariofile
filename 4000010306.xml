    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="ひき肉は、炒め物に使うこともできるし、&#xA;ハンバーグや肉団子にもなる偉大な食材ですよ？" speaker_show="True" stop="True" voice="sf11004_01_0680" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="16_BendBackward" loop="True" to_idle="False" />
