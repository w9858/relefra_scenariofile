    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="もう、何言ってるんですか！&#xA;アタシはただ……か、買い物に来ただけですよ！" speaker_show="True" stop="True" voice="sf11004_01_0674" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="だったらメイも行くよ。&#xA;今日はお肉買おう、お肉ー！" speaker_show="True" stop="True" voice="ym11003_01_0705" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="そんなお金、どこに……。" speaker_show="True" stop="True" voice="sf11004_01_0675" />
