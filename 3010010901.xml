    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="高坂 信" data_id="311010008" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="メイちゃん、フーちゃん……。" speaker_show="True" stop="True" voice="ks11010_02_0177" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="ありがとう。&#xA;じゃあ、教えてもらえるかな？" speaker_show="True" stop="True" voice="ks11010_02_0178" />
    <move obj_id="高坂 信" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003013" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="八千代 命" text="うん、任せて！&#xA;くふふっ、師匠に教えるなんて、ちょっと新鮮かも。" speaker_show="True" stop="True" voice="ym11003_02_0361" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="お願いします、メイちゃん先生！" speaker_show="True" stop="True" voice="ks11010_02_0179" />
