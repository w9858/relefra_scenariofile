    <msg speaker="源 モモ" text="師匠たちがいなくたって、私たちは負けないもん！" speaker_show="True" stop="True" voice="mm11001_01_0476" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="そうだね。私たちだって立派なツキカゲだってこと、&#xA;思い知らせてあげようよ。" speaker_show="True" stop="True" voice="ig11006_01_0150" />
