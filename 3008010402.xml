    <telop text="命と楓の家" in_time="1.0" show_time="2.0" out_time="1.0" wait="True" />
    <msg window_type="SystemMsg" text="その日の夜のこと……" stop="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="おー、チッチ、いい食いっぷりだねぇ～。&#xA;もっと食べるかい？　おー、いい子いい子。" speaker_show="True" stop="True" voice="ym11003_02_0141" />
    <msg_window_show show="False" />
    <move obj_id="八千代 命" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="……" speaker_show="True" stop="True" voice="sf11004_02_0156" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="ん？&#xA;フーってば何ぼーっとしてるんだよ～。" speaker_show="True" stop="True" voice="ym11003_02_0142" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="べ、別にボーっとなんて。" speaker_show="True" stop="True" voice="sf11004_02_0157" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="してたよー。&#xA;口半開きにしてポカーンてさ。" speaker_show="True" stop="True" voice="ym11003_02_0143" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="そんなことは！" speaker_show="True" stop="True" voice="sf11004_02_0158" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="フー、フー、あーん。&#xA;フーの大好きなポテチだよん。" speaker_show="True" stop="True" voice="ym11003_02_0144" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="別に食べませんよ。&#xA;アタシにまで餌付けしようとしないでください。" speaker_show="True" stop="True" voice="sf11004_02_0159" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="ちぇっ。じゃーこのポテチはチッチにあげよーっと。&#xA;ほーらチッチ、カロリーたっぷりで美味しいよ～。" speaker_show="True" stop="True" voice="ym11003_02_0145" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="ダメですよ！　そんな脂っこいものあげたら&#xA;病気になっちゃうじゃないですか。" speaker_show="True" stop="True" voice="sf11004_02_0160" />
    <delete obj_id="八千代 命" />
    <move obj_id="相模 楓" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="チッチ、ほら、アタシの所においで～。" speaker_show="True" stop="True" voice="sf11004_02_0161" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="はぁ～、今日もモフモフで気持ちいい～。" speaker_show="True" stop="True" voice="sf11004_02_0162" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="よしよし、チッチ。いい子で留守番してた？&#xA;アタシは今日も色々あり過ぎて疲れたよ～。" speaker_show="True" stop="True" voice="sf11004_02_0163" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="（タヌキは懐いてくれないし、タヌキは懐いて&#xA;くれないし、タヌキは懐いてくれないし……）" speaker_show="True" stop="True" voice="sf11004_02_0164" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="はぁ……。" speaker_show="True" stop="True" voice="sf11004_02_0165" />
    <move obj_id="相模 楓" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="どしたの、フー？" speaker_show="True" stop="True" voice="ym11003_02_0146" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="いや、なんか懐いてくれるのって嬉しいですね。" speaker_show="True" stop="True" voice="sf11004_02_0166" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="メイもフーに懐いてるよー。" speaker_show="True" stop="True" voice="ym11003_02_0147" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="わかってますよー。" speaker_show="True" stop="True" voice="sf11004_02_0167" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="まーそう気にしなさんな。懐かない動物だって、&#xA;彼らには彼らの事情があるんだから。" speaker_show="True" stop="True" voice="ym11003_02_0148" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="それにさ、好かれたくてツキカゲやってる&#xA;わけじゃないじゃん。メイたちはさ。" speaker_show="True" stop="True" voice="ym11003_02_0149" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="だから、タヌキに好かれなくっても&#xA;守ってあげようよ。" speaker_show="True" stop="True" voice="ym11003_02_0150" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="そう、ですね……&#xA;って、なんでそれ知ってるんです!?" speaker_show="True" stop="True" voice="sf11004_02_0168" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="偵察任務は誰にも気づかれないようにってね" speaker_show="True" stop="True" voice="ym11003_02_0151" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
    <sound_stop cue_name="Adv1" />
