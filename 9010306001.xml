<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/9010306001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="02_wasabimo_001" />
    <fade_in time="0.5" wait="True" />
    <msg text="ある日の放課後。授業中居眠りしていた命は、&#xD;&#xA;五恵にノートを⾒せてもらっていた。" speaker_show="True" stop="True" window_type="SystemMsg" />
    <create type="Character" obj_id="石川 五恵" data_id="311006004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003004" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="いや〜悪いね、ごえちゃん。&#xA;昨日遅くまでゲームしてたから寝不足でさ～。" speaker_show="True" stop="True" voice="ym11003_02_0811" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……。" speaker_show="True" stop="True" voice="ig11006_02_0526" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ん？&#xA;どったのごえちゃん。暗い顔して。" speaker_show="True" stop="True" voice="ym11003_02_0812" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……メイちゃんが眠れてないのって、&#xA;財団の取り調べとかのせいだよね……。" speaker_show="True" stop="True" voice="ig11006_02_0527" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="モウリョウと通じてたことについて、&#xA;あれこれ聞かれてるみたいって、楓ちゃんが。" speaker_show="True" stop="True" voice="ig11006_02_0528" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ありゃ。&#xA;ごえちゃん知ってたか～。" speaker_show="True" stop="True" voice="ym11003_02_0813" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ま～ね～。予想してたこととはいえ、&#xA;ちょっち忙しいかも。最近、任務も多いし。" speaker_show="True" stop="True" voice="ym11003_02_0814" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……メイちゃん、今までも大変だったよね。" speaker_show="True" stop="True" voice="ig11006_02_0529" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ん？" speaker_show="True" stop="True" voice="ym11003_02_0815" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="学校に通いながらツキカゲの任務をこなすだけじゃなく&#xA;ダブルスパイとしてモウリョウの内部に入り込んで……" speaker_show="True" stop="True" voice="ig11006_02_0530" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="私、全然気づけなかった。&#xA;クラスでも一緒で、こんなに近くにいたのに……" speaker_show="True" stop="True" voice="ig11006_02_0531" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="メイちゃん一人に負担かけちゃったなって……。" speaker_show="True" stop="True" voice="ig11006_02_0532" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="何言ってんの、ごえっち！" speaker_show="True" stop="True" voice="ym11003_02_0816" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="え……。" speaker_show="True" stop="True" voice="ig11006_02_0533" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="敵を欺くにはまず味方から！&#xA;バレバレじゃダブルスパイにならないじゃん！" speaker_show="True" stop="True" voice="ym11003_02_0817" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="今回のことで、メイのスパイとしての有能さが&#xA;証明されたってもんだよ！" speaker_show="True" stop="True" voice="ym11003_02_0818" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ユッキーに初さん、それにごえっちも、ツキカゲの&#xA;優秀なメンバーが見事に騙されちゃったんだから！" speaker_show="True" stop="True" voice="ym11003_02_0819" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="……そっか……&#xA;うん、そうだよね。" speaker_show="True" stop="True" voice="ig11006_02_0534" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="それに、こっちもずっと黙ってたんだから&#xA;お互い様だよ！" speaker_show="True" stop="True" voice="ym11003_02_0820" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だからもう、この件については&#xA;言いっこナシってことで！" speaker_show="True" stop="True" voice="ym11003_02_0821" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="わかった。&#xA;じゃあ、もう隠し事も無しだね。" speaker_show="True" stop="True" voice="ig11006_02_0535" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ん～それはどうかな……？&#xA;女の⼦だもん——" speaker_show="True" stop="True" voice="ym11003_02_0822" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="——！" speaker_show="True" stop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <lip_sync target1="石川 五恵" target2="八千代 命" />
    <msg speaker="命・五恵" text="秘密の一つや二つあるよ！" speaker_show="True" stop="True" voice="9010306001_01" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="だね。" speaker_show="True" stop="True" voice="ig11006_02_0537" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そーゆーこと！" speaker_show="True" stop="True" voice="ym11003_02_0824" />
  </command>
</adv>