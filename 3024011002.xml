    <char_spine_cheek obj_id="高坂 信" type="Cheek03_strong" loop="True" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="10_FaceBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="や、やだなぁ、照れちゃうよぉ。" speaker_show="True" stop="True" voice="ks11010_03_0152" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="白虎" text="私はどうだ？　カッコイイだろ！" speaker_show="True" stop="True" voice="bk11008_03_0140" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="テレジア" text="ああ。パフェみたいで良い。" speaker_show="True" stop="True" voice="tr11007_03_0066" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="白虎" text="パフェ!?　確かにそれは美味いが……&#xA;カッコイイかあ？　ん……いいな。うん！" speaker_show="True" stop="True" voice="bk11008_03_0141" />
