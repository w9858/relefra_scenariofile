    <create type="Character" obj_id="高坂 信" data_id="311010012" position_id="left" x="300" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うんうん、ホント。カトさんの横だと&#xA;信、自分が見劣りしちゃうよ。" speaker_show="True" stop="True" voice="ks11010_03_0153" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="何を仰いますか！　シン様は天使、いいえ神！&#xA;ご謙遜などされる必要は全く持って皆無です！" speaker_show="True" stop="True" voice="kt11009_03_0149" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そ、そう……？" speaker_show="True" stop="True" voice="ks11010_03_0154" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="はい！　ですからお願いが……。どうかそのお姿で&#xA;写真を撮影させて下さいませんか？" speaker_show="True" stop="True" voice="kt11009_03_0150" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="10_FaceBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="高坂 信" type="Cheek03_strong" loop="True" />
    <msg speaker="高坂 信" text="ええーーっ!?　そ、それはちょっと……&#xA;恥ずかしいから………ごめんなさい。" speaker_show="True" stop="True" voice="ks11010_03_0155" />
    <char_spine_cheek obj_id="高坂 信" type="Cheek01_default" loop="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <create_effect effect_id="blue" effect_type="MampWorry" target_obj="カトリーナ" />
    <char_spine_eye obj_id="カトリーナ" type="Eye03_tear" loop="True" />
    <msg speaker="カトリーナ" text="そ、そうですか……。いえ……こちらこそ調子に乗って&#xA;申し訳ありませんでした……うぅぅ。" speaker_show="True" stop="True" voice="kt11009_03_0151" />
