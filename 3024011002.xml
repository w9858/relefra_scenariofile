    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="高坂 信" data_id="311010012" position_id="left" x="0" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <create_effect effect_id="doyon" effect_type="MampWorry" target_obj="高坂 信" />
    <msg speaker="高坂 信" text="そ、そうだよ…ね。何があるか判らないんだから&#xA;写真は……撮っておくべきだよね。任務の前に……。" speaker_show="True" stop="True" voice="ks11010_03_0157" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="-300" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="縁起でもないこと言わないで下さいよ！&#xA;モモち、あんたのせいよ……。" speaker_show="True" stop="True" voice="sf11004_03_0282" />
