    <msg speaker="八千代 命" text="ちょっ、師匠ぉ。いーじゃん写真の１枚や２枚！&#xA;カトーさん、超頑張って作ってくれたんだから。" speaker_show="True" stop="True" voice="ym11003_03_0268" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-300" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうです！　任務に赴けば、汚れたり破けたりするかも&#xA;しれませんし、今のうちに記録しておくべきかと。" speaker_show="True" stop="True" voice="ah11005_03_0173" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="え、カトさんの作ってくれた服が……汚れたり&#xA;破けたり………。それは、ヤだな……。" speaker_show="True" stop="True" voice="ks11010_03_0156" />
