    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="そ、そんな危険な事になりそうだったら、&#xA;すぐ私とテレちゃんが踏み込みますから！" speaker_show="True" stop="True" voice="ig11006_03_0212" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そ、そうですよ！　信先輩が血まみれになるような&#xA;大怪我する事態は決して…………あ。" speaker_show="True" stop="True" voice="mm11001_03_0320" />
