    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="白虎" text="そうか？　それはそれでカッコイイと思うぞ！&#xA;スパイは、血しぶき浴びてなんぼだろ♪" speaker_show="True" stop="True" voice="bk11008_03_0142" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ち、血しぶき!?" speaker_show="True" stop="True" voice="kt11009_03_0152" />
