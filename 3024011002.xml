    <char_spine_cheek obj_id="カトリーナ" type="Cheek03_strong" loop="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="はぁあああ……シン様。なんとお美しい！&#xA;いいえ、神々しいと言っても過言では……" speaker_show="True" stop="True" voice="kt11009_03_0147" />
    <delete obj_id="love" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="300" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、あれ!?　どうしてカトリーナさんまで&#xA;ドレスを着て……確か裏から潜入って話じゃ。" speaker_show="True" stop="True" voice="mm11001_03_0319" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek01_default" loop="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ごめんなさい。どうしてもシン様と一緒に&#xA;綺麗な格好がしてみたくて、つい……。" speaker_show="True" stop="True" voice="kt11009_03_0148" />
