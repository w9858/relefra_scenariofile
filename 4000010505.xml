    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" />
    <msg speaker="白虎" text="へえ～、ぬいぐるみ作りか……。&#xA;発明はできるのに、こういうのは苦手なんだな。" speaker_show="True" stop="True" voice="bk11008_01_0232" />
