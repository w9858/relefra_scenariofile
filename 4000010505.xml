    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="白虎" text="どうしたんだ、初芽？" speaker_show="True" stop="True" voice="bk11008_01_0231" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" loop="True" to_idle="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="False" />
