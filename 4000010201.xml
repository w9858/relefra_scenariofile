    <char_spine_animation obj_id="八千代 命" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" face_anim_type="008_Facial_serious" />
    <msg speaker="八千代 命" text="こちら千代女。&#xA;ターゲット・源モモのアジトに潜入成功。" speaker_show="True" stop="True" voice="ym11003_01_0611" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="現時刻、一六〇〇。&#xA;これよりミッションスタート。" speaker_show="True" stop="True" voice="ym11003_01_0612" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="…な～んて！　雰囲気出してみたけど、&#xA;単なるモモちへの抜き打ちテストなんだよね。" speaker_show="True" stop="True" voice="ym11003_01_0613" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="スパイたるもの、自分自身が監視される可能性にも&#xA;気をつけねばナラヌ。" speaker_show="True" stop="True" voice="ym11003_01_0614" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="この超小型カメラと盗聴器を部屋の各所に設置する。&#xA;帰ってきたモモちは、これに気づけるか…？" speaker_show="True" stop="True" voice="ym11003_01_0615" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="天井の隅、ベッドの裏、イスの裏、&#xA;あとは机の引き出しに…あれ、鍵がかかってる？" speaker_show="True" stop="True" voice="ym11003_01_0616" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="101_Facial_trouble" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="しかも、ツキカゲスパイアイテムの万能キーが&#xA;効かない、古い南京錠だ！" speaker_show="True" stop="True" voice="ym11003_01_0617" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="ふふ～ん、しかしメイほどのスパイになれば、&#xA;これぐらいピッキングで…" speaker_show="True" stop="True" voice="ym11003_01_0618" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
