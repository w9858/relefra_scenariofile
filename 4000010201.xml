    <char_spine_animation obj_id="源 モモ" body_anim_type="09_PutHands" loop="False" to_idle="True" force_end="False" />
    <char_spine_cheek obj_id="源 モモ" type="Cheek02_medium" loop="True" />
    <msg speaker="源 モモ" text="ま、まぁ中には強さと関係なさそうなこともあるけど、&#xA;意外なところに秘密があるかもしれないし…！" speaker_show="True" stop="True" voice="mm11001_01_0916" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="いやでも、綺麗ポイントとかは…" speaker_show="True" stop="True" voice="ym11003_01_0627" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="11_Point" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="それは強さの秘密というか、スパイとしての&#xA;振る舞い方の勉強として師匠の姿を観察してて！" speaker_show="True" stop="True" voice="mm11001_01_0917" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="師匠だったら映画のスパイみたいにパーティ会場に&#xA;潜入しても似合うだろうなとか！" speaker_show="True" stop="True" voice="mm11001_01_0918" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="つまり師匠のかっこよさは、もう世界レベルだと思うの！&#xA;特に流れるような黒髪が――" speaker_show="True" stop="True" voice="mm11001_01_0919" />
