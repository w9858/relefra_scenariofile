    <msg speaker="八千代 命" text="よし、開いた。" speaker_show="True" stop="True" voice="ym11003_01_0619" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="引き出しを取り外し、その裏に盗聴器を仕掛けて…&#xA;ん、なんじゃ、このノートは？" speaker_show="True" stop="True" voice="ym11003_01_0620" />
