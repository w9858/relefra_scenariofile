    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="ぜえ、ぜえ、ぜえ…め、メイちゃん…&#xA;な、何やってるの…？" speaker_show="True" stop="True" voice="mm11001_01_0914" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="101_Facial_trouble" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
