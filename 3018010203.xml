    <msg window_type="SystemMsg" text="雪は初芽の家で、囮作戦用に使った&#xA;魔女っ子の衣装を受け取っていた。" stop="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="モモちゃん、元気になるといいですね。" speaker_show="True" stop="True" voice="ah11005_02_0629" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="大丈夫よ、私の弟子なんだから。" speaker_show="True" stop="True" voice="hy11002_02_0549" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="でも、写真が撮れないだけで落ち込むなんて、&#xA;まだまだ精神的な未熟さがあるわね。" speaker_show="True" stop="True" voice="hy11002_02_0550" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふ、スパイだって常に鉄みたいな心を持っている&#xA;わけじゃありませんよ。" speaker_show="True" stop="True" voice="ah11005_02_0630" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="普段は柔らかく、必要な時には硬く。&#xA;長穂さんだってそうだったでしょう？" speaker_show="True" stop="True" voice="ah11005_02_0631" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……確かにそうね。" speaker_show="True" stop="True" voice="hy11002_02_0551" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="それじゃ、行ってくるわ。" speaker_show="True" stop="True" voice="hy11002_02_0552" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はい。" speaker_show="True" stop="True" voice="ah11005_02_0632" />
    <move obj_id="半蔵門 雪" x="1000" x_move_type="MoveTo" y_move_type="MoveValue" time="0.8" wait="True" />
    <move obj_id="青葉 初芽" x="0" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="……ふぅ。&#xA;実は私も、モモちゃんと同じなんですよねえ……。" speaker_show="True" stop="True" voice="ah11005_02_0633" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ああ、ハロウィンの時、ごえちゃんと一緒に&#xA;写真を撮っておけばよかった！" speaker_show="True" stop="True" voice="ah11005_02_0634" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="青葉 初芽" />
