    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="くぅ～、そこを何とか！&#xA;今ある玉で、ゼッタイ勝負つけるから！" speaker_show="True" stop="True" voice="ym11003_03_0374" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
