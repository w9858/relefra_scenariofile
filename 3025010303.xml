    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="うぅっ、倒れない。&#xA;ちゃんと的中したのにーーーー！" speaker_show="True" stop="True" voice="ym11003_03_0376" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
