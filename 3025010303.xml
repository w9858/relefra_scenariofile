    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="何かいきなり挙動不審になったね。" speaker_show="True" stop="True" voice="ks11010_03_0253" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="それに突然のニセ関西弁……。&#xA;怪しいとしか言いようがない！" speaker_show="True" stop="True" voice="sf11004_03_0378" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠、あのオジサン、絶対にヤってます！&#xA;無駄な戦いは止めましょう。" speaker_show="True" stop="True" voice="sf11004_03_0379" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="でもフー、ここまででメイが&#xA;いくらつぎ込んだか知ってるよね？" speaker_show="True" stop="True" voice="ym11003_03_0378" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ここで引き下がったらオジサンの思う壺！" speaker_show="True" stop="True" voice="ym11003_03_0379" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="これまで騙され続けた子供たちのためにも、&#xA;メイが当たりをゲットしなくちゃ！" speaker_show="True" stop="True" voice="ym11003_03_0380" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そう言われると、やめろって言いづらく&#xA;なっちゃいますけど……。" speaker_show="True" stop="True" voice="sf11004_03_0380" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ん？　信さん何してるんですか？&#xA;さっきから景品の箱をじーっと見て。" speaker_show="True" stop="True" voice="sf11004_03_0381" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="メイちゃんフーちゃん、&#xA;いい作戦を思いついたよ。" speaker_show="True" stop="True" voice="ks11010_03_0254" />
