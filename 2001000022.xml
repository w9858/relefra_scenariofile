    <msg speaker="ドルテ" text="ウフフ……" speaker_show="True" stop="True" voice="dt14003_01_0003" />
    <msg speaker="文鳥の女" text="あの男達の匂いは覚えたか？" speaker_show="True" stop="True" voice="bo14001_01_0024" />
