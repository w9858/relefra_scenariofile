    <create type="Character" obj_id="半蔵門 雪" data_id="311002001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="私達を襲った白虎に付着していた砂は、&#xA;九天サイエンスの施設で使われているものだった。" speaker_show="True" stop="True" voice="hy11002_01_0180" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="そこで今夜、半蔵が施設に潜入し、最上階にある&#xA;サーバーから内部データを入手しました。" speaker_show="True" stop="True" voice="ah11005_01_0115" />
