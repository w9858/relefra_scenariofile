    <move obj_id="源 モモ" position_id="left" x="-200" time="1.0" x_move_type="MoveTo" wait="True" />
    <msg speaker="源 モモ" text="連絡先、ゲットしてきました！" speaker_show="True" stop="True" voice="mm11001_01_0252" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="遅すぎるわ。もう飲み終わってパフェにいってる。&#xA;もっと頑張りなさい。" speaker_show="True" stop="True" voice="hy11002_01_0122" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" speaker_show="True" text="はい、師匠！" voice="mm11001_01_0253" stop="True" />
