    <msg speaker="半蔵門 雪" text="今日は、思っている事を顔に出さない訓練よ。&#xA;何があってもポーカーフェイスを崩さないこと。" speaker_show="True" stop="True" voice="hy11002_01_0123" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="はい！" speaker_show="True" stop="True" voice="mm11001_01_0254" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="102_Facial_unique" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
