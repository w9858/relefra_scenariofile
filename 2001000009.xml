    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="あ、あの！&#xA;ちょっとよろしいですか！" speaker_show="True" stop="True" voice="mm11001_01_0245" />
    <msg speaker="北斗 凪" text="は、はい!?" speaker_show="True" stop="True" voice="hn99002_01_0004" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="決して怪しい者じゃないんですけど、&#xA;ちょっとお話をいたしたく……。" speaker_show="True" stop="True" voice="mm11001_01_0246" />
    <msg speaker="北斗 凪" text="その制服……あなたも空崎高校の人ですよね。" speaker_show="True" stop="True" voice="hn99002_01_0005" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="そうそう！&#xA;あの、何年生ですか!?" speaker_show="True" stop="True" voice="mm11001_01_0247" />
    <msg speaker="北斗 凪" text="２年生ですけど……。" speaker_show="True" stop="True" voice="hn99002_01_0006" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="じゃあ同じだ！&#xA;私、Ｃ組の源モモっていうんだ。" speaker_show="True" stop="True" voice="mm11001_01_0248" />
    <msg speaker="北斗 凪" text="Ａ組の北斗凪です……。" speaker_show="True" stop="True" voice="hn99002_01_0007" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="Ａ組!?　結愛と同じだ！" speaker_show="True" stop="True" voice="mm11001_01_0249" />
    <msg speaker="北斗 凪" text="あ、畠山さんのお友達……？" speaker_show="True" stop="True" voice="hn99002_01_0008" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="うん！&#xA;それで、あの……。" speaker_show="True" stop="True" voice="mm11001_01_0250" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg_window_show show="False" />
    <transition type="Out" color_change="False" time="0.5" wait="False" effect="Default" />
    <bg show="True" resource_type="AnimSS" file="No_040" />
