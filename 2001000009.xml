    <msg speaker="源 モモ" text="は～。お風呂、気持ちよかった～。&#xA;……ん？" speaker_show="True" stop="True" voice="mm11001_01_0256" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
