    <delete obj_id="源 モモ" />
    <transition type="In" color_change="False" time="0.5" wait="True" effect="Default" />
    <msg speaker="源 モモ" text="よければ、私と友達になってくれませんか！" speaker_show="True" stop="True" voice="mm11001_01_0251" />
    <msg speaker="北斗 凪" text="ええっ？&#xA;あっ、は、はい……？" speaker_show="True" stop="True" voice="hn99002_01_0009" />
