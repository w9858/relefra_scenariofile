<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="PromoAdv/PromoAdv_002" />
  </info>
  <command>
    <Start />
    <auto_mode active="True" />
    <function_menu_show show="False" />
    <bg file="30_birthdayparty_01" />
    <load_cue_sheet cue_sheet="BGM/BGM18_Adv6" />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_in time="1.0" wait="True" />
    <create obj_id="青葉 初芽" data_id="311005009" x="0" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="本日、４月２日はなんと。&#xA;私、青葉 初芽の誕生日です。" stop="True" window_type="CharMsg" voice="Hatsume_001" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="って、お祝いしてくれたんですから、知ってますよね？&#xA;すみません、つい浮かれてしまって……" stop="True" window_type="CharMsg" voice="Hatsume_002" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="青葉 初芽" text="改めまして、お祝いしてくれてありがとうございました。&#xA;とっても嬉しかったです。" stop="True" window_type="CharMsg" voice="Hatsume_003" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="おかげさまでこの１年、&#xA;本当に楽しいことばかりでした。" stop="True" window_type="CharMsg" voice="Hatsume_004" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="青葉 初芽" text="ちょっとスリリングな事もありましたけど……" stop="True" window_type="CharMsg" voice="Hatsume_005" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" loop="False" />
    <msg speaker="青葉 初芽" text="それも今となっては、素敵なスパイスですね。" stop="True" window_type="CharMsg" voice="Hatsume_006" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="07_ChestBothHands" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" loop="True" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek02_medium" loop="True" />
    <msg speaker="青葉 初芽" text="これからも、一緒に過ごしてもらえると嬉しいです。" stop="True" window_type="CharMsg" voice="Hatsume_007" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek01_default" loop="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="青葉 初芽" text="実は、今日からの１年をもっと素敵に過ごせるように&#xA;新しい発明品を作ってみました。" stop="True" window_type="CharMsg" voice="Hatsume_008" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" loop="True" />
    <msg speaker="青葉 初芽" text="名付けて！伝わるくん３号！&#xA;早速使ってみますね！えいっ！" stop="True" window_type="CharMsg" voice="Hatsume_009" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="青葉 初芽" text="伝達完了！これでお友達ですね！" stop="True" window_type="CharMsg" voice="Hatsume_010" />
    <msg_window_show show="False" />
    <fade_out time="2" type="Default" wait="True" />
    <msg speaker="青葉 初芽" text="私の気持ち、伝わりましたか？" stop="True" window_type="CharMsg" voice="Hatsume_011" />
  </command>
</adv>