    <msg speaker="乱獅子 ゆら" text="凪の部隊、乱獅子ゆら！　進発！！&#xA;張り切っていくぜ！" speaker_show="True" stop="True" voice="ry12002_01_0133" />
    <msg_window_show show="False" />
