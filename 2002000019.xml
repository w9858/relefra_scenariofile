    <delete obj_id="薩摩 しぶき" />
    <fade_in time="1.0" type="Default" wait="True" />
    <telop text="逆さ塔" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001004" position_id="left" x="0" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" loop="True" to_idle="True" body_anim_type="11_Point" force_end="False" />
    <msg speaker="宗近 飛粋" text="私は、乱獅子ゆらをメンバーに入れる事を&#xA;凪の一員として、推薦します！" speaker_show="True" stop="True" voice="mh12001_01_0201" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="もしも。乱獅子ゆらが何か不祥事を起こしましたら&#xA;自分も連帯して責任を取ります！" speaker_show="True" stop="True" voice="mh12001_01_0202" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="ゆらちゃんなら絶対大丈夫ですけどね！&#xA;私はそれぐらいの覚悟があって推薦します。" speaker_show="True" stop="True" voice="mh12001_01_0203" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="CutinTransmission" />
    <fade_out time="1.0" type="Default" wait="True" />
    <screen_effect mode="Flashback" show="False" />
