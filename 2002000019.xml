    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="300" anchor="Bottom" y="0" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" anchor="Bottom" y="0" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="-300" anchor="Bottom" y="0" />
    <fade_in time="1.0" type="Default" wait="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="…………飛粋。" speaker_show="True" stop="True" voice="ry12002_01_0131" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="ここまで言ってくれている飛粋の&#xA;気持ちを裏切ったら、しぶきは貴様を許さぬぞ。" speaker_show="True" stop="True" voice="ss12003_01_0165" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="へへ、飛粋は見る目があるぜ。パイセン達も&#xA;期待してろよ！　じゃ鍛錬室行ってくるぜ！" speaker_show="True" stop="True" voice="ry12002_01_0132" />
    <msg_window_show show="False" />
    <move x="1500" x_move_type="MoveTo" time="0.5" wait="True" obj_id="乱獅子 ゆら" position_id="left" y="0" y_move_type="MoveValue" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="……ったく手の掛かる奴が増えたのう。" speaker_show="True" stop="True" voice="ss12003_01_0166" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="最終的に責任とるのは隊長のしぶきちだからね。&#xA;そこを認めるあたり、優しいよね。" speaker_show="True" stop="True" voice="th12004_01_0111" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="ヨモギさんも、ゆらは大丈夫と言っとったしの。&#xA;正義を成すために使える奴は使うだけじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0167" />
