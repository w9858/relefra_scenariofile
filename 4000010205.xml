<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010205" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="False" />
    <bg show="True" file="10_terminalmo_01" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="あら、きれいな空。&#xA;雨、すっかり上がりましたね。" speaker_show="True" stop="True" voice="ah11005_01_0538" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そうね。今日はもう止まないかと思った。&#xA;びしょ濡れでのランニングを覚悟したのだけど。" speaker_show="True" stop="True" voice="hy11002_01_0609" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="ね、雪ちゃん、せっかくですし、&#xA;ちょっと寄り道していきませんか？" speaker_show="True" stop="True" voice="ah11005_01_0539" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="新しくできたパンケーキ屋さん、&#xA;美味しいって評判なんですよ。" speaker_show="True" stop="True" voice="ah11005_01_0540" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="悪いけど、このあとトレーニングがあるから。" speaker_show="True" stop="True" voice="hy11002_01_0610" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="トレーニングなら、終わるまで待ちますよ。" speaker_show="True" stop="True" voice="ah11005_01_0541" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="気持ちはありがたいけど、今日は&#xA;少し時間をかけて走り込みをしたいの。" speaker_show="True" stop="True" voice="hy11002_01_0611" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="でも、あちこち濡れてますし、今日くらい&#xA;お休みしてもいいんじゃないですか？" speaker_show="True" stop="True" voice="ah11005_01_0542" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そういうわけにはいかない。&#xA;この街を守るのが私たちの使命だもの。" speaker_show="True" stop="True" voice="hy11002_01_0612" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="それはそうですけど……でも、雪ちゃん、&#xA;たまには女子高生らしいこともしないと。" speaker_show="True" stop="True" voice="ah11005_01_0543" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そんな時間があったらトレーニングを……。" speaker_show="True" stop="True" voice="hy11002_01_0613" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="その気持ちもわかりますけど、&#xA;いつか後悔するんじゃありませんか？" speaker_show="True" stop="True" voice="ah11005_01_0544" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="後悔？" speaker_show="True" stop="True" voice="hy11002_01_0614" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="高校生のうちにしかできないこと、&#xA;たくさんあると思うんです。" speaker_show="True" stop="True" voice="ah11005_01_0545" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="……優先すべきは、街の平和だわ。" speaker_show="True" stop="True" voice="hy11002_01_0615" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="ふふっ、雪ちゃんらしい答えですね。&#xA;だけど、私には雪ちゃんも大切ですよ。" speaker_show="True" stop="True" voice="ah11005_01_0546" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="初芽……。" speaker_show="True" stop="True" voice="hy11002_01_0616" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="今日は諦めますけど、また誘いますね。&#xA;今度、トレーニングがないとき、つき合ってください。" speaker_show="True" stop="True" voice="ah11005_01_0547" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="Walk" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <move obj_id="青葉 初芽" position_id="left" x="1500" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.5" wait="True" />
    <delete obj_id="青葉 初芽" />
    <wait time="0.5" />
    <move obj_id="半蔵門 雪" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="（トレーニングは日々の積み重ね。&#xA;私に休みなんて必要ないわ）" speaker_show="True" stop="True" voice="hy11002_01_0617" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="（でも……女子高生らしいこと、か……）" speaker_show="True" stop="True" voice="hy11002_01_0618" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>