  <command>
    <Start />
    <fade_out time="0.1" type="Default" />
    <Comment comment="軍議の社" />
    <load_cue_sheet cue_sheet="BGM/BGM04_Base" />
    <sound type="Bgm" cue_name="Base" />
    <bg show="True" file="03_tsukikage_001" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="200" anchor="Bottom" y="0" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" x="-200" anchor="Bottom" y="0" position_id="left" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="False" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="解散する前に、信の新しい特技を皆に見せておくね。&#xA;びっくりすると思うけど。" speaker_show="True" stop="True" voice="ks11010_01_0019" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="False" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="師匠、アメリカで何か身につけてきたんですか？" speaker_show="True" stop="True" voice="ym11003_01_0043" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="うん。信、右腕は治ったんだけど、&#xA;左腕はダメだったんだ。" speaker_show="True" stop="True" voice="ks11010_01_0020" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="False" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="え…。" speaker_show="True" stop="True" voice="ym11003_01_0044" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="09_PutHands" loop="False" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="実は左腕、今、義手だけど、もう本物そっくりでしょ。&#xA;言われるまで分からなかったよね？" speaker_show="True" stop="True" voice="ks11010_01_0021" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="ほら自在に動くから心配しないで。&#xA;慣れるまで時間はかかったけど。" speaker_show="True" stop="True" voice="ks11010_01_0022" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_eye obj_id="八千代 命" type="Eye03_tear" loop="True" />
    <msg speaker="八千代 命" text="…。" speaker_show="True" stop="True" voice="ym11003_01_0045" />
    <msg_window_show show="False" />
    <delete obj_id="高坂 信" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" x="0" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（…！！　師匠がそんな顔をするなんて…！！）" speaker_show="True" stop="True" voice="sf11004_01_0041" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="0" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="ふふふ。メイちゃん。見ててね。" speaker_show="True" stop="True" voice="ks11010_01_0023" />
    <msg_window_show show="False" />
    <transition type="Out" color_change="True" time="1.0" wait="True" />
    <delete obj_id="高坂 信" />
    <Comment comment="武器庫" />
    <bg show="True" file="03_tsukikage_005" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <transition type="In" color_change="True" time="1.0" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="0" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="信たちの先にある、射撃の的に注目してね。&#xA;あれを信が打ち抜いちゃうから。" speaker_show="True" stop="True" voice="ks11010_01_0024" />
    <msg_window_show show="False" />
    <delete obj_id="高坂 信" />
    <msg window_type="SystemMsg" text="そう言うと、高坂信は義手の左腕を外す。&#xA;その中には銃が装備されていた――。" stop="True" />
    <msg_window_show show="False" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" x="200" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="白虎" data_id="311007001" x="-200" y="0" anchor="Bottom" position_id="left" />
    <create_effect effect_id="exclamation_yuki" effect_type="MampExclamation" life_time="0.5" target_obj="半蔵門 雪" />
    <create_effect effect_id="exclamation_byakko" effect_type="MampExclamation" life_time="0.5" target_obj="白虎" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="白虎" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="！　左腕に仕込み銃！？" speaker_show="True" stop="True" voice="hy11002_01_0033" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="おおっ、なんかこんなマンガ見た事あるぞ。" speaker_show="True" stop="True" voice="bk11008_01_0017" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="白虎" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="0" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <sound type="Se" cue_name="HoldGun" />
    <msg speaker="高坂 信" text="ここから睡眠弾や麻酔弾、記憶消去弾が&#xA;撃てるんだよ。こんな風に、えーい！！" speaker_show="True" stop="True" voice="ks11010_01_0025" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="SuppressorGun" />
    <delete obj_id="高坂 信" />
    <wait time="1.0" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" x="0" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="…的の真ん中にしっかり命中しています。" speaker_show="True" stop="True" voice="ig11006_01_0017" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="200" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" x="-200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="訓練もしてるから実戦で使えるよ。文字通り&#xA;奥の手だから、ここぞという時以外は封印するけどね。" speaker_show="True" stop="True" voice="ks11010_01_0026" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="…ヒュー。師匠、わりとぶっ飛んでるね。&#xA;いいと思うそれ、かっこいい。" speaker_show="True" stop="True" voice="ym11003_01_0046" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="False" to_idle="True" force_end="False" />
    <msg speaker="高坂 信" text="やった、メイちゃんを驚かせた！&#xA;そういうリアクションが欲しかったんだ。" speaker_show="True" stop="True" voice="ks11010_01_0027" />
    <msg_window_show show="False" />
    <delete obj_id="八千代 命" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="（しんみりと暗くなるよりは、賑やかになって&#xA;欲しい。そういう事ですね、シン様）" speaker_show="True" stop="True" voice="kt11009_01_0021" lip_sync="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="さすがシン様ですね！　初芽ちゃんも&#xA;発明家としてワクワクする仕掛けでしょ？" speaker_show="True" stop="True" voice="kt11009_01_0022" />
    <msg_window_show show="False" />
    <move obj_id="カトリーナ" x="-200" x_move_type="MoveTo" y="0" time="0.3" wait="True" position_id="left" y_move_type="MoveValue" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" x="200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="はい、ちょっと見せてもらっていいですか？&#xA;ごくり。" speaker_show="True" stop="True" voice="ah11005_01_0022" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" x="200" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="信、私服（夏）の予定" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" x="-200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="それはちょっと気をつけた方がいいよ師匠。&#xA;初さんの技術、まだ危なっかしい所もあるから。" speaker_show="True" stop="True" voice="ym11003_01_0047" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="101_Facial_trouble" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="高坂 信" text="初芽ちゃん相変わらずだね。&#xA;いいよ、好きなだけ見て。" speaker_show="True" stop="True" voice="ks11010_01_0028" />
    <delete obj_id="高坂 信" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" x="0" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（信さんは師匠の危ない所を助けてくれた。&#xA;それで自分があんな怪我を……）" speaker_show="True" stop="True" voice="sf11004_01_0042" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（凄く感謝したいし、師匠の師匠なんだから&#xA;もっとこう、しっかり接しないといけないのに）" speaker_show="True" stop="True" voice="sf11004_01_0043" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="101_Facial_trouble" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（うぅーなんでこんなモヤモヤしてるのよ、&#xA;アタシの馬鹿。完璧とはいえないじゃない）" speaker_show="True" stop="True" voice="sf11004_01_0044" lip_sync="False" />
  </command>
