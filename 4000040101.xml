  <command>
    <Start />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="一部のキャラクターイラストと背景は仮の物を使用" />
    <bg show="True" resource_type="Background" file="44_raimuhouseev_01" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="名前、敷島来夢。誕生日、2月23日。&#xA;身長、160cm。3サイズ、84・54・85。" speaker_show="True" stop="True" voice="sr13001_02_0602" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="020_Facial_thinking" body_anim_type="03_BackBothHands" />
    <msg speaker="敷島 来夢" text="容姿端麗、品行方正、成績優秀にして&#xA;清く正しく美しくがモットーの…" speaker_show="True" stop="True" voice="sr13001_02_0603" />
    <move obj_id="敷島 来夢" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="021_Facial_exciting" body_anim_type="15_Negative" loop="False" />
    <msg speaker="敷島 真留瀬" text="お姉様、嘘はいけませんわ。&#xA;バイオデータから下は一旦削除してください。" speaker_show="True" stop="True" voice="sm13002_02_0245" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <msg speaker="敷島 来夢" text="嘘とは心外ね、マル。&#xA;どこが嘘だと言うのかしら？" speaker_show="True" stop="True" voice="sr13001_02_0604" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="020_Facial_thinking" body_anim_type="12_DropShoulders" loop="False" />
    <msg speaker="敷島 真留瀬" text="品行方正の辺りでしょうか。データベースの&#xA;最初から虚偽の入力は、見過ごせませんわ。" speaker_show="True" stop="True" voice="sm13002_02_0246" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="False" />
    <msg speaker="敷島 来夢" text="ふぅん？　じゃあどう入力するのが&#xA;私のデータとして相応しいと？" speaker_show="True" stop="True" voice="sr13001_02_0605" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="020_Facial_thinking" body_anim_type="11_Point" loop="False" />
    <msg speaker="敷島 真留瀬" text="『容姿端麗、成績優秀にしてハレンチ』&#xA;これならばお姉様のデータとして妥当かと。" speaker_show="True" stop="True" voice="sm13002_02_0247" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="敷島 来夢" text="じゃあ、そうしておきましょう。次は…&#xA;ああ、無敵甲冑の画像を乗せないとね。" speaker_show="True" stop="True" voice="sr13001_02_0606" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="敷島 来夢" text="このように変身して悪を懲らす正義のヒーローが&#xA;敷島来夢の隠された真の姿なのであった！" speaker_show="True" stop="True" voice="sr13001_02_0607" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" />
    <msg speaker="敷島 真留瀬" text="お姉様、隠している姿を資料に盛り込むのは&#xA;いかがなものでしょう？" speaker_show="True" stop="True" voice="sm13002_02_0248" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" loop="False" />
    <msg speaker="敷島 真留瀬" text="嘘を連ねてしまうと、データベースの&#xA;意味そのものがなくなってしまいますわ。" speaker_show="True" stop="True" voice="sm13002_02_0249" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="False" />
    <msg speaker="敷島 来夢" text="仕方ないわね。&#xA;………わーかったわよ。" speaker_show="True" stop="True" voice="sr13001_02_0610" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="020_Facial_thinking" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="妹の真留瀬のバストは現在89cmではあるが&#xA;未だ発展途上につき、今後も要観察。また…" speaker_show="True" stop="True" voice="sr13001_02_0612" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="敷島 来夢" text="頭脳明晰な部下を持ち、彼女らのバストサイズも&#xA;来夢自ら毎月きちんと計測しており…" speaker_show="True" stop="True" voice="sr13001_02_0613" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="敷島 真留瀬" text="お、お姉様お姉様。胸の話はそのくらいで。&#xA;御趣味などの入力に移ってください。" speaker_show="True" stop="True" voice="sm13002_02_0251" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="敷島 来夢" text="趣味、バーなどの酒場を開拓することや&#xA;読書を好むが、最も打ち込める趣味は…" speaker_show="True" stop="True" voice="sr13001_02_0614" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="敷島 来夢" text="やはり、日課ともいえる妹とのハレンチ。&#xA;ハレンチと妹に尽きるであろう……。ふぅー。" speaker_show="True" stop="True" voice="sr13001_02_0615" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="009_Facial_blush" body_anim_type="16_BendBackward" loop="True" />
    <msg speaker="敷島 真留瀬" text="そういう書き方をされてしまいますと&#xA;私がハレンチな妹のように聞こえますわ。" speaker_show="True" stop="True" voice="sm13002_02_0252" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" loop="False" />
    <msg speaker="敷島 真留瀬" text="これは、お姉様のデータであって、私のは&#xA;また別なのですから、やり直してください。" speaker_show="True" stop="True" voice="sm13002_02_0253" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" />
    <msg speaker="敷島 来夢" text="おかしいわね、何を入力しようとしても&#xA;結局、マルのことになってしまう……。" speaker_show="True" stop="True" voice="sr13001_02_0616" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" />
    <msg speaker="敷島 来夢" text="つまり、敷島来夢という人間を情報化するならば&#xA;『妹愛』のひと言に尽きるということね。" speaker_show="True" stop="True" voice="sr13001_02_0617" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="敷島 真留瀬" text="お、お姉様………素敵ですわ。&#xA;素敵すぎるデータです。ほぅ……。" speaker_show="True" stop="True" voice="sm13002_02_0254" />
    <msg speaker="敷島 来夢" text="ふっふっふ。ＺＥＴのデータベースNo.000は&#xA;こうして私の愛と素敵から始まるのであった。" speaker_show="True" stop="True" voice="sr13001_02_0618" />
    <fade_out time="1" wait="True" />
    <sound_stop cue_name="Adv1" />
  </command>
</adv>