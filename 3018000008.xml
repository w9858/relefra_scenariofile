    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="いいですよ。" speaker_show="True" stop="True" voice="ig11006_02_0342" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="（この人のカメラ……デジカメじゃなくて、&#xA;その場で現像できるインスタントカメラだ。）" speaker_show="True" stop="True" voice="ig11006_02_0343" lip_sync="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="（それに、この人の服装……仮装、なのかな？）" speaker_show="True" stop="True" voice="ig11006_02_0344" lip_sync="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="（でも、生地の摩耗具合から考えて、&#xA;ハロウィン用に最近作った衣装とは思えない……。）" speaker_show="True" stop="True" voice="ig11006_02_0345" lip_sync="False" />
