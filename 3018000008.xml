    <telop text="商店街" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="ツキカゲは商店街で作戦行動を開始した。" stop="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002014" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="見てみると、昨日よりも仮装をしている人が&#xA;増えていますね。" speaker_show="True" stop="True" voice="ig11006_02_0339" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうね。でも、明日から行われる仮装パレードは、&#xA;もっと多くの仮装者が集まるわ。" speaker_show="True" stop="True" voice="hy11002_02_0506" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="仮装パレードはソラサキハロウィンの&#xA;メインイベント……" speaker_show="True" stop="True" voice="hy11002_02_0507" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="約2500名の市民が、仮装をして大通りを練り歩く。" speaker_show="True" stop="True" voice="hy11002_02_0508" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="そうですね、私も去年見ましたけど、&#xA;すごく盛大なイベントです。" speaker_show="True" stop="True" voice="ig11006_02_0340" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="もしパレードの最中に通り魔が出現したら、&#xA;大騒ぎになるわ。なんとしても今日中に捕まえる。" speaker_show="True" stop="True" voice="hy11002_02_0509" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい。" speaker_show="True" stop="True" voice="ig11006_02_0341" />
    <msg speaker="女性２" text="あ、あの２人、可愛ない？" speaker_show="True" stop="True" voice="99041_02_0001" />
    <msg speaker="女性３" text="衣装もヤバ。超スタイルいい～。" speaker_show="True" stop="True" voice="99144_02_0001" />
    <msg speaker="女性２" text="すいませーん、&#xA;写真撮っていいですかー？" speaker_show="True" stop="True" voice="99041_02_0002" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ！　師匠とごえちゃん、&#xA;さっそく通行人から写真を撮られてます。" speaker_show="True" stop="True" voice="mm11001_02_0619" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="やはり元の素材がいいですから。&#xA;衣装に凝った甲斐がありました。" speaker_show="True" stop="True" voice="ah11005_02_0611" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="むむ……師匠の美しさを周囲から認められる嬉しさと、&#xA;師匠の写真が誰かの手に渡ることへのジェラシー……" speaker_show="True" stop="True" voice="mm11001_02_0620" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="わかります！　私もできるなら、&#xA;五右衛門のあの姿は私だけのものにしたい……" speaker_show="True" stop="True" voice="ah11005_02_0612" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="でも、これは任務ですから、グッと我慢です。&#xA;ところで、今のところ怪しい人は見かけませんね。" speaker_show="True" stop="True" voice="ah11005_02_0613" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="はい。千代女、風魔からも、&#xA;特に連絡は入っていません。" speaker_show="True" stop="True" voice="mm11001_02_0621" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="いつ犯人が現れるかわかりません。&#xA;今回は長丁場になるかもしれませんね。" speaker_show="True" stop="True" voice="ah11005_02_0614" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、また誰かが近づいて行きますよ。" speaker_show="True" stop="True" voice="mm11001_02_0622" />
    <delete obj_id="源 モモ" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="赤星 イタ子" data_id="399005001" position_id="left" x="0" y="0" anchor="Bottom" />
