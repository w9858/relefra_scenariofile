    <char_spine_cheek obj_id="結城 友奈" type="Cheek02_medium" loop="True" />
    <msg speaker="結城 友奈" text="はぁ～、気持ちイイ～♪　力加減も完璧で&#xA;ツボに指がフィットして……はぁ。" speaker_show="True" stop="True" voice="yy96001_01_0061" />
