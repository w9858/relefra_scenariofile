    <char_spine_animation obj_id="結城 友奈" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="結城 友奈" text="うっわ……強いなぁ～、五恵さん！" speaker_show="True" stop="True" voice="yy96001_01_0055" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="友奈ちゃんだって。そういう腕の取り方って&#xA;どこで習ったの？　何ていう流派の体術？" speaker_show="True" stop="True" voice="ig11006_01_0468" />
    <char_spine_animation obj_id="結城 友奈" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="結城 友奈" text="私のは、お父さんに習った物なんです。&#xA;だから、流派とかあまりなくて。" speaker_show="True" stop="True" voice="yy96001_01_0056" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="だったら、少しだけ&#xA;かけ手の指を変えてみたら良いかも。" speaker_show="True" stop="True" voice="ig11006_01_0469" />
    <char_spine_animation obj_id="結城 友奈" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="結城 友奈" text="こうですか？　あっ、ホントだ！&#xA;全然、力が入りやすい！" speaker_show="True" stop="True" voice="yy96001_01_0057" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あと、三方受けの角度は、肘を少し開くと&#xA;次に来る攻撃が見やすくなる気がするよ。" speaker_show="True" stop="True" voice="ig11006_01_0470" />
    <char_spine_animation obj_id="結城 友奈" face_anim_type="003_Facial_laugh" loop="True" to_idle="True" force_end="False" />
    <msg speaker="結城 友奈" text="わあ～、すごいすごい！&#xA;アドバイス、ありがとうございます！" speaker_show="True" stop="True" voice="yy96001_01_0058" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ううん。巨大生物と戦ってるっていうから&#xA;もっと大雑把かと思ったけど……" speaker_show="True" stop="True" voice="ig11006_01_0471" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="人体の急所も正確に把握しているみたいで&#xA;ちょっとビックリしちゃった。" speaker_show="True" stop="True" voice="ig11006_01_0472" />
    <char_spine_animation obj_id="結城 友奈" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="結城 友奈" text="ああ、それはきっと、マッサージが得意で&#xA;よくやってるからかもしれません。" speaker_show="True" stop="True" voice="yy96001_01_0059" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="フフッ、友奈ちゃんも？　実は私もなの。&#xA;やっぱり、武道やってると得意になるよね♪" speaker_show="True" stop="True" voice="ig11006_01_0473" />
    <char_spine_animation obj_id="結城 友奈" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="結城 友奈" text="はい！　人体の急所ってツボですもんね♪&#xA;あ、もしかして指圧にも流派の違いが？" speaker_show="True" stop="True" voice="yy96001_01_0060" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="え、それはどうなんだろう？&#xA;試しに、私のはね……こういう所をこう……。" speaker_show="True" stop="True" voice="ig11006_01_0474" />
    <msg_window_show show="False" />
