    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="テレちゃんと偶然、基地で会えるなんて。&#xA;しかも、こうして一緒に帰れて、なんだか嬉しいです。" speaker_show="True" stop="True" voice="ah11005_02_0927" />
    <char_spine_animation obj_id="テレジア" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="そうか？　これからだって、そういうときはあると思う。" speaker_show="True" stop="True" voice="tr11007_02_0309" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="そうですね。そういえばテレちゃん、&#xA;前に行ったカフェ、覚えていますか？" speaker_show="True" stop="True" voice="ah11005_02_0928" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="ティーノカフェのことか？" speaker_show="True" stop="True" voice="tr11007_02_0310" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はい！　私、あそこのケーキが大好きで……。&#xA;また行きませんか？" speaker_show="True" stop="True" voice="ah11005_02_0929" />
    <char_spine_animation obj_id="テレジア" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="テレジア" text="あの店はさすがに……いや、なんでもない。" speaker_show="True" stop="True" voice="tr11007_02_0311" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ああ、すみません。そうですよね。&#xA;あんなことがあった場所ですから、さすがに……。" speaker_show="True" stop="True" voice="ah11005_02_0930" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" lip_sync="False" text="（テレちゃんの気持ちを考えたら&#xA;別の場所にしたほうがいいですよね……。）" speaker_show="True" stop="True" voice="ah11005_02_0931" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="テレジア" text="別に構わない。急な話だったから、&#xA;つい不覚を取ってしまった。忘れてくれ。" speaker_show="True" stop="True" voice="tr11007_02_0312" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="相変わらず真面目ですね。&#xA;でも、やっぱり別の場所に行きましょうか。" speaker_show="True" stop="True" voice="ah11005_02_0932" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="他に行きたい店でもあるのか？" speaker_show="True" stop="True" voice="tr11007_02_0313" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="うーん、行きたいお店ですか……。&#xA;そうですね、急には思い浮かびませんけど……。" speaker_show="True" stop="True" voice="ah11005_02_0933" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="テレジア" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="でも、せっかくテレちゃんと行くのなら&#xA;変に妥協したくないっていうのはありますね。" speaker_show="True" stop="True" voice="ah11005_02_0934" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いいところがないか、&#xA;おいしいお店を調べておきます。" speaker_show="True" stop="True" voice="ah11005_02_0935" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="もしテレちゃんも何か希望があれば言ってくださいね。&#xA;食べたいものとか場所とか……。" speaker_show="True" stop="True" voice="ah11005_02_0936" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="テレジア" text="いや、私の希望は特にない。&#xA;だが、一応気にしておく。" speaker_show="True" stop="True" voice="tr11007_02_0314" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="はい。でも……良かったです。&#xA;こうしてまたテレちゃんと仲良くできて。" speaker_show="True" stop="True" voice="ah11005_02_0937" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <char_spine_eye obj_id="テレジア" type="Eye03_tear" loop="True" />
    <msg speaker="テレジア" text="……。" speaker_show="True" stop="True" voice="tr11007_02_0315" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="私にとってテレちゃんはずっと大切な友だちですから。" speaker_show="True" stop="True" voice="ah11005_02_0938" />
    <char_spine_animation obj_id="テレジア" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="テレジア" text="もういい。初芽の気持ちはわかってるから。" speaker_show="True" stop="True" voice="tr11007_02_0316" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek02_medium" loop="True" />
    <char_spine_eye obj_id="青葉 初芽" type="Eye03_tear" loop="True" />
    <msg speaker="青葉 初芽" text="テレちゃん……。" speaker_show="True" stop="True" voice="ah11005_02_0939" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek01_default" loop="True" />
    <msg speaker="青葉 初芽" text="それじゃ、一緒に行きたいお店、&#xA;たくさん調べておきますね。" speaker_show="True" stop="True" voice="ah11005_02_0940" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="全部つき合ってもらうつもりですから&#xA;覚悟しておいてください。" speaker_show="True" stop="True" voice="ah11005_02_0941" />
    <char_spine_eye obj_id="テレジア" type="Eye01_default" loop="True" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="全部か……ふふ。" speaker_show="True" stop="True" voice="tr11007_02_0317" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それじゃ、また連絡しますね。" speaker_show="True" stop="True" voice="ah11005_02_0942" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <sound_volume type="Bgm" cue_name="Adv5" volume="0" time="1.0" wait="True" />
