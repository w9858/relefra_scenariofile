<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010401" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0.0" wait="True" />
    <bg file="10_terminalmo_01" show="True" resource_type="Background" />
    <msg window_type="SystemMsg" text="その日――八千代命と相模楓は、&#xA;訓練を兼ねて鬼ごっこをしていた。" stop="True" />
    <msg_window_show show="False" />
    <fade_in time="0.5" wait="True" />
    <telop text="空崎駅前 昼" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="（スポーツ科学によれば、鬼ごっこは体力増強に&#xA;とても有効だという…）" speaker_show="True" stop="True" voice="ym11003_01_0728" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="（そのうえ、街中で行うことにより、&#xA;隠れる能力とそれを見破る能力を鍛える訓練にもなる！）" speaker_show="True" stop="True" voice="ym11003_01_0729" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ま、全部カトーさんの受け売りなんだけどねー。&#xA;さてさて、フーはどこに隠れたかな？" speaker_show="True" stop="True" voice="ym11003_01_0730" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……う～ん……。" speaker_show="True" stop="True" voice="ym11003_01_0731" />
    <msg_window_show show="False" />
    <delete obj_id="八千代 命" />
    <wait time="0.5" />
    <msg speaker="相模 楓" text="（ふっふっふ…さすがの師匠でも、&#xA;今のアタシは見つけられないでしょう！）" speaker_show="True" stop="True" voice="sf11004_01_0695" lip_sync="False" />
    <msg speaker="相模 楓" text="（パーフェクトに極めたアタシの変装術で見た目を変え、&#xA;通行人に紛れ込んでしまえば、わかるはずがない！）" speaker_show="True" stop="True" voice="sf11004_01_0696" lip_sync="False" />
    <msg_window_show show="False" />
    <wait time="0.5" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="（見つからない。&#xA;変装して通行人に紛れ込んだか…）" speaker_show="True" stop="True" voice="ym11003_01_0732" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="（フーの変装技術は本当にすごいからね。&#xA;身長以外はそっくりの見た目になるし）" speaker_show="True" stop="True" voice="ym11003_01_0733" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="よし、仕方ない！　こちらは&#xA;頭脳プレーで対抗するとしよう！" speaker_show="True" stop="True" voice="ym11003_01_0734" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="すぅ～…皆さーん、注もーく！　注目ー！&#xA;聞いてください、見て行ってください！" speaker_show="True" stop="True" voice="ym11003_01_0735" />
    <msg_window_show show="False" />
    <delete obj_id="八千代 命" />
    <wait time="0.5" />
    <msg speaker="相模 楓" text="（ん…？&#xA;何を始めたの、師匠は？）" speaker_show="True" stop="True" voice="sf11004_01_0697" lip_sync="False" />
    <msg_window_show show="False" />
    <wait time="0.5" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="今からこの八千代命が、ソラ高１年の後輩、&#xA;かわいいかわいい相模楓について話しまーす！" speaker_show="True" stop="True" voice="ym11003_01_0736" />
    <msg_window_show show="False" />
    <delete obj_id="八千代 命" />
    <wait time="0.5" />
    <msg speaker="相模 楓" text="（なっ!?）" speaker_show="True" stop="True" voice="sf11004_01_0698" lip_sync="False" />
    <msg_window_show show="False" />
    <wait time="0.5" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーはかわいいし努力家だし、&#xA;家事も料理もできるすっごく良い子なんです！" speaker_show="True" stop="True" voice="ym11003_01_0737" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="でも、そんな彼女には一風変わった性質があります。&#xA;聞いてください、相模楓のトリセツ！" speaker_show="True" stop="True" voice="ym11003_01_0738" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="急に不機嫌になることがあります。&#xA;理由を聞いたら無駄遣いを怒られます！" speaker_show="True" stop="True" voice="ym11003_01_0739" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="夜な夜な貯金通帳を見てニヤニヤします。&#xA;小さな預金額のアップにも気づいてあげましょう！" speaker_show="True" stop="True" voice="ym11003_01_0740" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="自販機があると、いつもお釣り口に手を入れます。&#xA;そういう余計なことには気づかなくていいそうです！" speaker_show="True" stop="True" voice="ym11003_01_0741" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="懲りずにとことん付き合っていけば…" speaker_show="True" stop="True" voice="ym11003_01_0742" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="400" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <move obj_id="八千代 命" position_id="left" x="-200" x_move_type="MoveTo" time="0.5" wait="False" />
    <move obj_id="相模 楓" position_id="left" x="-50" x_move_type="MoveTo" time="0.5" wait="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <msg speaker="相模 楓" text="わー、わー、わー！　何言ってるんですか、師匠！&#xA;街中で変なこと暴露しないでください！" speaker_show="True" stop="True" voice="sf11004_01_0699" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="はい、フー、捕まえた！" speaker_show="True" stop="True" voice="ym11003_01_0743" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="…捕まりました…&#xA;ずるいです、あんなの！" speaker_show="True" stop="True" voice="sf11004_01_0700" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふっふっ。メイの作戦勝ちだね！" speaker_show="True" stop="True" voice="ym11003_01_0744" />
    <msg_window_show show="False" />
    <move obj_id="相模 楓" position_id="left" x="200" x_move_type="MoveTo" time="1.0" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="うう…あんな話を街中でされて、&#xA;もうお嫁にいけません…！" speaker_show="True" stop="True" voice="sf11004_01_0701" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="大丈夫大丈夫！　その時はメイのフィアンセとして&#xA;迎え入れるからね。" speaker_show="True" stop="True" voice="ym11003_01_0745" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え…。" speaker_show="True" stop="True" voice="sf11004_01_0702" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="メイが一生面倒みてあげよう！" speaker_show="True" stop="True" voice="ym11003_01_0746" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" to_idle="True" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="し、師匠はまたそんな軽口を…！&#xA;でも…今日のことは許します。" speaker_show="True" stop="True" voice="sf11004_01_0703" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠は忘れっぽいですから、&#xA;自分が言ったことちゃんと覚えていてくださいね！" speaker_show="True" stop="True" voice="sf11004_01_0704" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" to_idle="True" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="うんうん、覚えとく覚えとくー。" speaker_show="True" stop="True" voice="ym11003_01_0747" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="絶対ですよ！" speaker_show="True" stop="True" voice="sf11004_01_0705" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>