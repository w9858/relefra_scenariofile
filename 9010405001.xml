    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <move obj_id="相模 楓" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.0" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="あ、初芽さん。&#xA;こんにちは。" speaker_show="True" stop="True" voice="sf11004_02_0681" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="楓ちゃん、こんにちは。&#xA;メイちゃんと待ち合わせですか？" speaker_show="True" stop="True" voice="ah11005_02_0847" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="はい。今日は買い出しの量が多いので、&#xA;一緒にスーパーに行く約束してるんです。" speaker_show="True" stop="True" voice="sf11004_02_0682" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="初芽さんも、誰かと待ち合わせですか？" speaker_show="True" stop="True" voice="sf11004_02_0683" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="ええ。ごえちゃんと。&#xA;私の家に遊びにきてくれるんです。" speaker_show="True" stop="True" voice="ah11005_02_0848" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そういえば、最近よく初芽さんの家に&#xA;行ってるって、ごえっちから聞きました。" speaker_show="True" stop="True" voice="sf11004_02_0684" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="テレジアと白虎も、初芽さんの家に&#xA;住んでるんですよね。" speaker_show="True" stop="True" voice="sf11004_02_0685" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうなんです。なので、&#xA;今、うちはとっても賑やかなんです。" speaker_show="True" stop="True" voice="ah11005_02_0849" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="よければ今度、楓ちゃんも&#xA;うちに遊びに来てください！" speaker_show="True" stop="True" voice="ah11005_02_0850" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え！ いいんですか！" speaker_show="True" stop="True" voice="sf11004_02_0686" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" lip_sync="False" text="（初芽さんのお家かあ……&#xA;きっとすごいんだろうな……）" speaker_show="True" stop="True" voice="sf11004_02_0687" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <msg speaker="相模 楓" lip_sync="False" text="（でも……そんなお家見たりしたら、&#xA;別世界すぎて卒倒しちゃうかも……）" speaker_show="True" stop="True" voice="sf11004_02_0688" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それでですね……もしよければ、そのうち楓ちゃんと&#xA;メイちゃんのお家にも遊びに行かせてもらえませんか？" speaker_show="True" stop="True" voice="ah11005_02_0851" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="えっ!?" speaker_show="True" stop="True" voice="sf11004_02_0689" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="実は前から、お二人が一緒に住んでいる&#xA;お家に行ってみたかったんです！" speaker_show="True" stop="True" voice="ah11005_02_0852" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そっ、それは……えっと……" speaker_show="True" stop="True" voice="sf11004_02_0690" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="相模 楓" lip_sync="False" text="（初芽さんがうちみたいなアパート⾒たら、&#xA;別の意味で卒倒しちゃう……！）" speaker_show="True" stop="True" voice="sf11004_02_0691" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="いっ、いや～……&#xA;うち、散らかってて汚いので……！" speaker_show="True" stop="True" voice="sf11004_02_0692" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうなんですか!?" speaker_show="True" stop="True" voice="ah11005_02_0853" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="へっ……!?&#xA;な、なんでそんなに嬉しそうなんですか……？" speaker_show="True" stop="True" voice="sf11004_02_0693" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="実は最近、とっても良いお掃除マシンを作ったんです！&#xA;そういうことなら、是非使っていただければと！" speaker_show="True" stop="True" voice="ah11005_02_0854" />
    <delete obj_id="相模 楓" />
    <move obj_id="青葉 初芽" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="このマシン１台で、部屋の片付け、掃き掃除、水拭き&#xA;乾拭き、さらには床のワックスがけまでできるんです！" speaker_show="True" stop="True" voice="ah11005_02_0855" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="もちろん、通常の掃除機としても使用できます。&#xA;業界最高レベルの吸引仕事率を実現しました！" speaker_show="True" stop="True" voice="ah11005_02_0856" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="さらには、電子ペットとして、思わず何時間でも&#xA;眺めてしまうような可愛さも追及して……" speaker_show="True" stop="True" voice="ah11005_02_0857" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="019_Facial_damage" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="……しまった。&#xA;変なスイッチ押しちゃったわ……。" speaker_show="True" stop="True" voice="sf11004_02_0694" />
