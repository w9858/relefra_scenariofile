    <msg speaker="石川 五恵" text="ここ何日か、元気ないよね。" speaker_show="True" stop="True" voice="ig11006_02_0366" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="うん……。" speaker_show="True" stop="True" voice="mm11001_02_0660" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="何を見てため息ついてたの？&#xA;……あ、雪先輩の仮装写真？" speaker_show="True" stop="True" voice="ig11006_02_0367" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="それで、なんでため息？&#xA;モモちなら、興奮して大喜びしそうなのに。" speaker_show="True" stop="True" voice="ym11003_02_0683" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="私も最初は喜んでたよ。&#xA;この写真だけで満足だった……。" speaker_show="True" stop="True" voice="mm11001_02_0661" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="でも、気づいちゃったんだよ！　せっかくだったら、&#xA;この姿の師匠と写真を撮っておけばよかったって!!" speaker_show="True" stop="True" voice="mm11001_02_0662" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ああ～……そういうこと。" speaker_show="True" stop="True" voice="ym11003_02_0684" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="20_PositiveDouble" loop="False" force_end="False" />
    <msg speaker="源 モモ" text="人の欲望って、底がないんだね……。" speaker_show="True" stop="True" voice="mm11001_02_0663" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="なんか哲学的なこと言い出した！" speaker_show="True" stop="True" voice="ym11003_02_0685" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="モモち、だったら雪先輩に頼んでみれば&#xA;いいんじゃないかな。" speaker_show="True" stop="True" voice="ig11006_02_0368" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="でも、師匠にそんなこと言うの恥ずかしいし……。" speaker_show="True" stop="True" voice="mm11001_02_0664" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="伝えなかったら、きっと後悔するよ……？&#xA;モモちはそれでいいの？" speaker_show="True" stop="True" voice="ig11006_02_0369" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="それは……。" speaker_show="True" stop="True" voice="mm11001_02_0665" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="伝えずに後悔するより、伝えた方がいいよ！&#xA;もしダメだったら、私が慰めてあげるから……！" speaker_show="True" stop="True" voice="ig11006_02_0370" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ごえちゃん……！" speaker_show="True" stop="True" voice="mm11001_02_0666" />
