    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <sound cue_name="Typing" />
    <msg speaker="轟 葉栖美" text="ダメだ、人形の起動が出来ない……！&#xA;ああもう、テクノロジーで負けてるなら…！" speaker_show="True" stop="True" voice="th12004_01_0118" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <sound cue_name="Typing" />
    <msg speaker="轟 葉栖美" text="意思伝達の特技でフォローするっ！　アタシは&#xA;無機物すらイケルようになってやる。" speaker_show="True" stop="True" voice="th12004_01_0119" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="True" />
    <msg speaker="轟 葉栖美" text="動いてくれ、動いてくれっ……！！&#xA;頼む、皆動いてくれ……っ！！" speaker_show="True" stop="True" voice="th12004_01_0120" />
    <msg_window_show show="False" />
    <sound_stop cue_name="Quest1" />
    <sound cue_name="RoboWalk" />
    <wait time="0.2" />
    <create_effect effect_id="hasumi_ex" effect_type="MampExclamation" life_time="2" target_obj="轟 葉栖美" />
    <sound cue_name="RoboWalk" />
    <wait time="1" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="011_Facial_surprise" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="011_Facial_surprise" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="！？　ほ、本当に動いた！！&#xA;あははは、凄いぞアタシ！！" speaker_show="True" stop="True" voice="th12004_01_0121" />
    <msg_window_show show="False" />
    <sound type="Bgm" cue_name="Quest1" />
