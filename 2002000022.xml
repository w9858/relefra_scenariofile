    <sound cue_name="Slash" volume="100" />
    <wait time="1" />
    <msg speaker="大八洲兵" text="……ひ、ひぃ、な、なんだこいつ。" speaker_show="True" stop="True" voice="99029_01_0002" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="悪い子というものは、仕置きされねばならん。&#xA;町中で暴れる特上の悪い子なぞは特上の仕置きじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0177" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="しぶきは、それを成すものだ。さぁ取り押さえるぞ。&#xA;少し荒っぽいが。まぁそれは我慢せい。な！" speaker_show="True" stop="True" voice="ss12003_01_0178" />
    <shake target="MsgWindow" x="10" y="10" vibrate="20" time="0.5" />
    <msg speaker="大八洲兵" text="ウッギャアアアアア！！！！！" speaker_show="True" stop="True" voice="99029_01_0003" />
    <msg_window_show show="False" />
    <delete obj_id="薩摩 しぶき" />
    <msg speaker="宗近 飛粋" text="こちら不動！　賊のリーダー確保に成功しました。" speaker_show="True" stop="True" voice="mh12001_01_0211" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="同じく、ええとアタシ様のコードネームは？&#xA;これか。同じく愛染！　任務完了！" speaker_show="True" stop="True" voice="ry12002_01_0153" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="２人ともナーイス！　ちなみに孔雀も&#xA;ミッションコンプだよ！" speaker_show="True" stop="True" voice="th12004_01_0123" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="やー。機械は動物と違って話は聞いてくれないと&#xA;思ってたけど、念じればなんとかなるもんだね。" speaker_show="True" stop="True" voice="th12004_01_0124" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="津守 ヨモギ" text="葉栖美がさらりと凄いことを言ってるが。&#xA;これで終わったな。" speaker_show="True" stop="True" voice="ty12005_01_0088" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="警察の重要拠点を襲えば全滅する。小学生でも&#xA;分かりそうなものじゃ。まぁ良い見せしめじゃろ。" speaker_show="True" stop="True" voice="ss12003_01_0179" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="正義は成された！　波風が一瞬立ったが&#xA;すぐに凪の状態に戻るであろう……。" speaker_show="True" stop="True" voice="ss12003_01_0180" />
