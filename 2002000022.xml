    <msg speaker="乱獅子 ゆら" text="何より驚いたのが、お前だよ飛粋。怒るとマジで&#xA;怖かったんだな。アタシ様でもゾクリときた。" speaker_show="True" stop="True" voice="ry12002_01_0155" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="そりゃあ怒るよ！　大事な友達をぼこぼこに&#xA;されたら誰だって、そうなるよ。" speaker_show="True" stop="True" voice="mh12001_01_0213" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="009_Facial_blush" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_eye obj_id="乱獅子 ゆら" type="Eye03_tear" loop="True" />
