    <char_spine_animation obj_id="乱獅子 ゆら" loop="True" to_idle="True" force_end="False" face_anim_type="016_Facial_smileBitter" />
    <msg speaker="乱獅子 ゆら" text="いや全然。と、友達か……へ、へへ。" speaker_show="True" stop="True" voice="ry12002_01_0157" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="……ゆらちゃん！？　どうしたの、&#xA;どこか痛いの！？" speaker_show="True" stop="True" voice="mh12001_01_0215" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="いやなんかこうさぁ……生まれて来たのもさ、&#xA;悪くねぇなって思ったのよ。" speaker_show="True" stop="True" voice="ry12002_01_0158" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="ゆらちゃん。" speaker_show="True" stop="True" voice="mh12001_01_0216" />
