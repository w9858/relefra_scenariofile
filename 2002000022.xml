    <transition effect="Default" type="In" color_change="True" time="0.5" wait="True" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="こちら孔雀。動かなかった防衛人形の起動に成功！&#xA;囚人達もキリキリと働いてるみたいよ！" speaker_show="True" stop="True" voice="th12004_01_0122" />
    <msg speaker="薩摩 しぶき" text="ふっ。しぶきも安心して攻めていけると&#xA;いうものじゃ！　１人たりとて逃がさんぞ！" speaker_show="True" stop="True" voice="ss12003_01_0175" />
    <msg speaker="大八洲兵" text="くっ！　あの黒スーツが危険なやつだ！&#xA;あいつを仕留めろ。" speaker_show="True" stop="True" voice="99029_01_0001" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="貴様ら運が悪かったな。凪では１番しぶきが手荒い。" speaker_show="True" stop="True" voice="ss12003_01_0176" />
    <delete obj_id="薩摩 しぶき" />
    <msg window_type="SystemMsg" text="しぶきは攻め来たる大八洲の部隊を文字通り&#xA;ちぎっては投げ、ちぎっては投げ続けた。" stop="True" />
    <sound cue_name="RoboDown" volume="100" />
