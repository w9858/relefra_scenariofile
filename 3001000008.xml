    <msg speaker="石川 五恵" text="オッケー。" speaker_show="True" stop="True" voice="ig11006_01_0155" />
    <sound type="Se" />
    <Comment comment="SE拳をぽきぽき鳴らす音&#xD;" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="え？　あ、ちょっと待って……こんなボロボロなのに、&#xA;あなたのパンチなんて食らったら……！" speaker_show="True" stop="True" voice="ms98005_01_0049" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="大丈夫だよ、結構ピンピンしてるみたいだし。" speaker_show="True" stop="True" voice="ig11006_01_0156" />
    <msg_window_show show="False" />
    <delete obj_id="石川 五恵" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="ショコラちゃん" text="待って待って、待ってってばぁぁぁ！" speaker_show="True" stop="True" voice="ms98005_01_0050" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="おいコラ、ガキ！　待てって言ってんだろうが！" speaker_show="True" stop="True" voice="ms98005_01_0051" />
