    <msg speaker="相模 楓" text="ふぅん、ウーメン総帥ね。&#xA;それがアンタたちのボスってわけ？" speaker_show="True" stop="True" voice="sf11004_01_0281" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="ウフフフフ、そうよぉ～ん♪&#xA;ウーメン総帥の愛が、世界を平和に導くのよぉ～ん♪" speaker_show="True" stop="True" voice="ms98005_01_0048" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あっそ。じゃ、ごえっちお願い。" speaker_show="True" stop="True" voice="sf11004_01_0282" />
