    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ふぅ…なんだか前にも見た光景ね。" speaker_show="True" stop="True" voice="sf11004_01_0278" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="うん。それにこっちの数が半分だった影響も、&#xA;ほとんど感じなかったかも。" speaker_show="True" stop="True" voice="ig11006_01_0153" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="また１人だけっていうのも気になるね。小さな組織&#xA;だって聞いてたけど、やっぱり不自然だよ。" speaker_show="True" stop="True" voice="mm11001_01_0480" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そうね…てことはショコラは単なる目くらましで、&#xA;この工場も本命じゃない可能性が高いわね…。" speaker_show="True" stop="True" voice="sf11004_01_0279" />
    <msg_window_show show="False" />
    <delete obj_id="相模 楓" />
    <delete obj_id="石川 五恵" />
