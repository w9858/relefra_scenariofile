    <create type="Character" obj_id="ショコラちゃん" data_id="398005001" position_id="left" y="0" x="0" anchor="Bottom" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="ウ…ウフ、フ…&#xA;ええ…その、通りよぉ、ん…。" speaker_show="True" stop="True" voice="ms98005_01_0039" />
    <delete obj_id="ショコラちゃん" />
    <create type="Character" obj_id="石川 五恵" data_id="311006001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="また起き上がってきた…&#xA;ほんと、タフさだけは一級品だよねぇ。" speaker_show="True" stop="True" voice="ig11006_01_0154" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="で？　アンタたちは、いったい何を企んでるの？&#xA;洗いざらい話した方が身のためよ？" speaker_show="True" stop="True" voice="sf11004_01_0280" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="ショコラちゃん" data_id="398005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="ウ…ウフ、フ…。" speaker_show="True" stop="True" voice="ms98005_01_0040" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="街にばら撒いた、チョコは…サンプルデータを&#xA;採るための…試作品に過ぎない…。" speaker_show="True" stop="True" voice="ms98005_01_0041" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="そして…採取したデータは、ついさっき…&#xA;ある場所へ、送らせてもらったわぁ…。" speaker_show="True" stop="True" voice="ms98005_01_0042" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="今頃は…あたしの愛しの、ダーリンが…&#xA;ラヴァーズチョコを、完成させているはずよぉ…。" speaker_show="True" stop="True" voice="ms98005_01_0043" />
    <delete obj_id="ショコラちゃん" />
    <create type="Character" obj_id="源 モモ" data_id="311001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ラヴァーズチョコ…？" speaker_show="True" stop="True" voice="mm11001_01_0481" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="ショコラちゃん" data_id="398005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="そのチョコをひとたび口にすれば…その人は一瞬で、&#xA;ラヴァーズ・アゲインの思想に染まる…。" speaker_show="True" stop="True" voice="ms98005_01_0044" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="そして、その唇からはチョコと同じ成分が分泌され、&#xA;人から人へと感染していくの…。" speaker_show="True" stop="True" voice="ms98005_01_0045" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="ウフフ、わかるかしら…？　彼らがディープキスを&#xA;繰り返すたび、あたしたちの同志は増えていくのよ…！" speaker_show="True" stop="True" voice="ms98005_01_0046" />
    <char_spine_animation obj_id="ショコラちゃん" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="ショコラちゃん" text="性別に関係なく、ラヴァーズ・アゲインの同志が！&#xA;私のダーリン、ウーメン総帥の奴隷がね！" speaker_show="True" stop="True" voice="ms98005_01_0047" />
