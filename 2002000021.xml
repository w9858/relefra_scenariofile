    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" face_anim_type="005_Facial_sad" />
    <msg speaker="乱獅子 ゆら" text="は、はぁっ……はぁっ……はぁっ。&#xA;あ、あやめてめぇ、すり潰してかまぼこにしてやる。" speaker_show="True" stop="True" voice="ry12002_01_0147" />
    <char_spine_animation obj_id="乱獅子 あやめ" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="回復が早い特技ってのは、ある意味辛いなぁ。&#xA;壊れりゃいっそ楽だろうに。痛みの繰り返しだ。" speaker_show="True" stop="True" voice="ra15002_01_0025" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="だがそれも限界だろ。最後のチャンスだゆらぁ。&#xA;戻ってこい。お前のために俺は無茶して来たんだ。" speaker_show="True" stop="True" voice="ra15002_01_0026" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="じょ、冗談じゃねぇ。飛粋と一緒に働くって……&#xA;アタシ様が、決めたんだ。飛粋と……。" speaker_show="True" stop="True" voice="ry12002_01_0148" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ああそ。じゃあ死ね！　馬鹿野郎が！！" speaker_show="True" stop="True" voice="ra15002_01_0027" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <msg window_type="SystemMsg" text="あやめ必殺の斬撃が、ゆらの首めがけて放たれた。&#xA;最期を覚悟した、ゆらが思わず口に出した言葉は…。" stop="True" />
    <msg speaker="乱獅子 ゆら" text="……飛粋。" speaker_show="True" stop="True" voice="ry12002_01_0149" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="True" />
