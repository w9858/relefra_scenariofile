    <wait time="1.5" />
    <msg speaker="宗近 飛粋" text="ここにいるよ、ゆらちゃん！！" speaker_show="True" stop="True" voice="mh12001_01_0207" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……飛粋！！" speaker_show="True" stop="True" voice="ry12002_01_0150" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="何！？　てめぇこの野郎。ゆらをたぶらかした&#xA;女じゃねぇか。許されねぇぞ！" speaker_show="True" stop="True" voice="ra15002_01_0028" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="……許さないのはこっちだよ。&#xA;ゆらちゃんをこんなにボロボロにして……。" speaker_show="True" stop="True" voice="mh12001_01_0208" />
    <wait time="0.5" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="！？　今、俺は震えたのか……コイツ！？" speaker_show="True" stop="True" voice="ra15002_01_0029" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="凪の部隊として、国家の安寧を乱す&#xA;暴徒を鎮圧する！" speaker_show="True" stop="True" voice="mh12001_01_0209" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……へへ。遅かったな。まぁ飛粋の活躍できる&#xA;場所も、少し空けといてやったぜ。" speaker_show="True" stop="True" voice="ry12002_01_0151" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="うん！　一緒にやろう、ゆらちゃん！" speaker_show="True" stop="True" voice="mh12001_01_0210" />
