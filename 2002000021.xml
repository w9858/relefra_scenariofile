    <create type="Character" obj_id="乱獅子 あやめ" data_id="315002001" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="その黒スーツ全然似合ってないぜ、ゆらよ。&#xA;ボロボロになってまで何をらしくねー事してるんだ。" speaker_show="True" stop="True" voice="ra15002_01_0021" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="来たか…！　ムチャクチャしやがって。&#xA;そこまで頭イかれてたのかよ。" speaker_show="True" stop="True" voice="ry12002_01_0143" />
    <char_spine_animation obj_id="乱獅子 あやめ" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" face_anim_type="011_Facial_surprise" />
    <msg speaker="乱獅子 あやめ" text="お前は絶っっっっ対国家のために働くなんて&#xA;性格はしてねぇ。何かチップでも埋められたか？" speaker_show="True" stop="True" voice="ra15002_01_0022" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="チップはねぇよ。そして言うとおり、別に&#xA;アタシ様は国家のために働いてねぇさ。" speaker_show="True" stop="True" voice="ry12002_01_0144" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="乱獅子ゆらは、飛粋のために！　働いてるんだ！&#xA;あいつがアタシ様と働きたいっていうから、やる！" speaker_show="True" stop="True" voice="ry12002_01_0145" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="……何だそりゃ。ゆらぁ。お前は俺のものだろ！？" speaker_show="True" stop="True" voice="ra15002_01_0023" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="アタシ様はアタシ様のものだね。&#xA;お前とはもうバイバイだ。姉様……いや乱獅子あやめ。" speaker_show="True" stop="True" voice="ry12002_01_0146" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="１人でいきがりやがって。じわじわと痛めつけて&#xA;恐怖で思い出させてやる。誰のものかってのをな！" speaker_show="True" stop="True" voice="ra15002_01_0024" />
    <msg_window_show show="False" />
