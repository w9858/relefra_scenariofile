    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="は～、いい匂い～。&#xA;この匂いだけでご飯３杯いけるね！" speaker_show="True" stop="True" voice="ym11003_01_0695" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="はいはい。&#xA;たまには師匠が作ってくれてもいいんですよ？" speaker_show="True" stop="True" voice="sf11004_01_0664" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <char_spine_animation obj_id="八千代 命" loop="True" to_idle="False" face_anim_type="002_Facial_smile" />
    <msg speaker="八千代 命" text="いや～、メイは作るより食べる担当だから。" speaker_show="True" stop="True" voice="ym11003_01_0696" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="11_Point" loop="False" to_idle="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="まあ、そんな感じですよね。&#xA;よし、できた！　はい、お待たせしましたー。" speaker_show="True" stop="True" voice="sf11004_01_0665" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="わぁ、美味しそう～！　いただきまーす！&#xA;モグモグ……。" speaker_show="True" stop="True" voice="ym11003_01_0697" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="09_PutHands" loop="True" to_idle="False" face_anim_type="003_Facial_laugh" />
    <msg speaker="八千代 命" text="むはー、生き返る～!!" speaker_show="True" stop="True" voice="ym11003_01_0698" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="ふぅ……普段は頼りになるのに……。" speaker_show="True" stop="True" voice="sf11004_01_0666" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="ん？　何か言った？" speaker_show="True" stop="True" voice="ym11003_01_0699" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="い、いえ、師匠の食べっぷり、&#xA;相変わらず見事だなと思って！" speaker_show="True" stop="True" voice="sf11004_01_0667" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="何それ、嫌味？" speaker_show="True" stop="True" voice="ym11003_01_0700" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="違いますよ！　ゆっくり食べてくださいね！" speaker_show="True" stop="True" voice="sf11004_01_0668" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="……なーんか、怪しいけど……ま、いっか。&#xA;これって、お代わりあるの？" speaker_show="True" stop="True" voice="ym11003_01_0701" />
