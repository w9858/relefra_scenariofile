    <msg speaker="八千代 命" text="ただいまー、はー、疲れたー。" speaker_show="True" stop="True" voice="ym11003_01_0688" />
    <move obj_id="八千代 命" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="お帰りなさい。&#xA;いつもよりお疲れみたいですね。" speaker_show="True" stop="True" voice="sf11004_01_0658" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="019_Facial_damage" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="今日は体育で体力使いまくり。&#xA;もー、お腹ペコペコのぺっこりんだよー。" speaker_show="True" stop="True" voice="ym11003_01_0689" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="はいはい、すぐ用意します。&#xA;何が食べたいですか？" speaker_show="True" stop="True" voice="sf11004_01_0659" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" loop="False" to_idle="False" />
    <char_spine_animation obj_id="八千代 命" loop="True" to_idle="False" face_anim_type="002_Facial_smile" />
    <msg speaker="八千代 命" text="んー、美味しいものならなんでもいーよー。&#xA;てか、もー、なーんも考えたくないー。" speaker_show="True" stop="True" voice="ym11003_01_0690" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="ちょっ……そのまま寝転がらないでくださいよ。&#xA;それじゃ、あるもので何か適当に作りますね。" speaker_show="True" stop="True" voice="sf11004_01_0660" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="ふぁーい……。&#xA;あ、賞味期限切れには気をつけてよねー。" speaker_show="True" stop="True" voice="ym11003_01_0691" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="……もしかして、あのときのこと、&#xA;まだ根に持ってるんですか？" speaker_show="True" stop="True" voice="sf11004_01_0661" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="そりゃそうだよ！　賞味期限不明のお菓子のせいで&#xA;しばらくお腹が痛かったんだから。&#xA;//企画書のネタ参照" speaker_show="True" stop="True" voice="ym11003_01_0692" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="でもあれ、本当にお菓子が原因ですかねえ？" speaker_show="True" stop="True" voice="sf11004_01_0662" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="どう言うコト？" speaker_show="True" stop="True" voice="ym11003_01_0693" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="だって、同じものを食べた半蔵さんは&#xA;ピンピンしてましたよ？" speaker_show="True" stop="True" voice="sf11004_01_0663" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="八千代 命" text="あー、ユッキーは胃袋も鋼鉄だから……。" speaker_show="True" stop="True" voice="ym11003_01_0694" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="True" time="0.5" wait="True" />
