    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="ねーねー、フーちゃん、&#xA;一緒に買い物に行かない？" speaker_show="True" stop="True" voice="mm11001_02_0716" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="相模 楓" text="ごめん、アタシパス。" speaker_show="True" stop="True" voice="sf11004_02_0563" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="え、何か用事でもあった？" speaker_show="True" stop="True" voice="mm11001_02_0717" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="07_ChestBothHands" loop="True" />
    <msg speaker="相模 楓" text="そういう訳じゃないんだけどね、師匠がまた&#xA;無駄遣いしちゃったせいで、今月ピンチなのよ。" speaker_show="True" stop="True" voice="sf11004_02_0564" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="源 モモ" text="そういうことなら、フーちゃんこれを見て！&#xA;じゃじゃーん！" speaker_show="True" stop="True" voice="mm11001_02_0718" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="商品券？&#xA;どうしたのこれ？" speaker_show="True" stop="True" voice="sf11004_02_0565" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="はい、これはフーちゃんの分だよ。" speaker_show="True" stop="True" voice="mm11001_02_0719" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" />
    <msg speaker="相模 楓" text="え、これアタシのお小遣い一ヶ月分くらい&#xA;あるじゃないの！" speaker_show="True" stop="True" voice="sf11004_02_0566" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="何でいきなりモモちがアタシに。" speaker_show="True" stop="True" voice="sf11004_02_0567" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="私からじゃなくて、カトリーナさんからなんだ。&#xA;沢山もらったからみんなで平等に分けてねって。" speaker_show="True" stop="True" voice="mm11001_02_0720" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="相模 楓" text="うっそー！　カトリーナさん！&#xA;ありがた過ぎて居ないのに後光が見える……。" speaker_show="True" stop="True" voice="sf11004_02_0568" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="源 モモ" text="そーいうことだから、ね、今から一緒に&#xA;買い物行こうよ！" speaker_show="True" stop="True" voice="mm11001_02_0721" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="洗剤の替えと、マヨネーズ、シャンプーも&#xA;もう少しでなくなるし、それから……" speaker_show="True" stop="True" voice="sf11004_02_0569" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" loop="True" />
    <msg speaker="源 モモ" text="フーちゃん？" speaker_show="True" stop="True" voice="mm11001_02_0722" />
    <msg speaker="相模 楓" text="それから、アレとこれも買って、&#xA;せっかくだから、１ランク上のを買ったとしたら……" speaker_show="True" stop="True" voice="sf11004_02_0570" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" loop="True" />
    <msg speaker="源 モモ" text="ねえフーちゃん、行かないの？" speaker_show="True" stop="True" voice="mm11001_02_0723" />
    <msg speaker="相模 楓" text="今までやつの金額を全部合計すると……" speaker_show="True" stop="True" voice="sf11004_02_0571" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="よし、これならいけるわ！" speaker_show="True" stop="True" voice="sf11004_02_0572" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="フーちゃん？　どうしたの？" speaker_show="True" stop="True" voice="mm11001_02_0724" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="相模 楓" text="今月ケチっていろいろ買いそびれてたんだけど、&#xA;これで全部、解決だわっ！" speaker_show="True" stop="True" voice="sf11004_02_0573" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="相模 楓" text="よーし買い物いくぞー！" speaker_show="True" stop="True" voice="sf11004_02_0574" />
    <msg_window_show show="False" />
    <fade_out time="1" wait="True" />
