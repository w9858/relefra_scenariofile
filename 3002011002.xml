<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3002011002" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="04_shopstreetmo_01" resource_type="Background" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009009" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="カトーさん、ヘアオイルって私は普段&#xA;使わないんですけど、やっぱり使うと違います？" speaker_show="True" stop="True" voice="ig11006_01_0254" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="そうね、トリートメントだけでもサラサラには&#xA;なるけど、こういう乾燥する時期にはオススメよ。" speaker_show="True" stop="True" voice="kt11009_01_0262" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="髪を乾かした後、ヘアオイルを少しつけて&#xA;お手入れするだけで、とっても綺麗になるし。" speaker_show="True" stop="True" voice="kt11009_01_0263" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="そうなんですね。私も使ってみようかな……。&#xA;あ、あそこのお店に色々ありそうですよ！" speaker_show="True" stop="True" voice="ig11006_01_0255" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="あら、入浴剤にシャンプーにトリートメント、&#xA;お風呂用品が随分たくさん並んでるわね。" speaker_show="True" stop="True" voice="kt11009_01_0264" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ちょうどセールをやってるみたいだし、&#xA;私も何か買って行こうかな。" speaker_show="True" stop="True" voice="ig11006_01_0256" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="ごえちゃん、ちょっと見て！&#xA;この棚に並んでいるの、全部入浴剤よね？" speaker_show="True" stop="True" voice="kt11009_01_0265" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="わー、いっぱいありますね！&#xA;ラベンダーにジャスミンにバラの香りも……。" speaker_show="True" stop="True" voice="ig11006_01_0257" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="ストロベリーにピーチにメロン……、&#xA;えっ、ちょっとやだ！" speaker_show="True" stop="True" voice="kt11009_01_0266" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="カレーの香りの入浴剤、なんて物まであるわ！" speaker_show="True" stop="True" voice="kt11009_01_0267" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="これ、プレゼント用に結構人気なんですよ。" speaker_show="True" stop="True" voice="ig11006_01_0258" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="お風呂に溶かして入ると、カレーの具の気分が&#xA;味わえるんだとかで……。" speaker_show="True" stop="True" voice="ig11006_01_0259" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="うーん、確かにスパイスの香りは美容や健康にも&#xA;良い効果をもたらすし……" speaker_show="True" stop="True" voice="kt11009_01_0268" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="なかなかレアな体験ができそうね。" speaker_show="True" stop="True" voice="kt11009_01_0269" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="あ～、気になる香りが多すぎて迷ってしまうわ！&#xA;ごえちゃん、あなたのおすすめはどれ？" speaker_show="True" stop="True" voice="kt11009_01_0270" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ふふ、カトーさん楽しそう。&#xA;元気になってくれて良かったです。" speaker_show="True" stop="True" voice="ig11006_01_0260" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="あら、私ったらつい興奮してしまったわね。" speaker_show="True" stop="True" voice="kt11009_01_0271" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="アメリカ人は基本的にシャワーで&#xA;済ませちゃうから……" speaker_show="True" stop="True" voice="kt11009_01_0272" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="これだけの種類の入浴剤が並んでいる光景は&#xA;何度見ても珍しくてね。ふふ。" speaker_show="True" stop="True" voice="kt11009_01_0273" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="わかります！　次々と新商品も出てくるから、&#xA;日本人の私でも迷っちゃうくらいです。" speaker_show="True" stop="True" voice="ig11006_01_0261" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="本当に日本人はお風呂……というか湯船に&#xA;浸かるのが好きよね。" speaker_show="True" stop="True" voice="kt11009_01_0274" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="そうですね。これだけ寒いと、湯船にゆっくり&#xA;浸かるのが最高のリラックスタイムになりますし。" speaker_show="True" stop="True" voice="ig11006_01_0262" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="カトーさんはやっぱり、普段から湯船じゃなくて、&#xA;シャワーばっかりなんですか？" speaker_show="True" stop="True" voice="ig11006_01_0263" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="そんなことないわ！&#xA;今では私もすっかりお風呂の虜よ。" speaker_show="True" stop="True" voice="kt11009_01_0275" />
  </command>
</adv>