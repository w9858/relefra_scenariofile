  <command>
    <Start />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" resource_type="Background" file="31_shoppingmallev_03" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="ショッピングモール" wait="True" />
    <create type="Character" data_id="311001003" position_id="left" x="-200" y="0" anchor="Bottom" obj_id="源 モモ" />
    <create type="Character" data_id="311004003" position_id="left" x="200" y="0" anchor="Bottom" obj_id="相模 楓" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="源 モモ" text="女子高生のお買い物って言ったら、&#xA;何はなくともお洋服だよね！" speaker_show="True" stop="True" voice="mm11001_02_0725" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="そう？　アタシはスーパーとか&#xA;ドラッグストアの方が色々売ってて好きだけど……。" speaker_show="True" stop="True" voice="sf11004_02_0575" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="でもまぁ、荷物になるからまずは&#xA;モモちの買い物に付き合ってあげるわ。" speaker_show="True" stop="True" voice="sf11004_02_0576" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="ありがとう！　わっ、このスカートかわいい～！&#xA;それからこっちのシャツも！" speaker_show="True" stop="True" voice="mm11001_02_0726" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="相模 楓" text="ほんと、可愛い服がいっぱいあるわね。&#xA;これも可愛いし、こっちもいいわね。" speaker_show="True" stop="True" voice="sf11004_02_0577" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="相模 楓" text="師匠も元が可愛いんだから、&#xA;もっとこーいう女の子らしい服着たらいいのに。" speaker_show="True" stop="True" voice="sf11004_02_0578" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="確かにメイちゃんて、&#xA;そういうイメージないねー。" speaker_show="True" stop="True" voice="mm11001_02_0727" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" />
    <msg speaker="相模 楓" text="そうなのよ、ラクに着れるのが最優先って感じで。&#xA;勿体ないわよね、似合う服いっぱいあるのに……。" speaker_show="True" stop="True" voice="sf11004_02_0579" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="あっ、ほら、この白いワンピースとか……。" speaker_show="True" stop="True" voice="sf11004_02_0580" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="源 モモ" text="可愛い！&#xA;メイちゃんに似合いそう！" speaker_show="True" stop="True" voice="mm11001_02_0728" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="でしょ。" speaker_show="True" stop="True" voice="sf11004_02_0581" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="メイちゃんがこれ着てるとこ、見てみたいな。&#xA;一緒に買い物、来ればよかったね。" speaker_show="True" stop="True" voice="mm11001_02_0729" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="う～ん。" speaker_show="True" stop="True" voice="sf11004_02_0582" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="フーちゃん？" speaker_show="True" stop="True" voice="mm11001_02_0730" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="相模 楓" text="よし、決めた！　アタシ、&#xA;このワンピースを師匠にプレゼントする！" speaker_show="True" stop="True" voice="sf11004_02_0583" />
    <char_spine_animation />
    <msg speaker="源 モモ" text="わぁ、フーちゃんてば弟子の鑑！&#xA;メイちゃん絶対喜ぶと思う！" speaker_show="True" stop="True" voice="mm11001_02_0731" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="当たり前でしょ？&#xA;アタシは完璧な弟子なんだから。" speaker_show="True" stop="True" voice="sf11004_02_0584" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="でもいいの？&#xA;この服の値段、商品券の金額ギリギリだけど……。" speaker_show="True" stop="True" voice="mm11001_02_0732" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" />
    <msg speaker="相模 楓" text="まぁ、何とかなるわよ。師匠も同じ額の商品券&#xA;貰ってる訳だし。" speaker_show="True" stop="True" voice="sf11004_02_0585" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="そっか！　けど本当フーちゃんは&#xA;メイちゃんのこと大好きだね。" speaker_show="True" stop="True" voice="mm11001_02_0733" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" />
    <msg speaker="源 モモ" text="私も師匠に何か、プレゼントしよーっと！" speaker_show="True" stop="True" voice="mm11001_02_0734" />
    <msg_window_show show="False" />
    <fade_out time="1" wait="True" />
    <sound_stop cue_name="Adv1" />
