    <sound type="Se" cue_name="Push" />
    <wait time="0.5" />
    <msg speaker="敷島 真留瀬" text="ふぁっ……お姉様、朝のハレンチはそのくらいで。&#xA;そろそろ朝礼のお時間ですわ。" speaker_show="True" stop="True" voice="sm13002_02_0255" />
    <screen_effect mode="FlashbackWhite" show="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="おや、シャワーもコーヒーも済ませたというのに&#xA;すっかり寝惚けていたようね。いけない。" speaker_show="True" stop="True" voice="sr13001_02_0620" />
    <msg speaker="来夢ガールズＡ" text="社長ぉ、もとい来夢様ぁ、もとい来夢社長様ぁ。&#xA;来夢ガールズ全員揃っておりますぅ♪" speaker_show="True" stop="True" voice="ra13003_02_0020" />
    <msg speaker="来夢ガールズＡ・Ｂ・Ｍ・Ｗ・Ｚ" text="おはようございまーす！" speaker_show="True" stop="True" voice="4000040102_union_001" />
    <create_effect effect_id="raimu01" effect_type="MampSmile" life_time="1" target_obj="敷島 来夢" attach_head_bone="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="う～ん、今日も艶やか健康美。&#xA;全員、張りがあって素晴らしいこと♪" speaker_show="True" stop="True" voice="sr13001_02_0621" />
    <delete obj_id="raimu01" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="みんなの働きぶりは、しっかり見ているから&#xA;頑張って業務に勤しむべし。解散！" speaker_show="True" stop="True" voice="sr13001_02_0622" />
    <msg speaker="来夢ガールズＡ・Ｂ・Ｍ・Ｗ・Ｚ" text="はーい♪" speaker_show="True" stop="True" voice="4000040102_union_002" />
    <move obj_id="敷島 来夢" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-300" y="0" anchor="Bottom" />
    <move obj_id="敷島 真留瀬" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="この後、午前の予定は、警視庁とのテレビ会議。&#xA;ＡＩ開発課との昼食会議となっております。" speaker_show="True" stop="True" voice="sm13002_02_0256" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="解ったわ。その前に、デザイン課に行くから&#xA;マルもついて来てちょうだい。" speaker_show="True" stop="True" voice="sr13001_02_0623" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1.0" wait="True" />
    <delete obj_id="敷島 真留瀬" />
