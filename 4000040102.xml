    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ムニャ…ムニャ……私の社長としての朝は&#xA;柔らかな弾力から始まる……。" speaker_show="True" stop="True" voice="sr13001_02_0619" />
    <sound type="Se" cue_name="Push" />
