    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <msg speaker="デザイナーＡ" text="肩のフォルムなのですが、内部の基盤の形状から&#xA;この出っ張りがどうしても必要でして……。" speaker_show="True" stop="True" voice="99116_02_0001" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ならばいっそ、それを逆手にとって……ほら&#xA;こんな形にすれば、一層美しいと思わない？" speaker_show="True" stop="True" voice="sr13001_02_0624" />
    <sound type="Se" cue_name="PhoneOff" />
    <wait time="0.5" />
    <sound type="Se" cue_name="PhoneOff" />
    <wait time="0.5" />
    <msg speaker="デザイナーＡ" text="ハッ！　なるほど、無駄を排除するばかりではなく&#xA;更に付け足すことで美感が増す……流石です社長！" speaker_show="True" stop="True" voice="99116_02_0002" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ん。後はよろしく。きりの良い所まで出来たら&#xA;また見るから、メールしておくように。" speaker_show="True" stop="True" voice="sr13001_02_0625" />
    <msg speaker="デザイナーＢ" text="社長、すみません！　例の…" speaker_show="True" stop="True" voice="99117_02_0001" />
    <char_spine_animation obj_id="敷島 来夢" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="あ、頭部の縮小化問題ね、今日中に方針が決まるから&#xA;今はアームの装甲に専念してて良いわ。" speaker_show="True" stop="True" voice="sr13001_02_0626" />
    <msg speaker="デザイナーＢ" text="はいっ！" speaker_show="True" stop="True" voice="99117_02_0002" />
    <move obj_id="敷島 来夢" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-300" y="0" anchor="Bottom" />
    <move obj_id="敷島 真留瀬" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="さて、そろそろ時間ね。&#xA;ネットは繋がってる？" speaker_show="True" stop="True" voice="sr13001_02_0627" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="はい、セッティングは社長室に。" speaker_show="True" stop="True" voice="sm13002_02_0257" />
