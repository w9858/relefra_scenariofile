    <transition effect="Default" type="In" color_change="False" time="1.0" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <msg speaker="警視庁人形対策課・課長" text="おはようございます、敷島社長。&#xA;朝早くからすみませんな。" speaker_show="True" stop="True" voice="99119_02_0001" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="どうも。今日の議題は、サイバー犯罪と&#xA;対人形対策についてでしたね。" speaker_show="True" stop="True" voice="sr13001_02_0628" />
    <msg speaker="警視庁人形対策課・警部補" text="先にいただいております資料を拝見すると&#xA;人形の装甲によって、弱点が変わると…。" speaker_show="True" stop="True" voice="99118_02_0001" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ええ。ですから、対抗策も相手の用いる&#xA;人形の種別によって個別に…" speaker_show="True" stop="True" voice="sr13001_02_0629" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="（………凄い。お姉様の頭の中には、全ての&#xA;知識と情報が精密に整理されている）" speaker_show="True" stop="True" voice="sm13002_02_0258" lip_sync="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="（相手によって素早く必要な情報を取り出し&#xA;駆使していく能力……見とれてしまいますわ）" speaker_show="True" stop="True" voice="sm13002_02_0259" lip_sync="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="以上が、本日こちらの提示できる事柄です。&#xA;何か御質問は？" speaker_show="True" stop="True" voice="sr13001_02_0630" />
    <msg speaker="警視庁人形対策課・課長" text="いえ、充分です。急がせて申し訳なかった。&#xA;社長もお忙しい所、大変でしたでしょう。" speaker_show="True" stop="True" voice="99119_02_0002" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="大丈夫ですよ。この程度は忙しいうちに&#xA;入りません。それでは、また。" speaker_show="True" stop="True" voice="sr13001_02_0631" />
