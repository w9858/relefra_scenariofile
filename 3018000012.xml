<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3018000012" />
  </info>
  <command>
    <Start />
    <load_cue_sheet />
    <Comment comment="一部のキャラクターイラストは仮の物を使用" />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" resource_type="Background" file="01_school1mo_002" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="教室" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="赤星イタ子の事件は解決し、&#xA;ハロウィンのパレードも無事に終わった翌日。" stop="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="昨日のハロウィン仮装パレード、すごかったー！" speaker_show="True" stop="True" voice="ha99001_02_0015" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="うんうん、私も少し見に行ってたよ。" speaker_show="True" stop="True" voice="hn99002_02_0020" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="False" />
    <msg speaker="北斗 凪" text="パレード、年々大規模になってるよね。&#xA;参加してる人たちの衣装もレベル高いし。" speaker_show="True" stop="True" voice="hn99002_02_0021" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="そういえば、結愛は仮装パレードに参加したの？" speaker_show="True" stop="True" voice="mm11001_02_0630" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="畠山 結愛" text="いやー、実はしなかったのさぁ。" speaker_show="True" stop="True" voice="ha99001_02_0016" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="仮装パレードに参加するよりも、&#xA;もっと重要なことができたからね！" speaker_show="True" stop="True" voice="ha99001_02_0017" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="重要なこと？" speaker_show="True" stop="True" voice="mm11001_02_0631" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="そう！　実はね、仮装パレード前日に、&#xA;1000年に１度のコスプレイヤーが出現したらしいんだ！" speaker_show="True" stop="True" voice="ha99001_02_0018" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="北斗 凪" text="そんなすごい人がいたの？" speaker_show="True" stop="True" voice="hn99002_02_0022" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="うん！　衣装は超ハイクオリティで、&#xA;仮装している人自身も、ものすごい美人さん！" speaker_show="True" stop="True" voice="ha99001_02_0019" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="女悪魔と魔女っ子の２人組だったらしいんだけどね。" speaker_show="True" stop="True" voice="ha99001_02_0020" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" />
    <msg speaker="源 モモ" text="(え、それって……。)" speaker_show="True" stop="True" voice="mm11001_02_0632" lip_sync="False" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="しかも性格も優しくて、気さくに&#xA;写真撮らせてくれるし、ポーズも取ってくれるって。" speaker_show="True" stop="True" voice="ha99001_02_0021" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="北斗 凪" text="へえ、そうなんだぁ。" speaker_show="True" stop="True" voice="hn99002_02_0023" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="もしかしたらパレード中にも現れるかもしれないと&#xA;思って、スマホを構えて探してたんだけどさー。" speaker_show="True" stop="True" voice="ha99001_02_0022" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="畠山 結愛" text="結局、見つけられなかったよ。" speaker_show="True" stop="True" voice="ha99001_02_0023" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="北斗 凪" text="そんなすごいコスプレイヤーさんなら、&#xA;ＳＮＳに写真が上がってるんじゃない？" speaker_show="True" stop="True" voice="hn99002_02_0024" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="畠山 結愛" text="うーん、そう思ったんだけど、なぜか１枚も&#xA;見つからないんだよね。" speaker_show="True" stop="True" voice="ha99001_02_0024" />
    <char_spine_animation obj_id="源 モモ" />
    <msg speaker="源 モモ" text="(カトリーナさんか初芽さんが、&#xA;工作して消してくれたんだろうな……。)" speaker_show="True" stop="True" voice="mm11001_02_0633" lip_sync="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="もしかして、芸能人だったんじゃないかな？&#xA;だから撮った人も写真を上げないよう言われてるとか。" speaker_show="True" stop="True" voice="mm11001_02_0634" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="あ、そっか。そうなのかも！&#xA;モモち、冴えてるぅ～。" speaker_show="True" stop="True" voice="ha99001_02_0025" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あはは……。" speaker_show="True" stop="True" voice="mm11001_02_0635" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="畠山 結愛" text="でもさ、目撃証言によると……魔女っ子の人は、&#xA;あの半蔵門先輩にそっくりだったんだって！" speaker_show="True" stop="True" voice="ha99001_02_0026" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="へえ……でも、それはきっと他人の空似だよね。" speaker_show="True" stop="True" voice="hn99002_02_0025" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="北斗 凪" text="あのクールな半蔵門先輩が、魔女っ子に仮装して&#xA;街を練り歩くなんて……思えないもん。" speaker_show="True" stop="True" voice="hn99002_02_0026" />
    <char_spine_animation obj_id="畠山 結愛" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="False" />
    <msg speaker="畠山 結愛" text="あはは、やっぱ凪っちも、そう思うよねえ。" speaker_show="True" stop="True" voice="ha99001_02_0027" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="(それ、本物なんだよね……&#xA;しかも、衣装は私が選んだし……。)" speaker_show="True" stop="True" voice="mm11001_02_0636" lip_sync="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="(ああ、師匠本人だって言いたい！&#xA;師匠の可愛さが証明されたと２人に伝えたい！)" speaker_show="True" stop="True" voice="mm11001_02_0637" lip_sync="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="(でも、言えないから、&#xA;私の胸の中に仕舞っておきます……!!)" speaker_show="True" stop="True" voice="mm11001_02_0638" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
    <sound_stop cue_name="Adv1" />
  </command>
</adv>