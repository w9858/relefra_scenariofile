    <VoiceCueSheet value="AdvVoice/9010108001" />
  </info>
  <command>
    <Start />
    <load_cue_sheet cue_sheet="BGM/BGM15_Adv5" />
    <sound type="Bgm" cue_name="Adv5" volume="0" />
    <sound_volume type="Bgm" cue_name="Adv5" time="1.0" wait="False" />
    <bg show="True" file="03_tsukikage_004" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="テレジア" data_id="311008007" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="02_Idle" force_end="True" loop="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、テレちゃん、基地に来てたんだね。&#xA;初芽さんに会いに来たの？" speaker_show="True" stop="True" voice="mm11001_02_0897" />
    <char_spine_animation obj_id="テレジア" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="テレジア" text="ああ。だが、今は発明に集中しているようで&#xA;初芽が出てくるまでここで待っている。" speaker_show="True" stop="True" voice="tr11007_02_0215" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そうなんだ。私も初芽さんに用があったんだけど……。&#xA;それなら、私とおしゃべりして待ってようよ。" speaker_show="True" stop="True" voice="mm11001_02_0898" />
    <char_spine_animation obj_id="テレジア" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="おしゃべり？&#xA;……特に報告するようなことはないが。" speaker_show="True" stop="True" voice="tr11007_02_0216" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="えーっと、そんな堅苦しいことじゃなくって……。&#xA;あの、初芽さんには何の用事で来たの？" speaker_show="True" stop="True" voice="mm11001_02_0899" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="テレジア" text="それは……。" speaker_show="True" stop="True" voice="tr11007_02_0217" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="……あ、もしかして任務に関すること？&#xA;それなら無理に話さなくてもいいよ。" speaker_show="True" stop="True" voice="mm11001_02_0900" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そういえば今日、いい天気だよね。&#xA;外にいるほうが気持ち良さそうじゃない？" speaker_show="True" stop="True" voice="mm11001_02_0901" />
    <char_spine_animation obj_id="テレジア" face_anim_type="005_Facial_sad" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="テレジア" text="天気……そうだな。&#xA;いい天気だったから……つい……。" speaker_show="True" stop="True" voice="tr11007_02_0218" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="テ、テレちゃん!?　どうしたの、急に？&#xA;私、何か変なこと言っちゃった!?" speaker_show="True" stop="True" voice="mm11001_02_0902" />
    <char_spine_animation obj_id="テレジア" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="……実は初芽の代わりに洗濯機を回そうとしたんだ。" speaker_show="True" stop="True" voice="tr11007_02_0219" />
    <char_spine_animation obj_id="テレジア" body_anim_type="12_DropShoulders" force_end="False" loop="True" face_anim_type="015_Facial_poker" />
    <msg speaker="テレジア" text="そうしたら泡がどんどん出てきてしまって……&#xA;どうしたらいいかわからなくなった。" speaker_show="True" stop="True" voice="tr11007_02_0220" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あはは、な～んだ、そんなことか。&#xA;あるある!!　よくあるよね、そういうことって。" speaker_show="True" stop="True" voice="mm11001_02_0903" />
    <char_spine_animation obj_id="テレジア" face_anim_type="011_Facial_surprise" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="テレジア" text="え!?" speaker_show="True" stop="True" voice="tr11007_02_0221" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="私も初めて洗濯したときそんな感じになって焦ったよ。&#xA;でも、テレちゃんでもそんな失敗するんだね～" speaker_show="True" stop="True" voice="mm11001_02_0904" />
    <char_spine_animation obj_id="テレジア" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="失敗……初芽に迷惑をかけるだろうか……。" speaker_show="True" stop="True" voice="tr11007_02_0222" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <create_effect effect_id="Sweat_momo" effect_type="MampSweat" life_time="1.0" target_obj="源 モモ" x="0" y="0" />
    <msg speaker="源 モモ" text="あ……ううん、きっと大丈夫だよ!!&#xA;それで洗濯機はどうしたの？" speaker_show="True" stop="True" voice="mm11001_02_0905" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="テレジア" text="まだそのままに……。&#xA;それで初芽に報告をしに……。" speaker_show="True" stop="True" voice="tr11007_02_0223" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そっか、それでここに来てたんだ。" speaker_show="True" stop="True" voice="mm11001_02_0906" />
    <char_spine_animation obj_id="テレジア" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="ああ。床が濡れたくらいですんだが、&#xA;水浸しになった他の洗濯物も洗い直さないと。" speaker_show="True" stop="True" voice="tr11007_02_0224" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="それなら今のうちに片づけちゃおうよ。&#xA;私も手伝うから。" speaker_show="True" stop="True" voice="mm11001_02_0907" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="それはとても助かる。&#xA;だが、モモも初芽に用があって来たんじゃないのか？" speaker_show="True" stop="True" voice="tr11007_02_0225" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="それはまあ……。&#xA;でも、私の用事は明日でも平気だから。" speaker_show="True" stop="True" voice="mm11001_02_0908" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="それよりもテレちゃんのほうを&#xA;先に解決したほうがいいでしょ？" speaker_show="True" stop="True" voice="mm11001_02_0909" />
    <char_spine_animation obj_id="テレジア" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="モモ……ありがとう。" speaker_show="True" stop="True" voice="tr11007_02_0226" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="大丈夫だよ、テレちゃん。&#xA;ふたりでやればすぐに終わるから。" speaker_show="True" stop="True" voice="mm11001_02_0910" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="False" />
    <sound_volume type="Bgm" cue_name="Adv5" volume="0" time="1.0" wait="True" />
  </command>
