    <wait time="1.5" />
    <msg speaker="来夢ガールズＭ" text="スキー　タノシー！" speaker_show="True" stop="True" voice="rb13005_03_0009" />
    <sound type="Se" cue_name="skate2" />
    <wait time="1.5" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001006" position_id="left" x="200" />
    <create type="Character" obj_id="アミィ" data_id="313002005" position_id="left" x="-200" />
    <char_spine_animation obj_id="アミィ" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="なるほど。&#xA;直滑降はなかなかの速度に達しまする。" speaker_show="True" stop="True" voice="am13002_03_0040" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="アミィはバランス制御が&#xA;完璧な分もう教えることはないわねー。" speaker_show="True" stop="True" voice="sr13001_03_0098" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="マスター。&#xA;次はスノーボードに挑戦したく思いまする。" speaker_show="True" stop="True" voice="am13002_03_0041" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="いいわよ。どんどんやっちゃいなさい。&#xA;ボード貸してあげる。" speaker_show="True" stop="True" voice="sr13001_03_0099" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1" wait="True" />
    <delete obj_id="敷島 来夢" />
    <delete obj_id="アミィ" />
    <create type="Character" obj_id="アミィ" data_id="313002005" position_id="left" x="0" />
    <transition effect="Default" type="In" color_change="False" time="1" wait="True" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="……重心のかけ方で進行方向を制御。&#xA;そして、ここで旋回しつつ――。" speaker_show="True" stop="True" voice="am13002_03_0042" />
    <sound type="Se" cue_name="skate1" />
    <wait time="1.5" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="アミィ" text="うむ……我ながら上々の出来にございまする。" speaker_show="True" stop="True" voice="am13002_03_0043" />
    <msg speaker="来夢ガールズＺ" text="ブラボー、アミィ！" speaker_show="True" stop="True" voice="rz13007_03_0006" />
    <delete obj_id="アミィ" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001006" position_id="left" x="200" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003003" position_id="left" x="-200" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ほとんど教えもしないのに、どうやって滑ればいいか&#xA;スキーの経験から学んで発展させてるわ。" speaker_show="True" stop="True" voice="sr13001_03_0100" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="さすが私のアミィ！&#xA;でも、ちょっと癪ね。" speaker_show="True" stop="True" voice="sr13001_03_0101" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様？" speaker_show="True" stop="True" voice="sm13002_03_0057" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="アミィー！&#xA;ちょっとスノボで勝負しない？" speaker_show="True" stop="True" voice="sr13001_03_0102" />
    <fade_out time="1" type="Default" wait="True" />
