    <create type="Character" obj_id="半蔵門 雪" data_id="311002009" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そんなに駄々をこねなくても、これから嫌でも&#xA;着ることになるわ。" speaker_show="True" stop="True" voice="hy11002_01_0414" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="へ？　そのココロは？" speaker_show="True" stop="True" voice="ym11003_01_0463" />
