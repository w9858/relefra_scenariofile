    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="-200" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="八千代 命" data_id="311003009" x="200" y="0" anchor="Bottom" position_id="left" />
    <Comment comment="命、ヴィラン風衣装の予定" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="わっ、初芽さんも同じ格好！&#xA;師匠コスプレウィークか何かですか？" speaker_show="True" stop="True" voice="sf11004_01_0504" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="何だって！　それなら誘っておくれよ～。&#xA;メイもそっちのセクシーなやつが着たいよ～。" speaker_show="True" stop="True" voice="ym11003_01_0462" />
