    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="さっきモモにぶつかった途端、効果が&#xA;切れたでしょう。まだまだ完成への道は遠いわ。" speaker_show="True" stop="True" voice="hy11002_01_0416" />
