    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="誤解が解けたみたいで、よかったわ。&#xA;残る課題はスーツ改良のみね。" speaker_show="True" stop="True" voice="hy11002_01_0415" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="そのスーツ、まだ未完成なんですか？" speaker_show="True" stop="True" voice="mm11001_01_0659" />
