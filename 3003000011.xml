    <char_spine_cheek obj_id="源 モモ" type="Cheek02_medium" loop="True" />
    <msg speaker="源 モモ" text="師匠、セクシー過ぎて目のやり場に&#xA;困ってしまうのですが、その格好は一体……。" speaker_show="True" stop="True" voice="mm11001_01_0658" />
