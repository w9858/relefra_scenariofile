    <create type="Character" obj_id="青葉 初芽" data_id="311005008" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="実は、裸にならなくても透明クリームの効果を&#xA;持続できる特別なスーツを開発していたんです。" speaker_show="True" stop="True" voice="ah11005_01_0209" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="18_ArmsCrossed" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃんに実験台になってもらって、密かに&#xA;改良を重ねていたんですが、まだまだですね。" speaker_show="True" stop="True" voice="ah11005_01_0210" />
    <msg_window_show show="False" />
    <move obj_id="青葉 初芽" position_id="left" x="-200" x_move_type="MoveTo" time="0.5" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="もしかしてこの間、師匠が隠したのは……。" speaker_show="True" stop="True" voice="ig11006_01_0299" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="はい、開発中のこのスーツです。" speaker_show="True" stop="True" voice="ah11005_01_0211" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="言ってくれればお手伝いしたのに。&#xA;どうして隠したりしたんですか？" speaker_show="True" stop="True" voice="ig11006_01_0300" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="実験を終えて、実用化されたものを見せた方が、&#xA;ごえちゃんもみんなも喜んでくれると思って。" speaker_show="True" stop="True" voice="ah11005_01_0212" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_eye obj_id="石川 五恵" type="Eye03_tear" loop="True" />
    <msg speaker="石川 五恵" text="そうだったんですね……。よかった……。&#xA;本当によかったですー。ぐずん……ぐずん。" speaker_show="True" stop="True" voice="ig11006_01_0301" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ごえちゃん!?　泣いてるんですか？&#xA;どうして……。ほら、ハンカチ使ってください。" speaker_show="True" stop="True" voice="ah11005_01_0213" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <move obj_id="青葉 初芽" position_id="left" x="-125" x_move_type="MoveTo" time="1.0" wait="False" />
    <move obj_id="石川 五恵" position_id="left" x="125" x_move_type="MoveTo" time="1.0" wait="True" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ありがとうございます。&#xA;安心したら涙が……。" speaker_show="True" stop="True" voice="ig11006_01_0302" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="ごえちゃんにこんなに心配をさせてしまうなんて、&#xA;師匠失格ですね。ごめんなさい。" speaker_show="True" stop="True" voice="ah11005_01_0214" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="違うんです、師匠のせいじゃありません。ただ……、&#xA;最近師匠の発明が失敗続きだったから。" speaker_show="True" stop="True" voice="ig11006_01_0303" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="もしかして、私がうまく出来ないせいで、&#xA;テストに失敗してたんじゃないかって……。" speaker_show="True" stop="True" voice="ig11006_01_0304" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="だから私に内緒で発明をしてたんだと思ったら、&#xA;師匠に申し訳なくて……。" speaker_show="True" stop="True" voice="ig11006_01_0305" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="あらあら、そうでしたか。ちゃんと説明しておけば&#xA;よかったですね。本当にごめんなさい。" speaker_show="True" stop="True" voice="ah11005_01_0215" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ごえちゃんは本当に優しい良い子ですね。&#xA;ぎゅー。" speaker_show="True" stop="True" voice="ah11005_01_0216" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <move obj_id="青葉 初芽" position_id="left" x="25" x_move_type="MoveTo" time="1.0" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_cheek obj_id="石川 五恵" type="Cheek02_medium" loop="True" />
    <msg speaker="石川 五恵" text="師匠！ そんな、人前で抱きしめられたら、&#xA;恥ずかしいです……。" speaker_show="True" stop="True" voice="ig11006_01_0306" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ふふ、いいじゃないですか。&#xA;ほら、頭もなでなでしてあげますね。なでなで。" speaker_show="True" stop="True" voice="ah11005_01_0217" />
