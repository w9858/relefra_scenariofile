<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010404" />
  </info>
  <command>
    <Start />
    <load_cue_sheet cue_sheet="BGM/BGM18_Adv6" />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_out time="0" type="Default" />
    <bg show="True" file="20_meiandfuhouse_02" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="-200" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふわぁー、美味しかった！　特にこのホロホロの&#xA;牛肉！　フー、これ高かったんじゃないの？" speaker_show="True" stop="True" voice="ym11003_01_0756" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="師匠はそんなこと気にしなくていいんですよ！&#xA;それに、これを買ってもなんと今月は黒字です！" speaker_show="True" stop="True" voice="sf11004_01_0732" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="おー、それでこそ我が弟子！　この調子なら&#xA;毎月高級牛でディナーができるんじゃない？" speaker_show="True" stop="True" voice="ym11003_01_0757" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="毎月って訳にはいきませんよ！&#xA;今日は記念日だから特別です！" speaker_show="True" stop="True" voice="sf11004_01_0733" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="それに、お肉が柔らかくなったのは、師匠の帰りが&#xA;遅過ぎて、予定より長く煮込めたからで……。" speaker_show="True" stop="True" voice="sf11004_01_0734" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="本当ごめんってば！　機嫌直してよー、フー！" speaker_show="True" stop="True" voice="ym11003_01_0758" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="別に、もう怒ってないですよ。" speaker_show="True" stop="True" voice="sf11004_01_0735" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" face_anim_type="002_Facial_smile" />
    <msg speaker="八千代 命" text="良かったー。それにしても、今日のカレーは&#xA;特別に美味しかったよ。" speaker_show="True" stop="True" voice="ym11003_01_0759" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="フーはいいお嫁さんになるんだろうねー。" speaker_show="True" stop="True" voice="ym11003_01_0760" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="10_FaceBothHands" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="そんな……。あっ、今日のお釣、まだ残ってるし、&#xA;コンビニでアイス買ってきてくれません？" speaker_show="True" stop="True" voice="sf11004_01_0736" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="いっ、いいのっ!?&#xA;やったー！　フーも一緒に買いに行こう！" speaker_show="True" stop="True" voice="ym11003_01_0761" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="アタシは片付けがあるんで、&#xA;師匠、好きなの買ってきてください。" speaker_show="True" stop="True" voice="sf11004_01_0737" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="くぅ～、なんて良くできた弟子！&#xA;それじゃ、行ってきまーす！" speaker_show="True" stop="True" voice="ym11003_01_0762" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="Walk" />
    <move obj_id="八千代 命" position_id="left" x="1000" time="2.0" wait="True" />
    <delete obj_id="八千代 命" />
    <move obj_id="相模 楓" position_id="left" x="0" time="0.5" x_move_type="MoveTo" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="本当はちょっと予算オーバーだけど、明日から&#xA;節約すればいいよね。さてとお皿、片付けちゃおう。" speaker_show="True" stop="True" voice="sf11004_01_0738" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="あ……、師匠、髪飾り置きっぱなしだ。" speaker_show="True" stop="True" voice="sf11004_01_0739" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="これ、本当に可愛いな……。&#xA;って、あれ？　値札がついたままだ。" speaker_show="True" stop="True" voice="sf11004_01_0740" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="アタシのはちゃんと取ってあったのに。こういう&#xA;所が師匠の抜けてるトコなのよ……えええ!!!" speaker_show="True" stop="True" voice="sf11004_01_0741" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="ちょっと何よこれ！　師匠とアタシの&#xA;１週間分の食費より高いじゃん！" speaker_show="True" stop="True" voice="sf11004_01_0742" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="これだけのお金があったら、毎日今日より豪華な&#xA;ごちそうが食べられるし。" speaker_show="True" stop="True" voice="sf11004_01_0743" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="101_Facial_trouble" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="フローズンチョコレートもコロッケも、かき氷も、&#xA;全部全部買えちゃうでしょ！" speaker_show="True" stop="True" voice="sf11004_01_0744" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="ったく、師匠ってば、無駄遣いはダメって&#xA;あれだけ言っといたのに……。" speaker_show="True" stop="True" voice="sf11004_01_0745" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="けど、見れば見るほど可愛いからなぁ。" speaker_show="True" stop="True" voice="sf11004_01_0746" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="今回はすごく嬉しかったから、&#xA;特別に許してあげるか！" speaker_show="True" stop="True" voice="sf11004_01_0747" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>