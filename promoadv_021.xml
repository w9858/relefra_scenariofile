<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="PromoAdv/PromoAdv_021" />
  </info>
  <command>
    <Start />
    <auto_mode active="True" />
    <function_menu_show show="False" />
    <bg file="44_raimuhouseev_01" />
    <load_cue_sheet cue_sheet="BGM/BGM18_Adv6" />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="フフフ、ついにこの日が来たのね！" speaker_show="True" stop="True" voice="sr13001_03_0384" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="そう、今日２月23日は、この私、&#xA;敷島来夢の誕生日！　つまりは……。" speaker_show="True" stop="True" voice="sr13001_03_0385" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="年に一度、親愛なる妹のマルや可愛いアミィ&#xA;そして来夢ガールズ達とコミュニケーション取り放題の日！" speaker_show="True" stop="True" voice="sr13001_03_0386" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="え？放題ではない？&#xA;それからハレンチはほどほどにですって！？" speaker_show="True" stop="True" voice="sr13001_03_0387" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="もう・・・いつも言ってるじゃない！&#xA;あれはハレンチではなくコミュニケーション！" speaker_show="True" stop="True" voice="sr13001_03_0388" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="それに、世界平和と日本経済の発展のために&#xA;行っていることなのよ！" speaker_show="True" stop="True" voice="sr13001_03_0389" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="あら？なんだか納得してないって顔ね。&#xA;じゃあもう少し簡単に説明してあげるわ！" speaker_show="True" stop="True" voice="sr13001_03_0390" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="敷島の社長業とZETの任務、その両方を&#xA;円滑に続けていくためにはチームワークが不可欠でしょ？" speaker_show="True" stop="True" voice="sr13001_03_0391" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="そこで！誕生日に濃密な時間を共有するという手段を使って&#xA;チームワークを強化してるってわけ。" speaker_show="True" stop="True" voice="sr13001_03_0391a" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="あら…もうこんな時間。マルとアミィ達が待っているから&#xA;このへんで失礼するわね。" speaker_show="True" stop="True" voice="sr13001_03_0391b" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="最後になったけど、お祝いの言葉をありがとう。&#xA;これからも、敷島来夢とZETをよろしくたのむわね！" speaker_show="True" stop="True" voice="sr13001_03_0391c" />
    <msg_window_show show="False" />
    <fade_out time="0.5" wait="True" />
  </command>
</adv>