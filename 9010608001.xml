<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/9010608001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="02_wasabimo_001" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="Wasabiのカレー、久しぶりだな。&#xA;五恵はよく来るのか？" speaker_show="True" stop="True" voice="tr11007_02_0330" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="うん。メイちゃんやモモちたちとね。&#xA;でも、こうしてテレちゃんとも来られて良かった。" speaker_show="True" stop="True" voice="ig11006_02_0621" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="私と食事だなんて、五恵も物好きだな。" speaker_show="True" stop="True" voice="tr11007_02_0331" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="そんなことないよ。&#xA;仲良くなるには、こういう時間も大切だと思うから。" speaker_show="True" stop="True" voice="ig11006_02_0622" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="テレジア" text="そうだな……。" speaker_show="True" stop="True" voice="tr11007_02_0332" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……。" speaker_show="True" stop="True" voice="ig11006_02_0623" />
    <msg speaker="テレジア" text="……。" speaker_show="True" stop="True" voice="tr11007_02_0333" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="仲間だからこそ、もっとお互いのことを&#xA;知っておかないとな。" speaker_show="True" stop="True" voice="tr11007_02_0334" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="うんうん。" speaker_show="True" stop="True" voice="ig11006_02_0624" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="だが、五恵のことなら、もうわかっている。" speaker_show="True" stop="True" voice="tr11007_02_0335" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="えっ？" speaker_show="True" stop="True" voice="ig11006_02_0625" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="テレジア" text="初芽の弟子で手甲を使った肉弾戦を得意とするスパイ。&#xA;番付は横綱だ。" speaker_show="True" stop="True" voice="tr11007_02_0336" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="そ、そんな、私なんてまだまだだよ。&#xA;それなら師匠のことはどう思ってるの？" speaker_show="True" stop="True" voice="ig11006_02_0626" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="テレジア" text="そうだな……初芽はしっかりしているが、&#xA;ときどき放っておけない。" speaker_show="True" stop="True" voice="tr11007_02_0337" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……うん、それはわかる気がする。" speaker_show="True" stop="True" voice="ig11006_02_0627" />
    <char_spine_animation obj_id="テレジア" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="テレジア" text="それに発明が失敗すると、よく爆発を起こす。&#xA;あれはかなり心配だ。" speaker_show="True" stop="True" voice="tr11007_02_0338" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="うんうん。さすがテレちゃんだね。&#xA;師匠のことよくわかってる。" speaker_show="True" stop="True" voice="ig11006_02_0628" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="やはり五恵もそう思うか。&#xA;……というか誰にでもわかることだな。" speaker_show="True" stop="True" voice="tr11007_02_0339" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="石川 五恵" text="そうだね。でも、意見が合って良かった。&#xA;やっぱり、テレちゃんとは仲良くなれそう。" speaker_show="True" stop="True" voice="ig11006_02_0629" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="今後の任務でタッグを組む可能性もあるだろう。&#xA;意識の共有は必要かもしれない。" speaker_show="True" stop="True" voice="tr11007_02_0340" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="うん。だから、あの……。&#xA;テレちゃんが良ければ、また一緒に食事してくれる？" speaker_show="True" stop="True" voice="ig11006_02_0630" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="テレジア" text="もちろんだ。&#xA;スパイにとって情報交換は重要。" speaker_show="True" stop="True" voice="tr11007_02_0341" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="テレジア" text="それに、また五恵とも食事がしたい。" speaker_show="True" stop="True" voice="tr11007_02_0342" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="あ、ありがとう、テレちゃん。" speaker_show="True" stop="True" voice="ig11006_02_0631" />
  </command>
</adv>