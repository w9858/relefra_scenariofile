    <VoiceCueSheet value="AdvVoice/4000010203" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="False" />
    <Comment comment="ツキカゲ基地" />
    <bg show="True" file="03_tsukikage_001" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、ちょっとお話があります。" speaker_show="True" stop="True" voice="ah11005_01_0525" />
    <msg_window_show show="False" />
    <move obj_id="青葉 初芽" position_id="left" x="-200" x_move_type="MoveTo" time="0.5" wait="True" y="0" y_move_type="MoveValue" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="初芽、どうしたの？&#xA;そんなに改まって。" speaker_show="True" stop="True" voice="hy11002_01_0593" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="今日のモモちゃん、凄く活躍していましたよねぇ。" speaker_show="True" stop="True" voice="ah11005_01_0526" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そうね、なかなか良くやってくれたと思うわ。&#xA;それで話って？" speaker_show="True" stop="True" voice="hy11002_01_0594" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、もっとモモちゃんを&#xA;褒めてあげなくちゃダメですよ。" speaker_show="True" stop="True" voice="ah11005_01_0527" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="何を突然。" speaker_show="True" stop="True" voice="hy11002_01_0595" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="モモちゃんが、どんどん成長していること、&#xA;一番よく知っているのは雪ちゃんですよね？" speaker_show="True" stop="True" voice="ah11005_01_0528" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="確かに、モモの成長には目を見張るものがあるわ。&#xA;それは私だって認めている。けど……。" speaker_show="True" stop="True" voice="hy11002_01_0596" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="些細なミスが、大きな失敗に繋がるのが&#xA;スパイの世界でしょう。" speaker_show="True" stop="True" voice="hy11002_01_0597" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃんの言う通りだとは思います。&#xA;けど、もう少し優しくしてあげても……。" speaker_show="True" stop="True" voice="ah11005_01_0529" />
    <char_spine_animation obj_id="半蔵門 雪" loop="True" to_idle="True" force_end="False" face_anim_type="005_Facial_sad" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="もう嫌なのよ。" speaker_show="True" stop="True" voice="hy11002_01_0598" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="え？" speaker_show="True" stop="True" voice="ah11005_01_0530" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="あんな思いをするのは嫌だから……。&#xA;また大切な仲間を失うわけにはいかないから……。" speaker_show="True" stop="True" voice="hy11002_01_0599" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん……。" speaker_show="True" stop="True" voice="ah11005_01_0531" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="モモちゃんなら大丈夫ですよ。" speaker_show="True" stop="True" voice="ah11005_01_0532" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="その根拠は……？" speaker_show="True" stop="True" voice="hy11002_01_0600" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ありません。けど、私にはわかるんです。&#xA;安心してください、モモちゃんは大丈夫です！" speaker_show="True" stop="True" voice="ah11005_01_0533" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そう自信満々に言われてしまうと、&#xA;妙に説得力があるけど……、でも！" speaker_show="True" stop="True" voice="hy11002_01_0601" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、弟子を信用するのも&#xA;師匠の大事な役目ですよ。" speaker_show="True" stop="True" voice="ah11005_01_0534" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="101_Facial_trouble" body_anim_type="06_ChestLeftHand" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="弟子を……信用……？" speaker_show="True" stop="True" voice="hy11002_01_0602" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
