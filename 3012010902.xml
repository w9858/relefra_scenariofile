    <create type="Character" obj_id="八千代 命" data_id="311003014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010009" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" loop="True" />
    <msg_window_show show="True" />
    <msg speaker="八千代 命" text="師匠、何を見たのか教えてくれないと&#xA;メイ、どうして良いかわかんないよ。" speaker_show="True" stop="True" voice="ym11003_02_0473" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="19_Amazed" loop="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" loop="True" />
    <msg speaker="高坂 信" text="そ、そう言われても、信だって&#xA;何を見たのか定かじゃないんだもん！" speaker_show="True" stop="True" voice="ks11010_02_0265" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="高坂 信" text="ただ、素早くてササッて動く……&#xA;謎の生物……みたいな？" speaker_show="True" stop="True" voice="ks11010_02_0266" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" />
    <msg speaker="八千代 命" text="それってさぁ…………ゴ" speaker_show="True" stop="True" voice="ym11003_02_0474" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="19_Amazed" loop="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" loop="True" />
    <msg speaker="高坂 信" text="ヤダヤダ、言わないで！&#xA;信、それ大っ嫌いなんだからー！" speaker_show="True" stop="True" voice="ks11010_02_0267" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" loop="True" />
    <msg speaker="八千代 命" text="しょうがないなぁ、もう……。&#xA;どの辺に逃げたの？　この辺？" speaker_show="True" stop="True" voice="ym11003_02_0475" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" loop="True" />
    <msg speaker="高坂 信" text="え、どうするつもり？" speaker_show="True" stop="True" voice="ks11010_02_0268" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="八千代 命" text="捕まえて正体が判れば、もう怖くないでしょ？&#xA;メイが謎の生物をお縄にしちゃる！" speaker_show="True" stop="True" voice="ym11003_02_0476" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="101_Facial_trouble" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="高坂 信" text="ダメだよ、危ない！　もし、針とか牙とか&#xA;それに、毒のある生き物だったらどうするの!?" speaker_show="True" stop="True" voice="ks11010_02_0269" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="八千代 命" text="そ、それもそっか……。でも、だとしたら&#xA;尚更、何なのか確かめておかないとだね。" speaker_show="True" stop="True" voice="ym11003_02_0477" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" />
    <msg speaker="高坂 信" text="ど、どうやって？　たぶんだけど……&#xA;岩の陰とかに逃げ込んだ気がするよ？" speaker_show="True" stop="True" voice="ks11010_02_0270" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="八千代 命" text="この岩？　よーし……それなら別の岩を&#xA;叩き付けて、追い出してやる。" speaker_show="True" stop="True" voice="ym11003_02_0478" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="017_Facial_shock" body_anim_type="19_Amazed" loop="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="017_Facial_shock" loop="True" />
    <msg speaker="高坂 信" text="え、ぇえ……そんなの相手が激怒して&#xA;こっちへ向かって来たら……怖い。" speaker_show="True" stop="True" voice="ks11010_02_0271" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="八千代 命" text="このまま何だか判らない方がずっと怖い！&#xA;メイに任せといてよ。３、２、１……！" speaker_show="True" stop="True" voice="ym11003_02_0479" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" />
    <msg speaker="八千代 命" text="どっせーーーーーい!!" speaker_show="True" stop="True" voice="ym11003_02_0480" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" loop="True" />
    <lip_sync target1="八千代 命" target2="高坂 信" />
    <msg speaker="命・信" text="ん………？" speaker_show="True" stop="True" voice="3012010902_union_001" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="017_Facial_shock" body_anim_type="16_BendBackward" loop="True" />
    <msg speaker="高坂 信" text="ひやああーーーーーーーーっ!!!?" speaker_show="True" stop="True" voice="ks11010_02_0273" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="017_Facial_shock" body_anim_type="15_Negative" loop="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="017_Facial_shock" loop="True" />
    <msg speaker="八千代 命" text="うぉおおお!?" speaker_show="True" stop="True" voice="ym11003_02_0482" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="017_Facial_shock" body_anim_type="19_Amazed" loop="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="017_Facial_shock" loop="True" />
    <msg speaker="高坂 信" text="ゴ、ゴ、ゴゴゴゴゴ！&#xA;ゴキブリーーーーーーーッ!!" speaker_show="True" stop="True" voice="ks11010_02_0274" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="15_Negative" loop="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" loop="True" />
    <msg speaker="八千代 命" text="違う違う、今のは大量のフナムシ！&#xA;まあどっちにせよ、刺激的ぃ～！" speaker_show="True" stop="True" voice="ym11003_02_0483" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" />
    <msg speaker="高坂 信" text="あ…あぁ…あ……あわわわわ……&#xA;こ、腰と膝が両方……ダメぇ……。" speaker_show="True" stop="True" voice="ks11010_02_0275" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="017_Facial_shock" body_anim_type="13_Question" loop="True" />
    <msg speaker="八千代 命" text="わーーーー、師匠しっかりーーっ！" speaker_show="True" stop="True" voice="ym11003_02_0484" />
