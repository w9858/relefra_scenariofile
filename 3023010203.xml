    <char_spine_animation />
    <msg speaker="半蔵門 雪" text="でも、着ぐるみを着ていたテレジアより&#xA;速度が出なかったのは、足運びのせいね。" speaker_show="True" stop="True" voice="hy11002_03_0187" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
