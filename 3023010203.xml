    <msg speaker="源 モモ" text="はぅああ……ウットリしかありません。&#xA;師匠……王子様みたいですぅ。" speaker_show="True" stop="True" voice="mm11001_03_0270" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="悪いわね、お姫様の方じゃなくて。&#xA;でも……" speaker_show="True" stop="True" voice="hy11002_03_0195" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="いつかあなたが、もっと上手になったら&#xA;今度は私をリードしてちょうだい。良いわね？" speaker_show="True" stop="True" voice="hy11002_03_0196" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="は、はいっ！　約束します！　私絶対、練習して&#xA;師匠の王子様になってみせますから！" speaker_show="True" stop="True" voice="mm11001_03_0271" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
