<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3001010602" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="01_school1mo_002" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ふぇ～、今日も無事に乗り切れたよぉ～。&#xA;このところ任務が続いてるから、もう眠くて眠くて～。" speaker_show="True" stop="True" voice="mm11001_01_0576" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="古文の時間、モモち危なかったよね。&#xA;もう少しで先生にも気づかれちゃうところだったし。" speaker_show="True" stop="True" voice="ig11006_01_0207" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="あの時はありがとね、ごえちゃん。&#xA;わざとペンケースを落としてまで起こしてくれて。" speaker_show="True" stop="True" voice="mm11001_01_0577" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="私、いつも助けられてばかりな気がするよ。" speaker_show="True" stop="True" voice="mm11001_01_0578" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ううん、そんなことないよ。" speaker_show="True" stop="True" voice="ig11006_01_0208" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あ、そうだ。今日はモモちに、相談に乗ってもらいたい&#xA;ことがあるんだけど、いいかな？" speaker_show="True" stop="True" voice="ig11006_01_0209" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="相談？　ごえちゃんが、私に？" speaker_show="True" stop="True" voice="mm11001_01_0579" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="010_Facial_shy" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="うん。バレンタインのことなんだけど…" speaker_show="True" stop="True" voice="ig11006_01_0210" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ずいぶん前からチョコは手作りにしようって決めて&#xA;たんだけど、そこまでしか考えてなくって。" speaker_show="True" stop="True" voice="ig11006_01_0211" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="101_Facial_trouble" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="それで、師匠にどんな風にチョコを渡せばいいのか、&#xA;ちょっと悩んじゃってるんだよね。" speaker_show="True" stop="True" voice="ig11006_01_0212" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="そう言えば私も、そこまで考えてなかったかも…！" speaker_show="True" stop="True" voice="mm11001_01_0580" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="でも、どうすればいいんだろ？&#xA;何かサプライズがあるといいのかな？" speaker_show="True" stop="True" voice="mm11001_01_0581" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="サプライズ？&#xA;例えば、どんな？" speaker_show="True" stop="True" voice="ig11006_01_0213" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="例えば、そうだなぁ……" speaker_show="True" stop="True" voice="mm11001_01_0582" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あ、これはどう？　大きな箱を家の前に置いといて、&#xA;初芽さんが来たらババーンって登場するっていうのは？" speaker_show="True" stop="True" voice="mm11001_01_0583" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="う、うん…確かにそれはサプライズだね…&#xA;すっごく勇気がいるけど…。" speaker_show="True" stop="True" voice="ig11006_01_0214" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あー、でも初芽さんの家ってタワーマンションだから、&#xA;他の人に箱を開けられちゃう可能性があるね。" speaker_show="True" stop="True" voice="mm11001_01_0584" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="それに考えてみたら、そういうサプライズを仕掛けると&#xA;初芽さんに余計な警戒をさせちゃうかも。" speaker_show="True" stop="True" voice="mm11001_01_0585" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ほら、私たちっていつ誰に狙われるかわからないし。" speaker_show="True" stop="True" voice="mm11001_01_0586" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="そっか、それはそうだね…。" speaker_show="True" stop="True" voice="ig11006_01_0215" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="うーん…てことは、サプライズ自体が難しいのかな…？" speaker_show="True" stop="True" voice="mm11001_01_0587" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="うっ…こうして考えてみると、&#xA;私もどんな風に、師匠にチョコを渡せばいいんだろ…？" speaker_show="True" stop="True" voice="mm11001_01_0588" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="むむむ、むむむむむぅ…！" speaker_show="True" stop="True" voice="mm11001_01_0589" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="わわっ、モモち、落ち着いて。" speaker_show="True" stop="True" voice="ig11006_01_0216" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="101_Facial_trouble" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ただでさえ、今は疲れが溜まって来てるんだから、&#xA;そんなに根を詰めたら倒れちゃうよ。" speaker_show="True" stop="True" voice="ig11006_01_0217" />
    <msg speaker="源 モモ" text="で、でも…！" speaker_show="True" stop="True" voice="mm11001_01_0590" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="じゃあ、もう少しシンプルに考えてみるのはどうかな？" speaker_show="True" stop="True" voice="ig11006_01_0218" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="私たちは普段、他の人とは違う体験を一杯してるから、&#xA;こういう時こそ普通にするのがイイのかも。" speaker_show="True" stop="True" voice="ig11006_01_0219" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="こういう時こそ普通に、か…&#xA;うん、それはごえちゃんの言う通りだね。" speaker_show="True" stop="True" voice="mm11001_01_0591" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ありがとう、ごえちゃん！&#xA;おかげで悩みが、１つなくなった気がするよ！" speaker_show="True" stop="True" voice="mm11001_01_0592" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="て、あれ？　私が相談に乗ってるつもりだったのに、&#xA;いつの間にか逆になっちゃってるような…？" speaker_show="True" stop="True" voice="mm11001_01_0593" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="ううん。私の悩みも今ので解決したと思うし、&#xA;それは、モモちが相談に乗ってくれたおかげだよ。" speaker_show="True" stop="True" voice="ig11006_01_0220" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="そ、そうかな…？" speaker_show="True" stop="True" voice="mm11001_01_0594" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ふふ、そうなんだよ。&#xA;だから今日はありがとね、モモち。" speaker_show="True" stop="True" voice="ig11006_01_0221" />
  </command>
</adv>