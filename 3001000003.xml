<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3001000003" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="05_momohousemo_01" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="バレンタイン、前日。" stop="True" />
    <msg_window_show show="False" />
    <create type="Character" obj_id="源 モモ" data_id="311001009" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004009" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006009" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="よーし！　チョコの材料も一杯買ってきたし、&#xA;さっそく始めよっか！" speaker_show="True" stop="True" voice="mm11001_01_0444" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="ええ、そうね。明日のバレンタイン本番まで、&#xA;そんなに時間もないことだし。" speaker_show="True" stop="True" voice="sf11004_01_0238" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="例の違法薬物の出所を調べるので忙しかったから、&#xA;結局、直前になっちゃったしね。" speaker_show="True" stop="True" voice="ig11006_01_0128" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あ、うん…。" speaker_show="True" stop="True" voice="mm11001_01_0445" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="あれ？　どうしたのよ、急に？" speaker_show="True" stop="True" voice="sf11004_01_0239" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="決め手となる情報がまだ見つかっていないのに、&#xA;こんなことしててもいいのかなぁ、って…。" speaker_show="True" stop="True" voice="mm11001_01_0446" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="大丈夫だよ、モモち。工場はもう潰してあるんだし、&#xA;出所だってあと３カ所まで絞れてるんだから。" speaker_show="True" stop="True" voice="ig11006_01_0129" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="そっか。うん、そうだよね。" speaker_show="True" stop="True" voice="mm11001_01_0447" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="それにしても、ごえっちがチョコ作りを&#xA;提案してくれて良かったわ。" speaker_show="True" stop="True" voice="sf11004_01_0240" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="アタシ、師匠にどんなチョコを渡せばいいのか、&#xA;けっこう悩んじゃってたんだ。" speaker_show="True" stop="True" voice="sf11004_01_0241" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="フーちゃん。それ、私もだよ。" speaker_show="True" stop="True" voice="mm11001_01_0448" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="安心するのはまだ早いよ、２人とも。&#xA;どうなるかは、これから作るチョコ次第なんだから。" speaker_show="True" stop="True" voice="ig11006_01_0130" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="まずはチョコを溶かすところからだよね？&#xA;じゃあ私、板チョコをお鍋で溶かしてくるね！" speaker_show="True" stop="True" voice="mm11001_01_0449" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="え？　鍋で？" speaker_show="True" stop="True" voice="sf11004_01_0242" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="モモち、ストップストップ。&#xA;チョコは湯煎じゃないと、だよ？" speaker_show="True" stop="True" voice="ig11006_01_0131" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ゆ、ゆせん…？" speaker_show="True" stop="True" voice="mm11001_01_0450" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="湯煎っていうのは、お湯で容器を温めながら、&#xA;その中でチョコを溶かす方法だよ。" speaker_show="True" stop="True" voice="ig11006_01_0132" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="チョコは高温で温めると油分が分離して、&#xA;すぐダメになっちゃんだ。" speaker_show="True" stop="True" voice="ig11006_01_0133" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="へー、そうだったんだ。&#xA;『湯船』かぁ、全然知らなかったよ。" speaker_show="True" stop="True" voice="mm11001_01_0451" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="101_Facial_trouble" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="それ、字が違う気がするけど…まぁいいわ。&#xA;アタシも正直、書けって言われたら自信ないし。" speaker_show="True" stop="True" voice="sf11004_01_0243" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <delete obj_id="相模 楓" />
    <move obj_id="源 モモ" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="石川 五恵" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="うぅ、何だか私がやると失敗しちゃう気がしてきたよぉ。&#xA;ごえちゃん、そこだけお願いしてもいい？" speaker_show="True" stop="True" voice="mm11001_01_0452" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="うん、オッケー。" speaker_show="True" stop="True" voice="ig11006_01_0134" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あ、そうだ。&#xA;その前に、温度計ってある？" speaker_show="True" stop="True" voice="ig11006_01_0135" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="えっ、体温計!?　ごえちゃん、熱でもあるの!?" speaker_show="True" stop="True" voice="mm11001_01_0453" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="101_Facial_trouble" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="モモち、体温計じゃなくて温度計ね。湯煎は温度を&#xA;見ながらやった方が、失敗が少ないんだよ。" speaker_show="True" stop="True" voice="ig11006_01_0136" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あ、そっかそっか。あー、びっくりした。&#xA;でも、ウチにあったかなぁ？" speaker_show="True" stop="True" voice="mm11001_01_0454" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="待っててね、ごえちゃん！&#xA;ちょっとお母さんに聞いて来るから！" speaker_show="True" stop="True" voice="mm11001_01_0455" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <sound type="Se" cue_name="Walk" />
    <Comment comment="【SE】モモが部屋を出て行く音" />
    <move obj_id="源 モモ" position_id="left" x="1000" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="2" wait="True" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="相模 楓" data_id="311004009" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="うーん、先が思いやられるわね…。" speaker_show="True" stop="True" voice="sf11004_01_0244" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="でも、モモち楽しそう。&#xA;私たちも頑張らなきゃだね、楓ちゃん。" speaker_show="True" stop="True" voice="ig11006_01_0137" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="相模 楓" />
    <delete obj_id="石川 五恵" />
    <bg show="True" file="05_momohouseev_01" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001009" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006009" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" loop="True" to_idle="True" force_end="False" />
    <create_effect effect_id="mampsleep_momo" effect_type="MampSleep" life_time="60" target_obj="源 モモ" />
    <msg speaker="源 モモ" text="すぴー、すぴー…えへへ、やっと完成したねぇ。&#xA;ありがとう、２人ともぉ…むにゃむにゃ。" speaker_show="True" stop="True" voice="mm11001_01_0456" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <create_effect effect_id="mampsleep_goe" effect_type="MampSleep" life_time="60" target_obj="石川 五恵" />
    <msg speaker="石川 五恵" text="くー、くー……うぅ、モモちぃ……&#xA;それはカレーのルーだよぉ……。" speaker_show="True" stop="True" voice="ig11006_01_0138" />
    <delete obj_id="源 モモ" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="相模 楓" data_id="311004009" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="ふぁ～あ……結局、泊まりになっちゃったわね。&#xA;ま、それなりに楽しかったけど。" speaker_show="True" stop="True" voice="sf11004_01_0245" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="師匠、喜んでくれるかなぁ？&#xA;アタシの手作りチョコ。" speaker_show="True" stop="True" voice="sf11004_01_0246" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="源 モモ" data_id="311001009" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="大丈夫だよ～、フーちゃん……すぴー、すぴー。" speaker_show="True" stop="True" voice="mm11001_01_0457" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="相模 楓" data_id="311004009" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="ふふ。ありがとう、モモち。" speaker_show="True" stop="True" voice="sf11004_01_0247" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="でも、アイツ…ショコラって言ったっけ？" speaker_show="True" stop="True" voice="sf11004_01_0248" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="あの時の言葉、単なる負け惜しみだと&#xA;いいんだけど…。" speaker_show="True" stop="True" voice="sf11004_01_0249" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="せっかくバレンタインチョコも作ったんだし、&#xA;明日は何事もないといいわね…。" speaker_show="True" stop="True" voice="sf11004_01_0250" />
  </command>
</adv>