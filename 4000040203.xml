    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="逆に今度は戦闘中にギャップが出そうでは&#xA;ありますが、普通といえば普通ですわね。" speaker_show="True" stop="True" voice="sm13002_02_0295" />
    <char_spine_animation obj_id="アミィ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="お気に召していただけましたか？&#xA;光栄です。御主人様。" speaker_show="True" stop="True" voice="am13002_02_0170" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ふふっ、うん。御主人様か……新鮮♪&#xA;ようやくこれで、心が決まったわ。" speaker_show="True" stop="True" voice="sr13001_02_0691" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="では、この口調のメインデータを&#xA;取り寄せるのでよろしいですか？" speaker_show="True" stop="True" voice="sm13002_02_0296" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="アミィ、こっちへおいで。" speaker_show="True" stop="True" voice="sr13001_02_0692" />
    <char_spine_animation obj_id="アミィ" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
