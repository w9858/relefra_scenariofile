    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="マスターてめえ、いったいどれが&#xA;気に入るってんだゴルァ！" speaker_show="True" stop="True" voice="am13002_02_0161" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="却下ですわ。敷島に荒くれは&#xA;必要ありませんもの。" speaker_show="True" stop="True" voice="sm13002_02_0290" />
    <char_spine_animation obj_id="アミィ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="アミィ" text="あ、あの……マ、マスター…わ、私は&#xA;ど、どうしたら……モジモジ。" speaker_show="True" stop="True" voice="am13002_02_0162" />
    <char_spine_animation obj_id="敷島 来夢" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="モジモジされてちゃ仕事にならないから&#xA;これも却下。次。" speaker_show="True" stop="True" voice="sr13001_02_0684" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="ヤッホー、マスター！　あたしアミィ！&#xA;超言うこときくからギャラ弾んでよねっ♪" speaker_show="True" stop="True" voice="am13002_02_0163" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="あ、これは意外と。" speaker_show="True" stop="True" voice="sr13001_02_0685" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="いけませんわ。任務中にこんな大声を&#xA;出されたら、失敗しかねません。" speaker_show="True" stop="True" voice="sm13002_02_0291" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="マスタ～、良い子ねぇ～ん。&#xA;さ、お尻ペンペンしたげるわよ～ん。チュッ♪" speaker_show="True" stop="True" voice="am13002_02_0164" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="おぉぉ？　こ、これは、新たな扉が……" speaker_show="True" stop="True" voice="sr13001_02_0686" />
    <char_spine_animation obj_id="敷島 真留瀬" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="開くといけないので、閉めておきますわね。" speaker_show="True" stop="True" voice="sm13002_02_0292" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="アミィ" text="ワンワンワンッ！" speaker_show="True" stop="True" voice="am13002_02_0165" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="え。" speaker_show="True" stop="True" voice="sr13001_02_0687" />
    <char_spine_animation obj_id="アミィ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="アミィ" text="ニャ～、ニャ～ン。" speaker_show="True" stop="True" voice="am13002_02_0166" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="ああ、ついに動物カテゴリーまで。" speaker_show="True" stop="True" voice="sm13002_02_0293" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
