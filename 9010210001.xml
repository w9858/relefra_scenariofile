  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="04_shopstreetmo_01" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002012" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="それじゃ、またね、モモ。" speaker_show="True" stop="True" voice="hy11002_02_0769" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" lip_sync="False" text="（モモも頑張っているわね。&#xA;私も帰ったらトレーニングをしないと……。）" speaker_show="True" stop="True" voice="hy11002_02_0770" />
    <move obj_id="半蔵門 雪" position_id="left" x="-200" x_move_type="MoveTo" time="0.3" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……雪ちゃん。" speaker_show="True" stop="True" voice="ks11010_02_0350" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="はい？　先輩。&#xA;こんなところで偶然ですね。" speaker_show="True" stop="True" voice="hy11002_02_0771" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うん。モモちゃんの特訓は終わり？&#xA;なんだか不思議だな、雪ちゃんが師匠なんて。" speaker_show="True" stop="True" voice="ks11010_02_0351" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="もしかして、ずっと見ていたんですか？" speaker_show="True" stop="True" voice="hy11002_02_0772" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="ううん、たまたま通りかかっただけだよ。&#xA;ずいぶん慕われてるみたいだね。" speaker_show="True" stop="True" voice="ks11010_02_0352" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうですか？" speaker_show="True" stop="True" voice="hy11002_02_0773" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="うん、雪ちゃんのこと大好きって感じ。&#xA;雪ちゃんも嬉しいんじゃないの？" speaker_show="True" stop="True" voice="ks11010_02_0353" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……少しは頼られる師匠であらねば。" speaker_show="True" stop="True" voice="hy11002_02_0774" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="雪ちゃんが弟子だった頃も、あんな感じだったよね。&#xA;ちょっと形は違うかもしれないけど……懐かしいなあ。" speaker_show="True" stop="True" voice="ks11010_02_0354" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" lip_sync="False" text="私が弟子だった頃……か。" speaker_show="True" stop="True" voice="hy11002_02_0775" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……ながもんに見せてあげたかったなあ。&#xA;今の雪ちゃんの姿。" speaker_show="True" stop="True" voice="ks11010_02_0355" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="師匠に……今の私を、ですか？" speaker_show="True" stop="True" voice="hy11002_02_0776" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="うん、雪ちゃんが師匠をしているところなんか見たら&#xA;きっと喜びまくりだっただろうから。" speaker_show="True" stop="True" voice="ks11010_02_0356" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="どうでしょうか。師匠の感性は独特でしたから、&#xA;私の不器用さに怒っているかもしれません。" speaker_show="True" stop="True" voice="hy11002_02_0777" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そんなことないって。&#xA;立派にモモちゃんの師匠してると思うよ。" speaker_show="True" stop="True" voice="ks11010_02_0357" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ありがとうございます。" speaker_show="True" stop="True" voice="hy11002_02_0778" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……ながもんって言えば、彼女のバイク、&#xA;今、雪ちゃんとこなんだよね？" speaker_show="True" stop="True" voice="ks11010_02_0358" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="はい。" speaker_show="True" stop="True" voice="hy11002_02_0779" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="今度さ、機会があったら、&#xA;信もながもんのバイクに乗せてよ。" speaker_show="True" stop="True" voice="ks11010_02_0359" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="それで、ながもんとの思い出の場所&#xA;ふたりで巡ろう。雪ちゃんさえ良かったら。" speaker_show="True" stop="True" voice="ks11010_02_0360" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……ええ、そうですね。" speaker_show="True" stop="True" voice="hy11002_02_0780" />
  </command>
