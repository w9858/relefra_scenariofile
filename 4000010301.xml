    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（うちの師匠は、人間としてはかなりダメダメな&#xA;部類に入ると思う）" speaker_show="True" stop="True" voice="sf11004_01_0619" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（まず家事をしない。今は当番制で家事をしているけど、&#xA;当番制に決める前までは食事は買い食いばかりで…）" speaker_show="True" stop="True" voice="sf11004_01_0620" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="（掃除なんて最低限しかしてなかった。&#xA;今でも何も言わないと当番をサボる）" speaker_show="True" stop="True" voice="sf11004_01_0621" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="12_DropShoulders" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（その上、家計のことも考えずに無駄遣いしたり、&#xA;ギターの道具やお菓子やゲームを買って…）" speaker_show="True" stop="True" voice="sf11004_01_0622" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（夏の暑い時期は半裸みたいな格好で部屋の中を&#xA;歩き回るし…）" speaker_show="True" stop="True" voice="sf11004_01_0623" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（そのくせ交友関係が広いから、時々アタシの知らない&#xA;女の人とかと話し込んでたりするし…！）" speaker_show="True" stop="True" voice="sf11004_01_0624" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（そりゃ確かにスパイとしてはすごいと思うけど…）" speaker_show="True" stop="True" voice="sf11004_01_0625" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（強い敵だってあっさり倒しちゃうし、&#xA;やたらと情報網が広いし…）" speaker_show="True" stop="True" voice="sf11004_01_0626" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="ちゃんとアタシを守ってくれるし…" speaker_show="True" stop="True" voice="sf11004_01_0627" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="（とにかく、師匠はスパイとしては凄いけど、&#xA;人間としてはダメダメで…！）" speaker_show="True" stop="True" voice="sf11004_01_0628" lip_sync="False" />
    <move obj_id="相模 楓" position_id="left" x="-125" x_move_type="MoveTo" time="0.3" wait="True" />
    <create type="Character" anchor="Bottom" obj_id="八千代 命" data_id="311003006" position_id="left" x="125" />
