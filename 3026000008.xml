    <move obj_id="敷島 真留瀬" position_id="left" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.1" wait="False" />
    <move obj_id="敷島 来夢" position_id="left" x="-150" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.1" wait="False" />
    <transition effect="Default" type="In" color_change="False" time="1" wait="True" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="やっぱりそりの中は少し狭いですわね。" speaker_show="True" stop="True" voice="sm13002_03_0070" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="アミィ" text="２人乗りのそりは密着したほうが安定しますゆえ。" speaker_show="True" stop="True" voice="am13002_03_0054" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="理由はまだあるわ。マルが立派に育ったからよ。&#xA;たわわとはこのことね。" speaker_show="True" stop="True" voice="sr13001_03_0124" />
