    <msg speaker="敷島 真留瀬" text="どんどん速くなってきましたわね……。" speaker_show="True" stop="True" voice="sm13002_03_0074" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" />
    <msg speaker="敷島 来夢" text="もっともっと速くなるわよ。&#xA;子供の時に遊んでたところより傾斜あるしね！" speaker_show="True" stop="True" voice="sr13001_03_0128" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" />
    <msg speaker="敷島 真留瀬" text="ス、スピードは落とせませんの？" speaker_show="True" stop="True" voice="sm13002_03_0075" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" />
    <msg speaker="敷島 来夢" text="無理！　それに速いほうが楽しいでしょ？" speaker_show="True" stop="True" voice="sr13001_03_0129" />
    <char_spine_animation force_end="False" loop="False" obj_id="敷島 真留瀬" face_anim_type="019_Facial_damage" body_anim_type="15_Negative" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 真留瀬" face_anim_type="019_Facial_damage" />
    <msg speaker="敷島 真留瀬" text="これ以上速くなるほうが無理ですわぁぁぁ……！" speaker_show="True" stop="True" voice="sm13002_03_0076" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" />
    <msg speaker="敷島 来夢" text="もしもの時は守ってあげるから。&#xA;心配しない――で。" speaker_show="True" stop="True" voice="sr13001_03_0130" />
