    <char_spine_animation force_end="False" loop="True" obj_id="敷島 真留瀬" face_anim_type="019_Facial_damage" />
    <msg speaker="敷島 真留瀬" text="盛大に転びましたわね……お姉様は大丈夫ですか？" speaker_show="True" stop="True" voice="sm13002_03_0078" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" />
    <msg speaker="敷島 来夢" text="……あはははっ！" speaker_show="True" stop="True" voice="sr13001_03_0131" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" />
    <msg speaker="敷島 来夢" text="あー、楽しかった。&#xA;お互い子供の時みたい。雪まみれね。" speaker_show="True" stop="True" voice="sr13001_03_0132" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 真留瀬" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" />
    <msg speaker="敷島 真留瀬" text="ええ。あの頃と変わりませんわ。" speaker_show="True" stop="True" voice="sm13002_03_0079" />
    <char_spine_animation force_end="False" loop="True" obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" />
    <msg speaker="敷島 来夢" text="ほんと久々にこんなに遊び倒したわ。&#xA;またみんなで来ましょうね。" speaker_show="True" stop="True" voice="sr13001_03_0133" />
