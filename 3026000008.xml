    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="次やったら、お姉様のお願いでも降りますわ。&#xA;いいですね？" speaker_show="True" stop="True" voice="sm13002_03_0072" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="はーい。で、準備はいいかしら？" speaker_show="True" stop="True" voice="sr13001_03_0126" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="ええ。お姉様のお好きなタイミングで。" speaker_show="True" stop="True" voice="sm13002_03_0073" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="それじゃ、アミィ。&#xA;背中を押してちょうだい。" speaker_show="True" stop="True" voice="sr13001_03_0127" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="アミィ" text="それでは、お２人ともいってらっしゃいませ。" speaker_show="True" stop="True" voice="am13002_03_0055" />
