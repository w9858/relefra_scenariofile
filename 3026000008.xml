    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="スキーも久しぶりにやると、楽しいですわね。" speaker_show="True" stop="True" voice="sm13002_03_0064" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="あ、ちょうどいいところに。見て見て&#xA;レンタルした用具セットにこんなのが入ってたわ。" speaker_show="True" stop="True" voice="sr13001_03_0117" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="そりですか？" speaker_show="True" stop="True" voice="sm13002_03_0065" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="小さな頃は冬になると一緒に滑ってたじゃない？" speaker_show="True" stop="True" voice="sr13001_03_0118" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="懐かしいですわね。前に座った私を&#xA;お姉様が後ろからいつも包むように……。" speaker_show="True" stop="True" voice="sm13002_03_0066" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="というわけで、今からこれで滑って遊ばない？" speaker_show="True" stop="True" voice="sr13001_03_0119" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様……当時のものより大きいとはいえ&#xA;そりは子供が遊ぶものですわ。" speaker_show="True" stop="True" voice="sm13002_03_0067" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="それが何だって言うの？" speaker_show="True" stop="True" voice="sr13001_03_0120" />
    <char_spine_cheek obj_id="敷島 真留瀬" type="Cheek02_medium" loop="True" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="えっと……子供っぽくって少し照れくさくありません？" speaker_show="True" stop="True" voice="sm13002_03_0068" />
    <char_spine_cheek obj_id="敷島 真留瀬" type="Cheek01_default" loop="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="人目を気にせず、思いっきり遊ぶための貸し切りよ。&#xA;ここに笑う人なんていないわ。" speaker_show="True" stop="True" voice="sr13001_03_0121" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="年に一度の誕生日……久しぶりに&#xA;マルとそりに乗りたかったなぁ。ちらちら。" speaker_show="True" stop="True" voice="sr13001_03_0122" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="もう……そんな言い方されたら私は断れませんわ。" speaker_show="True" stop="True" voice="sm13002_03_0069" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="やった！" speaker_show="True" stop="True" voice="sr13001_03_0123" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1" wait="True" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
