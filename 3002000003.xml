<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3002000003" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" />
    <bg show="True" file="25_katrinahouse_01" resource_type="Background" />
    <Comment comment="25_katrinahouse_01が来たらそれに設定しなおす" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="カトリーナの部屋" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <Comment comment="パジャマ命と楓にする" />
    <create type="Character" obj_id="八千代 命" data_id="311003008" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004011" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="はぁ～、いいお湯だった。牛乳のいい匂いに&#xA;包まれて子牛になった気分だったよ～。" speaker_show="True" stop="True" voice="ym11003_01_0356" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="子牛は多分、牛乳風呂には入んないですけどね。&#xA;カトリーナさん、本当にありがとうございました。" speaker_show="True" stop="True" voice="sf11004_01_0428" />
    <delete obj_id="八千代 命" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="カトリーナ" data_id="311009005" position_id="left" x="0" y="0" anchor="Bottom" />
    <Comment comment="パジャマカトにする" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="２人ともお肌ツヤツヤでとっても素敵よ。&#xA;さ、まずはメイちゃん、いらっしゃい。" speaker_show="True" stop="True" voice="kt11009_01_0235" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="髪の毛、乾かしてあげるから&#xA;ドレッサーの前に座って。" speaker_show="True" stop="True" voice="kt11009_01_0236" />
    <move obj_id="カトリーナ" position_id="left" x="125" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003008" position_id="left" x="-125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" loop="False" to_idle="False" force_end="False" />
    <char_spine_cheek obj_id="八千代 命" type="Cheek03_strong" loop="True" />
    <msg speaker="八千代 命" text="え～、そんな！&#xA;照れるよ～。" speaker_show="True" stop="True" voice="ym11003_01_0357" />
    <char_spine_cheek obj_id="八千代 命" type="Cheek01_default" loop="True" />
    <sound type="Se" />
    <Comment comment="SE　ドライヤーの音" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="いいからいいから。&#xA;乾いてきたから、さっきのヘアオイルつけるわね。" speaker_show="True" stop="True" voice="kt11009_01_0237" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ふわぁぁぁ～、甘酸っぱくていい匂い～。" speaker_show="True" stop="True" voice="ym11003_01_0358" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="これをつけてブラッシングすると、髪全体に&#xA;いい匂いが広がるのよ。" speaker_show="True" stop="True" voice="kt11009_01_0238" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ふはぁ～、本当だ！　動くたびにいい匂いがする～。&#xA;メイがメイじゃないみた～い。" speaker_show="True" stop="True" voice="ym11003_01_0359" />
    <shake target="Character" obj_id="八千代 命" x="20" vibrate="10" time="0.5" wait="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="メイちゃん、まだブラッシングの途中よ。&#xA;あまりぴょんぴょん跳ね回らないの。" speaker_show="True" stop="True" voice="kt11009_01_0239" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="あはは、興奮しちゃってつい。" speaker_show="True" stop="True" voice="ym11003_01_0360" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="メイちゃんももう少し気を使ってほしいわね、&#xA;こんなに元がいいんだから、勿体無いわ。" speaker_show="True" stop="True" voice="kt11009_01_0240" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="またまたー！　メイもフーも貧乏だから、&#xA;褒めても何も出ないよ～。" speaker_show="True" stop="True" voice="ym11003_01_0361" />
    <delete obj_id="八千代 命" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="相模 楓" data_id="311004011" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="いえ、冗談で言ってるんじゃないですよ。&#xA;師匠の髪は元々とっても綺麗なんです。" speaker_show="True" stop="True" voice="sf11004_01_0429" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="カトリーナ" data_id="311009005" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003008" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="そうね、くし通りもなめらかだし、手触りもよくて&#xA;とかしているとこっちまで、幸せな気持ちになるわ。" speaker_show="True" stop="True" voice="kt11009_01_0241" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="いやぁ、カトーさんから褒められちゃうと、&#xA;調子狂っちゃうなぁ～。" speaker_show="True" stop="True" voice="ym11003_01_0362" />
    <delete obj_id="八千代 命" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="相模 楓" data_id="311004011" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="師匠ってば、何照れてるんですか。&#xA;アタシが褒めてもそんな反応したことないのに。" speaker_show="True" stop="True" voice="sf11004_01_0430" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="カトリーナ" data_id="311009005" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003008" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="だって、フーならともかく、カトーさんから&#xA;褒められるなんて、予想外だったから……。" speaker_show="True" stop="True" voice="ym11003_01_0363" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="カトリーナ" text="あら、だって本当に綺麗なんだもの。はい、&#xA;お手入れ完了。メイちゃん、髪の毛触ってみて。" speaker_show="True" stop="True" voice="kt11009_01_0242" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="どきどきどき……。" speaker_show="True" stop="True" voice="ym11003_01_0364" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <create_effect effect_id="KiraKira_mei" effect_type="EffectKirakira" life_time="1.0" target_obj="八千代 命" />
    <msg speaker="八千代 命" text="わー！　何これ、すごーい！　ツヤツヤで触り心地も&#xA;ふわふわ！　髪が揺れるたびにいい匂い！" speaker_show="True" stop="True" voice="ym11003_01_0365" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="カトーさん、凄すぎ！　魔法みたい！&#xA;魔法使いカトーさん！　メイ、感激～！" speaker_show="True" stop="True" voice="ym11003_01_0366" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="ふふ、そんなに喜んでもらえて、嬉しいわ。" speaker_show="True" stop="True" voice="kt11009_01_0243" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>