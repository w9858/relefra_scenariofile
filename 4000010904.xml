    <delete obj_id="源 モモ" />
    <delete obj_id="カトリーナ" />
    <msg window_type="SystemMsg" text="食事をして小休止をした後、ツキカゲは&#xA;今日も基地内で合同訓練を行っていた。" stop="True" />
    <msg_window_show show="False" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="…あー。考えが甘かったーー！！&#xA;信先輩どこですかーー！！" speaker_show="True" stop="True" voice="mm11001_01_1027" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="根をあげるのは早いわよ、&#xA;少しでも神の叡智を学ぶのよ。" speaker_show="True" stop="True" voice="kt11009_01_0404" />
    <delete obj_id="源 モモ" />
    <delete obj_id="カトリーナ" />
    <Comment comment="信の服装は「私服（夏）」変更予定" />
