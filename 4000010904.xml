    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="高坂 信" text="（相変わらず潜伏スキルでは頼りにされてるなぁ）" speaker_show="True" stop="True" voice="ks11010_01_0251" lip_sync="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
