    <create obj_id="青葉 初芽" data_id="311005013" position_id="left" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="みなさん、今日は気温も高いので、&#xA;遊び疲れないようにしっかり休憩もしてくださいね。" speaker_show="True" stop="True" voice="ah11005_02_0390" />
    <delete obj_id="青葉 初芽" />
