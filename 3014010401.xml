    <move obj_id="源 モモ" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" />
    <move obj_id="相模 楓" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" />
    <move obj_id="石川 五恵" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time=".3" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="じゃあ私、売店で飲み物買ってくる！" speaker_show="True" stop="True" voice="mm11001_02_0500" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="モモち、私も一緒に行くよ。&#xA;フーちゃんも行かない？" speaker_show="True" stop="True" voice="ig11006_02_0235" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="19_Amazed" force_end="False" loop="False" mixing="True" />
    <msg speaker="相模 楓" text="アタシは遠慮しとく。" speaker_show="True" stop="True" voice="sf11004_02_0496" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="そっか、何か買ってこようか？" speaker_show="True" stop="True" voice="ig11006_02_0236" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="大丈夫。家から持ってきたから。ごくごく……。&#xA;ぷはー、運動後の冷たい水は美味しいわね。" speaker_show="True" stop="True" voice="sf11004_02_0497" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="フーちゃん、水筒持ってきたんだ！&#xA;すぐ飲めて便利だもんね。" speaker_show="True" stop="True" voice="mm11001_02_0501" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="まあ便利ってのもあるけど、こういう所の&#xA;飲み物とか食べ物って高いからね。" speaker_show="True" stop="True" voice="sf11004_02_0498" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="石川 五恵" text="確かに、普通に街で買うよりちょっと高めだもんね。" speaker_show="True" stop="True" voice="ig11006_02_0237" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="けど、普段食べている物でも、こういう所で食べると&#xA;美味しく感じちゃうんだよね～。何でだろう。" speaker_show="True" stop="True" voice="mm11001_02_0502" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="そこなのよ！　中身は全く一緒なんだから、&#xA;食べたければ家に帰ってから食べればいいでしょ？" speaker_show="True" stop="True" voice="sf11004_02_0499" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="言われてみると、確かにそうなんだよね。&#xA;けど、売店からいい匂いがするとつい……。" speaker_show="True" stop="True" voice="mm11001_02_0503" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="引き寄せられちゃってる人がいるね……。" speaker_show="True" stop="True" voice="ig11006_02_0238" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="あぁっ！&#xA;師匠がフラフラ売店にっ！" speaker_show="True" stop="True" voice="sf11004_02_0500" />
