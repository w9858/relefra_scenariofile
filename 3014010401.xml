    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="もう１回ウォータースライダーも乗りたいよねぇ。&#xA;ごえちゃん、何がいい？" speaker_show="True" stop="True" voice="ym11003_02_0556" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
