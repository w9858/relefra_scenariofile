  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="02_wasabimo_001" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <Comment comment="高坂信　私服" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ああ、シン様がまた私の店へ&#xA;来てくださるようになるだなんて……夢のようです！" speaker_show="True" stop="True" voice="kt11009_02_0474" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="信も、いつでもWasabiに&#xA;来られるようになって嬉しいよ。" speaker_show="True" stop="True" voice="ks11010_02_0521" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="日本を離れてる間、カトさんの作ってくれるフローズン&#xA;ホットチョコレートがすごく恋しかったんだ。" speaker_show="True" stop="True" voice="ks11010_02_0522" />
    <Comment comment="歓喜レベル1（この後、段階的に上がっていく）" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="シン様が、私の味を……!?&#xA;はぁぁ……光栄至極……！" speaker_show="True" stop="True" voice="kt11009_02_0475" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="それにしても、懐かしいな～。" speaker_show="True" stop="True" voice="ks11010_02_0523" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ここにいると、向こうに行く前、&#xA;ツキカゲのみんなと過ごした頃を思い出すよ。" speaker_show="True" stop="True" voice="ks11010_02_0524" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="他愛のないこと話したり、メイちゃんの歌を聞いたり、&#xA;甘い言葉しりとりや、愛してるゲームをしたり……" speaker_show="True" stop="True" voice="ks11010_02_0525" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="楽しかったなあ……。" speaker_show="True" stop="True" voice="ks11010_02_0526" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="シン様、その「愛してるゲーム」というのは？" speaker_show="True" stop="True" voice="kt11009_02_0476" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="あ、カトさんやったことない？" speaker_show="True" stop="True" voice="ks11010_02_0527" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="交互に「愛してる」って言い合って、照れたり笑ったり&#xA;しちゃった方が負けっていうゲームだよ。" speaker_show="True" stop="True" voice="ks11010_02_0528" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="カトリーナ" text="へえ……面白そうですね。" speaker_show="True" stop="True" voice="kt11009_02_0477" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="そうだ。&#xA;せっかくだし、今２人でやってみる？" speaker_show="True" stop="True" voice="ks11010_02_0529" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ええ、喜んで。&#xA;では、私からいきますね。" speaker_show="True" stop="True" voice="kt11009_02_0478" />
    <delete obj_id="高坂 信" />
    <move obj_id="カトリーナ" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="……シン様。貴方の優しいお言葉は、&#xA;いつでも私に喜びを覚えさせます。" speaker_show="True" stop="True" voice="kt11009_02_0479" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="そして貴方の温かな微笑みは、私のような罪人にも&#xA;赦しを与えてくださいます。" speaker_show="True" stop="True" voice="kt11009_02_0480" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="カトリーナ" text="貴方の幸福が、私の幸福。貴方の御心に、いつまでも&#xA;幸福の光が降り注ぐことを、心よりお祈りいたします。" speaker_show="True" stop="True" voice="kt11009_02_0481" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="……愛しております、シン様。" speaker_show="True" stop="True" voice="kt11009_02_0482" />
    <move obj_id="カトリーナ" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <Comment comment="高坂信　私服" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……な、長いね。&#xA;照れるどころか、圧倒されちゃったよ。" speaker_show="True" stop="True" voice="ks11010_02_0530" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="私のシン様への想いは、「愛してる」なんて&#xA;一言ではとても表せませんから。" speaker_show="True" stop="True" voice="kt11009_02_0483" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="な、なるほど……？" speaker_show="True" stop="True" voice="ks11010_02_0531" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="じゃあ、次、信ね。" speaker_show="True" stop="True" voice="ks11010_02_0532" />
    <delete obj_id="カトリーナ" />
    <move obj_id="高坂 信" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="えっと……いつも、こんな信のことを&#xA;褒めてくれてありがとう。" speaker_show="True" stop="True" voice="ks11010_02_0533" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="カトさん、愛してる。" speaker_show="True" stop="True" voice="ks11010_02_0534" />
    <move obj_id="高坂 信" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <Comment comment="歓喜レベル2" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek03_strong" loop="True" />
    <msg speaker="カトリーナ" text="………………………………！" speaker_show="True" stop="True" voice="kt11009_02_0484" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……えっと、カトさん……？" speaker_show="True" stop="True" voice="ks11010_02_0535" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="015_Facial_poker" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek01_default" loop="True" />
    <msg speaker="カトリーナ" text="……あ。申し訳ございません、シン様。私、今少々&#xA;記憶が飛んだようで……もう一度お願いできますか？" speaker_show="True" stop="True" voice="kt11009_02_0485" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="記憶が……!?&#xA;う、うん、わかった……。" speaker_show="True" stop="True" voice="ks11010_02_0536" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="愛してるよ、カトさん。" speaker_show="True" stop="True" voice="ks11010_02_0537" />
    <Comment comment="歓喜レベル3" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek03_strong" loop="True" />
    <msg speaker="カトリーナ" text="………………………………!!!!" speaker_show="True" stop="True" voice="kt11009_02_0486" />
    <Comment comment="【SE】バタンと倒れる音" />
    <sound type="Se" cue_name="KnockDown" />
    <delete obj_id="カトリーナ" />
    <wait time="1.0" />
    <move obj_id="高坂 信" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="え……!?&#xA;カトさんが倒れた!?" speaker_show="True" stop="True" voice="ks11010_02_0538" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="True" loop="True" mixing="True" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek03_strong" loop="True" />
    <msg speaker="カトリーナ" text="わが生涯に、一片の悔いなし……" speaker_show="True" stop="True" voice="kt11009_02_0487" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="0" y="0" anchor="Bottom" />
    <Comment comment="高坂信　私服" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="高坂 信" text="なんて安らかな笑顔……じゃなくて!!" speaker_show="True" stop="True" voice="ks11010_02_0539" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="だ、誰かー!!&#xA;誰か来てー!! カトさんがー!!!!" speaker_show="True" stop="True" voice="ks11010_02_0540" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="高坂 信" lip_sync="False" text="（もう、カトさんと安易に&#xA;このゲームをやるのはやめよう……！）" speaker_show="True" stop="True" voice="ks11010_02_0541" />
  </command>
