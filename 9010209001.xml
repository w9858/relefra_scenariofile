    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="そうだったんですね。&#xA;てっきり、何か入っているのかと……。" speaker_show="True" stop="True" voice="hy11002_02_0804" />
