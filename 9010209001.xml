    <msg speaker="カトリーナ" text="もちろんたっぷり入れてあるわよ。" speaker_show="True" stop="True" voice="kt11009_02_0310" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
