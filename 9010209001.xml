    <msg speaker="カトリーナ" text="いらっしゃい、雪ちゃん。&#xA;こんな時間に呼び出しちゃってごめんなさいね。" speaker_show="True" stop="True" voice="kt11009_02_0301" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
