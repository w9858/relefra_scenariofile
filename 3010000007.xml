    <char_spine_animation obj_id="水無月 ジュン" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="水無月 ジュン" text="どうも、ご連絡を頂きまして&#xA;ありがとうございます。" speaker_show="True" stop="True" voice="mj99009_02_0014" />
    <char_spine_animation obj_id="水無月 ジュン" body_anim_type="01_Stay" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="私の仕事に興味を持って頂けて&#xA;嬉しいですよ。" speaker_show="True" stop="True" voice="mj99009_02_0015" />
    <delete obj_id="Smile_jun" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="はいっ。この間の撮影で、&#xA;なんだかビビッと来ちゃいまして。" speaker_show="True" stop="True" voice="ym11003_02_0245" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
