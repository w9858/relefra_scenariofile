    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="ビビッと来たというか、ゾクッと来たというか。" speaker_show="True" stop="True" voice="ks11010_02_0118" />
    <char_spine_animation obj_id="水無月 ジュン" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="いいでしょう、それでは初めに&#xA;概要を説明させて頂きます。" speaker_show="True" stop="True" voice="mj99009_02_0016" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="相模 楓" text="よろしくお願いします。" speaker_show="True" stop="True" voice="sf11004_02_0252" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="フムン……私が目指すのはただ１つ、&#xA;至高のウェディングに他なりません。" speaker_show="True" stop="True" voice="mj99009_02_0017" />
    <char_spine_animation obj_id="水無月 ジュン" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="そのために必要なものが何か、&#xA;わかりますか？" speaker_show="True" stop="True" voice="mj99009_02_0018" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" face_anim_type="008_Facial_serious" />
    <msg speaker="相模 楓" text="愛し合う２人……ですか？" speaker_show="True" stop="True" voice="sf11004_02_0253" />
    <char_spine_animation obj_id="水無月 ジュン" face_anim_type="101_Facial_unique" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="水無月 ジュン" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
