    <char_spine_cheek obj_id="水無月 ジュン" type="Cheek03_strong" />
    <msg speaker="水無月 ジュン" text="そぉーう！　その通り!!&#xA;愛なくば全ては始まらず、また芸術たり得ない！" speaker_show="True" stop="True" voice="mj99009_02_0019" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" face_anim_type="015_Facial_poker" />
    <char_spine_animation obj_id="水無月 ジュン" body_anim_type="01_Stay" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="日常の中で育む関係もいいかもしれない。&#xA;しかし私は、その先を見たい……。" speaker_show="True" stop="True" voice="mj99009_02_0020" />
    <char_spine_animation obj_id="水無月 ジュン" face_anim_type="011_Facial_surprise" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="さすれば求められるのは何かッ？&#xA;分かりますかッ!?" speaker_show="True" stop="True" voice="mj99009_02_0021" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="ええっと……&#xA;ちなみに、水無月さんのお考えは？" speaker_show="True" stop="True" voice="ks11010_02_0119" />
    <char_spine_animation obj_id="水無月 ジュン" body_anim_type="02_Idle" force_end="False" loop="True" mixing="True" />
    <msg speaker="水無月 ジュン" text="愛より先に進むために必要なのは……&#xA;試練！　そう、すなわち千尋の谷!!" speaker_show="True" stop="True" voice="mj99009_02_0022" />
    <delete obj_id="ShockWave_jun" />
