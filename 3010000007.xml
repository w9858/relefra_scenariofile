    <msg speaker="八千代 命" text="あはは。&#xA;師匠はこういうタイプの人と縁があるよねぇ。" speaker_show="True" stop="True" voice="ym11003_02_0249" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
