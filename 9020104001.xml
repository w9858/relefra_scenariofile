<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/9020104001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="34_sakasatower_01" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <msg text="ある夜。飛粋と葉栖美は&#xD;&#xA;逆さ塔で戦闘訓練を行っていた。" speaker_show="True" stop="True" window_type="SystemMsg" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="アタシの訓練に付き合わせちゃって悪いねー、&#xA;飛粋ちゃん。" speaker_show="True" stop="True" voice="th12004_02_0275" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="最近の飛粋ちゃん、逆さ塔の囚人や&#xA;敵の能力をどんどん取り込んでるからさ。" speaker_show="True" stop="True" voice="th12004_02_0276" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="仮想敵として、これ以上ない適任なんだよね。" speaker_show="True" stop="True" voice="th12004_02_0277" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="いえいえ！&#xA;これくらいさせてください！" speaker_show="True" stop="True" voice="mh12001_02_0291" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="葉栖美さんには、料理教えてもらったり、&#xA;いつもお世話になりっぱなしなので！" speaker_show="True" stop="True" voice="mh12001_02_0292" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="でも、急にどうしたんですか？" speaker_show="True" stop="True" voice="mh12001_02_0293" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="んー、大した理由はないんだけど……" speaker_show="True" stop="True" voice="th12004_02_0278" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ゆらちゃんも正式加入して、凪の部隊の戦闘力&#xA;平均値爆上がり！って感じだからさ。" speaker_show="True" stop="True" voice="th12004_02_0279" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="アタシはアタッカーじゃないとはいえ、皆の足を&#xA;引っ張らないようにしたいなーって思ってね。" speaker_show="True" stop="True" voice="th12004_02_0280" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="なるほど……！" speaker_show="True" stop="True" voice="mh12001_02_0294" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="そういうことでしたら、不肖宗近飛粋、&#xA;いくらでもお⼿伝いさせていただきます！" speaker_show="True" stop="True" voice="mh12001_02_0295" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="轟 葉栖美" text="うん、よろしくね。&#xA;飛粋ちゃん。" speaker_show="True" stop="True" voice="th12004_02_0281" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="あ。そういえば。" speaker_show="True" stop="True" voice="mh12001_02_0296" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ん？ どした？" speaker_show="True" stop="True" voice="th12004_02_0282" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="実は私も……サポートもできるようになりたいんです！" speaker_show="True" stop="True" voice="mh12001_02_0297" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="おお、そうなん？" speaker_show="True" stop="True" voice="th12004_02_0283" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="はい！ いつまでもサポート役を葉栖美さん１人に&#xA;任せきりというわけにはいかないですし……" speaker_show="True" stop="True" voice="mh12001_02_0298" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="何より私、もっと色々出来るようになって、&#xA;凪の部隊の役に立ちたいんです！" speaker_show="True" stop="True" voice="mh12001_02_0299" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="葉栖美さん、今度、私にもバックアップについて&#xA;教えてもらえませんか？" speaker_show="True" stop="True" voice="mh12001_02_0300" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="飛粋ちゃん……。" speaker_show="True" stop="True" voice="th12004_02_0284" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" lip_sync="False" text="（……本当にこの子は、向上心の塊みたいな子だなあ……&#xA;先輩としては、ちょっと焦るわ……）" speaker_show="True" stop="True" voice="th12004_02_0285" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="しかしまあ、飛粋ちゃんがいてくれるおかげで、&#xA;こっちも慢心せずに精進できるってもんだ。" speaker_show="True" stop="True" voice="th12004_02_0286" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="え？&#xA;なんですか？" speaker_show="True" stop="True" voice="mh12001_02_0301" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="轟 葉栖美" text="ううん。&#xA;オッケー、任せといて。" speaker_show="True" stop="True" voice="th12004_02_0287" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="ありがとうございます、葉栖美さん！" speaker_show="True" stop="True" voice="mh12001_02_0302" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="なんのなんの。凪の部隊に貢献したいって&#xA;言ってくれる子に、助力は惜しまないよ。" speaker_show="True" stop="True" voice="th12004_02_0288" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="03_BackBothHands" force_end="False" loop="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="でも、とりあえず今日は、&#xA;こっちの訓練に付き合ってね。" speaker_show="True" stop="True" voice="th12004_02_0289" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="そろそろ行くよ、後輩！" speaker_show="True" stop="True" voice="th12004_02_0290" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="はい！&#xA;どんとこいです、先輩！" speaker_show="True" stop="True" voice="mh12001_02_0303" />
  </command>
</adv>