    <msg window_type="SystemMsg" text="ツキカゲメンバーにスパイの素質を見出された&#xA;ごらく部４人は、体験修行をしてみる事となった。" stop="True" />
    <msg_window_show show="False" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop in_time="1.0" show_time="2.0" out_time="1.0" wait="True" text="ツキカゲ基地" />
    <create type="Character" obj_id="船見 結衣" data_id="394003001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="結衣ちゃん、戦闘服への着替えは&#xA;終わりましたか？" speaker_show="True" stop="True" voice="ah11005_03_0040" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="001_Facial_blink" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="船見 結衣" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="船見 結衣" text="はい、サイズもピッタリです。" speaker_show="True" stop="True" voice="fy95003_03_0073" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <char_spine_cheek obj_id="船見 結衣" type="Cheek03_strong" loop="True" />
    <msg speaker="船見 結衣" text="それにしても……凄い服ですよね。&#xA;こんなに肌を出す必要ってあるのかな？" speaker_show="True" stop="True" voice="fy95003_03_0074" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="露出する事によるメリットもあるんですよ。&#xA;そこに敵の攻撃を誘導出来たりとか。" speaker_show="True" stop="True" voice="ah11005_03_0041" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="服の部分には防弾・防刃機能が備わっているので、&#xA;それ以外を集中して守れば良い、となるんです。" speaker_show="True" stop="True" voice="ah11005_03_0042" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="008_Facial_serious" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="船見 結衣" body_anim_type="20_PositiveDouble" force_end="False" loop="False" mixing="True" />
    <char_spine_cheek obj_id="船見 結衣" type="Cheek01_default" loop="False" />
    <msg speaker="船見 結衣" text="なるほど。" speaker_show="True" stop="True" voice="fy95003_03_0075" />
    <move obj_id="青葉 初芽" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="False" />
    <move obj_id="船見 結衣" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006002" position_id="left" x="330" y="0" anchor="Bottom" />
    <move obj_id="石川 五恵" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="False" mixing="True" />
    <msg speaker="石川 五恵" text="あなたの訓練は、私と師匠と。" speaker_show="True" stop="True" voice="ig11006_03_0030" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="テレちゃん、白虎ちゃんが&#xA;担当する事になりました。" speaker_show="True" stop="True" voice="ah11005_03_0043" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="結衣ちゃんは、あの４人の中で&#xA;誰よりも冷静な判断力を持っています。" speaker_show="True" stop="True" voice="ah11005_03_0044" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="その素質を生かせるのは、&#xA;後方支援タイプでしょう。" speaker_show="True" stop="True" voice="ah11005_03_0045" />
    <char_spine_animation obj_id="船見 結衣" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="船見 結衣" text="私が、皆の支援……。" speaker_show="True" stop="True" voice="fy95003_03_0076" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="020_Facial_thinking" force_end="False" loop="True" mixing="True" />
    <msg speaker="船見 結衣" text="(普段から京子にツッコミしてるから、&#xA;フォロー力みたいなのが鍛えられたのかな。)" speaker_show="True" stop="True" voice="fy95003_03_0077" lip_sync="False" />
    <delete obj_id="船見 結衣" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="テレジア" data_id="311008002" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="まあ白虎様なら、こんな子供に支援されるなんて&#xA;御免だけどな！" speaker_show="True" stop="True" voice="bk11008_03_0028" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="テレジア" text="お前の方が子供だろう。" speaker_show="True" stop="True" voice="tr11007_03_0012" />
    <char_spine_animation obj_id="白虎" face_anim_type="101_Facial_unique" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
