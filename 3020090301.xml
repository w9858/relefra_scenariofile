    <create type="Character" obj_id="青葉 初芽" data_id="311005002" position_id="left" x="230" y="0" anchor="Bottom" />
    <move obj_id="青葉 初芽" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="それでは準備は良いですか？&#xA;始めますよ～。" speaker_show="True" stop="True" voice="ah11005_03_0050" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="船見 結衣" text="(分からない事ばかりだけど……&#xA;とにかく頑張ろう。)" speaker_show="True" stop="True" voice="fy95003_03_0081" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="よーい、スタート！" speaker_show="True" stop="True" voice="ah11005_03_0051" />
