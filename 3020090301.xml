    <create type="Character" obj_id="青葉 初芽" data_id="311005002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="船見 結衣" data_id="394003001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="まあまあ。さて、今回の修業ですが、&#xA;ツキカゲ式の『どろけい』にしましょう。" speaker_show="True" stop="True" voice="ah11005_03_0046" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="船見 結衣" text="どろけいって、&#xA;小学生とかがやってるアレですか？" speaker_show="True" stop="True" voice="fy95003_03_0078" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="20_PositiveDouble" force_end="False" loop="False" mixing="True" />
    <msg speaker="青葉 初芽" text="ええ。人数も多いですし、逃げる側と追いかける側で&#xA;２人２組に分かれましょう。" speaker_show="True" stop="True" voice="ah11005_03_0047" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="勝負は15分間で、範囲はツキカゲ基地全体。&#xA;審判は私が務めます。" speaker_show="True" stop="True" voice="ah11005_03_0048" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="直接戦闘は危ないから禁止。&#xA;トラップの使用は有りとします。" speaker_show="True" stop="True" voice="ah11005_03_0049" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="船見 結衣" />
    <create type="Character" obj_id="石川 五恵" data_id="311006002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="良いですね。スパイに必要な&#xA;全体を見る目が養われます。" speaker_show="True" stop="True" voice="ig11006_03_0031" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="230" y="0" anchor="Bottom" />
    <move obj_id="白虎" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <create_effect effect_id="Smile" effect_type="MampSmile" life_time="2.0" target_obj="白虎" x="0" y="0" attach_head_bone="True" />
    <msg speaker="白虎" text="私は五恵とチームを組むぞ！&#xA;時間いっぱいまで逃げ回ってやる！" speaker_show="True" stop="True" voice="bk11008_03_0030" />
    <delete obj_id="Smile" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="白虎" />
    <create type="Character" obj_id="テレジア" data_id="311008002" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="船見 結衣" data_id="394003001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="テレジア" body_anim_type="06_ChestLeftHand" force_end="False" loop="False" mixing="True" />
    <msg speaker="テレジア" text="では、結衣は私とだな。" speaker_show="True" stop="True" voice="tr11007_03_0013" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="船見 結衣" text="追いかける側、ですね。&#xA;よろしくお願いします。" speaker_show="True" stop="True" voice="fy95003_03_0079" />
    <char_spine_animation obj_id="テレジア" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="テレジア" text="ああ。" speaker_show="True" stop="True" voice="tr11007_03_0014" />
    <move obj_id="テレジア" position_id="left" x="230" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <delete obj_id="テレジア" />
    <sound type="Se" cue_name="Hug" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <char_spine_cheek obj_id="船見 結衣" type="Cheek03_strong" loop="True" />
    <create type="Character" obj_id="テレジア" data_id="311008006" position_id="left" x="230" y="0" anchor="Bottom" />
    <move obj_id="テレジア" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <msg speaker="船見 結衣" text="え……え？&#xA;どうして服を脱ぎ始めるんですか!?" speaker_show="True" stop="True" voice="fy95003_03_0080" />
    <char_spine_animation obj_id="テレジア" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="テレジア" text="こうした方が、感覚が研ぎ澄まされる。" speaker_show="True" stop="True" voice="tr11007_03_0015" />
    <move obj_id="テレジア" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="False" />
    <move obj_id="船見 結衣" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006002" position_id="left" x="330" y="0" anchor="Bottom" />
    <move obj_id="石川 五恵" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="て、テレちゃん。転んだりした時に危ないから、&#xA;服は着たままでいよう……？" speaker_show="True" stop="True" voice="ig11006_03_0032" />
    <char_spine_animation obj_id="船見 結衣" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
