  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" />
    <bg show="True" file="03_tsukikage_001" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="ツキカゲ基地" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002009" x="-200" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005007" x="200" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="009_Facial_blush" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽、かなり肌に密着しているけれど、&#xA;本当にサイズはこれであっているのよね。" speaker_show="True" stop="True" voice="hy11002_01_0456" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はい。想定通りのぴったり感です。&#xA;まるで雪ちゃんのために仕立てたようですね！" speaker_show="True" stop="True" voice="ah11005_01_0275" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="この圧迫感、慣れるのに少しかかりそうだけど、&#xA;今日はどんなテストをするの？" speaker_show="True" stop="True" voice="hy11002_01_0457" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="今日のところは透明クリームは塗らずに、&#xA;スーツを着てどのくらい動けるかを測定します。" speaker_show="True" stop="True" voice="ah11005_01_0276" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="普段、雪ちゃんがしているトレーニングと同じ&#xA;動きをしてください。" speaker_show="True" stop="True" voice="ah11005_01_0277" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="私はその横でデータを収集していますが、&#xA;気にせず普段通りに動いてみてくださいね。" speaker_show="True" stop="True" voice="ah11005_01_0278" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="了解。じゃあ普段通りのトレーニングを&#xA;一通り、やらせてもらうわ。" speaker_show="True" stop="True" voice="hy11002_01_0458" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" to_idle="True" />
    <msg speaker="青葉 初芽" text="お願いします！" speaker_show="True" stop="True" voice="ah11005_01_0279" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
    <wait time="0.5" />
    <move obj_id="半蔵門 雪" x="-1000" x_move_type="MoveTo" time="0.1" />
    <move obj_id="青葉 初芽" x="0" x_move_type="MoveTo" time="0.1" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <transition effect="Default" type="In" color_change="False" time="0.5" wait="True" />
    <msg speaker="半蔵門 雪" text="はっ、はっ、はっ、はっ……。" speaker_show="True" stop="True" voice="hy11002_01_0459" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふむふむ。腕立て、腹筋、スクワット。&#xA;雪ちゃんの平常時のペースと同等に動けてますね。" speaker_show="True" stop="True" voice="ah11005_01_0280" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="続いて素振り。&#xA;はぁーっ！　やー！　てぇーい！" speaker_show="True" stop="True" voice="hy11002_01_0460" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いいですね、見た目には全く支障が見えません。&#xA;雪ちゃん、この辺で少し休憩しましょうか。" speaker_show="True" stop="True" voice="ah11005_01_0281" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <move obj_id="半蔵門 雪" x="-200" x_move_type="MoveTo" time="0.5" wait="False" />
    <move obj_id="青葉 初芽" x="200" x_move_type="MoveTo" time="0.5" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽、こんな感じで大丈夫だったかしら？" speaker_show="True" stop="True" voice="hy11002_01_0461" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はい、その姿で訓練している雪ちゃんも&#xA;素敵です！" speaker_show="True" stop="True" voice="ah11005_01_0282" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽、からかわないでちょうだい。&#xA;こっちは真剣にテストをしているのに。" speaker_show="True" stop="True" voice="hy11002_01_0462" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふ、ごめんなさい。あまりにも&#xA;セクシーだったのでつい。" speaker_show="True" stop="True" voice="ah11005_01_0283" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="スーツの着心地や動きやすさの率直な感想を&#xA;聞かせてもらっても良いですか？" speaker_show="True" stop="True" voice="ah11005_01_0284" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうね、太もも周りが少し動かしづらかったので&#xA;蹴りやジャンプの時に支障があるかもしれないわ。" speaker_show="True" stop="True" voice="hy11002_01_0463" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="14_Positive" force_end="False" loop="False" to_idle="True" />
    <msg speaker="青葉 初芽" text="ふむふむ、太もも周りの動きですね。&#xA;メモメモ。あとはどうですか？" speaker_show="True" stop="True" voice="ah11005_01_0285" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="もう少し細かく動きを確認してからでも良い？&#xA;せっかくの機会を無駄にはできないし。" speaker_show="True" stop="True" voice="hy11002_01_0464" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="02_Idle" force_end="False" loop="True" />
    <move obj_id="半蔵門 雪" x="-1000" x_move_type="MoveTo" time="1.0" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="頼もしい限りです。では、雪ちゃんの気になる&#xA;箇所を検証していてください。" speaker_show="True" stop="True" voice="ah11005_01_0286" />
    <msg_window_show show="False" />
    <move obj_id="青葉 初芽" x="1000" x_move_type="MoveTo" time="1.0" wait="True" />
    <wait time="0.1" />
    <delete obj_id="青葉 初芽" />
    <wait time="0.5" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005008" x="200" y="-1000" />
    <move obj_id="半蔵門 雪" x="-200" x_move_type="MoveTo" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽？　どこに行ったのかしら？" speaker_show="True" stop="True" voice="hy11002_01_0465" />
    <move obj_id="青葉 初芽" y="1000" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <wait time="0.5" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <create_effect effect_id="yuki_Exclamation" effect_type="MampExclamation" life_time="1.0" target_obj="半蔵門 雪" />
    <msg speaker="半蔵門 雪" text="初芽！　その格好は。" speaker_show="True" stop="True" voice="hy11002_01_0466" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="私もテストに混ぜてもらおうと思って&#xA;着替えてきました。どうですか？" speaker_show="True" stop="True" voice="ah11005_01_0287" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="どうって……。&#xA;ていうか、予備のスーツなんてあったの？" speaker_show="True" stop="True" voice="hy11002_01_0467" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="14_Positive" force_end="False" loop="False" to_idle="True" />
    <msg speaker="青葉 初芽" text="はい、雪ちゃんを待っている間に&#xA;ささっともう１着、作ってみました。" speaker_show="True" stop="True" voice="ah11005_01_0288" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="すぐ作れるなら、サイズに関しては&#xA;特に問題にならなかったのでは？" speaker_show="True" stop="True" voice="hy11002_01_0468" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="まぁまぁ、似合ってますよ雪ちゃん。&#xA;この服は、ごえちゃんより雪ちゃんですね、ふふ。" speaker_show="True" stop="True" voice="ah11005_01_0289" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="全く。そう言えば、テストが終わったら、&#xA;ちゃんと五恵にも一連の説明はするのよ。" speaker_show="True" stop="True" voice="hy11002_01_0469" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はい、もちろんです！&#xA;ではテストを再開しましょう。" speaker_show="True" stop="True" voice="ah11005_01_0290" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>