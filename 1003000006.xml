    <msg speaker="テレジア" text="……ふむ。なるほど。" speaker_show="True" stop="True" voice="tr11007_01_0022" lip_sync="True" window_type="CharMsg" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" loop="True" force_end="False" to_idle="False" />
