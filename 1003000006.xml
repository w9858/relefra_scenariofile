    <char_spine_animation obj_id="テレジア" body_anim_type="02_Idle" face_anim_type="008_Facial_serious" loop="True" force_end="False" to_idle="False" />
    <msg speaker="テレジア" text="（とったご飯粒をそのまま己で食べたか。&#xA;相模楓、やはり無駄を好まないな）" speaker_show="True" stop="True" voice="tr11007_01_0020" lip_sync="False" window_type="CharMsg" />
    <delete obj_id="テレジア" />
