    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" force_end="False" to_idle="False" />
    <msg speaker="源 モモ" text="そういえばフーちゃんのこれが大好き！&#xA;っていう食べ物って何かあるの？" speaker_show="True" stop="True" voice="mm11001_01_0068" lip_sync="True" window_type="CharMsg" />
    <delete obj_id="テレジア" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" force_end="False" to_idle="False" />
    <msg speaker="相模 楓" text="そうねぇ。完璧なアタシは基本何でもいけるけども。&#xA;やはり卵や豆腐のようなコスパいい食材が好きよ。" speaker_show="True" stop="True" voice="sf11004_01_0096" window_type="CharMsg" lip_sync="True" />
    <delete obj_id="源 モモ" />
    <create obj_id="八千代 命" data_id="311003006" x="200" type="Character" position_id="left" y="0" anchor="Bottom" />
