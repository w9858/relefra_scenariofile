    <msg speaker="青葉 初芽" text="よく見ていますねモモちゃん。&#xA;じゃあ逆に雪ちゃんが苦手なもの分かります？" speaker_show="True" stop="True" voice="ah11005_01_0034" lip_sync="True" window_type="CharMsg" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" force_end="False" to_idle="False" />
    <msg speaker="源 モモ" lip_sync="True" window_type="CharMsg" text="えっ！？　師匠そういうのあるんですか。" voice="mm11001_01_0070" stop="True" speaker_show="True" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" loop="True" force_end="False" to_idle="False" />
    <msg speaker="青葉 初芽" text="顔には出さないようにしているんですけど&#xA;あるんですよ。" speaker_show="True" stop="True" voice="ah11005_01_0035" lip_sync="True" window_type="CharMsg" />
