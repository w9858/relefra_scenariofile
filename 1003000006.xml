    <move obj_id="八千代 命" x="0" x_move_type="MoveTo" time="0.5" y="0" position_id="left" y_move_type="MoveValue" wait="False" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" force_end="False" to_idle="False" />
    <msg speaker="テレジア" text="（そう言われた高坂信は幸せそうにフローズンホット&#xA;チョコレートを楽しんでいる。あれは美味いからな）" speaker_show="True" stop="True" voice="tr11007_01_0021" window_type="CharMsg" lip_sync="False" />
    <move obj_id="テレジア" x="1000" x_move_type="MoveTo" time="0.5" y="0" position_id="left" y_move_type="MoveValue" wait="False" />
