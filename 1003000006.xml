    <char_spine_animation obj_id="石川 五恵" body_anim_type="10_FaceBothHands" loop="True" force_end="False" to_idle="False" />
    <msg speaker="青葉 初芽" text="ぬいぐるみとかでもそうですが、ごえちゃんは&#xA;可愛いものを愛でる時、前後不覚になりがちです。" speaker_show="True" stop="True" voice="ah11005_01_0037" lip_sync="True" window_type="CharMsg" />
