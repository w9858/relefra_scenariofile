    <msg speaker="高坂 信" text="ふふふ、スパイは嘘つきって言うよモモちゃん。" speaker_show="True" stop="True" voice="ks11010_01_0061" lip_sync="True" window_type="CharMsg" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" force_end="False" to_idle="False" />
    <msg speaker="源 モモ" text="うぅ、どっちなんだろう。こうなったら私も&#xA;テレちゃんのように師匠を観察します。じーっ。" speaker_show="True" stop="True" voice="mm11001_01_0072" lip_sync="True" window_type="CharMsg" />
