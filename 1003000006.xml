    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" force_end="False" to_idle="False" />
    <msg speaker="青葉 初芽" text="テレちゃんは昔から真面目ですねぇ。" speaker_show="True" stop="True" voice="ah11005_01_0033" lip_sync="True" window_type="CharMsg" />
    <char_spine_animation obj_id="テレジア" body_anim_type="13_Question" loop="True" force_end="False" to_idle="False" />
    <msg speaker="テレジア" text="雪は意外と甘い物が好きなんだな。" speaker_show="True" stop="True" voice="tr11007_01_0024" lip_sync="True" window_type="CharMsg" />
