    <move obj_id="相模 楓" x="350" x_move_type="MoveValue" time="0.4" position_id="left" y="0" y_move_type="MoveValue" wait="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" loop="True" force_end="False" to_idle="False" />
    <msg speaker="相模 楓" text="モモちは、お子様が好きなものが好みよねぇー。&#xA;ほら、ほっぺにご飯粒ついているし。" speaker_show="True" stop="True" voice="sf11004_01_0095" lip_sync="True" window_type="CharMsg" />
