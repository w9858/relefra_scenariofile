    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="メイちゃん！　それじゃあ&#xA;お姉さんのいるお店の客引きだよ！" speaker_show="True" stop="True" voice="mm11001_01_0987" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="メイ嘘はついてないもーん。可愛い女子が勢ぞろい！&#xA;ついでにカレーもすっごく美味しい！" speaker_show="True" stop="True" voice="ym11003_01_0816" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="カレーは、ついでなんだ……。" speaker_show="True" stop="True" voice="mm11001_01_0988" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="となったら、入るっきゃないでしょ！&#xA;はいはい、そこの人、ご案内～！" speaker_show="True" stop="True" voice="ym11003_01_0817" />
