    <char_spine_animation obj_id="源 モモ" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="はぐはぐっ……。美味しい！&#xA;何杯でもお代わりできそう！　もぐもぐ……。" speaker_show="True" stop="True" voice="mm11001_01_0990" />
