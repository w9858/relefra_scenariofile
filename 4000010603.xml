    <msg speaker="石川 五恵" text="師匠、凄いです！　いつの間にか満席です。" speaker_show="True" stop="True" voice="ig11006_01_0595" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="モモちゃんとメイちゃんが、&#xA;お外で頑張ってくれているお陰ですね。" speaker_show="True" stop="True" voice="ah11005_01_0657" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
