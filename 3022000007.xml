    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" resource_type="Background" file="62_AutumnMountainev_01" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="山中" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <sound type="Se" cue_name="Run" />
    <wait time="0.5" />
    <create type="Character" obj_id="相模 楓" data_id="311004018" position_id="left" x="-300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003017" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふ～、なんとかイノシシは&#xA;撒けたみたいだねぇ。" speaker_show="True" stop="True" voice="ym11003_03_0129" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ですね……はぁ……はぁ……。" speaker_show="True" stop="True" voice="sf11004_03_0143" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フー、走り疲れちゃってんじゃん。&#xA;まだまだ修行不足だなぁ。" speaker_show="True" stop="True" voice="ym11003_03_0130" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="(引率に集中し過ぎて、精神的にも体力的にも&#xA;余裕がなくなってるのかも……。)" speaker_show="True" stop="True" voice="sf11004_03_0144" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そうだ、こんな事してる場合じゃないです！&#xA;遅れを取り戻さないと！" speaker_show="True" stop="True" voice="sf11004_03_0145" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="皆は？&#xA;早く合流して、順路に戻りましょう！" speaker_show="True" stop="True" voice="sf11004_03_0146" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="んー、この辺りには&#xA;メイとフーしかいないっぽいよ。" speaker_show="True" stop="True" voice="ym11003_03_0131" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="イノシシから逃げる時に、&#xA;散り散りになっちゃったんだね。" speaker_show="True" stop="True" voice="ym11003_03_0132" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <create_effect effect_id="fuu01" effect_type="MampWorry" target_obj="相模 楓" />
    <msg speaker="相模 楓" text="うぅ……野生動物の急襲に対する打ち合わせも、&#xA;事前にしておくべきでした……。" speaker_show="True" stop="True" voice="sf11004_03_0147" />
    <delete obj_id="fuu01" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="いや、普通そこまで予想出来ないから。" speaker_show="True" stop="True" voice="ym11003_03_0133" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="皆を探しましょう。&#xA;そもそも、アタシ達のいる場所はどこなんですか？" speaker_show="True" stop="True" voice="sf11004_03_0148" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="どこだろうねぇ。&#xA;山の中腹である事は、間違いないと思うけど。" speaker_show="True" stop="True" voice="ym11003_03_0134" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そ、そんな……。" speaker_show="True" stop="True" voice="sf11004_03_0149" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="現在地すら分からなくて、&#xA;メンバーもバラバラ……。" speaker_show="True" stop="True" voice="sf11004_03_0150" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="(せっかく、師匠に楽しんでもらおうと思ってたのに、&#xA;これじゃ……。)" speaker_show="True" stop="True" voice="sf11004_03_0151" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ふふっ。" speaker_show="True" stop="True" voice="ym11003_03_0135" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ねえフー、&#xA;なんだか面白くなってきたね！" speaker_show="True" stop="True" voice="ym11003_03_0136" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え……？" speaker_show="True" stop="True" voice="sf11004_03_0152" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だってそうじゃん！　イノシシに襲われたり、&#xA;遭難しかけたり、こんなハプニングなかなかないよ？" speaker_show="True" stop="True" voice="ym11003_03_0137" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="後で振り返った時に『あったねそんな事ー！』って&#xA;笑い合える、いい思い出になるよ。" speaker_show="True" stop="True" voice="ym11003_03_0138" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="師匠……。" speaker_show="True" stop="True" voice="sf11004_03_0153" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="そうだ。折角だし、&#xA;２人で紅葉狩りデートしよっか。" speaker_show="True" stop="True" voice="ym11003_03_0139" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="こんなに綺麗な山なんだもん、&#xA;フーと一緒に歩きたいな。" speaker_show="True" stop="True" voice="ym11003_03_0140" />
    <move obj_id="八千代 命" position_id="left" x="0" x_move_type="MoveTo" time="2" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <msg speaker="相模 楓" text="ふわっ……&#xA;あ、あの、師匠!?" speaker_show="True" stop="True" voice="sf11004_03_0154" />
    <msg speaker="相模 楓" text="(て、手、握られちゃってる……!?)" speaker_show="True" stop="True" voice="sf11004_03_0155" lip_sync="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="さ、行こう！" speaker_show="True" stop="True" voice="ym11003_03_0141" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
