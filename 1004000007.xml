  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="工場 コンピュータールーム" />
    <bg show="True" file="22_plant_03" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="しぶきちは言葉が強いからアレだけど、&#xA;アタシ達も素性は全部明かしたんだ。" speaker_show="True" stop="True" voice="th12004_01_0010" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="ツキカゲと凪の部隊は運命共同体って考えでいいと&#xA;思うよ。ただパジャマパーティはしないってだけで。" speaker_show="True" stop="True" voice="th12004_01_0011" />
    <delete obj_id="轟 葉栖美" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="パジャマパーティも是非したいんですけどねぇ。" speaker_show="True" stop="True" voice="ah11005_01_0043" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="だから、それはせんのじゃってゆーとろうが！&#xA;あきらめい。" speaker_show="True" stop="True" voice="ss12003_01_0032" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="テレジア" data_id="311008001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="テレジア" text="初芽は、そういうのを諦めない。決して、な。" speaker_show="True" stop="True" voice="tr11007_01_0031" />
    <delete obj_id="テレジア" />
    <delete obj_id="薩摩 しぶき" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="互いに連絡をとれる状況にはしておこうぜ。&#xA;そのうえで、色々と情報を共有だな。" speaker_show="True" stop="True" voice="ty12005_01_0004" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="ええ、よろしく。特に大八洲師団については、&#xA;凪の部隊、因縁の敵だろうし。" speaker_show="True" stop="True" voice="kt11009_01_0042" />
    <delete obj_id="津守 ヨモギ" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="それでは我らは撤収するぞ。" speaker_show="True" stop="True" voice="ss12003_01_0033" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="も、もうですか？　折角ですからお茶でも…。" speaker_show="True" stop="True" voice="ig11006_01_0050" />
    <move obj_id="薩摩 しぶき" position_id="left" x="1000" x_move_type="MoveTo" time="1.0" wait="True" y="0" y_move_type="MoveValue" />
    <delete obj_id="薩摩 しぶき" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あう、残念……。" speaker_show="True" stop="True" voice="ig11006_01_0051" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="本当めげないわねぇ。あれだけ言われたのに。" speaker_show="True" stop="True" voice="sf11004_01_0121" />
    <delete obj_id="相模 楓" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="とにかく、被害をなくし敵を撃滅する事が一番よ。&#xA;相手もそれは分かってるようで安心したわ。" speaker_show="True" stop="True" voice="hy11002_01_0061" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="高坂 信" data_id="311010001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="カトさん、繋ぎお疲れ様。" speaker_show="True" stop="True" voice="ks11010_01_0065" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" loop="True" to_idle="False" force_end="False" />
    <char_spine_eye obj_id="カトリーナ" type="Eye03_tear" loop="True" />
    <msg speaker="カトリーナ" text="そのシン様のお言葉を聴いただけで&#xA;疲労が宇宙まで飛んでいきましたよ！" speaker_show="True" stop="True" voice="kt11009_01_0043" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="源 モモ" data_id="311001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006001" position_id="left" x="-300" y="0" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="とにかくまず被害を止めるのが&#xA;先で間違いは無いけど……。" speaker_show="True" stop="True" voice="mm11001_01_0101" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="ええ。あの好き放題言ってくれた小さいのは、&#xA;どこかでキチっとシめとかないとね。" speaker_show="True" stop="True" voice="sf11004_01_0122" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="いつか仲良くなれるといいね、ごえちゃん。" speaker_show="True" stop="True" voice="mm11001_01_0102" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="うんっ。" speaker_show="True" stop="True" voice="ig11006_01_0052" />
    <delete obj_id="源 モモ" />
    <delete obj_id="相模 楓" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="テレジア" data_id="311008001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="テレジア" text="私も力を貸そう。自分がしてもらった事だからな。" speaker_show="True" stop="True" voice="tr11007_01_0032" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="テレちゃん、ありがとうございます。" speaker_show="True" stop="True" voice="ah11005_01_0044" />
    <delete obj_id="テレジア" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="白虎" data_id="311007001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="あれ！？　ちょっとそんなに爽やかに&#xA;されるとアタシの心が荒んでるみたいになるでしょ！" speaker_show="True" stop="True" voice="sf11004_01_0123" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="白虎" text="いや分かるぞ楓。ああいうのはシめておかねば。&#xA;お前と私は同じような思考をしているな！" speaker_show="True" stop="True" voice="bk11008_01_0027" />
    <delete obj_id="白虎" />
    <move obj_id="相模 楓" position_id="left" x="0" x_move_type="MoveTo" time="0.5" wait="True" y="0" y_move_type="MoveValue" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（それはそれでなんかちょっとフクザツ……）" speaker_show="True" stop="True" voice="sf11004_01_0124" lip_sync="False" />
