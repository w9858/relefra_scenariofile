<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/9010309001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="02_wasabimo_001" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="メイちゃん。モウリョウとのダブルスパイ役、&#xA;本当にお疲れ様。" speaker_show="True" stop="True" voice="kt11009_02_0328" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="今日は、私からの慰労会だと思って&#xA;いっぱい食べていってね。" speaker_show="True" stop="True" voice="kt11009_02_0329" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="はい。&#xA;特製カレー、召し上がれ。" speaker_show="True" stop="True" voice="kt11009_02_0330" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="わーい！&#xA;いただきまーす！" speaker_show="True" stop="True" voice="ym11003_02_0930" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="もぐもぐ……ん〜、うま！&#xA;これが２年がかりの任務完了の味か〜！" speaker_show="True" stop="True" voice="ym11003_02_0931" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="⻑い間、本当に頑張ってくれたわね。" speaker_show="True" stop="True" voice="kt11009_02_0331" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ほんと、ここまで長かった～。&#xA;カトーさんも、おつかれさま～。" speaker_show="True" stop="True" voice="ym11003_02_0932" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="カトリーナ" text="ありがとう。" speaker_show="True" stop="True" voice="kt11009_02_0332" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="でも、一番危険な役を買って出てくれたのは&#xA;メイちゃんだもの。心から感謝しているわ。" speaker_show="True" stop="True" voice="kt11009_02_0333" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="だからね、カレー以外にも、&#xA;メイちゃんを労うことがしたいと思って。" speaker_show="True" stop="True" voice="kt11009_02_0334" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="何か、やりたいことや行きたい所はあるかしら？" speaker_show="True" stop="True" voice="kt11009_02_0335" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="え、本当!?&#xA;なんでもいいの!?" speaker_show="True" stop="True" voice="ym11003_02_0933" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="カトリーナ" text="ええ、もちろん。" speaker_show="True" stop="True" voice="kt11009_02_0336" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ん〜、そーだなー……パーッと温泉旅行とか～&#xA;いっそ世界一周旅行ってのもいいな！" speaker_show="True" stop="True" voice="ym11003_02_0934" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="うーん……迷うな～！&#xA;何がいいかな～！" speaker_show="True" stop="True" voice="ym11003_02_0935" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="ゆっくり考えて。" speaker_show="True" stop="True" voice="kt11009_02_0337" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……うーん……" speaker_show="True" stop="True" voice="ym11003_02_0936" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="……いや、やっぱいいや！&#xA;メイは、何もしなくていい！" speaker_show="True" stop="True" voice="ym11003_02_0937" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="本当？&#xA;なんでも言ってくれていいのよ？" speaker_show="True" stop="True" voice="kt11009_02_0338" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="うん！&#xA;メイは今、この平穏な毎日を満喫したい！" speaker_show="True" stop="True" voice="ym11003_02_0938" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="メイちゃん……。" speaker_show="True" stop="True" voice="kt11009_02_0339" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="……そうね。" speaker_show="True" stop="True" voice="kt11009_02_0340" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="失いたくないものがあるのは、いい人⽣の証だ……" speaker_show="True" stop="True" voice="ym11003_02_0939" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーと一緒にご飯食べる時間とか、モモちたちとの&#xA;学校⽣活とか、この最⾼に美味しいカレーとかね！" speaker_show="True" stop="True" voice="ym11003_02_0940" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="カトーさん、おかわりー！" speaker_show="True" stop="True" voice="ym11003_02_0941" />
  </command>
</adv>