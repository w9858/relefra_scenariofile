    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="300" anchor="Bottom" y="0" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="0" anchor="Bottom" y="0" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" position_id="left" x="-300" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="凄いよ、メイちゃん！&#xA;言われてみると、確かにそうかも！" speaker_show="True" stop="True" voice="ig11006_01_0500" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="でしょでしょ？　「ぐっすり眠る」の「ぐっすり」は&#xA;グッド・スリープの略！" speaker_show="True" stop="True" voice="ym11003_01_0585" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="メイもこの仮説に気付いた時、&#xA;自分が天才じゃないかって震えたよ。" speaker_show="True" stop="True" voice="ym11003_01_0586" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="モモちは、どう思う？" speaker_show="True" stop="True" voice="ym11003_01_0587" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="あっ、うんうん、そうだね。そうだと思う……。&#xA;ソワソワ……ソワソワ……。" speaker_show="True" stop="True" voice="mm11001_01_0856" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="おーい、モモち？　本当にそう思ってる？&#xA;てかメイの話、聞いてた？" speaker_show="True" stop="True" voice="ym11003_01_0588" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="はっ、ごめんごめん！&#xA;えっと、注目の外国人力士の話だよね？" speaker_show="True" stop="True" voice="mm11001_01_0857" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="全っ然聞いてないし……。" speaker_show="True" stop="True" voice="ym11003_01_0589" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="モモち、どうしたの？　さっきからずっと、&#xA;スマホ握りしめてソワソワしてるけど。" speaker_show="True" stop="True" voice="ig11006_01_0501" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="わっ、スマホが汗でびしょ濡れ！&#xA;モモち、何でそんなに緊張してるのよ？" speaker_show="True" stop="True" voice="ym11003_01_0590" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="ごめんごめん、いつ師匠から呼ばれるか&#xA;わからないから。" speaker_show="True" stop="True" voice="mm11001_01_0858" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="また新しい修行？&#xA;ユッキーってば本当、スパルタ指導だねぇ。" speaker_show="True" stop="True" voice="ym11003_01_0591" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="違うよー、肩たたき。" speaker_show="True" stop="True" voice="mm11001_01_0859" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="この間、雪先輩に渡してた券のこと？" speaker_show="True" stop="True" voice="ig11006_01_0502" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="うん！　師匠から声がかかったら、&#xA;すぐに反応できるようにスタンバイしてるんだ。" speaker_show="True" stop="True" voice="mm11001_01_0860" />
    <msg_window_show show="False" />
