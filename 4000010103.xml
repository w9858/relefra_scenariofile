    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="はっ、師匠！　肩たたきですねっ!?" speaker_show="True" stop="True" voice="mm11001_01_0861" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="作戦会議よ。みんな、すぐに集まって。" speaker_show="True" stop="True" voice="hy11002_01_0581" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="（うぅー残念！）" speaker_show="True" stop="True" voice="mm11001_01_0862" lip_sync="False" />
