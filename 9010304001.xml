    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="ほらチッチ、ご飯入れ替えるわよ～。" speaker_show="True" stop="True" voice="sf11004_02_0649" />
    <msg_window_show show="False" />
