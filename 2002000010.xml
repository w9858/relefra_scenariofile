<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/2002000009" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="True" />
    <bg file="36_school_02" color="#ffffffff" />
    <fade_in time="1.0" type="Default" wait="True" />
    <telop text="黒百合学園　屋上" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003004" x="300" y="0" position_id="left" anchor="Bottom" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004004" x="-300" y="0" position_id="left" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="もぐもぐ。はぁぁ……葉栖美さんのお弁当&#xA;どれも美味しいです！　この卵焼きとか！" speaker_show="True" stop="True" voice="mh12001_01_0108" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="じゃろ？　これを食べられるのは凪の特権といえる。&#xA;はっすーは家事スキル振り切ってるからのー。" speaker_show="True" stop="True" voice="ss12003_01_0119" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="その卵焼きはカニと三つ葉を入れてる奴だね。&#xA;まだまだあるから落ち着いて食べなよ。" speaker_show="True" stop="True" voice="th12004_01_0069" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="隊長も家事スキル完璧さんなんですか？" speaker_show="True" stop="True" voice="mh12001_01_0109" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="掃除は自信があるぞ。しぶきは汚れが嫌いだからな。&#xA;散らかってるの見ると、掃除しないと気がすまぬ。" speaker_show="True" stop="True" voice="ss12003_01_0120" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="アタシが部屋、少しでも賑やかにすると&#xA;もー張り切って整理整頓するからね、しぶきちは。" speaker_show="True" stop="True" voice="th12004_01_0070" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="逆さ塔とて、もっとピカピカにしたいがのう。&#xA;それはまぁ別物と割り切っている。" speaker_show="True" stop="True" voice="ss12003_01_0121" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="そして料理の腕は完敗じゃ。はっすーが強すぎるわ。&#xA;しぶきの腕を一両と比喩すれば、一貫はあるぞ。" speaker_show="True" stop="True" voice="ss12003_01_0122" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="まぁしぶきちは隊長さんとして忙しいから。&#xA;アタシじゃとても隊長は務まらないからね。" speaker_show="True" stop="True" voice="th12004_01_0071" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="隊長はどうして凪の部隊に入られたのか&#xA;聞いてもいいですか？　ドキドキ。" speaker_show="True" stop="True" voice="mh12001_01_0110" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="しぶきは生まれつき力が強かった。じゃが&#xA;親は、普通の娘に育てたかったようでな。" speaker_show="True" stop="True" voice="ss12003_01_0123" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="普通に過ごしているだけでは力を持て余しての。&#xA;そうだ悪でも狩ろう、と夜な夜な活動しとった。" speaker_show="True" stop="True" voice="ss12003_01_0124" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="16_BendBackward" force_end="False" loop="True" face_anim_type="011_Facial_surprise" />
    <msg speaker="轟 葉栖美" text="その発想が、まずぶっ飛んでるんだって。" speaker_show="True" stop="True" voice="th12004_01_0072" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="パワーがあるせいか、しぶきは良く御飯を食べる。&#xA;ここまで食うなら働かねばと、思うんじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0125" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="で、ヨモギさんにスカウトされたというわけじゃな。&#xA;あの時は嬉しかったぞ。力を正しく使えるのだから。" speaker_show="True" stop="True" voice="ss12003_01_0126" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="もともと正義に燃える人だったんですね。素敵です。&#xA;葉栖美さんの事情も、お聞きして良いですか？" speaker_show="True" stop="True" voice="mh12001_01_0111" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="アタシの所にもスカウトが来たんだよ。やーびっくり&#xA;したね。いきなり国家の人間がきたんだから。" speaker_show="True" stop="True" voice="th12004_01_0073" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="新潟に動物と意思疎通ができる少女がいるという&#xA;情報が入っての、調査してからのスカウトじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0127" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="捨てられた動物とか拾ってるうちにね、だいたい&#xA;相手が何を考えてるか分かるようになってきてさ。" speaker_show="True" stop="True" voice="th12004_01_0074" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="そしたら、こっちの言う事まで聞いてくれる&#xA;ようになったんだ。いやーびっくりしたね。" speaker_show="True" stop="True" voice="th12004_01_0075" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="凄いですね、動物と心通わせるなんて。" speaker_show="True" stop="True" voice="mh12001_01_0112" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="そうじゃ。はっすーの特技は優しく温かい。" speaker_show="True" stop="True" voice="ss12003_01_0128" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="009_Facial_blush" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_cheek obj_id="轟 葉栖美" type="Cheek02_medium" loop="True" />
    <msg speaker="轟 葉栖美" text="な、なんだよー照れるって！" speaker_show="True" stop="True" voice="th12004_01_0076" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="それをスパイ活動に使わせるのは心が痛む。じゃが、&#xA;国が荒波に飲まれないためには必要な力じゃ。" speaker_show="True" stop="True" voice="ss12003_01_0129" />
    <char_spine_cheek obj_id="轟 葉栖美" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="アタシも協力してくれている動物も、&#xA;納得したうえでやってるからいーんだって。" speaker_show="True" stop="True" voice="th12004_01_0077" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="例えば昆虫や機械人形と意思疎通できるんですか？" speaker_show="True" stop="True" voice="mh12001_01_0113" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="いい質問だね、昆虫は種類による。人形は無理。&#xA;基本的に霊長類に近いほどイケルよ。" speaker_show="True" stop="True" voice="th12004_01_0078" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="他に質問あるか？　せっかくのジャストでワイルドな&#xA;コミュニケーションじゃ。答えるぞ！　ハッハッハ！" speaker_show="True" stop="True" voice="ss12003_01_0130" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="次の任務は私設情報機関との連携と聞きましたけど。&#xA;意外です。任務は国だけで動くと思っていました。" speaker_show="True" stop="True" voice="mh12001_01_0114" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="2000年冒頭に起きた未曾有の同時多発テロ以来、&#xA;警察も使えるものは何でも使おうってスタンスでね。" speaker_show="True" stop="True" voice="th12004_01_0079" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="私設情報機関とは連携を続けているわけ。&#xA;仲が良くてズブズブってわけでもないのよ。" speaker_show="True" stop="True" voice="th12004_01_0080" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="……私設情報機関の在り方について、しぶきは&#xA;否定的じゃ。腕は認めている。秩序の問題じゃ。" speaker_show="True" stop="True" voice="ss12003_01_0131" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="正義の為に動くなら国に所属して仕事すれば&#xA;良いものを、あいつらときたら勝手に動き回る。" speaker_show="True" stop="True" voice="ss12003_01_0132" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="混乱が増すと思わんか。警察と同等の力があっては&#xA;ならん。奴等いつか国家に組み込んでやるわ。" speaker_show="True" stop="True" voice="ss12003_01_0133" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="（隊長が腕を認めている私設情報機関…どういう人達が&#xA;いるんだろう。いつか会ってみたいな）" speaker_show="True" stop="True" voice="mh12001_01_0115" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="薩摩 しぶき" />
    <delete obj_id="轟 葉栖美" />
  </command>
</adv>