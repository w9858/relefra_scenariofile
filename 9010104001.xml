  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="ツキカゲ基地 ラボ室 03_tsukikage_007" />
    <bg show="True" file="03_tsukikage_007" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004004" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="つまり、その人の思考を模倣してこそ、&#xA;きちんとした変装になるのよ。" speaker_show="True" stop="True" voice="sf11004_02_0592" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="なるほど～。フーちゃんの変装は&#xA;見た目だけじゃないってことなんだね。" speaker_show="True" stop="True" voice="mm11001_02_0796" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="でも、私の場合、そもそも見た目も&#xA;うまくいかないんだよなあ。" speaker_show="True" stop="True" voice="mm11001_02_0797" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そこは、ひたすら練習しかないわね。&#xA;まずは、メイクから練習したらどう？" speaker_show="True" stop="True" voice="sf11004_02_0593" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="わぁ、フーちゃん、練習に付き合ってくれるの？&#xA;嬉しいな、ありがとう！" speaker_show="True" stop="True" voice="mm11001_02_0798" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="う……もう、しょうがないなあ。&#xA;それならさっさと始めてみて。" speaker_show="True" stop="True" voice="sf11004_02_0594" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="わかった。それじゃ、えーっと……こう？&#xA;それともこうなのかな？" speaker_show="True" stop="True" voice="mm11001_02_0799" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="いや、そうじゃなくて……。この場合は、&#xA;もっと目元のラインをハッキリ書かないと。" speaker_show="True" stop="True" voice="sf11004_02_0595" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、そうか。えーっと……。&#xA;じゃ、目元をもっとハッキリさせて……。" speaker_show="True" stop="True" voice="mm11001_02_0800" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="違うわよ。そうじゃなくて……あー、もう！&#xA;アタシがやってあげるから目をつぶってて。" speaker_show="True" stop="True" voice="sf11004_02_0596" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unigue" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="わー、ごめんごめん！&#xA;それじゃ、お願いします。" speaker_show="True" stop="True" voice="mm11001_02_0801" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" lip_sync="False" text="（まったくもう……だいたいなんでアタシが……。&#xA;ん？　そっか、どうせなら……くくく……。）" speaker_show="True" stop="True" voice="sf11004_02_0597" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <fade_in time="1.0" type="Default" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="018_Facial_smug" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="はい、これで完成。&#xA;もう目を開けていいわよ。" speaker_show="True" stop="True" voice="sf11004_02_0598" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ホント？　ありがとう！&#xA;わー、どんなふうになったのかなあ……。" speaker_show="True" stop="True" voice="mm11001_02_0802" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="こらこら、鏡を見ない！" speaker_show="True" stop="True" voice="sf11004_02_0599" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="えっ、どうなったのか見ちゃダメなの!?" speaker_show="True" stop="True" voice="mm11001_02_0803" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そりゃそうよ。だって鏡とかで自分の姿を見たら、&#xA;見た目に影響されちゃうからね。" speaker_show="True" stop="True" voice="sf11004_02_0600" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そっか。そういうものなんだね。&#xA;わかった。じゃあ、これで外に行ってみる！" speaker_show="True" stop="True" voice="mm11001_02_0804" />
    <msg_window_show show="False" />
    <move obj_id="源 モモ" position_id="left" x="-1500" x_move_type="MoveTo" time="1.6" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あ、ちょっ……モモち！&#xA;……行っちゃった。ま、いっか。" speaker_show="True" stop="True" voice="sf11004_02_0601" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
    <delete obj_id="源 モモ" />
    <delete obj_id="相模 楓" />
    <transition effect="Default" type="In" color_change="False" time="0.5" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004004" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="も～！　ちょっと、フーちゃん！&#xA;これ、なんなの!?" speaker_show="True" stop="True" voice="mm11001_02_0805" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あ、戻ってきたのね。おかえり。" speaker_show="True" stop="True" voice="sf11004_02_0602" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="おかえり、じゃないよ。&#xA;だいたいなんで猫のメイクなの？" speaker_show="True" stop="True" voice="mm11001_02_0806" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="いろんな人にジロジロ見られて&#xA;すっごい恥ずかしかったよ～。" speaker_show="True" stop="True" voice="mm11001_02_0807" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え、それで帰ってきちゃったの？&#xA;言ったわよね、変装の極意はなりきることだって。" speaker_show="True" stop="True" voice="sf11004_02_0603" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="だから、今こそモモちは&#xA;猫になりきるべきなのよ!!" speaker_show="True" stop="True" voice="sf11004_02_0604" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ハッ、そうか！　わかった！&#xA;それじゃ、えーっと……にゃー！" speaker_show="True" stop="True" voice="mm11001_02_0808" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そんなんじゃ、ぜんぜんダメ。&#xA;もっと、なりきって。" speaker_show="True" stop="True" voice="sf11004_02_0605" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="猫になりきるって、んーっと……。&#xA;にゃー！　……こうかにゃ？" speaker_show="True" stop="True" voice="mm11001_02_0809" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="まだまだ！　もっともっとよ！" speaker_show="True" stop="True" voice="sf11004_02_0606" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="013_Facial_angry" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="フー!!　怒ったにゃー!!" speaker_show="True" stop="True" voice="mm11001_02_0810" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="そうそう、その調子！&#xA;モモちが猫に見えてきたかも！" speaker_show="True" stop="True" voice="sf11004_02_0607" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ホント!?&#xA;やった！　頑張るにゃー!!" speaker_show="True" stop="True" voice="mm11001_02_0811" />
  </command>
