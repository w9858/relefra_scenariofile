    <char_spine_animation obj_id="青葉 初芽" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（はぁ、ボンテージスーツの試作品、&#xA;やっぱり作り直すべきでしょうか）" speaker_show="True" stop="True" voice="ah11005_01_0257" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（そうだ、いつものようにごえちゃんに相談して、&#xA;２人で考えてみれば……）" speaker_show="True" stop="True" voice="ah11005_01_0258" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（けど最近、私の発明は失敗続き。ごえちゃんにも&#xA;きっと相当な負担がかかっているんですよね）" speaker_show="True" stop="True" voice="ah11005_01_0259" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="（今回だって、テスト以前にスーツのサイズまで&#xA;間違ってしまうだなんて、ありえない失敗です）" speaker_show="True" stop="True" voice="ah11005_01_0260" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（というかそもそも、私が着ても胸周りが苦しい服を、&#xA;ごえちゃんに着せてテストだなんて！）" speaker_show="True" stop="True" voice="ah11005_01_0261" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（はぁ～、そんなことにさえ気がつけないような&#xA;体たらくで、ごえちゃんを巻き込む事は出来ません）" speaker_show="True" stop="True" voice="ah11005_01_0262" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（一体どうすれば……）" speaker_show="True" stop="True" voice="ah11005_01_0263" lip_sync="False" />
    <msg_window_show show="False" />
    <Comment comment="【SE】近づいてくる足音" />
    <sound type="Se" cue_name="Walk" />
    <wait time="2.0" />
    <move obj_id="青葉 初芽" position_id="left" x="-200" x_move_type="MoveTo" time="0.5" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="あら初芽。あなたも今、帰り？" speaker_show="True" stop="True" voice="hy11002_01_0450" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん。はい、そうなんです。" speaker_show="True" stop="True" voice="ah11005_01_0264" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="随分暗い顔してるじゃないの。&#xA;最近、発明も失敗続きみたいだし、大丈夫？" speaker_show="True" stop="True" voice="hy11002_01_0451" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん～！　少しだけ私の話を&#xA;聞いてもらっても良いでしょうか？" speaker_show="True" stop="True" voice="ah11005_01_0265" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="もちろんよ。こういう時のための幼馴染でしょ。&#xA;言って楽になることなら、何でも聞くわ。" speaker_show="True" stop="True" voice="hy11002_01_0452" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="実は最近、雪ちゃんも知っての通り、失敗続きで&#xA;ごえちゃんに負担をかけ続けていて……。" speaker_show="True" stop="True" voice="ah11005_01_0266" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ごえちゃんはいつも笑顔で協力してくれるんですが、&#xA;この状態が続くのは良くないと思うんです。" speaker_show="True" stop="True" voice="ah11005_01_0267" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="実は昨日も、１つ試作品が出来上がったのですが、&#xA;流石にごえちゃんにはテストを頼みづらくて。" speaker_show="True" stop="True" voice="ah11005_01_0268" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" face_anim_type="002_Facial_smile" />
    <msg speaker="半蔵門 雪" text="なるほど、状況は理解したわ。&#xA;それなら、私が手伝うわよ。" speaker_show="True" stop="True" voice="hy11002_01_0453" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、ありがとうございます。&#xA;お気持ち、とっても嬉しいです。" speaker_show="True" stop="True" voice="ah11005_01_0269" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="けど、失敗続きの私の実験台を、&#xA;雪ちゃんにお願いするのも……。" speaker_show="True" stop="True" voice="ah11005_01_0270" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（はっ！　雪ちゃんのスリムなボディライン！&#xA;これはいけるかもしれません！）" speaker_show="True" stop="True" voice="ah11005_01_0271" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、是非、お願いしたいです！&#xA;お手伝い、してもらえますか？" speaker_show="True" stop="True" voice="ah11005_01_0272" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="もちろんよ。あなたの役に立てて嬉しいわ。" speaker_show="True" stop="True" voice="hy11002_01_0454" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="（こんなに親身になってくれている雪ちゃんの&#xA;ボディラインが…なんて考えてしまうとは……）" speaker_show="True" stop="True" voice="ah11005_01_0273" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="（私は何てダメな子なんでしょう……）" speaker_show="True" stop="True" voice="ah11005_01_0274" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="初芽？　何をしているの？&#xA;さぁ、研究室に行くわよ！" speaker_show="True" stop="True" voice="hy11002_01_0455" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
