    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="200" y="0" anchor="Bottom" />
    <msg speaker="源 モモ" text="師匠！　おつかれさまです！&#xA;それで、どんな訓練を……？" speaker_show="True" stop="True" voice="mm11001_01_0210" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
