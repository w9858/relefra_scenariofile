    <msg speaker="半蔵門 雪" text="私達が戦う理由は同じ。&#xA;……ついてきなさい。" speaker_show="True" stop="True" voice="hy11002_01_0110" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="はいっ！&#xA;宜しくお願いします、師匠っ！" speaker_show="True" stop="True" voice="mm11001_01_0214" />
