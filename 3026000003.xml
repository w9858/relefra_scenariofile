    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="はあ……私は何をしていますの。お姉様は……&#xA;ご自分で誕生日を楽しもうとしているだけですわ。" speaker_show="True" stop="True" voice="sm13002_03_0031" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="というか……ここどこですの？&#xA;ゲレンデに向かっていたはずですのに。" speaker_show="True" stop="True" voice="sm13002_03_0032" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
