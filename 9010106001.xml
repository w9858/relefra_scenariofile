    <create type="Character" obj_id="源 モモ" data_id="311001007" position_id="left" x="-125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="……うん、これくらいだね。&#xA;あんまりやると、かえって痛くなっちゃうから。" speaker_show="True" stop="True" voice="ig11006_02_0485" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="いつもありがとうね、ごえちゃん。&#xA;ごえちゃんのマッサージ、本当に最高。" speaker_show="True" stop="True" voice="mm11001_02_0857" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" lip_sync="False" text="（モモち、元気になったみたいで良かった……。）" speaker_show="True" stop="True" voice="ig11006_02_0486" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="私でよければいつでもマッサージするから&#xA;遠慮なく言ってね。" speaker_show="True" stop="True" voice="ig11006_02_0487" />
