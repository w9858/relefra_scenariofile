    <char_spine_animation />
    <msg speaker="相模 楓" text="お弁当は、行楽の満足度を大きく左右しますからね。&#xA;食材選びから抜かれません。" speaker_show="True" stop="True" voice="sf11004_03_0110" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="なおかつ、良い物を安く買うチャンス――&#xA;力の入れどころですよ、ここは！" speaker_show="True" stop="True" voice="sf11004_03_0111" />
