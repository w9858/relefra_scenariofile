    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ふわ～……&#xA;なんとか間に合って良かったねぇ。" speaker_show="True" stop="True" voice="mm11001_03_0108" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そうね。でも、これはまだまだ&#xA;行楽準備における第一歩に過ぎないわ。" speaker_show="True" stop="True" voice="sf11004_03_0112" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="疲れた顔してる暇はないわよ、モモち。&#xA;今日は寝る間もないと思いなさい。" speaker_show="True" stop="True" voice="sf11004_03_0113" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="寝る間もないの!?" speaker_show="True" stop="True" voice="mm11001_03_0109" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="今回は本当、&#xA;楓ちゃん気合い入ってるね。" speaker_show="True" stop="True" voice="ig11006_03_0134" />
