    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ねえ、もし良ければ、信にも&#xA;お弁当作り手伝わせてくれるかな？" speaker_show="True" stop="True" voice="ks11010_03_0065" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
