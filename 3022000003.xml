    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="あ、みんな～。&#xA;随分急いでるみたいだけど、どうしたの？" speaker_show="True" stop="True" voice="ks11010_03_0062" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="信さんこんにちは！&#xA;明日のお弁当の材料を買いに行くんです！" speaker_show="True" stop="True" voice="sf11004_03_0109" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="スーパーで今やってるセールが&#xA;時間ぎりぎりらしくて……。" speaker_show="True" stop="True" voice="mm11001_03_0107" />
