    <create type="Character" obj_id="石川 五恵" data_id="311006003" position_id="left" x="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="ご覧の通り、ダッシュしてるんです。" speaker_show="True" stop="True" voice="ig11006_03_0133" />
