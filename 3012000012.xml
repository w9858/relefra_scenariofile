    <msg_window_show show="True" />
    <msg speaker="八千代 命" text="ふぃ～、食った食ったぁ～。&#xA;余は満足じゃ！" speaker_show="True" stop="True" voice="ym11003_02_0436" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" />
    <msg speaker="相模 楓" text="ちょっと師匠！　お持ち帰りする予定の&#xA;魚まで食べちゃったんですか!?" speaker_show="True" stop="True" voice="sf11004_02_0409" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" body_anim_type="14_Positive" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="016_Facial_smileBitter" loop="True" />
    <msg speaker="源 モモ" text="アハハ、直火で焼いたお魚って美味しいもん。&#xA;つい食べ過ぎちゃう気持ち、分かるなぁ。" speaker_show="True" stop="True" voice="mm11001_02_0356" />
    <delete obj_id="八千代 命" />
