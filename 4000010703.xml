<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000010703" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="商店街　夕方" />
    <bg show="True" file="04_shopstreetev_01" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="白虎" data_id="311007001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="えーっと、キッチンペーパーとラップか。&#xA;まずは先にスーパーに行って……。" speaker_show="True" stop="True" voice="bk11008_01_0282" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="いや、待て。忘れないうちに&#xA;カトリーナへのプレゼントを先に買おう。" speaker_show="True" stop="True" voice="bk11008_01_0283" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="でも、いったい何を買えば……&#xA;どんなものなら喜ぶんだろうな。" speaker_show="True" stop="True" voice="bk11008_01_0284" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="白虎" text="スパイス……は今さらだし、&#xA;食べる物なら自分で作りそうだからな。" speaker_show="True" stop="True" voice="bk11008_01_0285" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="そういえば、前に初芽がぬいぐるみを作ってたな。&#xA;いや、カトリーナには別の物がいいか。" speaker_show="True" stop="True" voice="bk11008_01_0286" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="いっそ、お菓子でも買って、２人で食べる&#xA;ってのも悪くないが……。" speaker_show="True" stop="True" voice="bk11008_01_0287" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="あー、もう、ダメだ！&#xA;いくら考えても良いプレゼントが浮かばん！" speaker_show="True" stop="True" voice="bk11008_01_0288" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="グダグダ考えるより誰かから情報を得たほうが早いな。&#xA;五恵たちにでも聞いてみるか。" speaker_show="True" stop="True" voice="bk11008_01_0289" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="お、モノミと、ラッパと、カマリ……。&#xA;どうしたんだ急に？" speaker_show="True" stop="True" voice="bk11008_01_0290" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="白虎" text="なんだ？　今、私は買い物で忙しいんだ。&#xA;お前たちと遊んでる暇はない。" speaker_show="True" stop="True" voice="bk11008_01_0291" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="おい、しつこいぞ！　……仕方ないな。&#xA;せっかくだ、私からクイズを出してやろう。" speaker_show="True" stop="True" voice="bk11008_01_0292" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="キッチンペーパーは、なぜキッチンというのか。&#xA;……ふっ、ふっ、ふっ、わからないだろう？" speaker_show="True" stop="True" voice="bk11008_01_0293" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="なんと、キッチンで使うからキッチンペーパーなんだ。&#xA;驚いたか？　私もカトリーナから教えてもらってな。" speaker_show="True" stop="True" voice="bk11008_01_0294" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="さあ、遊んでやったんだ。満足だろう？&#xA;私はもう行くぞ。ええと、今、五恵がいるのは……。" speaker_show="True" stop="True" voice="bk11008_01_0295" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="ん？　なんだお前たち、&#xA;私に何か言いたいことがあるみたいだな。" speaker_show="True" stop="True" voice="bk11008_01_0296" />
    <char_spine_animation obj_id="白虎" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="だったら、さっさと言え。……箱？&#xA;リボンが付いてて……プレゼントか？" speaker_show="True" stop="True" voice="bk11008_01_0297" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="もしかして、カトリーナへのプレゼントのことで&#xA;何か言いたいのか？" speaker_show="True" stop="True" voice="bk11008_01_0298" />
    <char_spine_animation obj_id="白虎" face_anim_type="101_Facial_unique" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="それはいったん置いといて。……って、&#xA;なんでジェスチャーゲームなんだよ！" speaker_show="True" stop="True" voice="bk11008_01_0299" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="くだらないゲームに付き合ってる暇はない。&#xA;何かあるなら……手？　手がどうかしたのか？" speaker_show="True" stop="True" voice="bk11008_01_0300" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="手……そうか！　それはいいアイディアだな。&#xA;よし、早速、買いに行こう！" speaker_show="True" stop="True" voice="bk11008_01_0301" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="白虎" />
    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="白虎" data_id="311007001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="くく、これはいいな。&#xA;カトリーナも喜んでくれるだろう。" speaker_show="True" stop="True" voice="bk11008_01_0302" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="ああ、早速、買ってきたぞ。&#xA;ほらお前たち、これのことを言ったんだよな？" speaker_show="True" stop="True" voice="bk11008_01_0303" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="白虎" text="そうかそうか。&#xA;お前たちもカトリーナに日頃の礼を伝えたかったのか。" speaker_show="True" stop="True" voice="bk11008_01_0304" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="これでバッチリだな。&#xA;カトリーナが喜ぶ姿が目に浮かぶ。" speaker_show="True" stop="True" voice="bk11008_01_0305" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="白虎" text="……よし、こうなったら&#xA;一刻も早くカトリーナの元に帰るぞ。" speaker_show="True" stop="True" voice="bk11008_01_0306" />
  </command>
</adv>