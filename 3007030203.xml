    <msg speaker="青葉 初芽" text="東郷さんのアイデアは特殊すぎて……&#xA;実装には多くの困難がありそうです。" speaker_show="True" stop="True" voice="ah11005_01_0480" />
    <char_spine_animation obj_id="東郷 美森" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="東郷 美森" text="どのような事でしょう？" speaker_show="True" stop="True" voice="tm96002_01_0068" />
