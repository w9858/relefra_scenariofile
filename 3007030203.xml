    <msg speaker="相模 楓" text="友奈のスマホの電源なら、友奈が持たないと&#xA;意味ないんじゃないの？" speaker_show="True" stop="True" voice="sf11004_01_0569" />
    <move obj_id="相模 楓" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="東郷 美森" data_id="396002003" position_id="left" x="-200" anchor="Bottom" y="0" />
