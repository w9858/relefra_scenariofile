    <delete obj_id="乃木 園子" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="ちょい待ち。木造アパートを見たことが……ない？" speaker_show="True" stop="True" voice="ym11003_01_0541" />
    <move obj_id="八千代 命" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
