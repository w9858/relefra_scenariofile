    <delete obj_id="乃木 園子" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" anchor="Bottom" y="0" />
    <create type="Character" obj_id="犬吠埼 樹" data_id="396004003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="犬吠埼 樹" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="犬吠埼 樹" text="あ～……。すみません、楓さん。園子さんは&#xA;大金持ちのお嬢様なので……金銭感覚がちょっと。" speaker_show="True" stop="True" voice="ii96004_01_0032" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="へー、じゃあ初さんと同じ人種だ。" speaker_show="True" stop="True" voice="ym11003_01_0540" />
    <delete obj_id="八千代 命" />
    <delete obj_id="犬吠埼 樹" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" anchor="Bottom" y="0" />
    <create type="Character" obj_id="乃木 園子" data_id="396006003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="カゴに入れた物は全部返してきて！　基本うちは&#xA;割引商品やタイムセールの物しか買いませんっ！" speaker_show="True" stop="True" voice="sf11004_01_0575" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="タイムセールってなんですか～？" speaker_show="True" stop="True" voice="ns96006_01_0045" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="101_Facial_trouble" body_anim_type="16_BendBackward" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="なっ!?　そんなことさえ知らないの？　なのに何で&#xA;うちに来たいなんて言ったのよ。" speaker_show="True" stop="True" voice="sf11004_01_0576" />
