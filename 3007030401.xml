    <char_spine_animation obj_id="乃木 園子" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="はい～。" speaker_show="True" stop="True" voice="ns96006_01_0047" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="そりゃあ、どうやら桁外れの金持ちだねえ。&#xA;うち来たら卒倒しちゃうかもよ？　ド貧乏だから。" speaker_show="True" stop="True" voice="ym11003_01_0542" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="大丈夫大丈夫～♪　えいえいおー！" speaker_show="True" stop="True" voice="ns96006_01_0048" />
