    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" anchor="Bottom" y="0" />
    <create type="Character" obj_id="乃木 園子" data_id="396006003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="え～？　何もしてないよ～？&#xA;普通に、お買い物のお手伝いをしてただけ～。" speaker_show="True" stop="True" voice="ns96006_01_0043" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="どこがよ！　目に入った物を全部カゴに入れちゃって！&#xA;こんなんじゃ、お金がいくらあっても足りないわ！" speaker_show="True" stop="True" voice="sf11004_01_0574" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="え～？？" speaker_show="True" stop="True" voice="ns96006_01_0044" />
