    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="え、未来の世界って、そういうの無いの？&#xA;それって、これから退化してくってことー？" speaker_show="True" stop="True" voice="ym11003_01_0537" />
    <move obj_id="八千代 命" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="犬吠埼 樹" data_id="396004003" position_id="left" x="-200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="犬吠埼 樹" face_anim_type="005_Facial_sad" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="犬吠埼 樹" text="う～ん？　というか、たぶん……神樹様の世界では&#xA;機械っていう物が、それほど重要じゃないのかもです。" speaker_show="True" stop="True" voice="ii96004_01_0030" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="はー、この時代もエコ化に傾いてるからなー。&#xA;未来が機械より自然ってのは、ちょっと納得。" speaker_show="True" stop="True" voice="ym11003_01_0538" />
    <delete obj_id="犬吠埼 樹" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="0" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="コラーー！　アンタ何やってんのよ！" speaker_show="True" stop="True" voice="sf11004_01_0572" />
    <move obj_id="相模 楓" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="フー？　自分こそ大きな声出して、何してんの。" speaker_show="True" stop="True" voice="ym11003_01_0539" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="だって師匠！　園子がメチャクチャなんです！" speaker_show="True" stop="True" voice="sf11004_01_0573" />
