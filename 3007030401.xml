    <msg speaker="相模 楓" text="じゃあ、何でテンション上がってんのよ！" speaker_show="True" stop="True" voice="sf11004_01_0571" />
    <move obj_id="犬吠埼 樹" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="False" />
    <move obj_id="相模 楓" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="乃木 園子" data_id="396006003" position_id="left" x="-300" anchor="Bottom" y="0" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="だって～、違う時代の違う土地にいるってだけで&#xA;もう興奮マックスなのです～♪" speaker_show="True" stop="True" voice="ns96006_01_0041" />
    <char_spine_animation obj_id="犬吠埼 樹" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="犬吠埼 樹" text="それは私もです。不思議ですけど、過去っていうか&#xA;未来に来ちゃったみたいなワクワク感があって。" speaker_show="True" stop="True" voice="ii96004_01_0029" />
    <char_spine_animation obj_id="乃木 園子" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乃木 園子" text="うんうん！&#xA;ロボとかドローンとか初めて見たしね～。" speaker_show="True" stop="True" voice="ns96006_01_0042" />
    <delete obj_id="相模 楓" />
    <delete obj_id="犬吠埼 樹" />
