    <msg speaker="敷島 来夢" text="やー、遊んだわねー。&#xA;日が暮れるまで外で遊ぶなんて、いつぶりかしら？" speaker_show="True" stop="True" voice="sr13001_03_0149" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="我ながらちょっとはしゃぎすぎたから、&#xA;お風呂に入って、食事したらすぐ寝ちゃいそう。" speaker_show="True" stop="True" voice="sr13001_03_0150" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="でしたら、みんなで露天風呂に入りません？" speaker_show="True" stop="True" voice="sm13002_03_0091" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="一度外に出る必要がありますけど、&#xA;このペンションに併設されているみたいですわ。" speaker_show="True" stop="True" voice="sm13002_03_0092" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="マル、えらい！&#xA;オーナーに聞いてたけど、すっかり忘れてたわ。" speaker_show="True" stop="True" voice="sr13001_03_0151" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="宿泊施設に何があるか、もしもに備えて&#xA;確認していただけですわ。" speaker_show="True" stop="True" voice="sm13002_03_0093" />
    <move obj_id="敷島 真留瀬" position_id="left" x="300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <delete obj_id="敷島 来夢" />
