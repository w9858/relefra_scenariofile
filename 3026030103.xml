    <char_spine_animation obj_id="敷島 真留瀬" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="ふふ、お気に召してくれたようで、&#xA;頑張った甲斐がありましたわね。アミィ。" speaker_show="True" stop="True" voice="sm13002_03_0098" />
    <char_spine_animation obj_id="アミィ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="アミィ" face_anim_type="003_Facial_laugh" force_end="False" loop="True" />
    <msg speaker="アミィ" text="大成功でございまする。" speaker_show="True" stop="True" voice="am13002_03_0072" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ありがとう、みんな！　ビックリしちゃった。&#xA;こんなサプライズがあるなんて。" speaker_show="True" stop="True" voice="sr13001_03_0155" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="アミィ" text="すべて真留瀬殿の計画。マスターが来夢ガールズと&#xA;過ごしている間に事をなしたのでございまする。" speaker_show="True" stop="True" voice="am13002_03_0073" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="アミィたちの協力があってこそですわ。" speaker_show="True" stop="True" voice="sm13002_03_0099" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="みんなのおかげで&#xA;今日からの一年も最高の毎日が送れる気がする！" speaker_show="True" stop="True" voice="sr13001_03_0156" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様。&#xA;いつまでもここにいたらお体が冷えてしまいますわ。" speaker_show="True" stop="True" voice="sm13002_03_0100" />
    <char_spine_eye obj_id="敷島 来夢" type="Eye01_default" loop="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="そうね。マルたちがくれたこの光景をもっと&#xA;見ていたいけど、温泉に入るとしましょうか。" speaker_show="True" stop="True" voice="sr13001_03_0157" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="みんな、私が温めてあげるわ。" speaker_show="True" stop="True" voice="sr13001_03_0158" />
    <msg speaker="来夢ガールズA・B・Z" text="きゃーーっ♪" speaker_show="True" stop="True" voice="3026030103_union_004" />
