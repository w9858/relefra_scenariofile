    <telop text="ツキカゲ基地" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" />
    <create obj_id="石川 五恵" data_id="311006005" x="-300" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" x="300" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="014_Facial_angryStrong" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <create_effect effect_id="wave" effect_type="EffectShockWave" target_obj="カトリーナ" />
    <msg speaker="カトリーナ" text="『可愛い高坂信ちゃんへ』くぅ……何者なの&#xA;私のシン様を勝手に、可愛い信ちゃん呼ばわりして！" speaker_show="True" stop="True" voice="kt11009_03_0103" />
    <delete obj_id="wave" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="べ、別に悪口ではないのでそこは……。" speaker_show="True" stop="True" voice="ig11006_03_0195" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="昨日、連絡を貰って調べてみたら、うちのクラスでも&#xA;同じ招待状を受け取った人が、２名いたわ。" speaker_show="True" stop="True" voice="hy11002_03_0200" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="-300" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="300" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="アタシのバイト先の子も、知ってるって言ってました。&#xA;もう結構、話題になってるみたいですね。" speaker_show="True" stop="True" voice="sf11004_03_0268" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いったい、ニコラスの目的は何なんでしょう？&#xA;単なるパーティなら良いんですけど……。" speaker_show="True" stop="True" voice="ah11005_03_0153" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" position_id="left" x="-300" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="300" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="でも、参加しちゃった子に何かあってからじゃ&#xA;遅いし、ここはツキカゲが手を打つべきじゃない？" speaker_show="True" stop="True" voice="ym11003_03_0251" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="うん。街中の子が対象だととても止めきれないし&#xA;私達で、このパーティに行ってみるしかないよ。" speaker_show="True" stop="True" voice="mm11001_03_0291" />
    <delete obj_id="八千代 命" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="-300" />
    <create type="Character" obj_id="白虎" data_id="311007008" position_id="left" x="0" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="300" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="信が行くよ。招待された本人だし、落とし物を&#xA;届けた時に尾行されてなかったのが気になるんだ。" speaker_show="True" stop="True" voice="ks11010_03_0095" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <create_effect effect_id="warai" effect_type="MampSmile" target_obj="白虎" />
    <msg speaker="白虎" text="なら当然、私も行くぞ！　本当のパーティだったら&#xA;行って御馳走にならんと悪いからなー！" speaker_show="True" stop="True" voice="bk11008_03_0100" />
    <delete obj_id="warai" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="遊びじゃないのよ、白虎。&#xA;これは歴とした潜入作戦なんだから。" speaker_show="True" stop="True" voice="hy11002_03_0201" />
    <char_spine_animation obj_id="白虎" face_anim_type="013_Facial_angry" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="白虎" text="わあーってるわい！　お前ら最近、&#xA;私が超一流の傭兵だってことを忘れすぎだぞ！" speaker_show="True" stop="True" voice="bk11008_03_0101" />
    <delete obj_id="高坂 信" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="-300" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="確かに、白虎ちゃんなら何かあっても切り抜け&#xA;られるよね。怪しまれる確率も少ないだろうし。" speaker_show="True" stop="True" voice="ig11006_03_0196" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="300" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="テレジア" text="本物の子供だから。" speaker_show="True" stop="True" voice="tr11007_03_0055" />
    <char_spine_animation obj_id="白虎" face_anim_type="101_Facial_unique" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="白虎" text="ムッフ、そうだろう！　………あん？&#xA;おいテレ公、お前いま何つったああーーっ！" speaker_show="True" stop="True" voice="bk11008_03_0102" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="白虎" />
