    <create type="Character" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="0" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうと決まれば、クリスマス当日までに我々の役割を&#xA;決めて綿密な作戦を立てましょう。" speaker_show="True" stop="True" voice="hy11002_03_0202" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="メイとフーは学校と街で情報収集を続けるから、&#xA;初さんは通信器とか道具の準備に入ってくれる？" speaker_show="True" stop="True" voice="ym11003_03_0253" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ええ。当日も私は基地に残って、全員の動きを把握し&#xA;指示出しに専念するつもりです。" speaker_show="True" stop="True" voice="ah11005_03_0155" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="五恵とテレジアは、何かあった時にすぐ飛び込めるよう&#xA;会場になる建物の下見を入念にすること。" speaker_show="True" stop="True" voice="hy11002_03_0203" />
