  <info>
    <VoiceCueSheet value="AdvVoice/9010105001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="03_tsukikage_007" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005004" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="うーん……どうしてこうなっちゃったんでしょう。&#xA;困りましたね……。" speaker_show="True" stop="True" voice="ah11005_02_0766" />
    <move obj_id="青葉 初芽" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="初芽さん？　どうしたんですか？" speaker_show="True" stop="True" voice="mm11001_02_0824" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="実は……新薬ができたんですけど、&#xA;イマイチ思った通りの効果にならないんです。" speaker_show="True" stop="True" voice="ah11005_02_0767" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="イマイチって、どんな薬なんですか？" speaker_show="True" stop="True" voice="mm11001_02_0825" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="身体能力が高まるものなんですよ。&#xA;もちろん一時的にですけどね。" speaker_show="True" stop="True" voice="ah11005_02_0768" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="えええっ！　そんなすごいものなんですか！" speaker_show="True" stop="True" voice="mm11001_02_0826" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="020_Facial_thinking" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="でも、私が何回か試してみたんですが&#xA;思ったように効果が出ないんですよ。……はあ……。" speaker_show="True" stop="True" voice="ah11005_02_0769" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="わー、そんなに落ち込まないでください。&#xA;あの、良かったら……私が協力しましょうか？" speaker_show="True" stop="True" voice="mm11001_02_0827" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="いえ、さすがに他の人に試してもらうわけには……。&#xA;それに、モモちゃんに頼んだら雪ちゃんに怒られます。" speaker_show="True" stop="True" voice="ah11005_02_0770" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="でも、困っている初芽さんを放ってはおけないですよ。&#xA;きっと師匠だって許してくれると思います。" speaker_show="True" stop="True" voice="mm11001_02_0828" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="で、でも……さすがにそれは……！" speaker_show="True" stop="True" voice="ah11005_02_0771" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="大丈夫です！　パワーアップの薬なんて&#xA;試してみたいですから。いただきます！　ゴクン。" speaker_show="True" stop="True" voice="mm11001_02_0829" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="あっ、モモちゃんっ！&#xA;……の、飲んじゃったんですか!?" speaker_show="True" stop="True" voice="ah11005_02_0772" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="はい。……す、すごいです！&#xA;みるみる力がみなぎってきますよ！" speaker_show="True" stop="True" voice="mm11001_02_0830" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ああ……えーっと……。スパイスを併用することで&#xA;もっと効果を高めることができるはずです。" speaker_show="True" stop="True" voice="ah11005_02_0773" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="すごいです!!&#xA;ぜんぜんイマイチじゃないですよ！" speaker_show="True" stop="True" voice="mm11001_02_0831" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="この薬があれば&#xA;ツキカゲの戦力アップ間違いなしですね！" speaker_show="True" stop="True" voice="mm11001_02_0832" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="101_Facial_trouble" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そ、それが……薬が切れると副作用として&#xA;デコピンを受けているような感覚になるんですよ。" speaker_show="True" stop="True" voice="ah11005_02_0774" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="え？　デコピン……？" speaker_show="True" stop="True" voice="mm11001_02_0833" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="019_Facial_damage" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あっ！　い、今きました！&#xA;イタタ……連続でデコピンされてます！" speaker_show="True" stop="True" voice="mm11001_02_0834" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ああっ、大丈夫ですか？&#xA;やはりモモちゃんにも副作用が出てしまいましたか。" speaker_show="True" stop="True" voice="ah11005_02_0775" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="今、急いで冷やす湿布を持って来ますね！&#xA;ちょっと待っていてください。" speaker_show="True" stop="True" voice="ah11005_02_0776" />
  </command>
</adv>