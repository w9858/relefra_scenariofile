    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="まあ、そういうプレゼントは私の時だけで&#xA;良いとして、今回はどうしようかしらねぇ。" speaker_show="True" stop="True" voice="kt11009_02_0101" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="テレジア" text="そうだな……。ん？" speaker_show="True" stop="True" voice="tr11007_02_0108" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="カトリーナは雪が欲しいのか。&#xA;よし、お前の時は雪を縛り上げてやるゾ！" speaker_show="True" stop="True" voice="bk11008_02_0038" />
