    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="はぁ…。さっきから無駄話ばかり。&#xA;どうやら良い案は浮かばないようね。" speaker_show="True" stop="True" voice="hy11002_02_0202" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="まあ、こんな事もあろうかと、もうすでに&#xA;奥の手をスタンバイさせてあるわ。" speaker_show="True" stop="True" voice="hy11002_02_0203" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
