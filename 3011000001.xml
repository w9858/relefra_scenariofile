    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="そりゃあもう！　持って帰って～&#xA;あーんなことやこーんなこ…" speaker_show="True" stop="True" voice="ym11003_02_0380" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="メイ。" speaker_show="True" stop="True" voice="hy11002_02_0201" />
