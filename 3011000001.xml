    <msg speaker="半蔵門 雪" text="何でも喜ぶとは思うけれど&#xA;適当に済ませたくはないし。" speaker_show="True" stop="True" voice="hy11002_02_0199" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="やっぱさ！　ユッキーにリボンかけて&#xA;『私がプレゼントよ』ってのが良いよ！" speaker_show="True" stop="True" voice="ym11003_02_0379" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="-300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="テレジア" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="テレジア" text="あぁ……それはそうかもしれないな。" speaker_show="True" stop="True" voice="tr11007_02_0107" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="そうね。" speaker_show="True" stop="True" voice="kt11009_02_0100" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="そう……なの？" speaker_show="True" stop="True" voice="ks11010_02_0198" />
