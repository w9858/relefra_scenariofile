    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" force_end="False" loop="False" mixing="True" />
    <msg speaker="相模 楓" text="いやそれ、師匠とか弟子とか関係なく&#xA;そもそも方向性がおかしいです。" speaker_show="True" stop="True" voice="sf11004_02_0341" />
