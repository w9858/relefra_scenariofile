    <char_spine_animation obj_id="カトリーナ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="私も、考えすぎて目眩がしてきたわ。&#xA;サプライズって難しい。フゥ……。" speaker_show="True" stop="True" voice="kt11009_02_0099" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="15_Negative" force_end="False" loop="False" mixing="True" />
    <create_effect effect_id="Sweat_sin" effect_type="MampSweat" life_time="2" target_obj="高坂 信" x="0" y="0" attach_head_bone="True" />
    <move obj_id="カトリーナ" x="-20" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <msg speaker="高坂 信" text="うん。そう言って、信に寄りかかったら&#xA;そのうち２人で倒れるから、戻ってね。" speaker_show="True" stop="True" voice="ks11010_02_0197" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <create_effect effect_id="smile_byakko" effect_type="MampSmile" life_time="1.5" target_obj="白虎" x="0" y="0" attach_head_bone="True" />
    <msg speaker="白虎" text="なんだなんだー？　何を悩んでるんだー？&#xA;白虎様に相談してみろ！　ホレホレ何だ？" speaker_show="True" stop="True" voice="bk11008_02_0036" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="だーかーらー！&#xA;モモちの誕生日の事だってば！" speaker_show="True" stop="True" voice="sf11004_02_0340" />
    <delete obj_id="白虎" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="テレジア" data_id="311008003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="テレジア" text="え、誕生日？　それだけ……？" speaker_show="True" stop="True" voice="tr11007_02_0106" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="それだけとはいっても、どんなパーティとか&#xA;何を贈ったら喜んでもらえるか……とか。" speaker_show="True" stop="True" voice="ah11005_02_0234" />
