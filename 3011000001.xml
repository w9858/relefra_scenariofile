    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="何でだよ！　簡単でイイじゃんか！" speaker_show="True" stop="True" voice="bk11008_02_0037" />
