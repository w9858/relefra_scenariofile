    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="そうそう。直接肌に塗らないと効果時間が&#xA;短くなるから、毎回裸にならないといけない所とか。" speaker_show="True" stop="True" voice="sf11004_01_0510" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="裸だと武器の持ち込みが制限されてしまうのも、&#xA;戦闘には不便だし。" speaker_show="True" stop="True" voice="hy11002_01_0449" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="いやいや、それより何より裸で敵陣に侵入って、&#xA;どーなのよ！　乙女としてはリスキー過ぎるっしょ？" speaker_show="True" stop="True" voice="ym11003_01_0477" />
    <delete obj_id="相模 楓" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" position_id="left" x="0" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="言われてみると、そうだよね。&#xA;どうしよう、ちょっと心配になってきちゃった。" speaker_show="True" stop="True" voice="mm11001_01_0688" />
