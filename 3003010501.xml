    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="まだ試作品ではありますが、このスーツが完成&#xA;すれば、みんなの悩みが全て解決できちゃいます！" speaker_show="True" stop="True" voice="ah11005_01_0245" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="………………。" speaker_show="True" stop="True" voice="ah11005_01_0246" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="101_Facial_trouble" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="はっ、私としたことが、あまりの感激と興奮で、&#xA;つい１人で喋りすぎてしまいましたね。" speaker_show="True" stop="True" voice="ah11005_01_0247" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="とりあえず、早速試着をしてみましょう。" speaker_show="True" stop="True" voice="ah11005_01_0248" />
    <msg_window_show show="False" />
    <transition type="Out" time="0.5" wait="True" />
    <delete obj_id="青葉 初芽" />
    <sound cue_name="Hug" />
    <msg speaker="青葉 初芽" text="んん～。あら？　おかしいですね？&#xA;むぐぐっ……う～ん、う～ん。" speaker_show="True" stop="True" voice="ah11005_01_0249" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005008" position_id="left" x="0" anchor="Bottom" />
    <msg_window_show show="False" />
    <transition type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ふぅっ、ようやく着れましたが……。&#xA;はぁ……はぁ……、これは……ちょっと……。" speaker_show="True" stop="True" voice="ah11005_01_0250" />
    <msg_window_show show="False" />
    <transition type="Out" time="0.5" wait="True" />
    <sound cue_name="Hug" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005007" />
    <transition type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" />
    <msg speaker="青葉 初芽" text="はぁ、はぁ……。着るのも大変でしたけど、&#xA;脱ぐのもこんなに大変だとは。" speaker_show="True" stop="True" voice="ah11005_01_0251" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="いろいろ問題が見つかってしまいましたが、&#xA;今回の試着で気づいた最大の問題点は圧迫感！" speaker_show="True" stop="True" voice="ah11005_01_0252" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="肌にフィットするようタイトな設計には&#xA;していましたが、胸周りが特にキツすぎますね。" speaker_show="True" stop="True" voice="ah11005_01_0253" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="これではテストを開始する前に、胸が苦しくて&#xA;呼吸困難で倒れてしまいます。" speaker_show="True" stop="True" voice="ah11005_01_0254" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="せっかく完成した試作品ではありますが、&#xA;また１から作り直しをすべきでしょうかね……。" speaker_show="True" stop="True" voice="ah11005_01_0255" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="うーん……。どうしましょうか。" speaker_show="True" stop="True" voice="ah11005_01_0256" />
