    <fade_in time="1.0" type="Default" wait="True" />
    <msg window_type="SystemMsg" text="数日前のことーー" stop="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006005" position_id="left" x="0" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="こんなに凄い発明をしてくれた師匠には、言い出し&#xA;づらいけど、まだまだ問題点は多いんだよね。" speaker_show="True" stop="True" voice="ig11006_01_0314" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="相模 楓" data_id="311004005" position_id="left" x="0" />
