<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000020201" />
  </info>
  <command>
    <Start />
    <load_cue_sheet cue_sheet="BGM/BGM19_Adv7" />
    <sound type="Bgm" cue_name="Adv7" />
    <fade_out time="0" type="Default" wait="False" />
    <camera scale_x="2" scale_y="2" time="0" move_x="0" move_y="-150" />
    <bg show="True" file="34_sakasatower_01" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="逆さ塔" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002004" position_id="left" x="0" y="-1000" anchor="Bottom" />
    <wait time="0.5" />
    <move obj_id="乱獅子 ゆら" y="1000" y_move_type="MoveValue" time="0.3" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="荒ぶる獅子の力を見せてやる！&#xA;ストライクショーッ……！！" speaker_show="True" stop="True" voice="ry12002_01_0189" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="あ、違う！？　ソーリーソーリー。&#xA;……改めて。" speaker_show="True" stop="True" voice="ry12002_01_0190" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <camera scale_x="1" scale_y="1" time="1" easing="Unset" move_y="0" move_x="0" />
    <wait time="1.0" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="アタシ様の名は乱獅子ゆら！！&#xA;よろしくボーイ・アンド・ガール！" speaker_show="True" stop="True" voice="ry12002_01_0191" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="黒百合の二年、ピッチピチの女子高生だ。" speaker_show="True" stop="True" voice="ry12002_01_0192" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="しかし、その裏の顔は！" speaker_show="True" stop="True" voice="ry12002_01_0193" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="国家の安寧とやらを守る凪の部隊、&#xA;その切り込み役だ。お前らイイネ！　くれ。" speaker_show="True" stop="True" voice="ry12002_01_0194" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="コードネームは愛染。アタシ様の正体、&#xA;クラスの連中には内緒だぜ。マジでマジで。" speaker_show="True" stop="True" voice="ry12002_01_0195" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="自己紹介コーナーって事で、アタシ様が&#xA;いかにアタシ様であるかを切々と説くわ。" speaker_show="True" stop="True" voice="ry12002_01_0196" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="まず身長と血液型を教えてくれるかなって？&#xA;そりゃ１５７センチ、AB型よ。" speaker_show="True" stop="True" voice="ry12002_01_0197" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="性格は、博愛主義者を自称している。" speaker_show="True" stop="True" voice="ry12002_01_0198" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="後はおしゃべりで、賑やかなのが好きだな。" speaker_show="True" stop="True" voice="ry12002_01_0199" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="正直真面目すぎる雰囲気苦手なんだ、間が持たない。" speaker_show="True" stop="True" voice="ry12002_01_0200" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="なんか、こういう経験ない？　今ここで俺があいつの&#xA;事殴ったらどうなるんだろうって想像すること。" speaker_show="True" stop="True" voice="ry12002_01_0201" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="その想像を実際やってしまう、お騒がせ系が&#xA;アタシ様だ。おかげでよく隊長にはシめられてる。" speaker_show="True" stop="True" voice="ry12002_01_0202" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="趣味はカラオケとかだな。やっぱ騒ぐのが好きだ。&#xA;なんなら一晩中喋ってられっかんな。" speaker_show="True" stop="True" voice="ry12002_01_0203" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="しゃべりすぎて、ルームメイトの飛粋が&#xA;朝、隣で気絶してたこともあったぜ。ナハハ。" speaker_show="True" stop="True" voice="ry12002_01_0204" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="じゃあここらでアタシ様の仕事服を見せてやる。" speaker_show="True" stop="True" voice="ry12002_01_0206" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="すっげーカッコいいから拍手喝采くれ。" speaker_show="True" stop="True" voice="ry12002_01_0207" />
    <msg_window_show show="False" />
    <transition effect="Shutter" type="Out" color_change="True" time="1.0" wait="True" />
    <camera move_y="-50" scale_x="1.5" scale_y="1.5" time="1" easing="Unset" />
    <msg speaker="乱獅子 ゆら" text="スタンディングバイ！" speaker_show="True" stop="True" voice="ry12002_01_0208" />
    <msg_window_show show="False" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="乱獅子 ゆら" data_id="312002001" />
    <transition effect="Shutter" type="In" color_change="True" time="1.0" wait="False" />
    <Comment comment="ゆら・凪の部隊の服" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="どーよ。&#xA;どーーーーよ。" speaker_show="True" stop="True" voice="ry12002_01_0209" />
    <camera_reset time="0.5" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="武器は刀を使う。これで悪人を&#xA;三枚におろすわけだ。" speaker_show="True" stop="True" voice="ry12002_01_0210" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <camera move_y="0" scale_x="1" scale_y="1" time="1" easing="Unset" />
    <msg speaker="乱獅子 ゆら" text="そんで特技がある。再生能力がはやいってこと。&#xA;滅多なことじゃあ、アタシ様は死なない。スッゲ。" speaker_show="True" stop="True" voice="ry12002_01_0211" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="だからこそ、凪で切り込み隊長やってるわけだ。&#xA;ゲーム的に言えば伏兵踏み、罠踏み役ってね。" speaker_show="True" stop="True" voice="ry12002_01_0212" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="初手アタシ様。これぞ凪の部隊、勝利の方程式。&#xA;これテストに出ることもあるから。" speaker_show="True" stop="True" voice="ry12002_01_0213" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="あー…それと。実はアタシ様って大八洲師団って&#xA;犯罪組織の首領の娘なんだよね。" speaker_show="True" stop="True" voice="ry12002_01_0214" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="でも安心してくれ。今は正義側だから。" speaker_show="True" stop="True" voice="ry12002_01_0215" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="飛粋がアタシ様に光をくれたんだ。……へへ。&#xA;凪の部隊が、アタシ様の居場所だよ。" speaker_show="True" stop="True" voice="ry12002_01_0216" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="身内の不始末として、ファックスな姉どもと、&#xA;ダディはアタシ様が捕まえたいと思ってる。" speaker_show="True" stop="True" voice="ry12002_01_0217" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="いーじょう！！" speaker_show="True" stop="True" voice="ry12002_01_0218" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>