    <char_spine_animation obj_id="北斗 凪" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="北斗 凪" text="きゃっ？　ご、ごめんね！" speaker_show="True" stop="True" voice="hn99002_01_0012" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="101_Facial_trouble" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="凪ちゃん、ごめんね。&#xA;私が変なふうに打っちゃったから……。&#xA;//呼称不明・要確認" speaker_show="True" stop="True" voice="ig11006_01_0506" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="北斗 凪" text="ううん、大丈夫。じゃ、次は私からね。&#xA;いくよー……あ、あれ？" speaker_show="True" stop="True" voice="hn99002_01_0013" />
    <sound type="Se" />
    <Comment comment="空振りする凪" />
