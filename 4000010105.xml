    <msg speaker="源 モモ" text="だったら今のうちに練習してみたら？" speaker_show="True" stop="True" voice="mm11001_01_0880" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="True" force_end="False" />
