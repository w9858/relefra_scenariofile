    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="それじゃ、次はボールをよく見ながら上げて、&#xA;そのボールを片手で包むように叩いてみて。" speaker_show="True" stop="True" voice="mm11001_01_0883" />
