    <char_spine_animation obj_id="畠山 結愛" face_anim_type="001_Facial_blink" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="畠山 結愛" text="試合って……５人しかいないけど？" speaker_show="True" stop="True" voice="ha99001_01_0016" />
