    <msg speaker="白虎" text="じゃあ、飛粋も一緒に寝よう。&#xA;私と一緒だとよく眠れるみたいだぞ！" speaker_show="True" stop="True" voice="bk11008_02_0026" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="いいの？　やったー！&#xA;それじゃあお邪魔します。" speaker_show="True" stop="True" voice="mh12001_02_0064" />
    <char_spine_animation obj_id="白虎" face_anim_type="020_Facial_thinking" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="さすがに１つの布団に３人は窮屈だな。&#xA;ちゃんと寝られるか……ZZZZZ。" speaker_show="True" stop="True" voice="bk11008_02_0027" />
    <create_effect effect_id="ManpSleep_byakko" effect_type="MampSleep" target_obj="白虎" x="0" y="0" attach_head_bone="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ふふ、白虎ちゃんてば、話しながら寝ちゃっ……&#xA;ZZZZZ……。" speaker_show="True" stop="True" voice="mh12001_02_0065" />
