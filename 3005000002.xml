    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="……。&#xA;うー、ダメだ！　全然眠れない！" speaker_show="True" stop="True" voice="mh12001_02_0056" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="久々の１人部屋っていうのも逆に落ち着かないし&#xA;それより何より、部屋が広すぎて！" speaker_show="True" stop="True" voice="mh12001_02_0057" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="020_Facial_thinking" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="どうしよう。完全に目が覚めちゃった……。" speaker_show="True" stop="True" voice="mh12001_02_0058" />
