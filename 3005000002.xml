    <msg speaker="宗近 飛粋" text="ひゃっ！　ごめんなさい！&#xA;覗き見するつもりじゃなかったんだけど……。" speaker_show="True" stop="True" voice="mh12001_02_0062" />
    <char_spine_animation obj_id="白虎" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="もしかしてお前、眠れないのか？" speaker_show="True" stop="True" voice="bk11008_02_0025" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="う、うん。&#xA;何だか部屋が広すぎて緊張しちゃって。" speaker_show="True" stop="True" voice="mh12001_02_0063" />
