    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="宗近 飛粋" text="はい！&#xA;今日から宜しくお願いします！" speaker_show="True" stop="True" voice="mh12001_02_0050" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="広い家だが、まあ自分の家だと思って、&#xA;ゆっくりしてくれ。" speaker_show="True" stop="True" voice="bk11008_02_0023" />
