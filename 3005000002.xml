    <msg speaker="テレジア" text="ここは初芽の家だがな。私も白虎も、宗近と同じく&#xA;ここに居候をしている。よろしく。" speaker_show="True" stop="True" voice="tr11007_02_0098" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="宗近 飛粋" text="うん、よろしくね！" speaker_show="True" stop="True" voice="mh12001_02_0051" />
