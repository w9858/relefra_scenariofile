    <msg speaker="青葉 初芽" text="おやすみなさい。" speaker_show="True" stop="True" voice="ah11005_02_0198" />
    <msg_window_show show="False" />
    <bg file="24_freespace_01" color="#323232ff" effect_time="1.5" />
    <move obj_id="青葉 初芽" position_id="left" x="1500" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.0" wait="True" />
    <delete obj_id="青葉 初芽" />
    <move obj_id="宗近 飛粋" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.0" wait="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="行っちゃった。こんな広い１人部屋初めてだから&#xA;緊張しちゃう！　けど早く寝なくちゃね。" speaker_show="True" stop="True" voice="mh12001_02_0054" />
