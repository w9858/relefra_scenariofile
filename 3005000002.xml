    <move obj_id="宗近 飛粋" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1.0" wait="True" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ひ、広い……。これが初芽さんのお家。&#xA;お部屋も家具も素敵すぎて、素敵の大洪水です！" speaker_show="True" stop="True" voice="mh12001_02_0049" />
