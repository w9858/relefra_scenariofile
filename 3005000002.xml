    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="宗近 飛粋" text="隣は確か、テレちゃんの部屋だよね。&#xA;そーっと扉を開けて……。" speaker_show="True" stop="True" voice="mh12001_02_0060" />
