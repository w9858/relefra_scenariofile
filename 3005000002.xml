    <msg speaker="青葉 初芽" text="飛粋ちゃん、お風呂はどうでしたか？&#xA;ゆっくりできましたか？" speaker_show="True" stop="True" voice="ah11005_02_0196" />
    <move obj_id="青葉 初芽" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="はい！　プールみたいな大きなお風呂に、美味しい&#xA;夜ご飯。思いっきり楽しませてもらいました。" speaker_show="True" stop="True" voice="mh12001_02_0052" />
