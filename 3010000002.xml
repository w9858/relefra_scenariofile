    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="半蔵門 雪" text="ええ。この時期に相応しい&#xA;素敵なプレゼントになると思うわ。" speaker_show="True" stop="True" voice="hy11002_02_0189" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="むむ、これって……" speaker_show="True" stop="True" voice="ym11003_02_0202" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
