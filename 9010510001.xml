  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="03_tsukikage_007" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005007" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="あれ、信さん。珍しいですね。&#xA;どうしたんですか、わざわざ。" speaker_show="True" stop="True" voice="ah11005_02_0956" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="こんにちは、初芽ちゃん。&#xA;ごめんね、研究中に。" speaker_show="True" stop="True" voice="ks11010_02_0426" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ちょっと聞きたいことがあって&#xA;寄らせてもらったんだけど。今、少し大丈夫？" speaker_show="True" stop="True" voice="ks11010_02_0427" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="はい。なんですか？" speaker_show="True" stop="True" voice="ah11005_02_0957" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="実は、初芽ちゃんの&#xA;研究者としての知恵を借りたくて。" speaker_show="True" stop="True" voice="ks11010_02_0428" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <fade_in time="1.0" type="Default" wait="True" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="……なるほど、その手があったか。&#xA;さすが初芽ちゃんだね。" speaker_show="True" stop="True" voice="ks11010_02_0429" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="これくらいで大げさですよ。" speaker_show="True" stop="True" voice="ah11005_02_0958" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="いや、困ったとき初芽ちゃんの顔が浮かんで&#xA;聞きに来てみたけど……やっぱり正解だったよ。" speaker_show="True" stop="True" voice="ks11010_02_0430" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ごめん、邪魔しちゃったね。そろそろ帰るよ。" speaker_show="True" stop="True" voice="ks11010_02_0431" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="え、もう帰ってしまうんですか？" speaker_show="True" stop="True" voice="ah11005_02_0959" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ん？　だって研究中じゃないの？" speaker_show="True" stop="True" voice="ks11010_02_0432" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それはそうなんですけど……あの、もし良かったら&#xA;信さんのその腕、また見せてくださいね。" speaker_show="True" stop="True" voice="ah11005_02_0960" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="ああ、これ？&#xA;好きだねー、初芽ちゃんも。" speaker_show="True" stop="True" voice="ks11010_02_0433" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="だって、カッコイイですし……。&#xA;私も信さんみたいにできたらいいんですけど。" speaker_show="True" stop="True" voice="ah11005_02_0961" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="じゃ、情報をもらったお礼も兼ねて……はい。&#xA;心ゆくまで見ていいよ。" speaker_show="True" stop="True" voice="ks11010_02_0434" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="わあ～、ありがとうございます！&#xA;何度見てもいいですね、アニメの主人公みたいで！&#xA;//権利の都合でアニメタイトルは入れない形にしています" speaker_show="True" stop="True" voice="ah11005_02_0962" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="初めはビックリしたけどね。&#xA;便利だし、今は前向きにとらえてるよ。" speaker_show="True" stop="True" voice="ks11010_02_0435" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="まあ、少し手入れが大変っていうのはあるけど……。" speaker_show="True" stop="True" voice="ks11010_02_0436" />
    <create_effect effect_id="effectkirakira_hatsume" effect_type="EffectKirakira" life_time="1.0" target_obj="青葉 初芽" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="だったら私にメンテナンスさせてください！&#xA;一度やってみたかったんです。" speaker_show="True" stop="True" voice="ah11005_02_0963" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="もしほこりが入ったら機能に影響が出そうですし。&#xA;言ってもらえれば細かくやりますよ" speaker_show="True" stop="True" voice="ah11005_02_0964" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="え、いいの？　初芽ちゃんなら安心して頼めるよ。&#xA;信より、こういうの上手そうだから。" speaker_show="True" stop="True" voice="ks11010_02_0437" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="はい！　どうせなら、ついでに改造もして……。&#xA;あんな機能やこんな機能も追加したりして……。" speaker_show="True" stop="True" voice="ah11005_02_0965" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="か、改造……？　それはちょっと……。" speaker_show="True" stop="True" voice="ks11010_02_0438" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうですか。それは残念です。&#xA;でも、少しくらいなら……。" speaker_show="True" stop="True" voice="ah11005_02_0966" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="うーん、なんだか……初芽ちゃんの目つきが&#xA;いつもと違うような……？" speaker_show="True" stop="True" voice="ks11010_02_0439" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="楽しみですー。むふふ。" speaker_show="True" stop="True" voice="ah11005_02_0967" />
  </command>
