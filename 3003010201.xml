    <telop text="ツキカゲ基地" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="ボンテージスーツも無事完成したことだし、&#xA;今日のミッションは私が単独で潜入するわ。" speaker_show="True" stop="True" voice="hy11002_01_0422" />
    <move obj_id="半蔵門 雪" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="まじ？　ユッキー１人で大丈夫？&#xA;メイも一緒に行ってあげるよ。" speaker_show="True" stop="True" voice="ym11003_01_0469" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="敵からは見えないんだし、みんなで行った方が&#xA;成功率も上がるってモンでしょ？" speaker_show="True" stop="True" voice="ym11003_01_0470" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="確かにメイの言うことも一理あるわ。&#xA;けど今回は、潜入場所も狭いから１人で十分よ。" speaker_show="True" stop="True" voice="hy11002_01_0423" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ちぇっ……。&#xA;せっかくスーツ、着れると思ったのに。" speaker_show="True" stop="True" voice="ym11003_01_0471" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="石川 五恵" data_id="311006003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="メイちゃん、すっかり気に入ったみたいだね。" speaker_show="True" stop="True" voice="ig11006_01_0311" />
    <create type="Character" obj_id="八千代 命" data_id="311003003" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="いやぁ、慣れてきたらあのピタピタ具合、&#xA;妙に癖になっちゃってさ～。" speaker_show="True" stop="True" voice="ym11003_01_0472" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="わかります！　それじゃあ私たちも&#xA;ボンテージスーツ姿で援護しましょうか。" speaker_show="True" stop="True" voice="ah11005_01_0223" />
    <delete obj_id="八千代 命" />
    <delete obj_id="石川 五恵" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="無駄に露出度あげるの、やめてください！" speaker_show="True" stop="True" voice="sf11004_01_0508" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="そうだね、まだ予備のスーツも出来てないから&#xA;大事に使わなきゃ。" speaker_show="True" stop="True" voice="mm11001_01_0667" />
    <delete obj_id="源 モモ" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="じゃあ早速、準備にかかりましょう。&#xA;着替えてくるから、初芽はクリームの準備をお願い。" speaker_show="True" stop="True" voice="hy11002_01_0424" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="お任せください！" speaker_show="True" stop="True" voice="ah11005_01_0224" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="青葉 初芽" />
