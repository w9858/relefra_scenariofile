    <create type="Character" obj_id="八千代 命" data_id="311003003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="私も終わりました。" speaker_show="True" stop="True" voice="ig11006_01_0313" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="メイも全部使いきったよん！&#xA;ってモモち、まだ半分も塗れてないじゃん！" speaker_show="True" stop="True" voice="ym11003_01_0476" />
    <delete obj_id="八千代 命" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ご、ごめんなさい！" speaker_show="True" stop="True" voice="mm11001_01_0668" />
