    <Comment comment="演出要確認かも" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002016" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="お待たせ。じゃあ早速、ムラが出ないように、&#xA;みんなで一斉にクリームを塗ってもらえる？" speaker_show="True" stop="True" voice="hy11002_01_0425" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="-200" y="0" anchor="Bottom" />
