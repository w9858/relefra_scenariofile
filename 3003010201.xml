    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="03_tsukikage_003" resource_type="Background" />
    <fade_out time="0" type="Default" />
    <msg window_type="SystemMsg" text="初芽はついに、裸にならなくても透明クリームの&#xA;効果を持続できるボンテージスーツを完成させた！" stop="True" />
    <msg window_type="SystemMsg" text="そして迎えた、潜入ミッションの日ーー。" stop="True" />
    <msg_window_show show="False" />
