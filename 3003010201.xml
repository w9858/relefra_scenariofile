    <create type="Character" obj_id="半蔵門 雪" data_id="311002016" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="しっかしユッキー、よく平気でいられるねえ。" speaker_show="True" stop="True" voice="ym11003_01_0473" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="え？" speaker_show="True" stop="True" voice="hy11002_01_0426" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="だって全員女子とはいえ、合計10本の手で&#xA;全身にクリームを塗り塗りされてるんだよ！" speaker_show="True" stop="True" voice="ym11003_01_0474" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="その方が、効果が均一になって良いでしょう。" speaker_show="True" stop="True" voice="hy11002_01_0427" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="まぁ、そうなんだけどさ……。&#xA;さすがはユッキー、平常心にもほどがあるぜ。" speaker_show="True" stop="True" voice="ym11003_01_0475" />
    <delete obj_id="八千代 命" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="相模 楓" data_id="311004003" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="よし、一番乗り！　アタシのチューブ&#xA;使い切りました！" speaker_show="True" stop="True" voice="sf11004_01_0509" />
    <move obj_id="相模 楓" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002016" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="競争じゃないのよ。けど、さすが楓ね。&#xA;ムラなくきれいに塗れているわ。" speaker_show="True" stop="True" voice="hy11002_01_0428" />
    <delete obj_id="相模 楓" />
    <delete obj_id="半蔵門 雪" />
