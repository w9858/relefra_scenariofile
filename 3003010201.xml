    <create type="Character" obj_id="源 モモ" data_id="311001003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_cheek obj_id="源 モモ" type="Cheek02_medium" loop="True" />
    <msg speaker="源 モモ" text="えっと、その……、何ていうか……。" speaker_show="True" stop="True" voice="mm11001_01_0670" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="モモ、しっかりなさい。" speaker_show="True" stop="True" voice="hy11002_01_0430" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="10_FaceBothHands" loop="True" to_idle="False" force_end="False" />
    <create_effect effect_id="Sweat_momo" effect_type="MampSweat" life_time="1.0" target_obj="源 モモ" />
    <msg speaker="源 モモ" text="すみません！　師匠のボンテージ姿が余りにも&#xA;セクシー過ぎるので、緊張してしまって……。" speaker_show="True" stop="True" voice="mm11001_01_0671" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="020_Facial_thinking" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="はぁ……。" speaker_show="True" stop="True" voice="hy11002_01_0431" />
