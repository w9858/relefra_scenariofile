    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="お願いフー、あの橋見に行かせて！&#xA;そしたら後はちゃんとするから！" speaker_show="True" stop="True" voice="ym11003_03_0159" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="本当ですか～？" speaker_show="True" stop="True" voice="sf11004_03_0174" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="本当、本当!!" speaker_show="True" stop="True" voice="ym11003_03_0160" />
