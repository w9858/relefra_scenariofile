    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="16_BendBackward" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="う……ま、まあ&#xA;そうかも知れませんけど。" speaker_show="True" stop="True" voice="sf11004_03_0173" />
