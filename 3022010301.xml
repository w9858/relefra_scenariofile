    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="相模 楓" text="さっきから何か見つける度に目移りして、&#xA;皆を連れ回してますよね。" speaker_show="True" stop="True" voice="sf11004_03_0172" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="だーって、面白いものが&#xA;たくさんあるんだもん！" speaker_show="True" stop="True" voice="ym11003_03_0157" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="せっかく紅葉狩りに来てるんだからさ、&#xA;隅から隅まで楽しまなくちゃ損でしょ？" speaker_show="True" stop="True" voice="ym11003_03_0158" />
