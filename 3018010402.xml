    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="せっかくですから、&#xA;背景にも凝ろうと思ったんです。" speaker_show="True" stop="True" voice="ah11005_02_0648" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="021_Facial_exciting" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="さあさあ、着替えてきてください、ごえちゃん。" speaker_show="True" stop="True" voice="ah11005_02_0649" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
