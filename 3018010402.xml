    <char_spine_animation obj_id="青葉 初芽" face_anim_type="021_Facial_exciting" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="はあああ～、やっぱり最高です!!&#xA;ごえちゃん!!" speaker_show="True" stop="True" voice="ah11005_02_0650" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="ありがとうございます、師匠。" speaker_show="True" stop="True" voice="ig11006_02_0384" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="(でも、やっぱりこの衣装、ちょっと恥ずかしいな……。)" speaker_show="True" stop="True" voice="ig11006_02_0385" lip_sync="False" />
    <char_spine_animation />
    <msg speaker="石川 五恵" text="(師匠と２人きりだから、いいけど。)" speaker_show="True" stop="True" voice="ig11006_02_0386" lip_sync="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="う～ん、悪魔っ娘ごえちゃんと教会のミスマッチ……&#xA;やはり私の目に狂いはありませんでした！" speaker_show="True" stop="True" voice="ah11005_02_0651" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ごえちゃんが天使であり、それを悪魔の衣装が包み、&#xA;背景は神聖なる教会……まさにアウフヘーベン！" speaker_show="True" stop="True" voice="ah11005_02_0652" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="017_Facial_shock" body_anim_type="16_BendBackward" force_end="False" loop="True" />
