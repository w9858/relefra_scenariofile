    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうですね、たとえば「悪魔と契約する人間」、&#xA;というシチュエーションフォトはどうでしょうか！" speaker_show="True" stop="True" voice="ah11005_02_0660" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" />
    <msg speaker="石川 五恵" text="(珍しいな……師匠がこんなにはしゃぐなんて。)" speaker_show="True" stop="True" voice="ig11006_02_0391" lip_sync="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" />
