<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/9020304001" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="寮 しぶきと葉栖美の部屋 35_shibukiandhasumiroom_01" />
    <bg show="True" file="35_shibukiandhasumiroom_01" />
    <fade_in time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="ある日の夜。しぶきと葉栖美は、&#xA;いつものように二人で食卓についていた。" stop="True" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004002" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="では、いただきます。" speaker_show="True" stop="True" voice="ss12003_02_0320" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="はーい、召し上がれ。" speaker_show="True" stop="True" voice="th12004_02_0328" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="むぐむぐ……うむ。&#xA;はっすーの料理は、いつもながら最高じゃ！" speaker_show="True" stop="True" voice="ss12003_02_0321" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ありがと。しぶきちはいつもそう言ってくれるから、&#xA;こっちも作り甲斐があるよー" speaker_show="True" stop="True" voice="th12004_02_0329" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="あ。そっちのは新作だから、感想教えて。" speaker_show="True" stop="True" voice="th12004_02_0330" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="了解じゃ。むぐむぐ……&#xA;うむ、山椒が利いていてとっても美味じゃの！" speaker_show="True" stop="True" voice="ss12003_02_0322" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="お。よかった～。" speaker_show="True" stop="True" voice="th12004_02_0331" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="……む。この炊き込みご飯は、&#xA;この前弁当に入っていたのと同じやつか？" speaker_show="True" stop="True" voice="ss12003_02_0323" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="轟 葉栖美" text="あ、うん。&#xA;そうだよ。" speaker_show="True" stop="True" voice="th12004_02_0332" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="つまみ食いしたゆらが、随分気に入っとった。&#xA;また弁当にいれてほしいとか言っておったぞ。" speaker_show="True" stop="True" voice="ss12003_02_0324" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="あはは。じゃあ、今度お弁当に入れる時は、&#xA;ちょっと多めにするよ。" speaker_show="True" stop="True" voice="th12004_02_0333" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ゆらちゃんに分けてあげて。" speaker_show="True" stop="True" voice="th12004_02_0334" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="薩摩 しぶき" text="うむ。すまんの。" speaker_show="True" stop="True" voice="ss12003_02_0325" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="なんのなんの。それにしても、&#xA;ゆらちゃんもすっかりうちらに馴染んだねー。" speaker_show="True" stop="True" voice="th12004_02_0335" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="飛粋ちゃんはとっくに溶け込んでたし、&#xA;四人体制も板についてきたって感じがするよ。" speaker_show="True" stop="True" voice="th12004_02_0336" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="そうじゃな……。あの二人が加入したことによって、&#xA;凪の部隊の火力は格段に上がったしの。" speaker_show="True" stop="True" voice="ss12003_02_0326" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="一人ひとりにかかる負担も減ったよね。&#xA;ほんと頼もしい限りだよ。" speaker_show="True" stop="True" voice="th12004_02_0337" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" lip_sync="False" text="（……でも、ちょっとだけしぶきちと&#xA;二人だった時のことも懐かしかったりして……）" speaker_show="True" stop="True" voice="th12004_02_0338" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" lip_sync="False" text="（なーんて、こんなこと言ったら、&#xA;しぶきちに叱られちゃうな）" speaker_show="True" stop="True" voice="th12004_02_0339" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="……はっすーと二人の時は大変じゃったが、&#xA;今となっては懐かしいの。" speaker_show="True" stop="True" voice="ss12003_02_0327" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="あの時があったからこそ、しぶきは今、&#xA;はっすーが一番の相棒だと思えるのじゃ。" speaker_show="True" stop="True" voice="ss12003_02_0328" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="——！" speaker_show="True" stop="True" voice="th12004_02_0340" />
    <move obj_id="薩摩 しぶき" position_id="left" x="-125" x_move_type="MoveTo" time="0.3" wait="False" />
    <move obj_id="轟 葉栖美" position_id="left" x="125" x_move_type="MoveTo" time="0.3" wait="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="もー！ なんだよ、しぶきちー!!&#xA;わしゃわしゃわしゃー!!" speaker_show="True" stop="True" voice="th12004_02_0341" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="011_Facial_surprise" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="薩摩 しぶき" text="わわっ、なんじゃなんじゃ！&#xA;いきなり頭をわしゃわしゃするでない!!" speaker_show="True" stop="True" voice="ss12003_02_0329" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="あはは、ごめんごめん。" speaker_show="True" stop="True" voice="th12004_02_0342" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="……うん。しぶきち、これからもよろしくね！" speaker_show="True" stop="True" voice="th12004_02_0343" />
  </command>
</adv>