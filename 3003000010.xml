    <bg show="True" file="07_containerni_02" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="コンテナターミナル" in_time="0.5" show_time="1.0" out_time="0.5" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001001" position_id="left" x="100" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" position_id="left" x="400" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002016" x="-400" />
    <char_alpha obj_id="半蔵門 雪" enable="True" quality="Default" />
    <char_alpha_value obj_id="半蔵門 雪" value="0.7" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="015_Facial_poker" force_end="False" loop="True" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="クンクンクンクン……。あれ？&#xA;やっぱり師匠が近くにいる！" speaker_show="True" stop="True" voice="mm11001_01_0638" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="だから幻でしょう？" speaker_show="True" stop="True" voice="sf11004_01_0500" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="違うの、この前は残り香レベルだったけど、&#xA;今度はすぐ近くにいる！" speaker_show="True" stop="True" voice="mm11001_01_0639" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="半蔵さんなんて、どこにもいないわよ？" speaker_show="True" stop="True" voice="sf11004_01_0501" />
    <move obj_id="半蔵門 雪" x="-100" x_move_type="MoveTo" time="0.5" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="モモ、あなたたちは何をしているの？" speaker_show="True" stop="True" voice="hy11002_01_0404" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="はぅっ、耳元に漂う甘い吐息と澄んだお声……。&#xA;幻なんかじゃない！　師匠、そこにいるんですね？" speaker_show="True" stop="True" voice="mm11001_01_0640" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="半蔵さん、もしかしてクリームで透明に&#xA;なってるんですか？　何か事件でも？" speaker_show="True" stop="True" voice="sf11004_01_0502" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="02_Idle" loop="True" />
    <msg speaker="半蔵門 雪" text="先に質問したのは私よ。答えてちょうだい。&#xA;あなたたちは何をしているの？" speaker_show="True" stop="True" voice="hy11002_01_0405" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="09_PutHands" to_idle="True" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="スーパーヒーローを待ってるんです！" speaker_show="True" stop="True" voice="mm11001_01_0641" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="半蔵門 雪" text="スーパーヒーロー？" speaker_show="True" stop="True" voice="hy11002_01_0406" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" to_idle="True" force_end="False" loop="False" />
    <msg speaker="源 モモ" text="はい、最近空崎が平和過ぎておかしいなと思って&#xA;みんなでパトロールをしてたんです。" speaker_show="True" stop="True" voice="mm11001_01_0642" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そしたら、コンテナターミナルで&#xA;人形の残骸を見つけちゃって。" speaker_show="True" stop="True" voice="mm11001_01_0643" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="015_Facial_poker" loop="True" />
    <msg speaker="半蔵門 雪" text="人形の残骸……それはもしかして……。" speaker_show="True" stop="True" voice="hy11002_01_0407" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="私たち以外に、あんな風に人形を倒せる人なんて&#xA;そうそういない。ということはですよ！" speaker_show="True" stop="True" voice="mm11001_01_0644" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="正義のスーパーヒーローに違いない！　ってことで、&#xA;みんなでおびき出すことにしたんです。" speaker_show="True" stop="True" voice="mm11001_01_0645" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="半蔵門 雪" text="なるほど、理解は出来たわ。&#xA;けどそれは……。" speaker_show="True" stop="True" voice="hy11002_01_0408" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、もしかして師匠もスーパーヒーローが見たくて、&#xA;透明になって隠れてたんですか？" speaker_show="True" stop="True" voice="mm11001_01_0646" />
    <delete obj_id="源 モモ" />
    <delete obj_id="相模 楓" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="八千代 命" data_id="311003009" position_id="left" x="0" y="0" anchor="Bottom" />
    <Comment comment="命、ヴィラン風衣装の予定" />
