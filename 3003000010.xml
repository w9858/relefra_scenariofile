    <create type="Character" obj_id="源 モモ" data_id="311001001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create obj_id="半蔵門 雪" data_id="311002016" />
    <char_alpha obj_id="半蔵門 雪" enable="True" quality="Default" />
    <char_alpha_value obj_id="半蔵門 雪" value="0.7" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="どんな人でしたか？　むきむきのマッチョ？&#xA;それとも師匠のように強くて美しい人？" speaker_show="True" stop="True" voice="mm11001_01_0647" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="18_ArmsCrossed" loop="True" />
    <msg speaker="半蔵門 雪" text="期待させてしまって申し訳ないけど……、&#xA;恐らくそれは私よ。" speaker_show="True" stop="True" voice="hy11002_01_0409" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ええええ！" speaker_show="True" stop="True" voice="mm11001_01_0648" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そんな、憧れのスーパーヒーロー様の正体が&#xA;憧れの師匠だなんて……。" speaker_show="True" stop="True" voice="mm11001_01_0649" />
    <msg_window_show show="False" />
    <wait time="0.5" />
    <sound cue_name="Clash" type="Se" volume="100" />
    <Comment comment="【SE】ゴツンとぶつかる音" />
    <wait time="1.0" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="10_FaceBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="いたっ！　何もないのに頭打ったぁ～。&#xA;って、もしや今のは……。" speaker_show="True" stop="True" voice="mm11001_01_0650" />
    <msg speaker="半蔵門 雪" text="それも私よ。" speaker_show="True" stop="True" voice="hy11002_01_0410" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ごめんなさい、師匠！&#xA;私ったらパニクっちゃって……。" speaker_show="True" stop="True" voice="mm11001_01_0651" />
    <msg_window_show show="False" />
    <char_alpha_value obj_id="半蔵門 雪" value="0.8" time="1.0" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="あなたからは見えないのだから、仕方ないわ。" speaker_show="True" stop="True" voice="hy11002_01_0411" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="え、えええ！？　師匠、&#xA;透明クリームの効き目が切れてきてます！" speaker_show="True" stop="True" voice="mm11001_01_0652" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="あら、本当ね。&#xA;今回は予想以上に持続時間が短かったわね。" speaker_show="True" stop="True" voice="hy11002_01_0412" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="何でそんなに冷静なんですか！　あわあわあわ……。&#xA;どうしよう。何か着る物……わーん、ないよ～！" speaker_show="True" stop="True" voice="mm11001_01_0653" />
    <char_alpha_value obj_id="半蔵門 雪" value="0.9" time="1" wait="True" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="10_FaceBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="きゃー！　だんだんくっきり見えてきた～！&#xA;だめー！　裸が見えちゃうーーーー！" speaker_show="True" stop="True" voice="mm11001_01_0654" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="02_Idle" force_end="False" loop="True" face_anim_type="011_Facial_surprise" />
    <msg speaker="源 モモ" text="ほへ？" speaker_show="True" stop="True" voice="mm11001_01_0655" />
    <msg_window_show show="False" />
    <char_alpha_value obj_id="半蔵門 雪" value="1" time="1.0" wait="True" />
    <char_alpha obj_id="半蔵門 雪" enable="False" quality="Default" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="完全にクリームの効果が切れたようね。" speaker_show="True" stop="True" voice="hy11002_01_0413" />
    <msg speaker="源 モモ" text="は、裸じゃない……。" speaker_show="True" stop="True" voice="mm11001_01_0656" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="14_Positive" to_idle="True" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" to_idle="True" force_end="False" loop="True" face_anim_type="016_Facial_smileBitter" />
    <msg speaker="源 モモ" text="良かった～。&#xA;けど、師匠、そのセクシーなお衣装は一体……？" speaker_show="True" stop="True" voice="mm11001_01_0657" />
