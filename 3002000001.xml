    <create type="Character" obj_id="相模 楓" data_id="311004003" position_id="left" x="-200" />
    <create type="Character" obj_id="八千代 命" data_id="311003003" position_id="left" x="200" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フー、ちょっと待っておくれよ～。&#xA;メイもう歩けないよ……。" speaker_show="True" stop="True" voice="ym11003_01_0330" lip_sync="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あの程度の訓練で疲れるなんて、&#xA;師匠もまだまだですね。" speaker_show="True" stop="True" voice="sf11004_01_0403" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーだってさっきまで、&#xA;もう動けない～とか言ってたじゃんか！" speaker_show="True" stop="True" voice="ym11003_01_0331" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" to_idle="True" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek02_medium" loop="False" />
    <msg speaker="相模 楓" text="そっ、それは……。普段よりはハードだったから。&#xA;ともかく早くお風呂に入って疲れを癒しますよ。" speaker_show="True" stop="True" voice="sf11004_01_0404" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" to_idle="True" force_end="False" loop="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek01_default" loop="False" />
    <msg_window_show show="False" />
    <Comment comment="北風の吹く音(既存SEない…？？)" />
    <sound type="Se" />
    <wait time="0.5" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <shake target="Character" obj_id="八千代 命" x="10" vibrate="50" time="0.5" />
    <msg speaker="八千代 命" text="ぶるるっ。何でこんなに寒いんだよ～。&#xA;このままじゃ凍え死んじゃう。" speaker_show="True" stop="True" voice="ym11003_01_0332" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="空崎のあたりに寒波が到来してるんですよ。&#xA;……それにしても、寒い……。" speaker_show="True" stop="True" voice="sf11004_01_0405" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そうだ、師匠！　体を冷やさないために、&#xA;銭湯までダッシュで行きましょう！" speaker_show="True" stop="True" voice="sf11004_01_0406" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="16_BendBackward" to_idle="True" loop="True" face_anim_type="005_Facial_sad" />
    <msg speaker="八千代 命" text="えー、お風呂の前に更に疲れるなんて、&#xA;あんびりーばぼにも程があるよ～！" speaker_show="True" stop="True" voice="ym11003_01_0333" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="03_BackBothHands" to_idle="True" loop="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="じゃあ師匠は、この寒さの中で、&#xA;銭湯までの道を耐え切れるんですか？" speaker_show="True" stop="True" voice="sf11004_01_0407" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="寒いのもイヤだけど、疲れるのもイヤだよ～。&#xA;何とかしてくれよ～、頼れる我が弟子～！" speaker_show="True" stop="True" voice="ym11003_01_0334" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" face_anim_type="005_Facial_sad" />
    <msg speaker="相模 楓" text="こんな時だけ持ち上げないでください。&#xA;もっと近くに他の銭湯があれば良いんだけど……。" speaker_show="True" stop="True" voice="sf11004_01_0408" />
    <create_effect effect_id="left_mei" effect_type="MampLight" life_time="1.7" target_obj="八千代 命" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="そうだ！　それだよ！&#xA;メイ、すっごくいいこと思いついたよ！" speaker_show="True" stop="True" voice="ym11003_01_0335" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="確かこの近くに、&#xA;でっかいスーパー銭湯があったよね！" speaker_show="True" stop="True" voice="ym11003_01_0336" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="あるにはありますけど……、&#xA;あそこは普通の銭湯よりも高いんですよ。" speaker_show="True" stop="True" voice="sf11004_01_0409" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="15_Negative" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" to_idle="True" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="えー、ちょっとくらい高くても、たまには&#xA;色んなお風呂に入ってみたいよ～。" speaker_show="True" stop="True" voice="ym11003_01_0337" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あそこなら露天風呂もサウナもあるから、&#xA;フーも絶対楽しめるって！" speaker_show="True" stop="True" voice="ym11003_01_0338" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="13_Question" force_end="False" loop="True" face_anim_type="020_Facial_thinking" />
    <msg speaker="相模 楓" text="うーん……。" speaker_show="True" stop="True" voice="sf11004_01_0410" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" to_idle="True" force_end="False" loop="True" body_anim_type="06_ChestLeftHand" />
    <create_effect effect_id="kirakira_mei" effect_type="EffectKirakira" life_time="7.0" target_obj="八千代 命" />
    <msg speaker="八千代 命" text="想像してごらん、こんな寒い日に、フーとメイとで&#xA;露天風呂……。極楽だと思わないかい？" speaker_show="True" stop="True" voice="ym11003_01_0339" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="（うむむ……。師匠と一緒に露天風呂か……。&#xA;確かにちょっと高いんだけど、悪くないわね）" speaker_show="True" stop="True" voice="sf11004_01_0411" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="05_HipBothHands" force_end="False" loop="True" face_anim_type="015_Facial_poker" />
    <msg speaker="相模 楓" text="しょうがないですね。寒風に晒されて&#xA;風邪引いちゃうよりは安くつきますし。" speaker_show="True" stop="True" voice="sf11004_01_0412" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" to_idle="True" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="そうと決まれば、早く行きましょう！" speaker_show="True" stop="True" voice="sf11004_01_0413" />
    <msg_window_show show="False" />
