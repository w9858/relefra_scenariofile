    <char_spine_animation obj_id="源 モモ" body_anim_type="07_ChestBothHands" loop="False" to_idle="False" />
    <msg speaker="源 モモ" text="大変大変っ！　大変だよー！" speaker_show="True" stop="True" voice="mm11001_01_0689" />
