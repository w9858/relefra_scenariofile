    <char_spine_animation obj_id="相模 楓" body_anim_type="19_Amazed" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="何だ、そんなこと？　人騒がせねえ。" speaker_show="True" stop="True" voice="sf11004_01_0511" />
