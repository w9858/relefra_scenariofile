    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="白虎" text="花見？　何だそれは？&#xA;私も初めて聞いたけど、美味いのか？" speaker_show="True" stop="True" voice="bk11008_01_0098" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="白虎ちゃんは、食べ物の話ばっかりね。" speaker_show="True" stop="True" voice="kt11009_01_0298" />
    <char_spine_animation obj_id="白虎" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="白虎" text="食う、寝る、遊ぶ！&#xA;それより大事なことなんて、あまりないだろ？" speaker_show="True" stop="True" voice="bk11008_01_0099" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="白虎" text="で、どうなんだ？　その花見ってやつは、&#xA;美味いのか？　マズいのか？" speaker_show="True" stop="True" voice="bk11008_01_0100" />
