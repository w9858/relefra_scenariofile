    <create type="Character" anchor="Bottom" obj_id="半蔵門 雪" data_id="311002005" position_id="left" x="-0" />
    <create type="Character" anchor="Bottom" obj_id="青葉 初芽" data_id="311005005" position_id="left" x="-250" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="モモ、どうしたの？&#xA;何か事件でも？" speaker_show="True" stop="True" voice="hy11002_01_0471" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="源 モモ" text="はっ、師匠。すみません。&#xA;そういうことではないんですけど……。" speaker_show="True" stop="True" voice="mm11001_01_0690" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="けど、モモちゃんが慌てて入ってくるだなんて、&#xA;何かあったんですよね？　教えてください。" speaker_show="True" stop="True" voice="ah11005_01_0291" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="公園の桜が、咲き始めてたんです！&#xA;あと少しで満開になりそうだったからつい。" speaker_show="True" stop="True" voice="mm11001_01_0691" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="源 モモ" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" anchor="Bottom" obj_id="相模 楓" data_id="311004005" position_id="left" x="0" />
