    <create type="Character" anchor="Bottom" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-300" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" loop="True" to_idle="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="だから食べ物じゃないんだって……。" speaker_show="True" stop="True" voice="sf11004_01_0512" />
