    <char_spine_animation obj_id="白虎" face_anim_type="102_Facial_unique" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="白虎" text="ふぁ～眠い。" speaker_show="True" stop="True" voice="bk11008_01_0096" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="白虎、あなた最近寝てばかりね。&#xA;少しは店の手伝いでもしたらどうなの？" speaker_show="True" stop="True" voice="hy11002_01_0470" />
    <char_spine_animation obj_id="白虎" face_anim_type="019_Facial_damage" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <char_spine_eye obj_id="白虎" type="Eye03_tear" loop="True" />
    <msg speaker="白虎" text="うるさい！&#xA;春眠アカスリマッサージだ。" speaker_show="True" stop="True" voice="bk11008_01_0097" />
