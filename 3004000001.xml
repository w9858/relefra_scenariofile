    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="テレジア" text="花見？" speaker_show="True" stop="True" voice="tr11007_01_0118" />
