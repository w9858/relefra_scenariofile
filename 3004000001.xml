    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="春眠暁を覚えず、のことかしら？" speaker_show="True" stop="True" voice="kt11009_01_0297" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="石川 五恵" text="けど確かに、これだけのポカポカ陽気だと&#xA;眠くなっちゃうのもわかるよ。" speaker_show="True" stop="True" voice="ig11006_01_0315" />
