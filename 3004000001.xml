    <delete obj_id="カトリーナ" />
    <create type="Character" anchor="Bottom" obj_id="石川 五恵" data_id="311006005" position_id="left" x="0" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="石川 五恵" text="二択で答えるなら……、美味しいかな。" speaker_show="True" stop="True" voice="ig11006_01_0316" />
