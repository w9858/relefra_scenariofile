<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3013020202" />
  </info>
  <command>
    <Start />
    <load_cue_sheet />
    <sound cue_name="Adv1" type="Bgm" />
    <sound type="Se" volume="15" cue_name="EnvSeaside" />
    <bg file="61_Seasideev_01" show="True" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="海岸" in_time="1" show_time="2" out_time="1" wait="True" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="-200" y="0" anchor="Bottom" type="Character" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="しかしお前がそんなに肝冷やしながら、&#xA;やり遂げるとは、思わなかったぜ。" speaker_show="True" stop="True" voice="ty12005_02_0074" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="だよな。アタシ様も同感だ。" speaker_show="True" stop="True" voice="ry12002_02_0148" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="仲間のためなら何でもできるってか？&#xA;なかなかいい心構えじゃねーか。" speaker_show="True" stop="True" voice="ty12005_02_0075" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="そんなんじゃねーよ。ぶっちゃけ最初は&#xA;自分が楽しみたくて思いついたんだよ。" speaker_show="True" stop="True" voice="ry12002_02_0149" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="３人のバースデーパーティを開いたら&#xA;すげぇ楽しいだろうなってさ。" speaker_show="True" stop="True" voice="ry12002_02_0150" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="で、どうせ楽しむならド派手に沢山の人から祝われた&#xA;ほうが嬉しいんじゃないか、とか思っちまって。" speaker_show="True" stop="True" voice="ry12002_02_0151" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="テンション上がって柄にもなく、学校の奴らに&#xA;相談しちまっただけなんだよ。" speaker_show="True" stop="True" voice="ry12002_02_0152" />
    <char_spine_animation force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="３人のため、っつーかアタシ様が楽しくて&#xA;突っ走ってただけなんだよな。" speaker_show="True" stop="True" voice="ry12002_02_0153" />
    <char_spine_animation force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="いいじゃねーか、それでも。皆が協力してくれたのは&#xA;お前が楽しそうに提案したからだろ？" speaker_show="True" stop="True" voice="ty12005_02_0076" />
    <char_spine_animation force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="思い立ったらやってみる勢いも大事なんだよ。&#xA;てめーはそれを本能でわかってたんだろ。" speaker_show="True" stop="True" voice="ty12005_02_0077" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="まぁそうなんだけどさ。&#xA;自分でも驚いたぜ。" speaker_show="True" stop="True" voice="ry12002_02_0154" />
    <msg speaker="乱獅子 ゆら" text="誕生日会やりたいから手伝ってくれって、&#xA;自分からクラスの奴らに声かけたんだもんな。" speaker_show="True" stop="True" voice="ry12002_02_0155" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="元大八洲のアタシ様がだぜ？" speaker_show="True" stop="True" voice="ry12002_02_0156" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="冷静になって考えると、&#xA;今でも信じられねーぜ。" speaker_show="True" stop="True" voice="ry12002_02_0157" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="でも、やってよかっただろ？" speaker_show="True" stop="True" voice="ty12005_02_0078" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="まぁな。クラッカー鳴らした瞬間の３人の&#xA;驚いた顔とか、その後の嬉しそうな顔とか……" speaker_show="True" stop="True" voice="ry12002_02_0158" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="あんな顔されたら、&#xA;こっちまで嬉しくなっちまうだろ？" speaker_show="True" stop="True" voice="ry12002_02_0159" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="お陰で不安なんて一瞬で吹っ飛んでたよ。" speaker_show="True" stop="True" voice="ry12002_02_0160" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="14_Positive" force_end="False" loop="False" mixing="True" />
    <msg speaker="津守 ヨモギ" text="じゃあいいじゃねぇか。&#xA;成功おめでとう、ご苦労さん。" speaker_show="True" stop="True" voice="ty12005_02_0079" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <char_spine_cheek obj_id="乱獅子 ゆら" type="Cheek02_medium" loop="True" />
    <msg speaker="乱獅子 ゆら" text="んだよ、褒められても何もでねーぞ。&#xA;アタシ様はお祝いで全力出しつくしたからな。" speaker_show="True" stop="True" voice="ry12002_02_0161" />
    <char_spine_animation force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="あー、何か暑くなってきた！&#xA;アタシ様も海に入ってくる。じゃあな。" speaker_show="True" stop="True" voice="ry12002_02_0162" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="-1500" x_move_type="MoveValue" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <move obj_id="津守 ヨモギ" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1" wait="False" />
    <delete obj_id="乱獅子 ゆら" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="俺の目もまだまだ節穴じゃねえようだな。&#xA;凪にゆらを入れたのは間違いじゃなかった。" speaker_show="True" stop="True" voice="ty12005_02_0080" />
    <msg_window_show show="False" />
    <fade_out time="1" type="Default" wait="True" />
    <sound_stop cue_name="EnvSeaside" />
    <sound_stop cue_name="Adv1" />
  </command>
</adv>