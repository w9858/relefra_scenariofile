    <function_menu_show show="False" />
    <auto_mode active="True" />
    <bg resource_type="Background" file="04_shopstreetmo_01" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" loop="True" />
    <fade_in time="1" type="Default" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="今日も寒いね。どこか行くの？" speaker_show="True" stop="True" voice="01_01" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" loop="True" />
    <msg speaker="源 モモ" text="私は年越し蕎麦を買いに行くところなんだけど、&#xA;ちょっと悩んでるんだー。" speaker_show="True" stop="True" voice="01_02" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" />
    <msg speaker="源 モモ" text="年越しにお蕎麦じゃなくって、&#xA;うどんを食べるところもあるんだって聞いて。" speaker_show="True" stop="True" voice="01_03" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" loop="True" />
    <msg speaker="源 モモ" text="スパイスの効いたカレーにも合いそうだし、&#xA;どっちにしようかなって迷っちゃって。" speaker_show="True" stop="True" voice="01_04" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="んー……せっかくだし、&#xA;今年はうどんにしようかな。" speaker_show="True" stop="True" voice="01_05" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" />
    <msg speaker="源 モモ" text="それにしても、もう今年も終わりなんだね。&#xA;早いなぁ～。" speaker_show="True" stop="True" voice="01_06" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="020_Facial_thinking" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="源 モモ" text="ツキカゲに入って……師匠に鍛えてもらって……。" speaker_show="True" stop="True" voice="01_07" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" loop="True" />
    <msg speaker="源 モモ" text="まだまだ一人前とは言えないけど、&#xA;来年こそ立派なスパイになってみせる！" speaker_show="True" stop="True" voice="01_08" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="源 モモ" text="よぉーし、新たなミッションに向けて、滾っていくよ！" speaker_show="True" stop="True" voice="01_09" />
    <msg speaker="源 モモ" text="それじゃあ、良いお年を！" speaker_show="True" stop="True" voice="01_10" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" />
    <msg speaker="源 モモ" text="リリース ザ スパイス シークレット フレグランス、&#xA;事前登録受付中！" speaker_show="True" stop="True" voice="01_11" />
