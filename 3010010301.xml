    <msg speaker="八千代 命" text="おぉー、これが本物のウェディングドレス！" speaker_show="True" stop="True" voice="ym11003_02_0281" />
    <delete obj_id="Smile_mei" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="20_PositiveDouble" force_end="False" loop="False" mixing="True" />
    <msg speaker="八千代 命" text="まごうことなき純白だね。&#xA;すっごく綺麗。" speaker_show="True" stop="True" voice="ym11003_02_0282" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="010_Facial_shy" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="うん、なんだか&#xA;信たちが触ってもいいのかなって思っちゃう。" speaker_show="True" stop="True" voice="ks11010_02_0145" />
    <msg speaker="チャペルスタッフ" text="どうぞ、ご遠慮なさらずに&#xA;触れてみてください。" speaker_show="True" stop="True" voice="99091_02_0001" />
    <delete obj_id="高坂 信" />
    <move obj_id="八千代 命" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="では、さっそく失礼して……。" speaker_show="True" stop="True" voice="ym11003_02_0283" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <create_effect effect_id="Smile_mei" effect_type="MampSmile" life_time="1" target_obj="八千代 命" x="0" y="0" attach_head_bone="True" />
    <Comment comment="SE:生地に触る音（多分すべすべの生地）" />
    <sound type="Se" />
    <msg speaker="八千代 命" text="ふわー、すべすべ！&#xA;すべすべだよ、フー!!" speaker_show="True" stop="True" voice="ym11003_02_0284" />
    <delete obj_id="Smile_mei" />
    <move obj_id="八千代 命" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="これ無限に触ってられるやつだ、&#xA;えへへ～。" speaker_show="True" stop="True" voice="ym11003_02_0285" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" face_anim_type="002_Facial_smile" />
    <msg speaker="相模 楓" text="ドレスって、見たことはあっても&#xA;実際手に取ると違いますよね。" speaker_show="True" stop="True" voice="sf11004_02_0276" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="気持ちが引き締まるっていうか。" speaker_show="True" stop="True" voice="sf11004_02_0277" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="引き締まるの分かるー！&#xA;メイも今そうだもん。" speaker_show="True" stop="True" voice="ym11003_02_0286" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="ウェディングドレスを着た２人を幸せにしなくちゃって&#xA;使命感が湧いてくるね！" speaker_show="True" stop="True" voice="ym11003_02_0287" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek02_medium" loop="True" />
    <msg speaker="相模 楓" text="も、もうっ。&#xA;また師匠はそういうこと言う……。" speaker_show="True" stop="True" voice="sf11004_02_0278" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="メイちゃんが旦那さまっていうポジションは&#xA;確定なんだね。" speaker_show="True" stop="True" voice="ks11010_02_0146" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="右手にフー、左手に師匠……&#xA;フフフ、いいでしょ？" speaker_show="True" stop="True" voice="ym11003_02_0288" />
    <msg speaker="チャペルスタッフ" text="お時間が来ましたので、それぞれの試着室で&#xA;スタッフがお着替えのサポートをさせて頂きます。" speaker_show="True" stop="True" voice="99091_02_0002" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="お願いしまーす！&#xA;確かにこれ、１人じゃ着れないよね。" speaker_show="True" stop="True" voice="ym11003_02_0289" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="師匠、ドレスぐちゃぐちゃにしちゃ&#xA;ダメですよ？" speaker_show="True" stop="True" voice="sf11004_02_0279" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="しないもーん。&#xA;スタッフさんに身を委ねるから大丈夫だもーん。" speaker_show="True" stop="True" voice="ym11003_02_0290" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="フーこそ、&#xA;メイの着替えを覗かないでよね？" speaker_show="True" stop="True" voice="ym11003_02_0291" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" mixing="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek02_medium" loop="True" />
    <msg speaker="相模 楓" text="の、覗いたりなんてしませんっ！" speaker_show="True" stop="True" voice="sf11004_02_0280" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="ふふっ。&#xA;メイちゃん、からかわないの。" speaker_show="True" stop="True" voice="ks11010_02_0147" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="はーい。" speaker_show="True" stop="True" voice="ym11003_02_0292" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="じゃ、師匠。フー。&#xA;また後でね！" speaker_show="True" stop="True" voice="ym11003_02_0293" />
