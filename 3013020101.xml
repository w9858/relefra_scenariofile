  <command>
    <Start />
    <load_cue_sheet />
    <sound cue_name="Adv1" type="Bgm" />
    <fade_out time="0" type="Default" wait="False" />
    <msg window_type="SystemMsg" text="飛粋たちは、黒百合女子の学校行事、&#xA;臨海学校のため、海に来ていた。" stop="True" />
    <msg window_type="SystemMsg" text="緊急任務も無事終えて、ようやくみんなで&#xA;海遊びをする時間が訪れて……" stop="True" />
    <msg_window_show show="False" />
    <bg show="True" resource_type="Background" file="21_island_01" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="海岸" in_time="1" show_time="2" out_time="1" wait="True" />
    <create obj_id="宗近 飛粋" data_id="312001008" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <create_effect effect_id="his_" effect_type="MampSmile" life_time="2" target_obj="宗近 飛粋" />
    <msg speaker="宗近 飛粋" text="見て見て、ゆら！&#xA;小さい魚がいっぱいだよ！" speaker_show="True" stop="True" voice="mh12001_02_0169" />
    <delete obj_id="宗近 飛粋" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="おい、待てよ飛粋！　ったく、&#xA;よくこんな岩場で走り回れるな。" speaker_show="True" stop="True" voice="ry12002_02_0115" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="宗近 飛粋" data_id="312001008" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ゆら、早く早く～！&#xA;急がないと魚が逃げちゃうよー。" speaker_show="True" stop="True" voice="mh12001_02_0170" />
    <delete obj_id="宗近 飛粋" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="わーってるよ！　わーってるけど、クソッ。&#xA;足元がゴツゴツしてて歩きづれえんだよ。" speaker_show="True" stop="True" voice="ry12002_02_0116" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="宗近 飛粋" data_id="312001008" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="あははっ、転ばないように気をつけてね。" speaker_show="True" stop="True" voice="mh12001_02_0171" />
    <delete obj_id="宗近 飛粋" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="くっ……&#xA;アタシ様をこんなに苦戦させるなんて……" speaker_show="True" stop="True" voice="ry12002_02_0117" />
    <Comment comment="ズルッと滑る音" />
    <sound type="Se" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="ぐおぉっ！　何だこれ？&#xA;足元がメチャクチャ滑る！" speaker_show="True" stop="True" voice="ry12002_02_0118" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="宗近 飛粋" data_id="312001008" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ゆら！　海苔の生えてるところは滑るから、&#xA;よけてみて！" speaker_show="True" stop="True" voice="mh12001_02_0172" />
    <delete obj_id="宗近 飛粋" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="0" y="0" anchor="Bottom" type="Character" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="おう、やってみる。&#xA;はぁ、本当だ、さっきよりだいぶ歩きやすい。" speaker_show="True" stop="True" voice="ry12002_02_0119" />
    <delete obj_id="乱獅子 ゆら" />
    <create obj_id="宗近 飛粋" data_id="312001008" position_id="left" x="-200" y="0" anchor="Bottom" type="Character" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="1500" y="0" anchor="Bottom" type="Character" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="12_DropShoulders" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="はぁ、ようやく追いついた。&#xA;で、魚はどこだ？" speaker_show="True" stop="True" voice="ry12002_02_0120" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="ほら、この水たまりのとこ。" speaker_show="True" stop="True" voice="mh12001_02_0173" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="本当だ。小ちゃいのがいっぱいいるな！" speaker_show="True" stop="True" voice="ry12002_02_0121" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="それにしても、飛粋は本当に&#xA;生き物を見つけるのが得意だよなぁ。" speaker_show="True" stop="True" voice="ry12002_02_0122" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="そうかなぁ。&#xA;たまたま覗き込んだら居ただけで……あっ！" speaker_show="True" stop="True" voice="mh12001_02_0174" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="ん？　どうした？" speaker_show="True" stop="True" voice="ry12002_02_0123" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="あっちの岩陰にいるの、イソギンチャクだよ！" speaker_show="True" stop="True" voice="mh12001_02_0175" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="やっぱすげーよ。さすがのアタシ様も&#xA;そんなにすぐには見つけられねえ。" speaker_show="True" stop="True" voice="ry12002_02_0124" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="にしてもでっかいイソギンチャクだなぁ。&#xA;これがウニとかだったら大歓迎なのに。" speaker_show="True" stop="True" voice="ry12002_02_0125" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="種類によるんだけどね。&#xA;食べれるのは食べれるんだよ。" speaker_show="True" stop="True" voice="mh12001_02_0176" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="イソギンチャクを……食べる？" speaker_show="True" stop="True" voice="ry12002_02_0126" />
  </command>
