<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/4000020401" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <Comment comment="黒百合　寮　部屋" />
    <bg show="True" file="35_shibukiandhasumiroom_01" resource_type="Background" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004004" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="はーい皆こんちわーっ。&#xA;凪の部隊所属、轟　葉栖美だよーっ。" speaker_show="True" stop="True" voice="th12004_01_0160" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="今回はアタシの自己紹介コーナーということで&#xA;ちょっくら付き合ってね。" speaker_show="True" stop="True" voice="th12004_01_0161" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="身長は１６０センチのＡ型。黒百合の３年生。&#xA;ムテキの女子高生だよ。あ、出身地は新潟ね。" speaker_show="True" stop="True" voice="th12004_01_0162" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="趣味は、料理かな。&#xA;作っているうちに凝ってきたんだ。" speaker_show="True" stop="True" voice="th12004_01_0163" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="凪の部隊でのコードネームは孔雀。&#xA;特技の関係でサポートに回る事が多いかな。" speaker_show="True" stop="True" voice="th12004_01_0164" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="一応、隊長であるしぶきちの補佐をしてるから&#xA;副隊長的な立場にはいるのかな？" speaker_show="True" stop="True" voice="th12004_01_0165" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="で、特技っていうのは、動物と意思疎通が&#xA;出来るってことなんだ。いやいやマジで。" speaker_show="True" stop="True" voice="th12004_01_0166" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="例えばネズミに、地下から潜り込んで&#xA;配線コードかじってきて、とかね。" speaker_show="True" stop="True" voice="th12004_01_0167" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="もちろん、その際はお礼のゴハンとかあげるよ。" speaker_show="True" stop="True" voice="th12004_01_0168" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="動物は種類が多いから、応用も利くわけ。" speaker_show="True" stop="True" voice="th12004_01_0169" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="……なんかこうして自分の特技、説明するの&#xA;微妙に恥ずいね。しぶきちとか平気だろうけど。" speaker_show="True" stop="True" voice="th12004_01_0170" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="ごほん、ちょっと自分語り入るよー。" speaker_show="True" stop="True" voice="th12004_01_0171" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="そもそもコトの起こりは両親の離婚からだったんだ。" speaker_show="True" stop="True" voice="th12004_01_0172" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="父親が駄目男でさ。一人になったら&#xA;死んじゃいそうだし。アタシは父親側についたのよ、" speaker_show="True" stop="True" voice="th12004_01_0173" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="なんか見捨てられないのよね。" speaker_show="True" stop="True" voice="th12004_01_0174" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="こういう性格してるからか、家には父親以外にも&#xA;捨て犬や捨て猫や捨てトカゲがいたりしてさ。" speaker_show="True" stop="True" voice="th12004_01_0175" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="同じ事言うけど、&#xA;…なんか放っておけないじゃない。" speaker_show="True" stop="True" voice="th12004_01_0176" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="まとめて面倒見ているウチに、なんだか動物たちと&#xA;自然と意思疎通できるようになってたんだ。" speaker_show="True" stop="True" voice="th12004_01_0177" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="で、アタシの特技を聞きつけた&#xA;凪の部隊がスカウトに来たと。" speaker_show="True" stop="True" voice="th12004_01_0178" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="世のため人のために自分の特技が活かせるのは&#xA;悪い気がしなかったけど……。" speaker_show="True" stop="True" voice="th12004_01_0179" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="アタシがいなくなると父親が心配だから&#xA;断ったんだよ。そしたら話を聞いていた父親が――。" speaker_show="True" stop="True" voice="th12004_01_0180" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="このままじゃお前は俺に人生縛られる、&#xA;俺は成長してみせるからお前は行けと言ったのさ。" speaker_show="True" stop="True" voice="th12004_01_0181" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="いやーあの時は我が父ながら&#xA;カッコ良かったねぇ。見直したよー。" speaker_show="True" stop="True" voice="th12004_01_0182" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="ってなわけで、今に至ると。" speaker_show="True" stop="True" voice="th12004_01_0183" />
    <transition effect="Shutter" type="Out" time="0.5" wait="True" />
    <delete obj_id="轟 葉栖美" />
    <msg speaker="轟 葉栖美" text="凪の制服を見せてあげる。" speaker_show="True" stop="True" voice="th12004_01_0184" />
    <Comment comment="葉栖美・凪の部隊の服" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="0" y="0" anchor="Bottom" />
    <transition effect="Shutter" type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="ほい、これ！　スーツ気にいってるんだよね。" speaker_show="True" stop="True" voice="th12004_01_0185" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="しぶきち…隊長は、お色気を振りまく任務とかだと&#xA;アタシを指名するから、体型には気をつけてるよ。" speaker_show="True" stop="True" voice="th12004_01_0186" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="まぁアタシはアタッカーじゃないからね。&#xA;そういう小技で貢献していかないと。" speaker_show="True" stop="True" voice="th12004_01_0187" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="そんな感じで日夜、青春したり国を守ったりしてるよ。&#xA;親友のしぶきち達と一緒にね。" speaker_show="True" stop="True" voice="th12004_01_0188" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="これからも宜しくねー！&#xA;聞いてくれて、アリガト！！" speaker_show="True" stop="True" voice="th12004_01_0189" />
  </command>
</adv>