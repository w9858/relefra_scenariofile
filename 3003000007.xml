    <msg speaker="半蔵門 雪" text="はぁ……はぁ……。" speaker_show="True" stop="True" voice="hy11002_01_0387" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek02_medium" loop="True" />
    <msg speaker="青葉 初芽" text="ミッション完了。お疲れ様でし……まぁ！&#xA;雪ちゃん、いつにも増してセクシー。" speaker_show="True" stop="True" voice="ah11005_01_0194" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <create_effect effect_id="hatume_Heart" effect_type="EffectHeart" life_time="3.0" target_obj="青葉 初芽" />
    <msg speaker="青葉 初芽" text="密着したスーツに滴る汗、荒ぶる吐息！　何だか&#xA;見てはいけないものを見ているような気分です。" speaker_show="True" stop="True" voice="ah11005_01_0195" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="はぁ……はぁ……。初芽、ふざけてないで、&#xA;まずは水分補給をさせてちょうだい。" speaker_show="True" stop="True" voice="hy11002_01_0388" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek01_default" loop="True" />
    <msg speaker="青葉 初芽" text="す、すみません。あまりに妖艶な魅力につい。&#xA;はい、お水をどうぞ。" speaker_show="True" stop="True" voice="ah11005_01_0196" />
    <move obj_id="青葉 初芽" x="125" x_move_type="MoveTo" time="0.5" wait="True" />
    <Comment comment="水を飲む音" />
    <sound type="Se" />
    <wait time="1.0" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <char_spine_cheek obj_id="半蔵門 雪" type="Cheek01_default" loop="True" />
    <msg speaker="半蔵門 雪" text="はぁ、生き返ったわ。" speaker_show="True" stop="True" voice="hy11002_01_0389" />
    <move obj_id="青葉 初芽" x="200" x_move_type="MoveTo" time="0.5" wait="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="珍しいですね。雪ちゃんがあの程度の戦闘で&#xA;汗だくになるだなんて。" speaker_show="True" stop="True" voice="ah11005_01_0197" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="自分でも驚いたわ。" speaker_show="True" stop="True" voice="hy11002_01_0390" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="11_Point" force_end="False" loop="True" face_anim_type="015_Facial_poker" />
    <msg speaker="青葉 初芽" text="もしかして、改良したボンテージスーツ&#xA;プロトタイプ２号の影響でしょうか？" speaker_show="True" stop="True" voice="ah11005_01_0198" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="前回のタイプ１に比べて、動きやすさは大幅に&#xA;改善されたと思うわ。けど通気性がね。" speaker_show="True" stop="True" voice="hy11002_01_0391" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="通気性？　ですか。" speaker_show="True" stop="True" voice="ah11005_01_0199" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="肌への密着度を上げたお陰で、全身にラップを&#xA;巻かれたみたいに汗が噴き出してきて……。" speaker_show="True" stop="True" voice="hy11002_01_0392" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="なるほど！　思いがけず&#xA;サウナスーツのような発汗効果が！" speaker_show="True" stop="True" voice="ah11005_01_0200" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" force_end="False" loop="False" to_idle="True" />
    <msg speaker="半蔵門 雪" text="発汗目的の服ならば相当優秀だと思うわ。&#xA;けど、残念ながら戦闘に使うのは厳しそうね。" speaker_show="True" stop="True" voice="hy11002_01_0393" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="敵を倒す前に、水分を奪われて&#xA;脱水症状になるリスクも増えてしまうし。" speaker_show="True" stop="True" voice="hy11002_01_0394" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="確かにこのままでは雪ちゃんが、塩を浴びた&#xA;ナメクジさんのようにセクシーになってしまいます。" speaker_show="True" stop="True" voice="ah11005_01_0201" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" force_end="False" loop="False" to_idle="True" />
    <msg speaker="半蔵門 雪" text="ナメクジがセクシーかどうかは、&#xA;さて置くとしても……。" speaker_show="True" stop="True" voice="hy11002_01_0395" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="戦闘以外の要素で他のみんなを危険に&#xA;晒すことはできないわ。" speaker_show="True" stop="True" voice="hy11002_01_0396" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="道のりは険しそうですが、すぐに&#xA;タイプ３の製作を開始しましょう！" speaker_show="True" stop="True" voice="ah11005_01_0202" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ええ、一刻も早くシャワーも浴びたいし、&#xA;帰りましょう。" speaker_show="True" stop="True" voice="hy11002_01_0397" />
