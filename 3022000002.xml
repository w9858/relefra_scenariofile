<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3022000002" />
  </info>
  <command>
    <Start />
    <load_cue_sheet cue_sheet="BGM/BGM04_Base" />
    <sound type="Bgm" cue_name="Base" />
    <bg file="03_tsukikage_003" color="#ffffffff" effect="Immediately" effect_time="0.3" wait="False" />
    <telop text="ツキカゲ基地" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="モモと五恵が基地に戻った頃、先に着いていた楓は&#xA;他のツキカゲメンバーに集まってもらっていた。" stop="True" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="-300" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" x="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="どうしたの、フーちゃん？&#xA;皆を集めるなんて珍しいわね。" speaker_show="True" stop="True" voice="kt11009_03_0087" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="青葉 初芽" text="先程の任務で、何か気になる事があったり&#xA;したのでしょうか？" speaker_show="True" stop="True" voice="ah11005_03_0122" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="いいえ、任務に問題はありません。これからアタシが&#xA;行うのは、新たなミッションの提案です。" speaker_show="True" stop="True" voice="sf11004_03_0097" />
    <sound_volume cue_name="Base" volume="20" time="2" />
    <wait time="1" />
    <sound type="Se" cue_name="Spike" />
    <wait time="1" />
    <sound_volume cue_name="Base" volume="100" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="来る週末――&#xA;ツキカゲの皆で紅葉狩りに行きませんか？" speaker_show="True" stop="True" voice="sf11004_03_0098" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="カトリーナ" />
    <move obj_id="相模 楓" x="-200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" x="200" anchor="Bottom" />
    <create_effect effect_id="mei01" effect_type="MampSmile" target_obj="八千代 命" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="八千代 命" text="おおーっ、紅葉狩り？&#xA;いいじゃん行きたい！　楽しそう！" speaker_show="True" stop="True" voice="ym11003_03_0101" />
    <delete obj_id="mei01" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="何、フーが企画してくれたの？　ありがとー！&#xA;お礼に頭を撫でてあげよう、なでなで。" speaker_show="True" stop="True" voice="ym11003_03_0102" />
    <move obj_id="八千代 命" x="50" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek03_strong" loop="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="はぅ……も、もう、&#xA;子供扱いしないでくださいっ。" speaker_show="True" stop="True" voice="sf11004_03_0099" />
    <char_spine_cheek obj_id="相模 楓" loop="True" />
    <delete obj_id="八千代 命" />
    <move obj_id="相模 楓" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <create type="Character" obj_id="半蔵門雪" data_id="311002005" x="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="カトリーナ" text="たまには息抜きも必要だと思うわ。&#xA;留守番は私に任せて、行ってらっしゃい。" speaker_show="True" stop="True" voice="kt11009_03_0088" />
    <char_spine_animation obj_id="半蔵門雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門雪" text="私も賛成よ。秋の山の景色は&#xA;きっと素晴らしいでしょうね。" speaker_show="True" stop="True" voice="hy11002_03_0086" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" />
    <msg speaker="相模 楓" text="事前にビューポイントを徹底的に調べて、&#xA;効率的に回れるようにしておきます。" speaker_show="True" stop="True" voice="sf11004_03_0100" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="山頂で売っているソフトクリームも&#xA;美味しいようなので、期待していてください。" speaker_show="True" stop="True" voice="sf11004_03_0101" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="なお、その際のレシートを見せると、下山時の&#xA;ロープウェイが安くなるらしいです。お得です。" speaker_show="True" stop="True" voice="sf11004_03_0102" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="半蔵門雪" />
    <move obj_id="相模 楓" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="テレジア" data_id="311008003" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="テレジア" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="テレジア" text="凄い下調べだな。" speaker_show="True" stop="True" voice="tr11007_03_0036" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="当然です！&#xA;絶対成功させたいですから！" speaker_show="True" stop="True" voice="sf11004_03_0103" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="お弁当はこちらで用意するので、レジャーシートや&#xA;テントとかは初芽さん達にお任せしてもいいですか？" speaker_show="True" stop="True" voice="sf11004_03_0104" />
    <delete obj_id="テレジア" />
    <create type="Character" obj_id="半蔵門雪" data_id="311002005" x="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005005" x="300" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="True" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" force_end="True" loop="True" />
    <msg speaker="青葉 初芽" text="え、ええ。任せてください。" speaker_show="True" stop="True" voice="ah11005_03_0123" />
    <char_spine_animation obj_id="半蔵門雪" face_anim_type="011_Facial_surprise" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門雪" text="テントまで用意するとは、&#xA;まるでキャンプのようね。" speaker_show="True" stop="True" voice="hy11002_03_0087" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="源 モモ" data_id="311001001" x="-400" y="0" anchor="Bottom" />
    <move obj_id="源 モモ" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="0.3" wait="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="師匠、初芽さん。週末ってほら、&#xA;メイちゃんの誕生日だから……。" speaker_show="True" stop="True" voice="mm11001_03_0104" />
    <msg speaker="源 モモ" text="喜んでもらいたくて、&#xA;フーちゃん頑張ってるんです。" speaker_show="True" stop="True" voice="mm11001_03_0105" />
    <char_spine_animation obj_id="半蔵門雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門雪" text="なるほど、納得がいったわ。" speaker_show="True" stop="True" voice="hy11002_03_0088" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="師匠思いの楓ちゃん、&#xA;可愛いですね～。" speaker_show="True" stop="True" voice="ah11005_03_0124" />
    <delete obj_id="源 モモ" />
    <delete obj_id="半蔵門雪" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="-200" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" x="200" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="週末かぁ、なんだかワクワクしてきた！&#xA;今から待ち遠しいね、フー。" speaker_show="True" stop="True" voice="ym11003_03_0103" />
    <char_spine_cheek obj_id="相模 楓" type="Cheek02_medium" loop="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そ、そう言ってもらえると&#xA;アタシも嬉しいです……。" speaker_show="True" stop="True" voice="sf11004_03_0105" />
    <char_spine_cheek obj_id="相模 楓" loop="True" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="雨天中止とか嫌だし、てるてる坊主作んなくちゃ。&#xA;あ、おやつは何円まで？" speaker_show="True" stop="True" voice="ym11003_03_0104" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" loop="True" />
    <msg speaker="相模 楓" text="と、とりあえず、その辺りは&#xA;アタシ達で準備しますから任せてくださいっ。" speaker_show="True" stop="True" voice="sf11004_03_0106" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="（いい思い出にしてもらうために、&#xA;頑張らなくちゃ！）" speaker_show="True" stop="True" voice="sf11004_03_0107" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
    <sound_stop cue_name="Base" />
  </command>
</adv>