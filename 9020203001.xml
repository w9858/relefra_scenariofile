    <msg speaker="薩摩 しぶき" text="……そうじゃ！" speaker_show="True" stop="True" voice="ss12003_02_0294" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="お。なんだなんだ？" speaker_show="True" stop="True" voice="ry12002_02_0289" />
