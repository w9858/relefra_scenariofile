    <transition effect="Default" type="In" color_change="False" time="1.0" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" />
    <msg speaker="青葉 初芽" text="お待たせしました。ごえちゃん、&#xA;それではまず、これに着替えてください。" speaker_show="True" stop="True" voice="ah11005_02_0726" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="これって、ナース服……ですよね……？" speaker_show="True" stop="True" voice="ig11006_02_0436" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふ、何でもまずは形から、ですよ。&#xA;五恵ちゃん。" speaker_show="True" stop="True" voice="ah11005_02_0727" />
    <fade_out time="1" type="Default" wait="True" />
