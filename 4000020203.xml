  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <fade_out time="0" type="Default" wait="False" />
    <msg window_type="SystemMsg" text="任務完了から数時間後――" stop="True" />
    <msg_window_show show="False" />
    <bg show="True" file="35_hisuiandyuraroom_01" color="#ffffffff" resource_type="Background" />
    <fade_in time="0.8" type="Default" wait="True" />
    <telop text="飛粋・ゆらの部屋" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002002" position_id="left" x="0" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="ったく、飛粋のやつ、&#xA;あれからアタシ様と一言も口をききやがらねえ。" speaker_show="True" stop="True" voice="ry12002_01_0231" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="一体アタシ様が何をしたっつーんだよ。" speaker_show="True" stop="True" voice="ry12002_01_0232" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="Clash" />
    <wait time="0.3" />
    <sound type="Se" cue_name="Clash" />
    <Comment comment="SE扉をノックする音" />
    <wait time="1.0" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="オウ、開いてるよー。" speaker_show="True" stop="True" voice="ry12002_01_0233" />
    <Comment comment="SE扉を開ける音" />
    <sound type="Se" cue_name="DoorOpen" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="-400" x_move_type="MoveTo" time="0.5" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003002" position_id="left" x="1000" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004002" position_id="left" x="1000" />
    <wait time="0.5" />
    <move obj_id="薩摩 しぶき" position_id="left" x="150" x_move_type="MoveTo" time="0.5" />
    <move obj_id="轟 葉栖美" position_id="left" x="400" x_move_type="MoveTo" time="0.5" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="うむ、邪魔するぞ。" speaker_show="True" stop="True" voice="ss12003_01_0212" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="やっほー、ゆらちゃん。&#xA;おっじゃまー♪" speaker_show="True" stop="True" voice="th12004_01_0141" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="隊長、はすみん。&#xA;どうしたんだよ？" speaker_show="True" stop="True" voice="ry12002_01_0234" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="飛粋ならどっかに出かけちまったぞ。" speaker_show="True" stop="True" voice="ry12002_01_0235" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="うん、知ってるよ。&#xA;だから、こうして出向いたんだ。" speaker_show="True" stop="True" voice="th12004_01_0142" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="あいつがいては、お前も落ち着いて&#xA;しぶきたちの話を聞けないだろうと思ってな。" speaker_show="True" stop="True" voice="ss12003_01_0213" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="False" />
    <msg speaker="乱獅子 ゆら" text="話ぃ？　なんだよ、一体。" speaker_show="True" stop="True" voice="ry12002_01_0236" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="うん。ねえ、ゆらちゃん。&#xA;どうして飛粋ちゃんが怒ってるのか分かる？" speaker_show="True" stop="True" voice="th12004_01_0143" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="あん？　そりゃあ、アタシ様のやり方が&#xA;気に入らねぇからだろ？" speaker_show="True" stop="True" voice="ry12002_01_0237" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="では、何故気に入らないのか、わかるか？" speaker_show="True" stop="True" voice="ss12003_01_0214" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="それは…………わからん。" speaker_show="True" stop="True" voice="ry12002_01_0238" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="アタシ様の能力に嫉妬してるから、とか？" speaker_show="True" stop="True" voice="ry12002_01_0239" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="轟 葉栖美" text="ぶっぶー。大ハズレー。" speaker_show="True" stop="True" voice="th12004_01_0144" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="飛粋はな、お前に傷付いてほしくないから、&#xA;あんな事を言ったんだと思うぞ。" speaker_show="True" stop="True" voice="ss12003_01_0215" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="……え？" speaker_show="True" stop="True" voice="ry12002_01_0240" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="まあ、より正確に言うなら、アタシらが思ってる事を&#xA;飛粋ちゃんが代表して言ってくれたって感じかな？" speaker_show="True" stop="True" voice="th12004_01_0145" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="ゆらよ。飛粋に代わってしぶきがもう一度言おう。&#xA;今のお前の戦い方は、危うすぎる。" speaker_show="True" stop="True" voice="ss12003_01_0216" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="な、なんでだよ！" speaker_show="True" stop="True" voice="ry12002_01_0241" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="お前の体は不死に近いが、&#xA;それとて絶対のものではない。" speaker_show="True" stop="True" voice="ss12003_01_0217" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="お前の回復が間に合わないほどの大技や、&#xA;治しきれない毒物を使う者がおるやもしれんじゃろ？" speaker_show="True" stop="True" voice="ss12003_01_0218" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="02_Idle" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="へっ、アタシ様の体は、敵の攻撃とか毒なんかに&#xA;やられるほどヤワじゃねーよ！" speaker_show="True" stop="True" voice="ry12002_01_0242" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="じゃあ、ゆらちゃんの回復力を無効化する能力を持つ敵が&#xA;出てきたら？" speaker_show="True" stop="True" voice="th12004_01_0146" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="そ、そんな奴がいるわけ……" speaker_show="True" stop="True" voice="ry12002_01_0243" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="いないとも限らないよ。世の中は広いし、&#xA;あらゆる事態が起こりうる。" speaker_show="True" stop="True" voice="th12004_01_0147" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="…………。" speaker_show="True" stop="True" voice="ry12002_01_0244" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="ゆらよ、お前も本当は薄々気付いてるのではないか？&#xA;飛粋の気持ちに……。" speaker_show="True" stop="True" voice="ss12003_01_0219" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="うっ……。" speaker_show="True" stop="True" voice="ry12002_01_0245" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="飛粋ちゃんはね、純粋に&#xA;ゆらちゃんのことが心配なんだよ。" speaker_show="True" stop="True" voice="th12004_01_0148" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="そうじゃのう。なんせ飛粋にとって、&#xA;ゆらは大切な友達なんじゃからな。" speaker_show="True" stop="True" voice="ss12003_01_0220" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="飛粋……。" speaker_show="True" stop="True" voice="ry12002_01_0246" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="そして、アタシとしぶきちにとっても、&#xA;ゆらちゃんは大事な後輩だよ。" speaker_show="True" stop="True" voice="th12004_01_0149" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <char_spine_eye obj_id="乱獅子 ゆら" type="Eye03_tear" loop="True" />
    <msg speaker="乱獅子 ゆら" text="はすみん……隊長……。" speaker_show="True" stop="True" voice="ry12002_01_0247" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="薩摩 しぶき" text="だから、ゆら。&#xA;お前はもっと己を大事に扱え。" speaker_show="True" stop="True" voice="ss12003_01_0221" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="005_Facial_sad" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="薩摩 しぶき" text="しぶきもはっすーも、そして飛粋も、&#xA;お前に何かあったら悲しいからな。" speaker_show="True" stop="True" voice="ss12003_01_0222" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……ああ、わかったぜ。&#xA;ありがとう、隊長、はすみん。" speaker_show="True" stop="True" voice="ry12002_01_0248" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="アタシ様、ちょっと飛粋に謝ってくるわ！" speaker_show="True" stop="True" voice="ry12002_01_0249" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="14_Positive" loop="False" to_idle="True" face_anim_type="002_Facial_smile" force_end="False" />
    <msg speaker="薩摩 しぶき" text="うむ、行ってこい。" speaker_show="True" stop="True" voice="ss12003_01_0223" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
</adv>