    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="私が未熟でした。すみません。" speaker_show="True" stop="True" voice="hy11002_01_0028" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="…師匠が弟子を助けるのは当たり前だよ。&#xA;信だって絶対同じ事をしてる。" speaker_show="True" stop="True" voice="ks11010_01_0008" />
