    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="め、メイちゃん師匠相手に凄い言い方をするね。" speaker_show="True" stop="True" voice="mm11001_01_0038" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="高坂 信" text="いいのモモちゃん、いつもこんな感じだったから。&#xA;戻ってきたなーって実感してるよ。" speaker_show="True" stop="True" voice="ks11010_01_0013" />
