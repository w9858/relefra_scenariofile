<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="PromoAdv/PromoAdv_020" />
  </info>
  <command>
    <Start />
    <auto_mode active="True" />
    <function_menu_show show="False" />
    <bg file="02_wasabimo_001" />
    <load_cue_sheet cue_sheet="BGM/BGM18_Adv6" />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_in time="0.5" wait="True" />
    <create obj_id="カトリーナ" data_id="311009003" type="Character" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" lip_sync_volume="0.01" text="ええっ!?　覚えていてくれたのですか？" voice="kt11009_03_0296" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="04_HipLeftHand" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="今日、12月21日が私、カトリーナ・トビーの&#xA;誕生日だということを……。" voice="kt11009_03_0297" />
    <char_spine_animation obj_id="カトリーナ" loop="True" body_anim_type="21_PutElbow" face_anim_type="009_Facial_blush" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek02_medium" loop="True" />
    <char_spine_eye obj_id="カトリーナ" type="Eye03_tear" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="あぁ……、私は何て幸せなんでしょう。" voice="kt11009_03_0298" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="私の神が、誰よりも早く&#xA;祝福の言葉をくださるなんて……。" voice="kt11009_03_0299" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" mixing="True" />
    <char_spine_cheek obj_id="カトリーナ" type="Cheek01_default" loop="True" />
    <char_spine_eye obj_id="カトリーナ" type="Eye01_default" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="この気持ちを英語で言うならば、オーマイゴッド！&#xA;あぁ、怖いくらいにぴったりな言葉……。" voice="kt11009_03_0300" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="はっ！　ごめんなさい。私としたことが&#xA;嬉しさの余り、１人で喋り過ぎてしまいました。" voice="kt11009_03_0301" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="今日は素敵なお祝いのメッセージ、&#xA;ありがとうございました。" voice="kt11009_03_0302" />
    <msg speaker="カトリーナ" stop="True" window_type="CharMsg" text="この喜びを糧に、これからも&#xA;ツキカゲの一員として精一杯頑張りますね。" voice="kt11009_03_0303" />
    <msg_window_show show="False" />
    <fade_out time="0.5" wait="True" />
  </command>
</adv>