    <telop text="新幹線 客席" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="アミィ" data_id="313002003" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="たまに乗る新幹線もいいわねー。&#xA;景色を見ながら、ひつまぶしのお弁当を食べる！" speaker_show="True" stop="True" voice="sr13001_02_0019" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="どえりゃーうみゃーってやつよ。" speaker_show="True" stop="True" voice="sr13001_02_0020" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="違う地方の人に会うからと言って&#xA;そんな愛知キャラを急に作らなくても。" speaker_show="True" stop="True" voice="sm13002_02_0011" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="スパイとして少しぐらい強いクセを&#xA;魅せておこうかなと。" speaker_show="True" stop="True" voice="sr13001_02_0021" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="……ふぅ。&#xA;景色を見ながらさすオイルは格別の味でありまする。" speaker_show="True" stop="True" voice="am13002_02_0013" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="アミィまでボケに回ると突っ込むのも大変ですわ。" speaker_show="True" stop="True" voice="sm13002_02_0012" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ねぇねぇキュートな貴方はどこ行くの？　東京？" speaker_show="True" stop="True" voice="sr13001_02_0022" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="あぁお姉様、目を離した隙に通行人を&#xA;ナンパしないで下さい。ハレンチな。" speaker_show="True" stop="True" voice="sm13002_02_0013" />
    <delete obj_id="アミィ" />
    <delete obj_id="敷島 真留瀬" />
    <move obj_id="敷島 来夢" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <create type="Character" obj_id="北斗 凪" data_id="399002001" position_id="left" x="-300" y="0" anchor="Bottom" />
    <move obj_id="北斗 凪" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="え！？　ええと私は空崎に住んでいまして……。" speaker_show="True" stop="True" voice="hn99002_02_0001" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="へぇー空崎なんだ。どう住み心地は？" speaker_show="True" stop="True" voice="sr13001_02_0023" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="いいところですよ。治安が悪いイメージが&#xA;あるみたいですけど。" speaker_show="True" stop="True" voice="hn99002_02_0002" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="警察がとても優秀なので、すぐに&#xA;悪い人達を捕まえてしまうんです。" speaker_show="True" stop="True" voice="hn99002_02_0003" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ほほーう。なるほどなるほど。&#xA;会話してくれてありがとう。" speaker_show="True" stop="True" voice="sr13001_02_0024" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="実は私、敷島の社長してるんだ。&#xA;よければ電話して。" speaker_show="True" stop="True" voice="sr13001_02_0025" />
    <char_spine_animation obj_id="北斗 凪" face_anim_type="011_Facial_surprise" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="北斗 凪" text="ふぇ！？　は、はい。" speaker_show="True" stop="True" voice="hn99002_02_0004" />
    <move obj_id="北斗 凪" position_id="left" x="-1000" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1" wait="True" />
    <move obj_id="敷島 来夢" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <delete obj_id="北斗 凪" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ツキカゲ、暗躍しているみたいね。" speaker_show="True" stop="True" voice="sr13001_02_0026" />
    <msg_window_show show="False" />
