    <bg show="True" resource_type="Background" file="12_maidcafe_03" effect="Immediately" effect_time="0" wait="False" />
    <transition type="In" color_change="True" wait="True" />
    <telop text="メイド喫茶" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="150" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="450" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004007" position_id="left" x="-1000" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="あぁ、こういうお店の事でしたか。&#xA;ホッとしましたわ。" speaker_show="True" stop="True" voice="sm13002_02_0017" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="でへへ。メイドのお姉ちゃんも可愛いわね。" speaker_show="True" stop="True" voice="sr13001_02_0032" />
    <move obj_id="相模 楓" position_id="left" x="-450" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="（んん？　あれは明日皆で会う予定の&#xA;敷島来夢じゃない。）" speaker_show="True" stop="True" voice="sf11004_02_0001" lip_sync="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="018_Facial_smug" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="（空崎に前乗りしているのね。&#xA;超絶大企業の社長。友好的に接してやろうじゃない！）" speaker_show="True" stop="True" voice="sf11004_02_0002" lip_sync="False" />
