    <fade_out time="0" type="Default" wait="False" />
    <msg window_type="SystemMsg" text="ＺＥＴの面子は空崎を目指していた。" stop="True" />
    <msg_window_show show="False" />
    <bg show="True" resource_type="Background" file="13_train_01" effect="Immediately" effect_time="0" wait="False" />
