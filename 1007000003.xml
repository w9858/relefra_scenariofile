    <telop text="競輪場" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="敷島 来夢" data_id="313001004" position_id="left" x="0" y="0" anchor="Bottom" />
    <create type="Character" obj_id="敷島 真留瀬" data_id="313003002" position_id="left" x="300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="アミィ" data_id="313002003" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ここが空崎か。" speaker_show="True" stop="True" voice="sr13001_02_0027" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="何故いきなり競輪場に来るんですか。" speaker_show="True" stop="True" voice="sm13002_02_0014" />
    <create_effect effect_id="warai" effect_type="MampSmile" target_obj="敷島 来夢" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="折角だから運試しを。&#xA;……ほら、当てた当てた！！" speaker_show="True" stop="True" voice="sr13001_02_0028" />
    <delete obj_id="warai" />
    <char_spine_animation obj_id="アミィ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="アミィが解析したわけでもないのに&#xA;流石マスターでございますな。" speaker_show="True" stop="True" voice="am13002_02_0014" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="長いこと会社を続けられている社長は&#xA;すべからく豪運なのよ。" speaker_show="True" stop="True" voice="sr13001_02_0029" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様は会社を継いで間もないでしょ。" speaker_show="True" stop="True" voice="sm13002_02_0015" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="018_Facial_smug" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ではこの儲けたお金で、&#xA;セクシーなお姉ちゃんがいるお店に行きますか。" speaker_show="True" stop="True" voice="sr13001_02_0030" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="マスターはまっこと色好みですな。&#xA;英雄の証かと。" speaker_show="True" stop="True" voice="am13002_02_0015" />
    <char_spine_animation obj_id="アミィ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="よければ合間合間にアミィも可愛がって下さい。" speaker_show="True" stop="True" voice="am13002_02_0016" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="アミィとマルはいつでも可愛がるわよ、よしよし。" speaker_show="True" stop="True" voice="sr13001_02_0031" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="もう突っ込むのも疲れましたわ。" speaker_show="True" stop="True" voice="sm13002_02_0016" />
    <msg_window_show show="False" />
