  <command>
    <Start />
    <sound cue_name="Adv1" volume="0" />
    <fade_out time="0" type="Default" />
    <msg window_type="SystemMsg" text="バレンタインの翌日、放課後。" stop="True" />
    <msg_window_show show="False" />
    <bg resource_type="Still" file="311006008" />
    <fade_in time="0.5" type="Default" wait="True" />
    <msg speaker="石川 五恵" text="今日は掃除当番だって言ってたけど、&#xA;師匠、ずいぶん遅いなぁ…。" speaker_show="True" stop="True" voice="ig11006_01_0222" />
    <msg speaker="石川 五恵" text="ここで待ってることは伝えてあるから、&#xA;そろそろ来るとは思うけど…。" speaker_show="True" stop="True" voice="ig11006_01_0223" />
    <msg speaker="石川 五恵" text="（でも、考えてみたらかえって良かったかも）" speaker_show="True" stop="True" voice="ig11006_01_0224" lip_sync="False" />
    <msg speaker="石川 五恵" text="（いつもの下校時間だと人も多かっただろうし、&#xA;こういう時は、やっぱり２人きりの方がいいもんね）" speaker_show="True" stop="True" voice="ig11006_01_0225" lip_sync="False" />
    <msg speaker="石川 五恵" text="（あ、でも…&#xA;ちょっぴりドキドキしてきちゃった…）" speaker_show="True" stop="True" voice="ig11006_01_0226" lip_sync="False" />
    <msg speaker="石川 五恵" text="（きっと昔の私には無理だったよね…&#xA;誰かに、バレンタインチョコを渡すなんて…）" speaker_show="True" stop="True" voice="ig11006_01_0227" lip_sync="False" />
    <msg speaker="石川 五恵" text="（でも師匠と出会って、ツキカゲに誘われて、&#xA;雪先輩や他のみんなとも知り合って、私は変わった…）" speaker_show="True" stop="True" voice="ig11006_01_0228" lip_sync="False" />
    <msg speaker="石川 五恵" text="（もし師匠がいなかったら、&#xA;私、ずっと憶病なままだったかも…）" speaker_show="True" stop="True" voice="ig11006_01_0229" lip_sync="False" />
    <msg speaker="石川 五恵" text="だから今日は、師匠にちゃんと感謝の気持ちを&#xA;伝えなくっちゃ…！" speaker_show="True" stop="True" voice="ig11006_01_0230" />
    <msg speaker="青葉 初芽" text="ううん、私の方こそいつも感謝してますよ。" speaker_show="True" stop="True" voice="ah11005_01_0144" />
    <msg speaker="石川 五恵" text="え…？" speaker_show="True" stop="True" voice="ig11006_01_0231" />
    <transition effect="Default" type="Out" time="0.5" wait="True" />
    <create type="Character" obj_id="石川 五恵" data_id="311006008" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005003" position_id="left" x="200" y="0" anchor="Bottom" />
    <load_cue_sheet cue_sheet="BGM/BGM15_Adv5" />
    <sound type="Bgm" cue_name="Adv5" />
    <bg file="01_school1mo_001" />
    <transition effect="Default" type="In" time="0.5" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="お待たせしました、ごえちゃん。" speaker_show="True" stop="True" voice="ah11005_01_0145" />
    <char_spine_cheek obj_id="石川 五恵" type="Cheek02_medium" loop="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" />
    <create_effect effect_id="goe_sweat" effect_type="MampSweat" life_time="1.0" target_obj="石川 五恵" />
    <msg speaker="石川 五恵" text="師匠!?　いつからそこに!?" speaker_show="True" stop="True" voice="ig11006_01_0232" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <create_effect effect_id="hatume_smile" effect_type="MampSmile" life_time="1.0" target_obj="青葉 初芽" />
    <msg speaker="青葉 初芽" text="フフ。ごえちゃんって大抵しっかりしてるのに、&#xA;たまに隙だらけの時がありますよね。" speaker_show="True" stop="True" voice="ah11005_01_0146" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="でも、今日はどうしたんです？&#xA;いつもはモモちゃんや楓ちゃんと一緒に帰るのに。" speaker_show="True" stop="True" voice="ah11005_01_0147" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="009_Facial_blush" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="えと、それは、その…。" speaker_show="True" stop="True" voice="ig11006_01_0233" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="あの、これ…う、受け取ってください…！&#xA;みんなと一緒に作った、バレンタインチョコです…！" speaker_show="True" stop="True" voice="ig11006_01_0234" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="今の私があるのは、師匠のおかげなんです…！&#xA;だから、そのことを伝えたくて…！" speaker_show="True" stop="True" voice="ig11006_01_0235" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" force_end="False" loop="True" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="フフ。ありがとうございます、ごえちゃん。" speaker_show="True" stop="True" voice="ah11005_01_0148" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="それじゃあ、私からも。&#xA;はい、どうぞ。" speaker_show="True" stop="True" voice="ah11005_01_0149" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="え!?　これって!?" speaker_show="True" stop="True" voice="ig11006_01_0236" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="私からのバレンタインチョコです。&#xA;ごえちゃんほど、上手ではないと思いますけど。" speaker_show="True" stop="True" voice="ah11005_01_0150" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="てことは…&#xA;このチョコ、もしかして師匠が…？" speaker_show="True" stop="True" voice="ig11006_01_0237" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" to_idle="True" />
    <msg speaker="青葉 初芽" text="はい。さっきも言った通り、私もごえちゃんには&#xA;とーっても感謝していますので。" speaker_show="True" stop="True" voice="ah11005_01_0151" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="だから、その想いを込めて作りたかったんです。" speaker_show="True" stop="True" voice="ah11005_01_0152" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <create_effect effect_id="goe_KiraKIra" effect_type="EffectKirakira" life_time="1.0" target_obj="石川 五恵" />
    <msg speaker="石川 五恵" text="ありがとうございます師匠。&#xA;私、すっごく嬉しいです。" speaker_show="True" stop="True" voice="ig11006_01_0238" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="いいえ、どういたしまして。" speaker_show="True" stop="True" voice="ah11005_01_0153" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ねぇ、ごえちゃん。もし良ければこれから２人で、&#xA;公園にでも行ってお散歩しませんか？" speaker_show="True" stop="True" voice="ah11005_01_0154" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="たまには師匠と弟子でのんびり、ね。" speaker_show="True" stop="True" voice="ah11005_01_0155" />
    <char_spine_animation obj_id="石川 五恵" body_anim_type="14_Positive" force_end="False" loop="False" to_idle="True" />
    <msg speaker="石川 五恵" text="はい。　もちろんです、師匠！" speaker_show="True" stop="True" voice="ig11006_01_0239" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
    <wait time="0.5" />
  </command>
