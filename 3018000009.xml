    <create type="Character" obj_id="半蔵門 雪" data_id="311002014" position_id="left" x="200" y="0" />
    <create type="Character" obj_id="石川 五恵" data_id="311006014" position_id="left" x="-200" y="0" />
    <char_spine_animation obj_id="赤星 イタ子" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" force_end="False" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="五右衛門、気づいていると思うけど、&#xA;この老人は一般人ではないわ。" speaker_show="True" stop="True" voice="hy11002_02_0513" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="石川 五恵" text="はい……地元の人間とは雰囲気が違います。" speaker_show="True" stop="True" voice="ig11006_02_0347" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="加えて、歩法が常人とは異なる。&#xA;覚えがあるわ……これは山に慣れた者の歩法よ。" speaker_show="True" stop="True" voice="hy11002_02_0514" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="『山伏』のように山地を拠点とする組織の&#xA;人間かもしれない……気を引き締めて。" speaker_show="True" stop="True" voice="hy11002_02_0515" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="石川 五恵" />
