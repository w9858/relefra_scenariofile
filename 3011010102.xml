    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="これは快挙ね。白虎ちゃんが、ひと口目を&#xA;自分以外に進呈するなんて。" speaker_show="True" stop="True" voice="kt11009_02_0109" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="良かったね、モモちゃん。貴重な光景が見れて&#xA;信も生きてて良かったよ……グスッ。" speaker_show="True" stop="True" voice="ks11010_02_0206" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="源 モモ" data_id="311001014" position_id="left" x="0" y="0" anchor="Bottom" />
