    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="源 モモ" data_id="311001014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="し、信先輩、分度器で計らなくても……！&#xA;均等すぎますよ、あはは！　あははは！" speaker_show="True" stop="True" voice="mm11001_02_0268" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="（ツキカゲが……今の空崎があるのも&#xA;あなたのお父様のおかげ……。）" speaker_show="True" stop="True" voice="hy11002_02_0221" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="（あなたのお父様と、生まれてくれたあなたに&#xA;感謝を込めて……ハッピーバースデー、モモ。）" speaker_show="True" stop="True" voice="hy11002_02_0222" lip_sync="False" />
