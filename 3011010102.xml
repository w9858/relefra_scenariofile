    <msg speaker="青葉 初芽" text="これなら、きっとモモちゃんが喜んでくれると&#xA;ごえちゃんを先頭に、みんなで作りました。" speaker_show="True" stop="True" voice="ah11005_02_0241" />
    <char_spine_eye obj_id="源 モモ" type="Eye03_tear" loop="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="ごえちゃん、ありがとう。私……胸がいっぱいで&#xA;何て言ったら良いか……。" speaker_show="True" stop="True" voice="mm11001_02_0264" />
