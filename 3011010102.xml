    <msg speaker="源 モモ" text="な、なんかちょっと恥ずかしいです……けど&#xA;いただきます、ア～～ン。ん～～～美味しっ♪" speaker_show="True" stop="True" voice="mm11001_02_0266" />
    <move obj_id="源 モモ" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
