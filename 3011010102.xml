    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="なんでアンタが偉そうなのよ！&#xA;ナイフよこしなさい、アタシが切るから！" speaker_show="True" stop="True" voice="sf11004_02_0358" />
    <char_spine_animation obj_id="白虎" face_anim_type="101_Facial_unique" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="ああーー！　そんなこと言って、自分のだけ&#xA;大きく取り分ける気だろう!?　そうはいくか！" speaker_show="True" stop="True" voice="bk11008_02_0055" />
