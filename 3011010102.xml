    <create type="Character" obj_id="源 モモ" data_id="311001014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="ケーキ……なのに、ロウソクがまるで……&#xA;空崎の夜景みたい……私の大好きな……空崎の。" speaker_show="True" stop="True" voice="mm11001_02_0263" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="ええ。あなたのお父様が命懸けで守った街。&#xA;そして、今はあなたが守っている空崎市よ。" speaker_show="True" stop="True" voice="hy11002_02_0219" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="200" y="0" anchor="Bottom" />
