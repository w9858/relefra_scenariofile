    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="八千代 命" text="こりゃ、独り占め……じゃないや&#xA;２人占めは許さーーーん！" speaker_show="True" stop="True" voice="ym11003_02_0392" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="みんなで頂きましょう。楽しく、平和に。&#xA;それが、モモの望みなのだから。" speaker_show="True" stop="True" voice="hy11002_02_0220" />
    <delete obj_id="八千代 命" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="源 モモ" data_id="311001014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="師匠……ありがとうございます！&#xA;今日は、人生で１番幸せな誕生日です！" speaker_show="True" stop="True" voice="mm11001_02_0267" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="白虎" text="ふん、モモがそう言うなら、しょうがない。&#xA;分けてやるぞ、皆の者！" speaker_show="True" stop="True" voice="bk11008_02_0054" />
