    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="何も言わなくて良いよ。&#xA;気持ちは、みんな同じだから。" speaker_show="True" stop="True" voice="ig11006_02_0091" />
    <char_spine_eye obj_id="源 モモ" type="Eye01_default" loop="True" />
    <delete obj_id="源 モモ" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="相模 楓" data_id="311004006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" body_anim_type="04_HipLeftHand" />
    <msg speaker="八千代 命" text="うん。モモちが想ってくれてるように&#xA;メイたちは、一緒に空崎を守る大切な仲間！" speaker_show="True" stop="True" voice="ym11003_02_0391" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="だから……もてなすだけじゃなくて、ちゃんと&#xA;アタシ達にも、祝わせなさいよね！　……ったく。" speaker_show="True" stop="True" voice="sf11004_02_0357" />
    <delete obj_id="八千代 命" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="白虎" data_id="311007002" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001014" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" body_anim_type="07_ChestBothHands" />
    <msg speaker="源 モモ" text="そう…だよね、仲間だもんね。&#xA;ごめん、ありがとーーー！" speaker_show="True" stop="True" voice="mm11001_02_0265" />
    <char_spine_animation obj_id="白虎" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <create_effect effect_id="Smile_byakko" effect_type="MampSmile" life_time="1.5" target_obj="白虎" x="0" y="0" attach_head_bone="True" />
    <msg speaker="白虎" text="ハピバだ、モモ！　ほら、口開けろ！&#xA;この白虎様が、ケーキをア～ンしてやるゾ♪" speaker_show="True" stop="True" voice="bk11008_02_0052" />
