    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="高坂 信" text="も～、みんな食いしん坊だなあ。&#xA;ケンカしないよう、信が均等に分けたげるよ。" speaker_show="True" stop="True" voice="ks11010_02_0207" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="21_PutElbow" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="ハァ～。どんな人にも分け隔てなく……。&#xA;シン様の対応は、いつ何時でもまさに神です！" speaker_show="True" stop="True" voice="kt11009_02_0110" />
