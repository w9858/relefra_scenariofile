    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="モモ……。" speaker_show="True" stop="True" voice="hy11002_03_0155" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="し、師匠!?&#xA;す、すみません、ありがとうございます！" speaker_show="True" stop="True" voice="mm11001_03_0221" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="動かないで。どうして転びかけたの？&#xA;足首でも捻った？" speaker_show="True" stop="True" voice="hy11002_03_0156" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="い、いえ単に滑っただけで……滑ったってその！&#xA;上手にじゃなくて、ツルッといった系の……" speaker_show="True" stop="True" voice="mm11001_03_0222" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="気をつけなさい。頭を打ったらどうするの。" speaker_show="True" stop="True" voice="hy11002_03_0157" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="すみません……。" speaker_show="True" stop="True" voice="mm11001_03_0223" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
