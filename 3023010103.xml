    <create type="Character" obj_id="源 モモ" data_id="311001005" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="うぅ、子供がスイスイ滑って私を&#xA;追い抜いていく……悔しいなぁ。" speaker_show="True" stop="True" voice="mm11001_03_0214" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="お、モモち。転ばないで滑ってるじゃん。&#xA;エライエライ。" speaker_show="True" stop="True" voice="ym11003_03_0243" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そ、そんなレベルじゃ駄目でしょ!?&#xA;どうしてスピード出ないんだろ……。" speaker_show="True" stop="True" voice="mm11001_03_0215" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="んー。姿勢じゃない？　やっぱ前傾しないと&#xA;速度は上がらないと思うけど。" speaker_show="True" stop="True" voice="ym11003_03_0244" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="ハッ、そうだよね！　スピードスケートの人は&#xA;みんな90度くらい腰曲げてるし！" speaker_show="True" stop="True" voice="mm11001_03_0216" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="うん。あと、大きく蹴るのが大事だよ。&#xA;チョコチョコ足踏みしてたら進まないし。" speaker_show="True" stop="True" voice="ym11003_03_0245" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="なるほど！　やってみるよ、ありがとね！" speaker_show="True" stop="True" voice="mm11001_03_0217" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ガンバー♪" speaker_show="True" stop="True" voice="ym11003_03_0246" />
    <msg_window_show show="False" />
