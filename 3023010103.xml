    <create type="Character" obj_id="半蔵門 雪" data_id="311002005" x="-1000" y="0" anchor="Bottom" />
    <sound type="Se" cue_name="ICE_SKATING_Stop_05_mono" />
    <wait time="2" />
    <move obj_id="半蔵門 雪" x="200" x_move_type="MoveTo" y_move_type="MoveValue" time="0.5" wait="True" />
    <move obj_id="源 モモ" x="0" x_move_type="MoveTo" y_move_type="MoveValue" time="1" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="あ、あの……師匠。どこにいたんですか？&#xA;転びかけた時、すぐ助けてくれて……。" speaker_show="True" stop="True" voice="mm11001_03_0225" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ずっと、あなたの後ろにいたわ。" speaker_show="True" stop="True" voice="hy11002_03_0159" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="えっ、そうなんですか!?　私てっきり&#xA;置いて行かれたとばっかり……。" speaker_show="True" stop="True" voice="mm11001_03_0226" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そんな訳ないでしょう。後ろから、&#xA;モモの滑り方を観察していたのよ。" speaker_show="True" stop="True" voice="hy11002_03_0160" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="そ、それじゃあ私のこと、ずっと見て!?" speaker_show="True" stop="True" voice="mm11001_03_0227" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ええ、そうよ。&#xA;師匠だもの。当然でしょう？" speaker_show="True" stop="True" voice="hy11002_03_0161" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <char_spine_cheek obj_id="源 モモ" type="Cheek02_medium" loop="True" />
    <msg speaker="源 モモ" text="ふ、ふぁ……。" speaker_show="True" stop="True" voice="mm11001_03_0228" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="顔が赤いわ、モモ。熱でもあるんじゃない？&#xA;具合が悪いなら、早く言いなさい。" speaker_show="True" stop="True" voice="hy11002_03_0162" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="具合イイです……最高にイイです。師匠、私&#xA;スケート上手になりますから……あのっ、" speaker_show="True" stop="True" voice="mm11001_03_0229" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="１週間、ずっと見ていてくださいね！" speaker_show="True" stop="True" voice="mm11001_03_0230" />
    <char_spine_cheek obj_id="源 モモ" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="甘えすぎない。" speaker_show="True" stop="True" voice="hy11002_03_0163" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
