    <move obj_id="源 モモ" x="-300" x_move_type="MoveTo" y_move_type="MoveValue" time="1" wait="True" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="師匠ぉ、自分だけスイスイどっか行っちゃって。&#xA;相変わらずお厳しぃ……。" speaker_show="True" stop="True" voice="mm11001_03_0218" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="源 モモ" text="急に速度を上げて、師匠をビックリさせちゃうん&#xA;だから……よぉーし、腰曲げて大きく蹴って！" speaker_show="True" stop="True" voice="mm11001_03_0219" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="ICE_SKATING_Stop_05_mono" />
    <wait time="2" />
    <move obj_id="源 モモ" x="1000" x_move_type="MoveTo" y_move_type="MoveValue" time="0.5" wait="True" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="源 モモ" data_id="311001005" x="-1000" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="017_Facial_shock" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <sound type="Se" cue_name="ICE_SKATING_Stop_05_mono" />
