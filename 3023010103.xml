    <create_effect effect_id="ｍｍ０１ｍｍ０１" effect_type="MampExclamation" life_time="1" target_obj="源 モモ" />
    <msg speaker="源 モモ" text="ほああっ!?　止まれない！　うわわわ…っ!?" speaker_show="True" stop="True" voice="mm11001_03_0220" />
