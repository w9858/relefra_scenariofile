    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" x="0" position_id="left" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="れ、冷静なカトリーナさんが、ど、&#xA;どうしたんですか？　これも演技？" speaker_show="True" stop="True" voice="mm11001_01_0040" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" x="0" position_id="left" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-300" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" x="300" position_id="left" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" to_idle="True" loop="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="ところが、これに限っては本物なのよモモ。" speaker_show="True" stop="True" voice="hy11002_01_0032" />
