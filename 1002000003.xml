    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" x="0" y="0" position_id="left" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="015_Facial_poker" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="め、メイちゃんが嬉しそうなのは何よりだし、&#xA;頼もしい戦力が増えたけど…。" speaker_show="True" stop="True" voice="mm11001_01_0042" />
