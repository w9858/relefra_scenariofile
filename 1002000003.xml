    <fade_in time="1.0" type="Default" wait="True" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" x="0" y="0" position_id="left" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_unique" force_end="False" body_anim_type="13_Question" loop="True" />
    <msg speaker="源 モモ" text="ところでさっきからカトリーナさんの&#xA;様子が何かおかしいんですけど。" speaker_show="True" stop="True" voice="mm11001_01_0039" />
