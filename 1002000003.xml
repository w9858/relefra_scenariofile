    <msg speaker="カトリーナ" text="また一緒に行動出来て嬉しいですシン様。" speaker_show="True" stop="True" voice="kt11009_01_0020" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="高坂 信" text="ありがとう、カトさん。" speaker_show="True" stop="True" voice="ks11010_01_0016" />
