    <Comment comment="信、私服（夏）の予定" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="15_Negative" to_idle="True" loop="False" force_end="False" />
    <msg speaker="高坂 信" text="それはちょっと目立ち過ぎちゃうかなぁ、と。" speaker_show="True" stop="True" voice="ks11010_01_0018" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" to_idle="True" loop="True" force_end="False" />
    <char_spine_animation obj_id="八千代 命" body_anim_type="15_Negative" to_idle="True" loop="False" force_end="False" />
    <msg speaker="八千代 命" text="師匠が目立つ、ないない！　あはは！" speaker_show="True" stop="True" voice="ym11003_01_0042" />
