    <create type="Character" obj_id="相模 楓" data_id="311004006" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="う、うぐぐぐ……ギギギ。" speaker_show="True" stop="True" voice="sf11004_01_0039" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ふ、楓ちゃん大丈夫！？" speaker_show="True" stop="True" voice="ig11006_01_0015" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="は？　全然大丈夫だけど！？&#xA;余裕シャクシャク丸ですけどぉ！？" speaker_show="True" stop="True" voice="sf11004_01_0040" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="あぅ、とてもそんな風には見えないよ…。" speaker_show="True" stop="True" voice="ig11006_01_0016" />
