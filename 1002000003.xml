    <create_effect effect_id="effectkirakira_katorina" effect_type="EffectKirakira" life_time="100" target_obj="カトリーナ" />
    <wait time="1.0" />
    <msg speaker="カトリーナ" text="祈っていたのよ、神に。つまりシン様に。&#xA;貴方達、神を見た事はある？　ここにいるのよ。" speaker_show="True" stop="True" voice="kt11009_01_0016" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="石川 五恵" data_id="311006006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" force_end="False" loop="True" to_idle="False" body_anim_type="13_Question" />
    <msg speaker="石川 五恵" text="え、ええ？　か、かみですか？" speaker_show="True" stop="True" voice="ig11006_01_0014" />
