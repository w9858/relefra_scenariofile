    <msg speaker="青葉 初芽" text="簡単に言うと師匠は無神論者だったのですが&#xA;信さんの微笑みの中に神を見たんです。" speaker_show="True" stop="True" voice="ah11005_01_0021" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="何を言っているか分からないだろうが&#xA;深くは考えない方がいいぜい。感じるんだ。" speaker_show="True" stop="True" voice="ym11003_01_0040" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="源 モモ" data_id="311001006" x="0" position_id="left" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="あ！！　でも前にカトリーナさんが&#xA;ツキカゲに入った理由を教えてくれた時…。" speaker_show="True" stop="True" voice="mm11001_01_0041" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" x="-200" position_id="left" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
    <Comment comment="信、私服（夏）の予定" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="そうよモモちゃん。私がツキカゲに入ったのは&#xA;シン様の笑顔に惹かれたから…。" speaker_show="True" stop="True" voice="kt11009_01_0019" />
