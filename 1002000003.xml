    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="シン様お待ちしていました。&#xA;神は必ず復活される。予言通りですね。" speaker_show="True" stop="True" voice="kt11009_01_0017" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="あ、あははは。ただいまカトさん。" speaker_show="True" stop="True" voice="ks11010_01_0015" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="おかえりなさいませ…。シン様。" speaker_show="True" stop="True" voice="kt11009_01_0018" />
