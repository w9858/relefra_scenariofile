    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="八千代 命" data_id="311003006" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="師匠なら余裕で制服着られるから復学すりゃいいのに。" speaker_show="True" stop="True" voice="ym11003_01_0041" />
