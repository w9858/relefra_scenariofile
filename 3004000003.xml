    <create type="Character" anchor="Bottom" obj_id="テレジア" data_id="311008005" position_id="left" x="200" />
    <create type="Character" anchor="Bottom" obj_id="白虎" data_id="311007003" position_id="left" x="-200" />
    <char_spine_animation obj_id="テレジア" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" force_end="True" loop="True" to_idle="False" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="白虎" text="おい、子供の母親～！　どこにいるんだ？&#xA;お前の子供は預かった！" speaker_show="True" stop="True" voice="bk11008_01_0116" />
    <char_spine_animation obj_id="テレジア" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="テレジア" text="白虎、それでは私たちが&#xA;誘拐犯のようではないか。" speaker_show="True" stop="True" voice="tr11007_01_0132" />
    <char_spine_animation obj_id="白虎" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="白虎" text="そうか。おい坊主、&#xA;お前、名前は何て言うんだ？" speaker_show="True" stop="True" voice="bk11008_01_0117" />
    <char_spine_animation obj_id="テレジア" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="子供" text="おかーさん！　おかーさーん！" speaker_show="True" stop="True" voice="99043_01_0006" />
    <char_spine_animation obj_id="白虎" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="白虎" text="お前の名前だったのか！&#xA;おい、おかーさん、お前の母親は……。" speaker_show="True" stop="True" voice="bk11008_01_0118" />
    <char_spine_animation obj_id="テレジア" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" face_anim_type="005_Facial_sad" />
    <msg speaker="テレジア" text="待て、白虎。&#xA;恐らくそれは名前じゃない。" speaker_show="True" stop="True" voice="tr11007_01_0133" />
    <char_spine_animation obj_id="白虎" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="白虎" text="くぅ～、これじゃ手がかりが&#xA;何もないじゃないか。どうすればいいんだ？" speaker_show="True" stop="True" voice="bk11008_01_0119" />
    <char_spine_animation obj_id="テレジア" body_anim_type="13_Question" loop="True" to_idle="False" face_anim_type="015_Facial_poker" />
    <msg speaker="テレジア" text="そうだな……では母親の特徴を教えてくれ。&#xA;子供、お前の母親はどんな人物だ？" speaker_show="True" stop="True" voice="tr11007_01_0134" />
    <char_spine_animation obj_id="白虎" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="子供" text="優しくて、可愛くて、眼鏡をしてて……&#xA;ぐずん……ぐずん……。" speaker_show="True" stop="True" voice="99043_01_0007" />
    <char_spine_animation obj_id="テレジア" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="テレジア" text="そうか。はぐれないよう、&#xA;私の手をしっかり掴んでいろ。" speaker_show="True" stop="True" voice="tr11007_01_0135" />
    <msg speaker="子供" text="はぁい……。" speaker_show="True" stop="True" voice="99043_01_0008" />
    <char_spine_animation obj_id="白虎" face_anim_type="101_Facial_unique" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="白虎" text="くっ、テレ公相手だと随分素直だな。" speaker_show="True" stop="True" voice="bk11008_01_0120" />
    <char_spine_animation obj_id="テレジア" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="テレジア" text="ひとまず、眼鏡をかけた女を探そう。" speaker_show="True" stop="True" voice="tr11007_01_0136" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
