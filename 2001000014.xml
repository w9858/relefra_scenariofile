    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="……。" speaker_show="True" stop="True" voice="mm11001_01_0305" />
    <msg_window_show show="False" />
    <move obj_id="源 モモ" position_id="left" x="200" x_move_type="MoveTo" time="1.0" wait="False" />
    <move obj_id="石川 五恵" position_id="left" x="-200" x_move_type="MoveTo" time="1.0" wait="True" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="モモち、おはよう。" speaker_show="True" stop="True" voice="ig11006_01_0097" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="ごえちゃん……おはよう。" speaker_show="True" stop="True" voice="mm11001_01_0306" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="どうしたの、何かあった？" speaker_show="True" stop="True" voice="ig11006_01_0098" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="……明日の夜に、任務があるよね。&#xA;きちんとできるかなって……不安で。" speaker_show="True" stop="True" voice="mm11001_01_0307" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="……そっか……。&#xA;えーと……んん……も、モモち、放課後時間ある？" speaker_show="True" stop="True" voice="ig11006_01_0099" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="え？&#xA;う、うん……？" speaker_show="True" stop="True" voice="mm11001_01_0308" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="1.0" wait="True" />
    <bg show="True" file="12_maidcafe_03" />
    <move obj_id="源 モモ" position_id="left" x="-150" x_move_type="MoveTo" time="0.0" wait="False" />
    <move obj_id="石川 五恵" position_id="left" x="-450" x_move_type="MoveTo" time="0.0" wait="True" />
    <Comment comment="メイド服で出迎える楓" />
    <create obj_id="相模 楓" data_id="311004007" position_id="left" x="400" anchor="Bottom" y="0" type="Character" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="01_Stay" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <transition effect="Default" type="In" color_change="False" time="1.0" wait="True" />
    <telop text="メイド喫茶　夕方" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg speaker="相模 楓" text="おかえりなさいませ、ご主人様ー！" speaker_show="True" stop="True" voice="sf11004_01_0150" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="フーちゃん……！&#xA;ここでバイトしてたんだ！" speaker_show="True" stop="True" voice="mm11001_01_0309" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="理由は色々あるけどね。変装芸に磨きもかけられるし。&#xA;……じゃ、弟子会はじめよっか。" speaker_show="True" stop="True" voice="sf11004_01_0151" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="101_Facial_trouble" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="え？" speaker_show="True" stop="True" voice="mm11001_01_0310" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="明日に備えて３人で集まろうと思って。" speaker_show="True" stop="True" voice="ig11006_01_0100" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="アタシがバイトだから、この店でやろうって&#xA;事になったの。休み時間にこうして話せるでしょ。" speaker_show="True" stop="True" voice="sf11004_01_0152" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="２人とも……私のために……。" speaker_show="True" stop="True" voice="mm11001_01_0311" />
    <msg_window_show show="False" />
    <move obj_id="相模 楓" position_id="left" x="300" x_move_type="MoveTo" time="0.5" wait="False" />
    <move obj_id="源 モモ" position_id="left" x="0" x_move_type="MoveTo" time="0.5" wait="False" />
    <move obj_id="石川 五恵" position_id="left" x="-300" x_move_type="MoveTo" time="0.5" wait="True" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="で、モモちはまーだブルーなわけ？" speaker_show="True" stop="True" voice="sf11004_01_0153" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="……ん……。&#xA;私、スパイ向いてないかなって。" speaker_show="True" stop="True" voice="mm11001_01_0312" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="はっ。たった数回怒られたぐらいで、&#xA;そこまでしぼんでたの？" speaker_show="True" stop="True" voice="sf11004_01_0154" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="でも……。" speaker_show="True" stop="True" voice="mm11001_01_0313" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="弟子になったばかりの時は、私も失敗してるし、&#xA;私の師匠だって失敗してるんだよ。" speaker_show="True" stop="True" voice="ig11006_01_0101" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="え、初芽さんも？" speaker_show="True" stop="True" voice="mm11001_01_0314" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="実は楓ちゃんもね。" speaker_show="True" stop="True" voice="ig11006_01_0102" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="16_BendBackward" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="ちょっと、それは言わないでよ……！" speaker_show="True" stop="True" voice="sf11004_01_0155" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="ご、ごめんごめん。" speaker_show="True" stop="True" voice="ig11006_01_0103" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="010_Facial_shy" body_anim_type="06_ChestLeftHand" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="……そっか、皆同じなんだ。" speaker_show="True" stop="True" voice="mm11001_01_0315" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="私達はチームなんだから。皆で１つ。&#xA;フォローもできるから、安心して。" speaker_show="True" stop="True" voice="ig11006_01_0104" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="――！&#xA;皆で、１つ……。" speaker_show="True" stop="True" voice="mm11001_01_0316" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="「お前のものは俺のもの、俺のものも俺のもの」" speaker_show="True" stop="True" voice="sf11004_01_0156" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="「ワンフォーオール・オールフォーワン」だよ。" speaker_show="True" stop="True" voice="ig11006_01_0105" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="……ツキカゲはチーム……私はその１人。" speaker_show="True" stop="True" voice="mm11001_01_0317" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="源 モモ" text="……そっか……！" speaker_show="True" stop="True" voice="mm11001_01_0318" />
  </command>
