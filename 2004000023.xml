    <sound type="Se" cue_name="Push" />
    <wait time="0.5" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="アミィ" text="今のはどのような意味があるのですか&#xA;お聞かせ下されマスター。" speaker_show="True" stop="True" voice="am13002_02_0053" />
    <move obj_id="敷島 来夢" position_id="left" x="0" x_move_type="MoveTo" time="0.5" wait="True" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="ぬっ、愛よ愛。" speaker_show="True" stop="True" voice="sr13001_02_0375" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="にしても、アミィに身代わりさせるとは&#xA;やるわねマル。ふわー。起きるか。" speaker_show="True" stop="True" voice="sr13001_02_0376" />
    <char_spine_animation obj_id="アミィ" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="アミィ" text="今日も良い天気でございまする、マスター。" speaker_show="True" stop="True" voice="am13002_02_0054" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="……やー。ＺＥＴはコードネームが武将系だから&#xA;アミィのしゃべり方に個性をつけてみたけど。" speaker_show="True" stop="True" voice="sr13001_02_0377" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="016_Facial_smileBitter" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="マルと同じく怪しい口調になってるわねぇ。" speaker_show="True" stop="True" voice="sr13001_02_0378" />
    <char_spine_animation obj_id="アミィ" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="アミィ" text="で、ありますか？" speaker_show="True" stop="True" voice="am13002_02_0055" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="で、あるよ。そういう所よ。" speaker_show="True" stop="True" voice="sr13001_02_0379" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="013_Facial_angry" body_anim_type="15_Negative" force_end="False" loop="False" />
    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="005_Facial_sad" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様失礼な。私は正当派令嬢口調ですわ" speaker_show="True" stop="True" voice="sm13002_02_0188" />
    <char_spine_animation obj_id="アミィ" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="アミィ" text="アミィはこの口調、気に入っておりまする。&#xA;コードネームのヒデヨシも。" speaker_show="True" stop="True" voice="am13002_02_0056" />
    <char_spine_animation obj_id="アミィ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="アミィ" text="今日も一日、楽しく過ごしましょうぞ。" speaker_show="True" stop="True" voice="am13002_02_0057" />
    <msg_window_show show="False" />
