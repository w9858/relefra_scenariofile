    <create type="Character" obj_id="クリッシー" data_id="316001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="クリッシー" text="基地が潰されたおかげでリザードが日本で&#xA;勢力図を拡げる事が遅れてしまいました。" speaker_show="True" stop="True" voice="ks16001_02_0028" />
    <msg speaker="リザード社員" text="しかしゾーイ様は本当に倒されたのでしょうか。&#xA;なにせゾーイ様は。" speaker_show="True" stop="True" voice="rs16002_02_0001" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="クリッシー" text="ゾーイ？　知らない人ですね。ＨＡＨＡＨＡ、&#xA;ここからはワタシの時代です。" speaker_show="True" stop="True" voice="ks16001_02_0029" />
    <msg speaker="リザード社員" text="……クリッシー様なら数年で目標を&#xA;実現できると思います。" speaker_show="True" stop="True" voice="rs16002_02_0002" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="クリッシー" text="ボォブ。素敵な事を言いますね。フフフ、&#xA;ワタシのレジェンドはここからはじまるのです。" speaker_show="True" stop="True" voice="ks16001_02_0030" />
