    <char_spine_animation obj_id="敷島 真留瀬" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="敷島 真留瀬" text="お姉様、朝ですわ。" speaker_show="True" stop="True" voice="sm13002_02_0187" />
    <char_spine_animation obj_id="敷島 来夢" face_anim_type="016_Facial_smileBitter" body_anim_type="24_bodyUnique" force_end="False" loop="True" />
    <msg speaker="敷島 来夢" text="むにゃ……朝起きの胸揉み。せいや。" speaker_show="True" stop="True" voice="sr13001_02_0374" />
    <move obj_id="敷島 来夢" position_id="left" x="100" x_move_type="MoveTo" time="0.5" wait="True" />
    <sound type="Se" cue_name="Push" />
