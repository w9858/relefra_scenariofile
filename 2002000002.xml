    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="False" />
    <create_effect effect_id="Smile_hisui" effect_type="MampSmile" life_time="1.0" target_obj="宗近 飛粋" x="0" y="0" />
    <msg speaker="宗近 飛粋" text="わぁ素敵！&#xA;なんだか仕事が出来る大人になった気分ですー！" speaker_show="True" stop="True" voice="mh12001_01_0057" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="このスーツこそが正義の証よ。防刃に防弾。&#xA;火にも強い特別素材じゃぞ。" speaker_show="True" stop="True" voice="ss12003_01_0075" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="じゃあ早速任務だね。実は今トラックが、&#xA;とあるビルの地下駐車場に向かってるんだ。" speaker_show="True" stop="True" voice="th12004_01_0029" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="07_ChestBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="いきなり任務！　とってもドキドキします。&#xA;でもこういうの覚悟して来ましたので！" speaker_show="True" stop="True" voice="mh12001_01_0058" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="今回の任務は悪徳企業の不正を暴くことじゃ。&#xA;社長室の机の中にある悪事の証拠を入手する。" speaker_show="True" stop="True" voice="ss12003_01_0076" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="02_Idle" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="気取られて証拠隠滅されないように裏道から&#xA;行くって事でアタシ達、凪の出番ってワケ。" speaker_show="True" stop="True" voice="th12004_01_0030" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ビルの構造はこの図面に記されているのじゃ。&#xA;到着するまでに頭の中へ叩きこんどけい。" speaker_show="True" stop="True" voice="ss12003_01_0077" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" force_end="False" to_idle="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="はい隊長！　今、私、スパイって感じがするなぁ。&#xA;やるぞっ……！" speaker_show="True" stop="True" voice="mh12001_01_0059" />
    <char_spine_animation obj_id="薩摩 しぶき" body_anim_type="05_HipBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="その力見せて貰うぞ。凪の部隊。進発！" speaker_show="True" stop="True" voice="ss12003_01_0078" />
