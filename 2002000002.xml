    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="あはは、トラックの中、色々資材が置いてあって&#xA;ドキドキしてきますね、素敵です！" speaker_show="True" stop="True" voice="mh12001_01_0054" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="飛粋はいちいちはしゃぐのー。&#xA;さぁこれが凪の部隊の制服じゃ。着替えい。" speaker_show="True" stop="True" voice="ss12003_01_0073" />
    <char_spine_animation obj_id="宗近 飛粋" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <delete obj_id="宗近 飛粋" />
    <msg speaker="宗近 飛粋" text="わぁ、なんだかとっても大人っぽいですね。&#xA;ええと、こう着るのかな？" speaker_show="True" stop="True" voice="mh12001_01_0055" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="101_Facial_trouble" body_anim_type="16_BendBackward" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ぬお！　こいつ意外と着痩せするタイプじゃな。&#xA;けしからん。はっすーより胸は無いが誤差か？" speaker_show="True" stop="True" voice="ss12003_01_0074" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="はいはい、しぶきち。新入りの下着姿を&#xA;じーっと見てないでアタシ達も着替えよ。" speaker_show="True" stop="True" voice="th12004_01_0027" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="あ。ネクタイは真っ直ぐにした方が&#xA;かっこいいよ飛粋ちゃん。こうして、こう。" speaker_show="True" stop="True" voice="th12004_01_0028" />
    <msg speaker="宗近 飛粋" text="あ、ありがとうございます葉栖美さん！" speaker_show="True" stop="True" voice="mh12001_01_0056" />
