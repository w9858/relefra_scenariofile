    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="宗近飛粋。野生の中で育った、その鋭い五感や&#xA;運動神経を皆の安全のために使ってみねぇか。" speaker_show="True" stop="True" voice="ty12005_01_0032" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="05_HipBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="津守 ヨモギ" text="警視庁、凪の部隊に招待してえんだ。&#xA;国の安全を守る正義の秘密組織だよ。" speaker_show="True" stop="True" voice="ty12005_01_0033" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="私の力が、皆の安全に繋がるなら&#xA;そんな素敵な事はありません。分かりました！" speaker_show="True" stop="True" voice="mh12001_01_0049" />
    <msg_window_show show="False" />
    <fade_out time="1.0" wait="True" type="Default" />
    <delete obj_id="宗近 飛粋" />
    <delete obj_id="津守 ヨモギ" />
    <screen_effect mode="Flashback" show="False" />
    <bg show="True" file="36_school_02" />
