    <create type="Character" obj_id="宗近 飛粋" data_id="312001004" position_id="left" x="-400" y="0" anchor="Bottom" />
    <create obj_id="轟 葉栖美" data_id="312004004" position_id="left" x="150" type="Character" y="0" anchor="Bottom" />
    <create obj_id="薩摩 しぶき" data_id="312003004" position_id="left" x="400" type="Character" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" to_idle="False" force_end="False" loop="False" />
    <msg speaker="宗近 飛粋" text="はい、だいたいは！　さらに詳しい組織の説明は&#xA;現地で先輩方に説明をうけろとヨモギさんが。" speaker_show="True" stop="True" voice="mh12001_01_0050" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="うむ。分からない事も多かろうが、先輩である&#xA;しぶき達がきちんと飛粋を導く。安心せい。" speaker_show="True" stop="True" voice="ss12003_01_0069" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" force_end="False" loop="True" to_idle="False" />
    <msg speaker="宗近 飛粋" text="ありがとうございます！　心強いです。&#xA;ええと……。" speaker_show="True" stop="True" voice="mh12001_01_0051" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" to_idle="False" force_end="False" loop="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="001_Facial_blink" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <move obj_id="薩摩 しぶき" x="0" x_move_type="MoveTo" time="0.5" y="0" wait="False" y_move_type="MoveValue" position_id="left" />
    <move obj_id="轟 葉栖美" x="400" x_move_type="MoveTo" time="0.5" y="0" position_id="left" y_move_type="MoveValue" wait="False" />
    <msg speaker="薩摩 しぶき" text="しぶきの事は隊長と呼ぶのじゃ。実際、凪の部隊&#xA;隊長をしておる。こう体から威厳が溢れてるじゃろ。" speaker_show="True" stop="True" voice="ss12003_01_0070" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" to_idle="False" force_end="False" loop="False" />
    <move obj_id="宗近 飛粋" x="-200" x_move_type="MoveTo" time="0.5" y="0" y_move_type="MoveValue" wait="False" position_id="left" />
    <msg speaker="宗近 飛粋" text="はい隊長！　素敵に宜しくお願いします！" speaker_show="True" stop="True" voice="mh12001_01_0052" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <move obj_id="薩摩 しぶき" x="150" x_move_type="MoveTo" time="0.5" y="0" y_move_type="MoveValue" position_id="left" wait="True" />
    <msg speaker="薩摩 しぶき" text="あまりアップになるでない！&#xA;顔と顔がくっつくじゃろ！" speaker_show="True" stop="True" voice="ss12003_01_0071" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="アタシの呼び方は……そーだなー。一応こっちが&#xA;年上だから葉栖美さんでいーんでないカナ？" speaker_show="True" stop="True" voice="th12004_01_0025" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" force_end="False" loop="True" to_idle="False" />
    <move obj_id="薩摩 しぶき" x="-100" x_move_type="MoveTo" time="0.5" y="0" y_move_type="MoveValue" wait="False" position_id="left" />
    <move obj_id="宗近 飛粋" x="150" x_move_type="MoveTo" time="0.5" y="0" wait="True" y_move_type="MoveValue" position_id="left" />
    <msg speaker="宗近 飛粋" text="葉栖美さん！　宗近飛粋。誕生日は７月２６日！&#xA;血液型はＢ！　宜しくお願いします。" speaker_show="True" stop="True" voice="mh12001_01_0053" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="轟 葉栖美" text="んふふ、握手握手。元気満点な子で&#xA;大変良きじゃない？　しぶきち。" speaker_show="True" stop="True" voice="th12004_01_0026" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="薩摩 しぶき" text="ま。悪を倒すには活力がいるからの。&#xA;よし飛粋よ。車が来る。乗りこむぞ。" speaker_show="True" stop="True" voice="ss12003_01_0072" />
