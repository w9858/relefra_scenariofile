    <msg speaker="ウーメン総帥" text="アナタたち…世界が平和に、ならなくても…&#xA;い…いいって、いうのぉ…？" speaker_show="True" stop="True" voice="us98006_01_0032" />
    <move obj_id="ウーメン総帥" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
