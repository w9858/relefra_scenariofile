    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そんなのアタシたちだって、世界が平和になった方が&#xA;イイに決まってるじゃない。" speaker_show="True" stop="True" voice="sf11004_01_0303" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="でも、他人を無理やり型にはめ込むようなやり方が&#xA;正しいなんて思えない。" speaker_show="True" stop="True" voice="sf11004_01_0304" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="だいたい、アンタたちラヴァーズ・アゲインは&#xA;アタシの大切な人を傷つけたのよ？" speaker_show="True" stop="True" voice="sf11004_01_0305" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="相模 楓" text="そんな連中が口にする世界平和なんて、&#xA;信じられるわけがないじゃない。" speaker_show="True" stop="True" voice="sf11004_01_0306" />
    <char_spine_animation obj_id="ウーメン総帥" face_anim_type="005_Facial_sad" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="ウーメン総帥" text="大きな目的のためには、犠牲がともなうものよぉ…&#xA;そんなことも、わからないなんて…。" speaker_show="True" stop="True" voice="us98006_01_0033" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="だから、わからないって言ってるでしょ？" speaker_show="True" stop="True" voice="sf11004_01_0307" />
    <msg speaker="相模 楓" text="と・に・か・く。" speaker_show="True" stop="True" voice="sf11004_01_0308" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="04_HipLeftHand" force_end="False" loop="True" face_anim_type="008_Facial_serious" />
    <msg speaker="相模 楓" text="アタシの師匠に怪我を負わせたこと、&#xA;これからたーっぷり、後悔させてあげるわ…！" speaker_show="True" stop="True" voice="sf11004_01_0309" />
    <char_spine_animation obj_id="ウーメン総帥" face_anim_type="101_Facial_trouble" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="ウーメン総帥" text="ま、待ちなさいよぉ…こんなに弱っているワタシを、&#xA;まだいたぶろうって言うのぉ…？" speaker_show="True" stop="True" voice="us98006_01_0034" />
    <char_spine_animation obj_id="ウーメン総帥" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="ウーメン総帥" text="ワタシ、心はか弱い乙女に過ぎないのよぉ～ん…？" speaker_show="True" stop="True" voice="us98006_01_0035" />
