    <bg file="20_meiandfuhouse_02" />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv6" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003005" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="パンパカパーン！　ドンドンドン！　パフパフゥ～！&#xA;ハッピーバースデー！" speaker_show="True" stop="True" voice="ym11003_03_0663" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="ってことで、今日、11月22日は&#xA;メイのお誕生日なんだよ～ん！" speaker_show="True" stop="True" voice="ym11003_03_0664" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="え？　知ってた？　そっかー。&#xA;覚えててくれてありがと！" speaker_show="True" stop="True" voice="ym11003_03_0665" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="018_Facial_smug" body_anim_type="17_Hush" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="じゃあ、&#xA;もしかしたらコレも知ってるかもだけど……。" speaker_show="True" stop="True" voice="ym11003_03_0666" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="今日はメイの可愛い弟子、フーの誕生日でも&#xA;あるんだよん♪" speaker_show="True" stop="True" voice="ym11003_03_0667" />
    <char_spine_animation obj_id="八千代 命" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="フーとお互いの誕生日をお祝いし合えるなんて、&#xA;メイってばほんとラッキーだよね。" speaker_show="True" stop="True" voice="ym11003_03_0668" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="あ、もちろんフーだけじゃなくて、みんなが&#xA;お祝いしてくれたのも最高に嬉しいけどね！" speaker_show="True" stop="True" voice="ym11003_03_0669" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="これからも、フーやみんなと一緒に、&#xA;時に楽しく、時にデンジャラスに！" speaker_show="True" stop="True" voice="ym11003_03_0670" />
    <char_spine_animation obj_id="八千代 命" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="刺激的な毎日を送って行きたいと思ってるんだー。" speaker_show="True" stop="True" voice="ym11003_03_0671" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="八千代 命" text="愛する空崎を守るためにも、更に更に&#xA;頑張っちゃうから、引き続き応援ヨロシクね♪" speaker_show="True" stop="True" voice="ym11003_03_0672" />
    <msg_window_show show="False" />
    <fade_out time="0.5" type="Default" wait="True" />
  </command>
