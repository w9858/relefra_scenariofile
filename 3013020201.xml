<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/3013020201" />
  </info>
  <command>
    <Start />
    <load_cue_sheet />
    <sound type="Bgm" cue_name="Adv1" />
    <sound cue_name="EnvSeaside" volume="15" />
    <fade_out time="0" type="Default" wait="False" />
    <msg window_type="SystemMsg" text="黒百合女子の臨海学校の夜、ゆらはクラスメイト達に&#xA;協力して貰い、飛粋たちの誕生日祝いを開催した。" stop="True" />
    <msg window_type="SystemMsg" text="飛粋もしぶきも葉栖美も、サプライズのお祝いに&#xA;驚きながらも大喜びで、パーティは大成功を収めた。" stop="True" />
    <msg window_type="SystemMsg" text="そして、その翌日……" stop="True" />
    <msg_window_show show="False" />
    <bg file="61_Seasideev_01" show="True" resource_type="Background" />
    <fade_in time="0.5" type="Default" wait="True" />
    <telop text="海岸" in_time="1" show_time="2" out_time="1" wait="True" />
    <create obj_id="乱獅子 ゆら" data_id="312002007" position_id="left" x="-200" y="0" anchor="Bottom" type="Character" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="いい日差しだ。日に焼けるにはちょうどいいぜ。" speaker_show="True" stop="True" voice="ry12002_02_0136" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="おいゆら、自由時間に何やってんだ？&#xA;他の奴らは泳ぎに行ってるぜ。" speaker_show="True" stop="True" voice="ty12005_02_0069" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="何って、日光浴だよ。任務で疲れた体で&#xA;わざわざ海に入ってまた疲れることもねーだろ。" speaker_show="True" stop="True" voice="ry12002_02_0137" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="若えのにババアみてえなこと言いやがって。&#xA;で、どうだったんだ？" speaker_show="True" stop="True" voice="ty12005_02_0070" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="何がだよ？" speaker_show="True" stop="True" voice="ry12002_02_0138" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="皆でやったんだろ？&#xA;サプライズのバースデーパーティ。" speaker_show="True" stop="True" voice="ty12005_02_0071" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="いやー、まだまだだね。どうせなら&#xA;もっとデカい場所でやりたかったし。" speaker_show="True" stop="True" voice="ry12002_02_0139" />
    <msg speaker="乱獅子 ゆら" text="3人のためにどでかい花火も&#xA;打ち上げてやりたかったんだけどな。" speaker_show="True" stop="True" voice="ry12002_02_0140" />
    <char_spine_animation obj_id="津守 ヨモギ" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="辛気臭え顔してんじゃねーよ。&#xA;楽しくなかったのか？" speaker_show="True" stop="True" voice="ty12005_02_0072" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="別に、楽しくなかった訳じゃねーけどよ。" speaker_show="True" stop="True" voice="ry12002_02_0141" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="せっかくなら、もっともっとデカく&#xA;ド派手にやりたかったんだよなー。" speaker_show="True" stop="True" voice="ry12002_02_0142" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="あー、思い出したらモヤモヤしてきた！&#xA;来年こそはド派手にぶっ放してやりてーな！" speaker_show="True" stop="True" voice="ry12002_02_0143" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="ははは、それは準備がもっと大変だろうな。&#xA;けど、楽しそうでいい夢じゃねぇか。" speaker_show="True" stop="True" voice="ty12005_02_0073" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="けどさ、今回はアタシ様ともあろうモンが、&#xA;柄にもなくすげぇ緊張してたんだよな……。" speaker_show="True" stop="True" voice="ry12002_02_0144" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="結局みんな喜んで手伝ってくれたけど、&#xA;学校の奴らが協力してくれるかも不安だったし。" speaker_show="True" stop="True" voice="ry12002_02_0145" />
    <msg speaker="乱獅子 ゆら" text="皆に協力させといて任務が終わらなかったら合わせる&#xA;顔がねえ…と思ったら任務中もドキドキだった。" speaker_show="True" stop="True" voice="ry12002_02_0146" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="んで、無事任務が終わったら終わったで、&#xA;本当に喜んでくれるかも心配だったしな……。" speaker_show="True" stop="True" voice="ry12002_02_0147" />
    <msg_window_show show="False" />
    <fade_out time="1" type="Default" wait="True" />
    <sound_stop cue_name="Adv1" />
    <sound_stop cue_name="EnvSeaside" />
  </command>
</adv>