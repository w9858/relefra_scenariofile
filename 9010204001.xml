    <msg speaker="相模 楓" lip_sync="False" text="（なるほど、半蔵さんはこういうとき、&#xA;目を伏せるんだ……。）" speaker_show="True" stop="True" voice="sf11004_02_0615" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002004" position_id="left" x="200" y="-1000" anchor="Bottom" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="……ってあれ？　さっきまでここにいたのに……。&#xA;どこに行っちゃったんだろう？" speaker_show="True" stop="True" voice="sf11004_02_0616" />
    <move obj_id="半蔵門 雪" position_id="left" x="0" x_move_type="MoveValue" y="1000" y_move_type="MoveValue" time="0.3" wait="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="まだまだね、楓。" speaker_show="True" stop="True" voice="hy11002_02_0652" speaker_front="False" />
    <move obj_id="相模 楓" position_id="left" x="-200" x_move_type="MoveValue" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="は、半蔵さん!?　いつの間に!!" speaker_show="True" stop="True" voice="sf11004_02_0617" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="今日ずっとつけていたでしょ？&#xA;さすがに気づくわよ。" speaker_show="True" stop="True" voice="hy11002_02_0653" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="そんな……でも、さすがですね。" speaker_show="True" stop="True" voice="sf11004_02_0618" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="もしかして監視の訓練？" speaker_show="True" stop="True" voice="hy11002_02_0654" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="相模 楓" text="いえ、その……実は、前に半蔵さんに&#xA;変装したときがあったんですけど……。" speaker_show="True" stop="True" voice="sf11004_02_0619" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="モモの加入試験のときのことね。" speaker_show="True" stop="True" voice="hy11002_02_0655" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="はい。自分でも納得できなかったので、&#xA;もっとクオリティーを高めようと観察を……。" speaker_show="True" stop="True" voice="sf11004_02_0620" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="それで私のことをつけていたのね。&#xA;さすが変装のエキスパートだわ。" speaker_show="True" stop="True" voice="hy11002_02_0656" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="見た目が似ているだけじゃ完璧とは言えませんから。&#xA;でも、気づかれてしまうなんて、変装以前の話ですね。" speaker_show="True" stop="True" voice="sf11004_02_0621" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="できれば半蔵さんの仕草や癖、息遣いなんかも&#xA;把握したかったんですが……残念です。" speaker_show="True" stop="True" voice="sf11004_02_0622" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="勝手につけ回すようなことをして&#xA;本当にすみませんでした。" speaker_show="True" stop="True" voice="sf11004_02_0623" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" force_end="False" loop="True" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="謝る必要はないわ。&#xA;スキルの向上はとても大切なことだもの。" speaker_show="True" stop="True" voice="hy11002_02_0657" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="私はただ、つけている理由を知りたかっただけよ。&#xA;でも、そういうことなら協力するわ。" speaker_show="True" stop="True" voice="hy11002_02_0658" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="011_Facial_surprise" body_anim_type="01_Stay" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="協力……ですか？" speaker_show="True" stop="True" voice="sf11004_02_0624" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ええ。私のデータが必要なんでしょう？&#xA;仕草でも癖でも……お好きにどうぞ。" speaker_show="True" stop="True" voice="hy11002_02_0659" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="009_Facial_blush" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="相模 楓" text="え？　あ、あの……？" speaker_show="True" stop="True" voice="sf11004_02_0625" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
