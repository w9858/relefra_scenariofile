    <telop text="無人島" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <msg speaker="乱獅子 ゆら" text="………あれ？" speaker_show="True" stop="True" voice="ry12002_01_0073" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="0" y="-1000" anchor="Bottom" />
    <scale_obj obj_id="宗近 飛粋" value="300" />
    <create_effect effect_id="shockwave" effect_type="EffectShockWave" target_obj="宗近 飛粋" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="102_Facial_unique" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="気がついた？　ゆらちゃん。" speaker_show="True" stop="True" voice="mh12001_01_0153" />
    <msg_window_show show="False" />
    <delete obj_id="shockwave" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
    <wait time="0.1" />
    <delete obj_id="宗近 飛粋" />
    <transition effect="Default" type="In" color_change="False" time="0.5" wait="True" />
    <Comment comment="ゆらの服装は「大八洲師団服」変更予定" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002008" position_id="left" x="-100" anchor="Bottom" y="0" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="-300" anchor="Bottom" y="0" />
    <move obj_id="乱獅子 ゆら" x="300" x_move_type="MoveTo" time="0.5" y="0" y_move_type="MoveValue" wait="False" position_id="left" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="101_Facial_trouble" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="怖っ！？　いきなりアップになるな！&#xA;ってかなんで密着してんだよ！" speaker_show="True" stop="True" voice="ry12002_01_0074" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="うなされてたから……。手を握れば&#xA;楽になるかなって。" speaker_show="True" stop="True" voice="mh12001_01_0154" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="べ、別にうなされてねーし！&#xA;あぁ～だんだんと思いだしてきたぁ！" speaker_show="True" stop="True" voice="ry12002_01_0075" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="お前確か施設が爆発する時、アタシ様を&#xA;かばうようにしたな！？　ありゃあ何でだ！？" speaker_show="True" stop="True" voice="ry12002_01_0076" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="宗近 飛粋" text="理由なんてないよ、体が動いちゃった。" speaker_show="True" stop="True" voice="mh12001_01_0155" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="101_Facial_trouble" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="よく分からない奴だなオメー。" speaker_show="True" stop="True" voice="ry12002_01_0077" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="……そう思われても仕方ないね。本当の&#xA;名前を名乗ってなかったもん。" speaker_show="True" stop="True" voice="mh12001_01_0156" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="私は宗近飛粋。宜しくね、ゆらちゃん。" speaker_show="True" stop="True" voice="mh12001_01_0157" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="むねちか……ひすい。&#xA;変な名前だな。まぁ覚えやすいけどよ。" speaker_show="True" stop="True" voice="ry12002_01_0078" />
    <msg_window_show show="False" />
