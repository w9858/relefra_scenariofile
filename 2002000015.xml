    <msg window_type="SystemMsg" text="離島の宗教施設で任務中、行方不明になった&#xA;宗近飛粋の捜索が続けられている。" stop="True" />
    <msg_window_show show="False" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="001_Facial_blink" body_anim_type="01_Stay" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="くぅ～。まだ見つからないなんて。かなり多くの&#xA;鳥さん達に探してもらってるんだけど……。" speaker_show="True" stop="True" voice="th12004_01_0108" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="探してくれた鳥にお礼をあげなくちゃいけねぇから、&#xA;バケツいっぱい魚だらけだな。" speaker_show="True" stop="True" voice="ty12005_01_0075" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="ヨモギさん、飛粋ちゃんが行方不明なんですから。&#xA;もう少しこう心配というか……。" speaker_show="True" stop="True" voice="th12004_01_0109" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="優しいな葉栖美は。大丈夫だよ。飛粋は。&#xA;さっさと見つけてやろう。" speaker_show="True" stop="True" voice="ty12005_01_0076" />
    <delete obj_id="轟 葉栖美" />
    <delete obj_id="津守 ヨモギ" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="う～む。潮の流れで別の島に流されている可能性&#xA;ありじゃな。捜索範囲を広げていくぞ。" speaker_show="True" stop="True" voice="ss12003_01_0161" />
    <msg_window_show show="False" />
    <transition effect="Default" type="Out" color_change="False" time="0.5" wait="True" />
    <delete obj_id="薩摩 しぶき" />
    <bg show="True" file="21_island_01" />
    <transition effect="Default" type="In" color_change="False" time="0.5" wait="True" />
    <telop text="無人島" in_time="0.5" show_time="0.5" out_time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="やっぱここ無人島だ。わりと流されちゃったのかな。&#xA;みんなと連絡がつかないよ…無人島…無人島。" speaker_show="True" stop="True" voice="mh12001_01_0150" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="009_Facial_blush" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="（こんな事言ったら隊長に怒られると思うけど&#xA;まずい状況なのに少しドキドキしちゃってる）" speaker_show="True" stop="True" voice="mh12001_01_0151" lip_sync="False" />
    <msg_window_show show="False" />
    <delete obj_id="宗近 飛粋" />
    <Comment comment="ゆらの服装は「大八洲師団服」変更予定" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002008" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……く、う。" speaker_show="True" stop="True" voice="ry12002_01_0068" />
    <msg_window_show show="False" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <create_effect effect_id="Exclamation_hisui" effect_type="MampExclamation" life_time="1.0" target_obj="宗近 飛粋" />
    <msg speaker="宗近 飛粋" text="わー！？　ゆらちゃんが倒れてる！？&#xA;私と同じで流されたんだ！？　大丈夫！？" speaker_show="True" stop="True" voice="mh12001_01_0152" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="宗近 飛粋" />
