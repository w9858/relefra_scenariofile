    <create type="Character" obj_id="乱獅子 あやめ" data_id="315002001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002008" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="01_Stay" force_end="False" loop="True" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ゆらぁ。お前また任務をしくじったな。嘆かわしい。&#xA;このあやめ様だけじゃなく、お父様も呆れ顔だぜ。" speaker_show="True" stop="True" voice="ra15002_01_0001" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……次はきっちりやるよ……。" speaker_show="True" stop="True" voice="ry12002_01_0069" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ゆらぁ。ゆらゆら、ゆらよ。そんなのは当たり前だろ！&#xA;まずは、任務を失敗した謝罪をしろってんだ！" speaker_show="True" stop="True" voice="ra15002_01_0002" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="ご…ごめんなさい…姉様。" speaker_show="True" stop="True" voice="ry12002_01_0070" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="一つ上の姉として、お前をうまれた時からこってりと&#xA;しつけてやった。俺は姉であり、お前の主だ。" speaker_show="True" stop="True" voice="ra15002_01_0003" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="主に恥をかかせてんじゃねぇぞ！？&#xA;あぁ、ゆらよ！　他の姉妹になめられるだろ！" speaker_show="True" stop="True" voice="ra15002_01_0004" />
    <msg_window_show show="False" />
    <move obj_id="乱獅子 あやめ" x="0" x_move_type="MoveTo" time="0.5" wait="True" position_id="left" y="0" y_move_type="MoveValue" />
    <sound type="Se" cue_name="Clash" />
    <shake target="Character" obj_id="乱獅子 あやめ" x="20" vibrate="10" time="0.5" wait="True" />
    <sound type="Se" cue_name="Clash" />
    <shake target="Character" obj_id="乱獅子 あやめ" x="20" vibrate="10" time="0.5" wait="True" y="0" />
    <sound type="Se" cue_name="Clash" />
    <wait time="0.5" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <shake target="Character" obj_id="乱獅子 ゆら" x="20" vibrate="20" time="0.5" wait="True" />
    <msg speaker="乱獅子 ゆら" text="……げほっ、ごほっ、くぅ……。" speaker_show="True" stop="True" voice="ry12002_01_0071" />
    <move obj_id="乱獅子 あやめ" x="-200" x_move_type="MoveTo" time="0.5" y="0" y_move_type="MoveValue" wait="True" position_id="left" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ゆらのお父様譲りの回復能力は凄ぇよ。&#xA;おかげで心おきなく教育できる。" speaker_show="True" stop="True" voice="ra15002_01_0005" />
    <char_spine_animation obj_id="乱獅子 あやめ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 あやめ" text="ゆらぁ。お前は俺のために生まれてきた。&#xA;だからもっと俺のために頑張るんだよ。" speaker_show="True" stop="True" voice="ra15002_01_0006" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="（じょ、冗談じゃねぇ……そんな理由なら&#xA;生まれてこない方が、よっぽど…上等だったぜ…）" speaker_show="True" stop="True" voice="ry12002_01_0072" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
    <delete obj_id="乱獅子 あやめ" />
    <delete obj_id="乱獅子 ゆら" />
    <screen_effect mode="FlashbackWhite" show="False" />
    <bg show="True" file="21_island_01" />
