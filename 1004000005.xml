    <delete obj_id="宗近 飛粋" />
    <delete obj_id="石川 五恵" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" x="200" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" x="-200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="凪の部隊はコードネームを明王からとってるんだ。&#xA;悪に対する怒りってところだな。" speaker_show="True" stop="True" voice="ty12005_01_0002" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="アタシ様は乱獅子ゆらだ。黒百合女子２年１組。&#xA;コードネームは……なんだっけ？　愛染だ！" speaker_show="True" stop="True" voice="ry12002_01_0011" />
    <delete obj_id="津守 ヨモギ" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" x="0" y="0" anchor="Bottom" position_id="left" />
