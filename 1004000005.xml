    <create type="Character" obj_id="石川 五恵" data_id="311006001" x="-400" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="002_Facial_smile" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="女子高生でスパイ。私たちと同じだ。" speaker_show="True" stop="True" voice="ig11006_01_0048" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="……む。" speaker_show="True" stop="True" voice="ss12003_01_0014" />
