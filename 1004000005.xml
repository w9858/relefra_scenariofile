    <create type="Character" obj_id="カトリーナ" data_id="311009001" x="300" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="凪の部隊。国家の平穏を保つべく裏で&#xA;暗躍する秘密組織……。" speaker_show="True" stop="True" voice="hy11002_01_0059" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="源 モモ" text="ご存じなんですね師匠。流石です！" speaker_show="True" stop="True" voice="mm11001_01_0098" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="つまり彼女達は手を組むべき味方というわけなのよ。&#xA;各地の情報機関と手を組もうと思った時……。" speaker_show="True" stop="True" voice="kt11009_01_0039" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="まず真っ先に。警察との連携を&#xA;密にしておかなきゃと思って。" speaker_show="True" stop="True" voice="kt11009_01_0040" />
    <delete obj_id="源 モモ" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="0" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="じゃあこの騒動は、手を組むに値するか&#xA;どうかアタシ達の実力を試されたとか？" speaker_show="True" stop="True" voice="sf11004_01_0117" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" x="-300" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" x="0" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" x="300" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="まさにそれ。やーごめんね。&#xA;でも、こちらの力も提示できたでしょ。" speaker_show="True" stop="True" voice="th12004_01_0005" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="試しが好きなんだよね～。凪の部隊って。" speaker_show="True" stop="True" voice="mh12001_01_0007" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="False" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="足を引っ張られるのは嫌だからのう。&#xA;納得するにはこれが手っ取り早いのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0012" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="ということでお騒がせしてごめんなさいでした。" speaker_show="True" stop="True" voice="mh12001_01_0008" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="ごめんなさいでした。" speaker_show="True" stop="True" voice="th12004_01_0006" />
    <delete obj_id="薩摩 しぶき" />
    <delete obj_id="轟 葉栖美" />
    <delete obj_id="宗近 飛粋" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" x="0" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="ツキカゲはさ、弟子っていうワードが&#xA;半人前を連想させてちょいと不安だったわけよ。" speaker_show="True" stop="True" voice="ry12002_01_0009" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="だから弟子たちをテストしてやったんだが&#xA;やー全然実戦イケルな。アタシ様的に合格だ。" speaker_show="True" stop="True" voice="ry12002_01_0010" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" x="300" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="八千代 命" data_id="311003001" x="0" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="-300" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="カトリーナ" text="まず試しが条件っていうから。驚かせちゃったわね。&#xA;でも皆なら絶対大丈夫と信じてたわ。" speaker_show="True" stop="True" voice="kt11009_01_0041" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="005_Facial_sad" body_anim_type="12_DropShoulders" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="まーったくカトーさんも人が悪いや。&#xA;味方さえ騙すなんて。" speaker_show="True" stop="True" voice="ym11003_01_0075" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="15_Negative" loop="False" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="どの口が言っているのか。" speaker_show="True" stop="True" voice="sf11004_01_0118" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="八千代 命" />
    <delete obj_id="相模 楓" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" x="-200" y="0" anchor="Bottom" position_id="left" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" x="200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="お前らの話はそこにいるトビー家の女……&#xA;カトリーナ・トビーから聞いたのじゃ。" speaker_show="True" stop="True" voice="ss12003_01_0013" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="だからこっちが自己紹介をする番だね。&#xA;それじゃはいはい！　私から行くよ！" speaker_show="True" stop="True" voice="mh12001_01_0009" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="凪の部隊・宗近飛粋。黒百合女子２年１組。&#xD;&#xA;コードネームは不動だよ。" speaker_show="True" stop="True" voice="mh12001_01_0010" />
    <msg_window_show show="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <move obj_id="薩摩 しぶき" x="150" x_move_type="MoveTo" time="0.5" wait="False" position_id="left" y="0" y_move_type="MoveValue" />
