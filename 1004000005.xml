    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="轟 葉栖美" text="アタシは轟葉栖美。黒百合の３年。&#xA;コードネームは孔雀だよ。宜しくツキカゲさん達。" speaker_show="True" stop="True" voice="th12004_01_0007" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="しぶきは薩摩しぶき。黒百合３年。凪の部隊、隊長。&#xA;コードネームは軍荼利じゃ。" speaker_show="True" stop="True" voice="ss12003_01_0015" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="津守 ヨモギ" text="で、俺は黒百合で教師をやってる津守ヨモギ。&#xA;こいつらのサポートとか色々やってる。" speaker_show="True" stop="True" voice="ty12005_01_0003" />
    <sound_volume type="Bgm" volume="0" time="1.0" wait="False" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <msg speaker="薩摩 しぶき" text="はじめにひとつ言っておくのじゃ。&#xA;しぶき達はお前達と仲良くやるつもりなどない！" speaker_show="True" stop="True" voice="ss12003_01_0016" />
