    <msg speaker="青葉 初芽" text="乱獅子といえば大八洲師団の頭領が&#xA;確か乱獅子原野という…。" speaker_show="True" stop="True" voice="ah11005_01_0041" />
    <msg_window_show show="False" />
    <move obj_id="青葉 初芽" x="-200" x_move_type="MoveTo" time="0.5" wait="True" position_id="left" y="0" y_move_type="MoveValue" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" x="200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="そーだ。アタシ様は大八洲の娘だったけど、せいやって&#xA;寝返った。肉親ども等しくダメ人間揃いでね～。" speaker_show="True" stop="True" voice="ry12002_01_0012" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="アタシ様が全員逮捕して刑務所エンドにしてやる。&#xA;それが出来の良い妹としての、出来る事さ。" speaker_show="True" stop="True" voice="ry12002_01_0013" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="相模 楓" data_id="311004001" x="-200" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（そんな素性の人間で大丈夫でしょうね？）" speaker_show="True" stop="True" voice="sf11004_01_0119" lip_sync="False" />
    <msg_window_show show="False" />
    <move obj_id="乱獅子 ゆら" x="450" x_move_type="MoveTo" time="0.5" wait="True" position_id="left" y="0" y_move_type="MoveValue" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001001" x="250" y="0" anchor="Bottom" position_id="left" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="宗近 飛粋" text="ちなみに、ゆらちゃんは私の素敵な友達！！" speaker_show="True" stop="True" voice="mh12001_01_0011" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="14_Positive" loop="False" to_idle="False" force_end="False" />
    <msg speaker="乱獅子 ゆら" text="……ま。まぁな……。へへ。" speaker_show="True" stop="True" voice="ry12002_01_0014" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="002_Facial_smile" body_anim_type="16_BendBackward" loop="False" to_idle="False" force_end="False" />
    <msg speaker="相模 楓" text="（あ、大丈夫だわコレ。テレジアと同じようなもんね）" speaker_show="True" stop="True" voice="sf11004_01_0120" lip_sync="False" />
