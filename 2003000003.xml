<?xml version="1.0" encoding="utf-16"?>
<adv file="spyceadvfile">
  <info>
    <VoiceCueSheet value="AdvVoice/2003000003" />
  </info>
  <command>
    <Start />
    <sound type="Bgm" cue_name="Briefing" />
    <bg show="True" file="03_tsukikage_001" />
    <fade_in time="0.5" wait="True" />
    <msg window_type="SystemMsg" text="一五〇〇　ツキカゲ基地&#xA;軍議の社に、ツキカゲの全メンバーが集まっていた。" stop="True" />
    <msg window_type="SystemMsg" text="藤林長穂と半蔵門雪、高坂信と八千代メイ、&#xA;カトリーナ・トビーと青葉初芽の３組の師弟である。" stop="True" />
    <create type="Character" obj_id="高坂 信" data_id="311010001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="八千代 命" data_id="311003001" position_id="left" x="125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="メイちゃんメイちゃん、この動画知ってる？&#xA;クラスで流行ってるんだけど。" speaker_show="True" stop="True" voice="ks11010_01_0085" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="初めて見る。師匠、動物系好きだなー。" speaker_show="True" stop="True" voice="ym11003_01_0211" />
    <delete obj_id="高坂 信" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="（いつものことながら、先輩とメイの師弟は、&#xA;一見友達にしか見えないわね……）" speaker_show="True" stop="True" voice="hy11002_01_0253" lip_sync="False" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="師匠、香水変えました？" speaker_show="True" stop="True" voice="ah11005_01_0126" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="さすが初芽ちゃん、気づいたのね。&#xA;初夏だから。" speaker_show="True" stop="True" voice="kt11009_01_0084" />
    <delete obj_id="青葉 初芽" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="（この２人には熟年夫婦のような落ち着きがある。&#xA;ただ、カトリーナさんと先輩の関係が……）" speaker_show="True" stop="True" voice="hy11002_01_0254" lip_sync="False" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="-125" y="0" anchor="Bottom" />
    <create type="Character" obj_id="高坂 信" data_id="311010001" position_id="left" x="125" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="シン様。&#xA;私もシン様が見つけた動物動画、見てみたいです。" speaker_show="True" stop="True" voice="kt11009_01_0085" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="016_Facial_smileBitter" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="カトさん……様付けはやめてって言ってるのに。" speaker_show="True" stop="True" voice="ks11010_01_0086" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="私はあなたの中に神を見ました。&#xA;だから、シン様と呼ばせていただきたいんです。" speaker_show="True" stop="True" voice="kt11009_01_0086" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="高坂 信" text="信ごときが神なんて……罰が当たっちゃうよ。" speaker_show="True" stop="True" voice="ks11010_01_0087" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="その謙遜。&#xA;シン様の美徳ですね。" speaker_show="True" stop="True" voice="kt11009_01_0087" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="高坂 信" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="（……カトリーナさんは、なぜ先輩を&#xA;あそこまで崇拝しているのかしら……）" speaker_show="True" stop="True" voice="hy11002_01_0255" lip_sync="False" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="……ところで、カトリーナさん。&#xA;今日の招集の目的はなんでしょうか？" speaker_show="True" stop="True" voice="hy11002_01_0256" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="……先日から起きている女子中学生連続誘拐事件よ。" speaker_show="True" stop="True" voice="kt11009_01_0088" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="調査の結果、失踪した少女には全員、&#xA;ある男との接点があった。彼が主犯でしょう。" speaker_show="True" stop="True" voice="kt11009_01_0089" />
    <msg speaker="カトリーナ" text="犯罪歴のない一般人の筈だけど……手口が巧妙過ぎる。&#xA;彼に協力している組織があるかもしれない。" speaker_show="True" stop="True" voice="kt11009_01_0090" />
    <move obj_id="カトリーナ" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="藤林 長穂" data_id="311011001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="藤林 長穂" body_anim_type="13_Question" loop="True" to_idle="False" />
    <msg speaker="藤林 長穂" text="……アイツらかー。" speaker_show="True" stop="True" voice="fn11011_01_0023" />
    <char_spine_animation obj_id="藤林 長穂" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="藤林 長穂" text="じゃ、この事件の主犯を捕まえれば、&#xA;そこからアイツらに辿り着けるかもしれないねー。" speaker_show="True" stop="True" voice="fn11011_01_0024" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <char_spine_animation obj_id="藤林 長穂" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="藤林 長穂" text="で、どーやって誘拐犯を捕まえるの？" speaker_show="True" stop="True" voice="fn11011_01_0025" />
    <char_spine_animation obj_id="カトリーナ" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="今回は、囮捜査をしようと思う。" speaker_show="True" stop="True" voice="kt11009_01_0091" />
    <char_spine_animation obj_id="藤林 長穂" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <move obj_id="藤林 長穂" position_id="left" x="-300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="False" />
    <move obj_id="カトリーナ" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="八千代 命" data_id="311003001" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="わーお、刺激的！　囮は誰がやるの？&#xA;狙われるのは中学生だから、メイたちかな？" speaker_show="True" stop="True" voice="ym11003_01_0212" />
    <char_spine_animation obj_id="藤林 長穂" face_anim_type="003_Facial_laugh" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="藤林 長穂" text="囮だったら、もっと適任がいるよー。ね？" speaker_show="True" stop="True" voice="fn11011_01_0026" />
    <delete obj_id="カトリーナ" />
    <delete obj_id="藤林 長穂" />
    <delete obj_id="八千代 命" />
    <create type="Character" obj_id="高坂 信" data_id="311010001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="……え？" speaker_show="True" stop="True" voice="ks11010_01_0088" />
    <create type="Character" obj_id="カトリーナ" data_id="311009001" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="カトリーナ" text="シン様、あなたの神の域と言える一般人を装う偽装力。&#xA;それがあれば、囮役の成功は間違いありません。" speaker_show="True" stop="True" voice="kt11009_01_0092" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="え？" speaker_show="True" stop="True" voice="ks11010_01_0089" />
    <create type="Character" obj_id="八千代 命" data_id="311003001" position_id="left" x="300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="八千代 命" text="あ、なるほど！　確かに師匠なら完璧だ！" speaker_show="True" stop="True" voice="ym11003_01_0213" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="え？" speaker_show="True" stop="True" voice="ks11010_01_0090" />
    <delete obj_id="高坂 信" />
    <delete obj_id="八千代 命" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002015" position_id="left" x="200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="11_Point" loop="True" to_idle="False" />
    <msg speaker="半蔵門 雪" text="先輩なら、確かに。" speaker_show="True" stop="True" voice="hy11002_01_0257" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="青葉 初芽" text="最適な人選ですね。" speaker_show="True" stop="True" voice="ah11005_01_0127" />
    <delete obj_id="半蔵門 雪" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="高坂 信" data_id="311010001" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="えええええええええええええええ!?" speaker_show="True" stop="True" voice="ks11010_01_0091" />
  </command>
</adv>