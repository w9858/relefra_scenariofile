    <create type="Character" anchor="Bottom" obj_id="クリッシー" data_id="316001001" position_id="left" x="0" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="しおんも冴魅も、欲が足りませんね。&#xA;空崎の攻略をワタシに一任するとは。" speaker_show="True" stop="True" voice="ks16001_01_0049" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="一番乗りして、美味しいところは&#xA;リザードが全て頂いてしまいましょう。" speaker_show="True" stop="True" voice="ks16001_01_0050" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="ボォブ。社長に連絡しておいてください。" speaker_show="True" stop="True" voice="ks16001_01_0051" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="もうすぐ空崎を手にいれると。そのための&#xA;経費としてショッピングをしますと。" speaker_show="True" stop="True" voice="ks16001_01_0052" />
    <msg speaker="リザード社員" text="はい、クリッシー様。&#xA;何をお買いになられるのですか？" speaker_show="True" stop="True" voice="rs16002_01_0010" />
    <char_spine_animation obj_id="クリッシー" body_anim_type="09_PutHands" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="空崎で一番高い建造物を調べて下さい。&#xA;それです。どうせなら高みから見下ろすのがいい。" speaker_show="True" stop="True" voice="ks16001_01_0053" />
    <char_spine_animation obj_id="クリッシー" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="クリッシー" text="（空崎の攻略も、連合も、ワタシがレジェンドになる&#xA;礎に過ぎません。次にいくためにも決着をつけます）" speaker_show="True" stop="True" voice="ks16001_01_0054" lip_sync="False" />
    <msg_window_show show="False" />
    <fade_out time="1.0" type="Default" wait="True" />
