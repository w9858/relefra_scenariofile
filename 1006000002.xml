    <create type="Character" anchor="Bottom" obj_id="源 モモ" data_id="311001001" position_id="left" x="200" />
    <create type="Character" anchor="Bottom" obj_id="半蔵門 雪" data_id="311002001" position_id="left" x="-200" />
