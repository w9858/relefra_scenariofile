    <char_spine_animation obj_id="相模 楓" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="資料を見返しているけど、リザードも、&#xA;倶楽部トリムヘイムも、本当にひどいね。" speaker_show="True" stop="True" voice="mm11001_01_0122" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="リザードは、軍事用の人形を売りたいから&#xA;戦争や内紛が起こるように煽ったりね。" speaker_show="True" stop="True" voice="sf11004_01_0128" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" loop="True" to_idle="False" />
    <char_spine_animation obj_id="相模 楓" body_anim_type="15_Negative" loop="False" to_idle="False" />
    <msg speaker="相模 楓" text="お金は稼ぎたいけど、流石に&#xA;このやり方はドン引きだわ。" speaker_show="True" stop="True" voice="sf11004_01_0129" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="こんな奴等はアタシ達でぶっ潰すのよ。" speaker_show="True" stop="True" voice="sf11004_01_0130" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" loop="True" to_idle="False" />
    <char_spine_animation obj_id="源 モモ" body_anim_type="14_Positive" loop="False" to_idle="False" />
    <msg speaker="源 モモ" text="うん！　よーしそのためにも鍛錬だ。" speaker_show="True" stop="True" voice="mm11001_01_0123" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" loop="True" to_idle="False" />
    <msg speaker="相模 楓" text="単純ねぇ。モモちらしいけど。" speaker_show="True" stop="True" voice="sf11004_01_0131" />
    <delete obj_id="相模 楓" />
    <delete obj_id="源 モモ" />
    <create type="Character" anchor="Bottom" obj_id="半蔵門 雪" data_id="311002001" position_id="left" x="0" />
