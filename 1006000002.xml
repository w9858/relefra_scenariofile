    <create type="Character" anchor="Bottom" obj_id="相模 楓" data_id="311004001" position_id="left" x="0" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="015_Facial_poker" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" />
