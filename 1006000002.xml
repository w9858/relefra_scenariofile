    <msg speaker="半蔵門 雪" text="では、私を上にのせて腕立てしてみなさい。" speaker_show="True" stop="True" voice="hy11002_01_0069" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" />
