    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="源 モモ" data_id="311001013" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="源 モモ" text="なんか大人っぽい感じになるんですね！" speaker_show="True" stop="True" voice="mm11001_02_0094" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="018_Facial_smug" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="アタシ様の足元にゃ及ばねえけど、&#xA;中々似合っているじゃないか。" speaker_show="True" stop="True" voice="ry12002_02_0063" />
    <delete obj_id="乱獅子 ゆら" />
    <delete obj_id="源 モモ" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003001" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004001" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="そうじゃな。このまま凪の部隊に&#xA;正式入隊をしてみてはどうじゃ？" speaker_show="True" stop="True" voice="ss12003_02_0115" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="しぶきち、勧誘まだ諦めてなかったんだね。&#xA;ま、アタシも大歓迎だけど。" speaker_show="True" stop="True" voice="th12004_02_0067" />
    <move obj_id="轟 葉栖美" position_id="left" x="300" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <move obj_id="薩摩 しぶき" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="津守 ヨモギ" data_id="312005001" position_id="left" x="-300" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="盛り上がってるとこ悪りぃが、お前ら３人、&#xA;ちょっと席外してくれねえか？" speaker_show="True" stop="True" voice="ty12005_02_0044" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="何故じゃ？　しぶきはまだ勧誘の途中…" speaker_show="True" stop="True" voice="ss12003_02_0116" />
    <char_spine_animation obj_id="津守 ヨモギ" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="津守 ヨモギ" text="モモと２人きりで話がしてえんだよ。&#xA;オラオラ、さっさと出てけ！" speaker_show="True" stop="True" voice="ty12005_02_0045" />
