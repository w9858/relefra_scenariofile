    <msg speaker="源 モモ" text="年上の頼れる先輩ですよ。明さん。" speaker_show="True" stop="True" voice="mm11001_01_1018" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="…頼れるって言われると嬉しいな。&#xA;よーーし、どんどん頼ってね。" speaker_show="True" stop="True" voice="ks11010_01_0230" />
