    <char_spine_animation obj_id="源 モモ" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="私は、この街が好きだから。守りたいと思って。" speaker_show="True" stop="True" voice="mm11001_01_1022" />
    <char_spine_animation obj_id="高坂 信" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="くじけそうな時もありましたけど、&#xA;皆のおかげで、ここにいます。" speaker_show="True" stop="True" voice="mm11001_01_1023" />
