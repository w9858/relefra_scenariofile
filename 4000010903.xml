    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="この味で育ちました！　商店街については&#xA;マスターしてるんで、何でも説明しますよ。" speaker_show="True" stop="True" voice="mm11001_01_1016" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="07_ChestBothHands" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="うん、モモちゃん、今日はありがとね。" speaker_show="True" stop="True" voice="ks11010_01_0228" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="003_Facial_laugh" body_anim_type="09_PutHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="高坂 信" text="フーちゃんとは結構お話したけど、&#xA;もっといろんな人と話したいなって。" speaker_show="True" stop="True" voice="ks11010_01_0229" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" to_idle="False" />
    <msg speaker="源 モモ" text="嬉しいです、信先輩と二人で遊べるなんて。" speaker_show="True" stop="True" voice="mm11001_01_1017" />
    <delete obj_id="高坂 信" />
    <delete obj_id="源 モモ" />
    <msg speaker="諸星 明" text="モモちゃん、その子はクラスメートかい？&#xA;いいね、俺のコロッケで友情をどんどん深めてよ。" speaker_show="True" stop="True" voice="ma99004_01_0011" />
    <Comment comment="信の服装は「私服（夏）」変更予定" />
    <create type="Character" obj_id="高坂 信" data_id="311010003" position_id="left" x="200" y="0" anchor="Bottom" />
