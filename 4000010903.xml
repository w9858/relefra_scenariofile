    <msg speaker="高坂 信" text="……信はね、この目立たない特技が役に立つなら&#xA;嬉しいってツキカゲをはじめたけど。" speaker_show="True" stop="True" voice="ks11010_01_0235" />
    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" to_idle="False" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="002_Facial_smile" force_end="False" loop="True" to_idle="False" />
