    <delete obj_id="宗近 飛粋" />
    <delete obj_id="轟 葉栖美" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="こちらチームは……そうね……。&#xA;モモ、あなたが行きなさい。" speaker_show="True" stop="True" voice="hy11002_02_0130" />
    <create type="Character" obj_id="源 モモ" data_id="311001004" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="源 モモ" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" force_end="False" loop="False" mixing="True" />
    <msg speaker="源 モモ" text="えー！　私がツキカゲの代表!?&#xA;本当に本当にいいんですか！" speaker_show="True" stop="True" voice="mm11001_02_0046" />
    <delete obj_id="半蔵門 雪" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="大丈夫ですよ。モモちゃんの味覚と嗅覚は、&#xA;特殊技能を持つ凪の部隊でも役に立つはずです。" speaker_show="True" stop="True" voice="ah11005_02_0193" />
