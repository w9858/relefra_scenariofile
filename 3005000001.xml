    <bg show="True" resource_type="Background" file="02_wasabimo_001" effect="Immediately" wait="False" effect_time="0.3" color="#ffffffff" />
    <fade_in time="0.5" wait="True" />
    <telop text="Wasabi" in_time="0.5" show_time="1.0" out_time="0.5" wait="True" />
    <create type="Character" obj_id="宗近 飛粋" data_id="312001002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="宗近 飛粋" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="宗近 飛粋" text="もぐもぐもぐ。ん～、素敵なスパイスの香り！&#xA;Wasabiのカレーはやっぱり最高です！" speaker_show="True" stop="True" voice="mh12001_02_0044" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="ああ、はるばる空崎まで食いにきた甲斐が&#xA;あったってモンだぜ。もぐもぐ、はぐはぐ。" speaker_show="True" stop="True" voice="ry12002_02_0034" />
    <delete obj_id="宗近 飛粋" />
    <create type="Character" obj_id="薩摩 しぶき" data_id="312003002" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="05_HipBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="こら！　しぶき達はカレーを食べるために&#xA;空崎まで来た訳ではないぞ！" speaker_show="True" stop="True" voice="ss12003_02_0085" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="轟 葉栖美" data_id="312004002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" mixing="True" />
    <msg speaker="轟 葉栖美" text="一番たくさん食べてるしぶきちが言うと、&#xA;全っ然、説得力ないけどねー。" speaker_show="True" stop="True" voice="th12004_02_0051" />
    <delete obj_id="轟 葉栖美" />
    <move obj_id="薩摩 しぶき" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <create type="Character" obj_id="カトリーナ" data_id="311009003" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="カトリーナ" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="カトリーナ" text="それで、ツキカゲに折り入っての&#xA;相談と言うのは？" speaker_show="True" stop="True" voice="kt11009_02_0075" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="まぁそう急ぐでない。ぷは～、食ったのう。&#xA;カレーの代金は警視庁宛に請求してくれ。" speaker_show="True" stop="True" voice="ss12003_02_0086" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="では本題に入る。今回は互いの今後を見据えて&#xA;ある提案を持ってきたのじゃ。" speaker_show="True" stop="True" voice="ss12003_02_0087" />
    <delete obj_id="カトリーナ" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="青葉 初芽" text="提案？　ですか。" speaker_show="True" stop="True" voice="ah11005_02_0192" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="今後も凪の部隊とツキカゲの共同ミッションの&#xA;機会は増えていくことじゃろう。" speaker_show="True" stop="True" voice="ss12003_02_0088" />
    <char_spine_animation obj_id="薩摩 しぶき" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="薩摩 しぶき" text="そこで、互いの組織の特徴や実力を&#xA;もっと知るべきだと思うんじゃ。" speaker_show="True" stop="True" voice="ss12003_02_0089" />
    <delete obj_id="青葉 初芽" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002004" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" mixing="True" />
    <msg speaker="半蔵門 雪" text="確かにそれは必須事項ね。何か具体的な手段を&#xA;提案しに来たと言うことかしら？" speaker_show="True" stop="True" voice="hy11002_02_0129" />
    <delete obj_id="薩摩 しぶき" />
    <create type="Character" obj_id="乱獅子 ゆら" data_id="312002002" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="ああ、とっておきのプランを今からこのアタシ様が&#xA;発表しちゃうぜ！　んじゃ、ドラムロールスタート！" speaker_show="True" stop="True" voice="ry12002_02_0035" />
    <delete obj_id="半蔵門 雪" />
    <move obj_id="乱獅子 ゆら" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="003_Facial_laugh" body_anim_type="20_PositiveDouble" force_end="False" loop="False" mixing="True" />
    <msg speaker="乱獅子 ゆら" text="だららららららら……じゃん！&#xA;その作戦とは！　テッテレー！" speaker_show="True" stop="True" voice="ry12002_02_0036" />
    <delete obj_id="乱獅子 ゆら" />
    <create type="Character" obj_id="石川 五恵" data_id="311006004" position_id="left" x="-200" y="0" anchor="Bottom" />
