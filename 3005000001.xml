    <char_spine_animation obj_id="石川 五恵" face_anim_type="016_Facial_smileBitter" body_anim_type="01_Stay" force_end="False" loop="True" mixing="True" />
    <msg speaker="石川 五恵" text="全部、自分で言っちゃうんだ……。" speaker_show="True" stop="True" voice="ig11006_02_0047" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" mixing="True" />
    <msg speaker="相模 楓" text="凪の部隊にも変な奴がいるのね。" speaker_show="True" stop="True" voice="sf11004_02_0024" />
