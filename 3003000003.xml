    <fade_in time="0.5" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" />
    <msg speaker="半蔵門 雪" text="はぁぁぁっ！" speaker_show="True" stop="True" voice="hy11002_01_0370" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="Slash" />
    <create_effect effect_id="斬撃" effect_type="EffectSlash" life_time="1.5" />
    <wait time="1.5" />
    <msg speaker="青葉 初芽" text="半蔵、その調子です！　透明効果は無事&#xA;持続中なので、人形も混乱しています。" speaker_show="True" stop="True" voice="ah11005_01_0173" />
    <msg speaker="半蔵門 雪" text="この程度の相手なら、一瞬で殲滅できそうだわ。" speaker_show="True" stop="True" voice="hy11002_01_0371" />
    <sound type="Se" />
    <Comment comment="SE ボコボコと人形を倒す音" />
    <msg speaker="青葉 初芽" text="今日の戦闘はスーツのテストも兼ねていますから、&#xA;あまり無理はしないでください！" speaker_show="True" stop="True" voice="ah11005_01_0174" />
    <char_spine_animation obj_id="半蔵門 雪" body_anim_type="18_ArmsCrossed" loop="False" />
    <msg speaker="半蔵門 雪" text="そうだったわね。機能性を確かめながら&#xA;ゆっくりいたぶってあげることにするわ。" speaker_show="True" stop="True" voice="hy11002_01_0372" />
    <msg speaker="青葉 初芽" text="今回はスーツの耐久性と&#xA;クリームの持続性を中心に検証してください。" speaker_show="True" stop="True" voice="ah11005_01_0175" />
    <msg speaker="半蔵門 雪" text="半蔵、了解。" speaker_show="True" stop="True" voice="hy11002_01_0373" />
    <msg_window_show show="False" />
    <sound type="Se" />
    <Comment comment="SE 攻撃音" />
    <Comment comment="半蔵、半透明に" />
    <Comment comment="キャラを透過状態→徐々に出現ができないため、moveで代用" />
    <msg speaker="青葉 初芽" text="はっ！　半蔵、直ちに敵を殲滅して撤収して&#xA;ください。クリームの効果が……。" speaker_show="True" stop="True" voice="ah11005_01_0176" />
    <msg_window_show show="False" />
    <char_alpha_value obj_id="半蔵門 雪" value="1" time="1" wait="True" />
    <char_alpha obj_id="半蔵門 雪" enable="False" quality="Default" />
    <wait time="1.0" />
    <Comment comment="半蔵の姿くっきり表示" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="切れる時は一瞬なのね。" speaker_show="True" stop="True" voice="hy11002_01_0374" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="一気にとどめをさすわよ。てーい！" speaker_show="True" stop="True" voice="hy11002_01_0375" />
    <msg_window_show show="False" />
    <delete obj_id="半蔵門 雪" />
    <sound type="Se" cue_name="Slash" />
    <create_effect effect_id="斬撃" effect_type="EffectSlash" life_time="1.5" />
    <wait time="1.5" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005015" position_id="left" x="0" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="ミッション達成です。" speaker_show="True" stop="True" voice="ah11005_01_0176a" />
    <move obj_id="青葉 初芽" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002009" position_id="left" x="-200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="雪ちゃん、お疲れ様でした。&#xA;スーツを着ていてくれて本当に良かったです。" speaker_show="True" stop="True" voice="ah11005_01_0177" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="101_Facial_trouble" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="半蔵門 雪" text="そうね、もし着ていなかったらと思うと、&#xA;ゾッとするわ。" speaker_show="True" stop="True" voice="hy11002_01_0377" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="どこか動きづらい箇所はありましたか？" speaker_show="True" stop="True" voice="ah11005_01_0178" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="まだまだ改良の余地がありそうだけど、&#xA;素材の耐久性はなかなか優秀ね。" speaker_show="True" stop="True" voice="hy11002_01_0378" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="そっちはどう？　私と初芽では戦闘中の動きも&#xA;随分違うから、別の感想があるんじゃない？" speaker_show="True" stop="True" voice="hy11002_01_0379" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="青葉 初芽" text="そうですね、実際に着てみると……ふふ。&#xA;このピッタリ感、癖になりそうです。" speaker_show="True" stop="True" voice="ah11005_01_0179" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="そういう感想を聞いた訳ではないのだけど……。" speaker_show="True" stop="True" voice="hy11002_01_0380" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="005_Facial_sad" loop="True" to_idle="True" force_end="False" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="青葉 初芽" text="けど、透明になる効果が突然切れてしまったので、&#xA;残念ながら、タイプ１は失敗です。" speaker_show="True" stop="True" voice="ah11005_01_0180" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" loop="True" to_idle="False" force_end="False" />
    <msg speaker="半蔵門 雪" text="まだまだ長い道のりが待ってそうね。" speaker_show="True" stop="True" voice="hy11002_01_0381" />
