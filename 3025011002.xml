    <char_spine_animation obj_id="高坂 信" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="高坂 信" text="しょうがないよ。カトさんだってお正月くらい&#xA;ゆっくりお休みしたいだろうし。" speaker_show="True" stop="True" voice="ks11010_03_0295" />
    <char_spine_animation obj_id="相模 楓" face_anim_type="008_Facial_serious" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
