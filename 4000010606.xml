    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="16_BendBackward" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="もぐもぐ……ん？&#xA;わーっ！　な、なんでごえちゃんがここに!?" speaker_show="True" stop="True" voice="ym11003_01_0834" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="101_Facial_trouble" body_anim_type="08_MouthBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ここで買い食いしてたこと、&#xA;フーにはナイショにして……！" speaker_show="True" stop="True" voice="ym11003_01_0835" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="008_Facial_serious" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="メイちゃん、子猫見なかった？" speaker_show="True" stop="True" voice="ig11006_01_0626" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="へっ、子猫？　見なかったけど……。&#xA;子猫探してるの？" speaker_show="True" stop="True" voice="ym11003_01_0836" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="14_Positive" loop="False" to_idle="True" force_end="False" />
    <msg speaker="石川 五恵" text="うん、雨で子猫がどこかにいっちゃって……。&#xA;あ、ごめんね、引き止めて。それじゃ……。" speaker_show="True" stop="True" voice="ig11006_01_0627" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="011_Facial_surprise" body_anim_type="15_Negative" loop="False" to_idle="True" force_end="False" />
    <msg speaker="八千代 命" text="ちょっと待って、ごえちゃん！&#xA;風邪引いちゃうから、とにかく傘に入って！" speaker_show="True" stop="True" voice="ym11003_01_0837" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="う、うん、ありがとう。ごめんね、メイちゃん。" speaker_show="True" stop="True" voice="ig11006_01_0628" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="ううん。寄るところがあるって言ってたの、&#xA;その子猫のことだったの？" speaker_show="True" stop="True" voice="ym11003_01_0838" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="005_Facial_sad" body_anim_type="07_ChestBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="うん。まだすごく小さいから心配で、&#xA;毎日、子猫用のミルクをあげてたの。" speaker_show="True" stop="True" voice="ig11006_01_0629" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="03_BackBothHands" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="そっか。それなら早く&#xA;言ってくれれば良かったのに。" speaker_show="True" stop="True" voice="ym11003_01_0839" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="……ごめんね。&#xA;隠すつもりはなかったんだけど……。" speaker_show="True" stop="True" voice="ig11006_01_0630" />
    <char_spine_animation obj_id="八千代 命" face_anim_type="001_Facial_blink" body_anim_type="13_Question" loop="True" to_idle="False" force_end="False" />
    <msg speaker="八千代 命" text="あれ？　今、あっちのほうに子猫が……。" speaker_show="True" stop="True" voice="ym11003_01_0840" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" loop="True" to_idle="False" force_end="False" />
    <msg speaker="石川 五恵" text="え？　どこ？　ちょっと見てくるね！" speaker_show="True" stop="True" voice="ig11006_01_0631" />
