    <msg speaker="石川 五恵" text="どうしよう、完全に見失っちゃったみたい……。&#xA;雨も強くなってきちゃったし……あれ？" speaker_show="True" stop="True" voice="ig11006_01_0625" />
    <move obj_id="石川 五恵" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.3" wait="True" />
