    <char_spine_animation obj_id="八千代 命" face_anim_type="002_Facial_smile" body_anim_type="09_PutHands" loop="True" />
    <msg speaker="八千代 命" text="は～、商品券の金額ギリギリで&#xA;ヒヤヒヤした～！" speaker_show="True" stop="True" voice="ym11003_02_0713" />
    <char_spine_animation obj_id="石川 五恵" face_anim_type="003_Facial_laugh" body_anim_type="04_HipLeftHand" loop="True" />
