    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="……何度⾒てもスゲーな、その能力。" speaker_show="True" stop="True" voice="ry12002_02_0316" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="015_Facial_poker" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="そういやはすみんは、どういう経緯で&#xA;それ身に付けたんだ？ ⽣まれつき？" speaker_show="True" stop="True" voice="ry12002_02_0317" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="101_Facial_unique" body_anim_type="19_Amazed" force_end="False" loop="False" />
    <msg speaker="轟 葉栖美" text="いやいや、ゆらちゃんじゃないんだから。" speaker_show="True" stop="True" voice="th12004_02_0310" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="015_Facial_poker" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="んーと、かいつまんで話すと、アタシの家、&#xA;父一人子一人だったんだけど、父親がダメダメでさ。" speaker_show="True" stop="True" voice="th12004_02_0311" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="その父や拾ってきた動物の面倒を⾒てるうちに、&#xA;自然と動物とも意思疎通できるようになったんよ。" speaker_show="True" stop="True" voice="th12004_02_0312" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="012_Facial_surpriseOpenMouth" body_anim_type="16_BendBackward" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="それで身につくモンか!?" speaker_show="True" stop="True" voice="ry12002_02_0318" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="016_Facial_smileBitter" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="やっぱ隊⻑や飛粋だけじゃなく、&#xA;はすみんもだいぶぶっ飛んでるよな〜。" speaker_show="True" stop="True" voice="ry12002_02_0319" />
    <char_spine_animation obj_id="轟 葉栖美" body_anim_type="01_Stay" force_end="False" loop="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="008_Facial_serious" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" lip_sync="False" text="（……しかし、ダメ父ね……）" speaker_show="True" stop="True" voice="ry12002_02_0320" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="15_Negative" force_end="False" loop="False" />
    <msg speaker="乱獅子 ゆら" lip_sync="False" text="（アタシ様のダディもなかなかのダメっぷりだけど、&#xA;それとはまたベクトル違いそうだぜ……）" speaker_show="True" stop="True" voice="ry12002_02_0321" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="002_Facial_smile" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="……ほんと、色んな奴がいておもしれーな、&#xA;凪の部隊!!" speaker_show="True" stop="True" voice="ry12002_02_0322" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="005_Facial_sad" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="おお、なんか雑にまとめたな。" speaker_show="True" stop="True" voice="th12004_02_0313" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="016_Facial_smileBitter" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="011_Facial_surprise" body_anim_type="02_Idle" force_end="False" loop="True" />
