    <msg speaker="轟 葉栖美" text="あれ、何してんの？&#xA;ゆらちゃん。" speaker_show="True" stop="True" voice="th12004_02_0305" />
    <char_spine_animation obj_id="乱獅子 ゆら" face_anim_type="009_Facial_blush" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="はすみん！&#xA;いいとこに来た！" speaker_show="True" stop="True" voice="ry12002_02_0314" />
    <char_spine_animation obj_id="乱獅子 ゆら" body_anim_type="12_DropShoulders" force_end="False" loop="True" />
    <msg speaker="乱獅子 ゆら" text="こいつ、なんとかしてくれ！" speaker_show="True" stop="True" voice="ry12002_02_0315" />
    <msg_window_show show="False" />
    <sound type="Se" cue_name="Cat" volume="100" />
    <delete obj_id="乱獅子 ゆら" />
    <move obj_id="轟 葉栖美" position_id="left" x="0" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="003_Facial_laugh" body_anim_type="07_ChestBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="おっ、猫さんだ。&#xA;なになに、どしたん～？" speaker_show="True" stop="True" voice="th12004_02_0306" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="015_Facial_poker" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="……ありゃ、飼い主さんとはぐれちゃったのかー。" speaker_show="True" stop="True" voice="th12004_02_0307" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="008_Facial_serious" body_anim_type="20_PositiveDouble" force_end="False" loop="False" />
    <msg speaker="轟 葉栖美" text="……ふむふむ……飼い主さんと同じ服を着てる&#xA;ゆらちゃんを⾒つけて、思わずついてきちゃったんだ。" speaker_show="True" stop="True" voice="th12004_02_0308" />
    <char_spine_animation obj_id="轟 葉栖美" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="轟 葉栖美" text="ということは、飼い主さんは⿊百合の⽣徒か〜。" speaker_show="True" stop="True" voice="th12004_02_0309" />
    <msg_window_show show="False" />
    <move obj_id="轟 葉栖美" position_id="left" x="200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="0.5" wait="True" />
