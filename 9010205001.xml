  <command>
    <Start />
    <sound type="Bgm" cue_name="Adv1" />
    <bg show="True" file="09_cafe_02" />
    <fade_in time="0.5" wait="True" />
    <create type="Character" obj_id="青葉 初芽" data_id="311005006" position_id="left" x="-200" y="0" anchor="Bottom" />
    <create type="Character" obj_id="半蔵門 雪" data_id="311002006" position_id="left" x="200" y="0" anchor="Bottom" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="03_BackBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="雪ちゃんがこうしてカフェに&#xA;つき合ってくれるなんて、珍しいですね。" speaker_show="True" stop="True" voice="ah11005_02_0787" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="たまにはね。その後、モモの様子はどう？&#xA;何かあるなら報告して。" speaker_show="True" stop="True" voice="hy11002_02_0676" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="うーん、特に報告するようなことは……。&#xA;相変わらずのいい子ですから。" speaker_show="True" stop="True" voice="ah11005_02_0788" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="半蔵門 雪" text="まあ、それはモモだから当然ね。" speaker_show="True" stop="True" voice="hy11002_02_0677" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="あと、私が困っていると&#xA;研究につき合ってくれることもありますよ。" speaker_show="True" stop="True" voice="ah11005_02_0789" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="普通は嫌がるのに面白がって協力してくれて……。&#xA;いい子ですよね、モモちゃんって。" speaker_show="True" stop="True" voice="ah11005_02_0790" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="……素直な子ではあるわ。" speaker_show="True" stop="True" voice="hy11002_02_0678" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="ふふっ、雪ちゃんに褒められたって知ったら、&#xA;モモちゃん大喜びですね。……なんだか羨ましいです。" speaker_show="True" stop="True" voice="ah11005_02_0791" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="02_Idle" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="羨ましい？" speaker_show="True" stop="True" voice="hy11002_02_0679" />
    <char_spine_animation obj_id="青葉 初芽" body_anim_type="14_Positive" force_end="False" loop="False" />
    <msg speaker="青葉 初芽" text="はい。私、雪ちゃんと長いつき合いなのに、&#xA;そんなふうに褒められたことないですから。" speaker_show="True" stop="True" voice="ah11005_02_0792" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そんなことないと思うけど……。" speaker_show="True" stop="True" voice="hy11002_02_0680" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="001_Facial_blink" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そうですか？&#xA;それなら今から私のことも褒めてみてください。" speaker_show="True" stop="True" voice="ah11005_02_0793" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="初芽はいつも便利な発明品を開発してくれている。&#xA;とても助かっているわ。" speaker_show="True" stop="True" voice="hy11002_02_0681" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="011_Facial_surprise" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="えっ、本当ですか？&#xA;そんなふうに言ってもらえて嬉しいです。ふふっ。" speaker_show="True" stop="True" voice="ah11005_02_0794" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="では、この調子であと10個くらい褒めてみてください。" speaker_show="True" stop="True" voice="ah11005_02_0795" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="008_Facial_serious" body_anim_type="13_Question" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="そうね、頭がいいのはもちろんだけど、&#xA;友達想いの優しいところとか……。" speaker_show="True" stop="True" voice="hy11002_02_0682" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="あとはみんなで高い志を目指そうとするところとか、&#xA;他には……。" speaker_show="True" stop="True" voice="hy11002_02_0683" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek02_medium" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="101_Facial_trouble" body_anim_type="10_FaceBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="わーっ！　わかりました、雪ちゃん！&#xA;もう、いいです！　充分です！" speaker_show="True" stop="True" voice="ah11005_02_0796" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="001_Facial_blink" body_anim_type="04_HipLeftHand" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="え、どうしたの？　まだ10個言ってないわよ。&#xA;それに、そんなに顔が赤くなって……。" speaker_show="True" stop="True" voice="hy11002_02_0684" />
    <char_spine_cheek obj_id="青葉 初芽" type="Cheek01_default" loop="False" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="08_MouthBothHands" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="だって、即答してくれるかもとは思っていましたけど、&#xA;そこまで褒めてくれるなんて……すごく嬉しいですっ！" speaker_show="True" stop="True" voice="ah11005_02_0797" />
    <move obj_id="青葉 初芽" position_id="left" x="100" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1" wait="True" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="011_Facial_surprise" body_anim_type="05_HipBothHands" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="ちょっ……初芽……。&#xA;こんなところで抱きついたら危な……！" speaker_show="True" stop="True" voice="hy11002_02_0685" />
    <move obj_id="青葉 初芽" position_id="left" x="-200" x_move_type="MoveTo" y="0" y_move_type="MoveValue" time="1" wait="True" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="002_Facial_smile" body_anim_type="06_ChestLeftHand" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="あ、すみません。嬉しくてつい……。" speaker_show="True" stop="True" voice="ah11005_02_0798" />
    <char_spine_animation obj_id="半蔵門 雪" face_anim_type="002_Facial_smile" body_anim_type="18_ArmsCrossed" force_end="False" loop="True" />
    <msg speaker="半蔵門 雪" text="もう、大げさよ。" speaker_show="True" stop="True" voice="hy11002_02_0686" />
    <char_spine_animation obj_id="青葉 初芽" face_anim_type="003_Facial_laugh" body_anim_type="11_Point" force_end="False" loop="True" />
    <msg speaker="青葉 初芽" text="そんなことないですよ。それじゃ、次からは&#xA;100個言ってもらうことにしますね。" speaker_show="True" stop="True" voice="ah11005_02_0799" />
  </command>
